{
  if (place.getEmployeeId() == null) {
    findDepartmentOrEmployee(place.getDepartment(),null);
  }
 else {
    requestFactory.find(place.getEmployeeId()).fire(new Receiver<EmployeeProxy>(){
      @Override public void onSuccess(      EmployeeProxy response){
        findDepartmentOrEmployee("",response);
      }
    }
);
  }
}
