{
  writeIfInstanceofBeanType(sw,bean);
  sw.indent();
  writeContext(sw,bean,"object");
  sw.print("return ");
  sw.println(bean.getFullyQualifiedValidatorName() + ".INSTANCE");
  sw.indent();
  sw.indent();
  sw.print(".validate(context, ");
  sw.print("(" + bean.getTypeCanonicalName() + ") object, ");
  sw.println("groups);");
  sw.outdent();
  sw.outdent();
  sw.outdent();
  sw.println("}");
}
