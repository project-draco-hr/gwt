{
  writeIfInstanceofBeanType(sw,bean);
  sw.indent();
  writeContext(sw,bean,"object");
  sw.print("return ");
  sw.print(bean.getValidatorInstanceName() + ".validate(");
  sw.print("context, ");
  sw.print("(" + bean.getTypeCanonicalName() + ") object, ");
  sw.println("groups);");
  sw.outdent();
  sw.println("}");
}
