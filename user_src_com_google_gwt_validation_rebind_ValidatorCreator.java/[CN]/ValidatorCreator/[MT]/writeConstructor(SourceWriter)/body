{
  sw.println("public " + getSimpleName() + "() {");
  sw.indent();
  sw.print("super(");
  boolean first=true;
  for (  Class<?> group : gwtValidation.groups()) {
    if (!first) {
      sw.print(", ");
    }
 else {
      first=false;
    }
    sw.print(group.getCanonicalName() + ".class");
  }
  sw.println(");");
  sw.outdent();
  sw.println("}");
}
