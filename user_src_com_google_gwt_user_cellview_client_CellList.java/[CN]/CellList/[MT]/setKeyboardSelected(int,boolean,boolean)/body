{
  if (!isRowWithinBounds(index)) {
    return;
  }
  Element elem=getRowElement(index);
  if (!selected || isFocused || stealFocus) {
    setStyleName(elem,style.cellListKeyboardSelectedItem(),selected);
  }
  setFocusable(elem,selected);
  if (selected && stealFocus) {
    elem.focus();
    onFocus();
  }
}
