{
  try {
    if (createMessagesArg) {
      createMessagesInterfaceFromClassName(classNameArg,outDirArg,Messages.class);
    }
 else {
      createConstantsInterfaceFromClassName(classNameArg,outDirArg,Constants.class);
    }
    return true;
  }
 catch (  Throwable e) {
    System.err.println(e.getMessage());
    return false;
  }
}
