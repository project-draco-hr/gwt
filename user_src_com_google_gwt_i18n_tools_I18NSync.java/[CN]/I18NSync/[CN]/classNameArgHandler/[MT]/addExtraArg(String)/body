{
  if (classNameArg != null) {
    System.err.println("Too many arguments.");
    return false;
  }
  try {
    File resourceFile=urlToResourceFile(str);
    checkValidResourceInputFile(resourceFile);
    classNameArg=str;
  }
 catch (  IOException e) {
    System.err.println("Error: " + e.getMessage());
    return false;
  }
  return true;
}
