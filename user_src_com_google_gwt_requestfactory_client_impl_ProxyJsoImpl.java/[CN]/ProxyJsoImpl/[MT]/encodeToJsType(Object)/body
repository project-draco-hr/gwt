{
  if (o instanceof BigDecimal || o instanceof BigInteger || o instanceof Long|| o instanceof String|| o instanceof Character) {
    return o.toString();
  }
 else   if (o instanceof Number) {
    return ((Number)o).doubleValue();
  }
 else   if (o instanceof Date) {
    return String.valueOf(((Date)o).getTime());
  }
 else   if (o instanceof ProxyImpl) {
    return ((ProxyImpl)o).getWireFormatId();
  }
 else   if (o instanceof Enum) {
    return (double)((Enum)o).ordinal();
  }
  return o;
}
