{
  CssRule r;
  if (nextSelectorCreatesRule) {
    r=new CssRule();
    addNode(r);
    currentRule=r;
  }
 else {
    r=(CssRule)currentRule;
    nextSelectorCreatesRule=true;
  }
  for (int i=0; i < selectors.getLength(); i++) {
    r.getSelectors().add(new CssSelector(valueOf(selectors.item(i))));
  }
}
