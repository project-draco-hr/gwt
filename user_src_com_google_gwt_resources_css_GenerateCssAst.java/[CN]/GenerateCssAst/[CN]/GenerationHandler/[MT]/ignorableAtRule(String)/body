{
  String ruleName=atRule.substring(1,atRule.indexOf(" "));
  String methodName="parse" + (Character.toUpperCase(ruleName.charAt(0))) + ruleName.substring(1);
  try {
    Method parseMethod=getClass().getDeclaredMethod(methodName,String.class);
    parseMethod.invoke(this,atRule);
  }
 catch (  NoSuchMethodException e) {
    errors.log(TreeLogger.WARN,"Ignoring @" + ruleName);
  }
catch (  IllegalAccessException e) {
    errors.log(TreeLogger.ERROR,"Unable to invoke parse method ",e);
  }
catch (  InvocationTargetException e) {
    Throwable cause=e.getCause();
    if (cause instanceof CSSException) {
      throw (CSSException)cause;
    }
 else     if (cause != null) {
      TreeLogger details=errors.branch(TreeLogger.ERROR,cause.getMessage());
      details.log(TreeLogger.DEBUG,"Full stack trace",cause);
    }
 else {
      TreeLogger details=errors.branch(TreeLogger.ERROR,"Unknown failure parsing " + ruleName);
      details.log(TreeLogger.DEBUG,"Full stack trace",e);
    }
  }
}
