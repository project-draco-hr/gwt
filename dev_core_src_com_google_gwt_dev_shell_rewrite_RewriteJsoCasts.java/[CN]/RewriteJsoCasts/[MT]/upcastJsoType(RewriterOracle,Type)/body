{
  StringBuilder sb=new StringBuilder();
  if (type.getSort() == Type.ARRAY) {
    Type elementType=type.getElementType();
    if (elementType.getSort() != Type.OBJECT || !rewriterOracle.isJsoOrSubtype(elementType.getInternalName())) {
      return null;
    }
    for (int i=0, j=type.getDimensions(); i < j; i++) {
      sb.append("[");
    }
  }
 else   if (type.getSort() == Type.OBJECT) {
    if (type.getInternalName().equals(JAVASCRIPTOBJECT_DESC) || !rewriterOracle.isJsoOrSubtype(type.getInternalName())) {
      return null;
    }
  }
 else {
    return null;
  }
  sb.append("L" + JAVASCRIPTOBJECT_DESC + ";");
  return Type.getType(sb.toString());
}
