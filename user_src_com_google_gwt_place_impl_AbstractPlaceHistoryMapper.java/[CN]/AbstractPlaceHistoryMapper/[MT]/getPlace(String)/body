{
  int colonAt=token.indexOf(':');
  if (colonAt > 0) {
    String initial=token.substring(0,colonAt);
    String rest=token.substring(colonAt + 1);
    PlaceTokenizer<?> tokenizer=getTokenizer(initial);
    if (tokenizer != null) {
      return tokenizer.getPlace(rest);
    }
  }
  return null;
}
