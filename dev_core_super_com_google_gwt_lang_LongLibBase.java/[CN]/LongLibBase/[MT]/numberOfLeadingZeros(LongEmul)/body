{
  int b2=Integer.numberOfLeadingZeros(getH(a));
  if (b2 == 32) {
    int b1=Integer.numberOfLeadingZeros(getM(a));
    if (b1 == 32) {
      return Integer.numberOfLeadingZeros(getL(a)) + BITS2 + 2 * BITS - 32;
    }
 else {
      return b1 + BITS2 - (32 - BITS);
    }
  }
 else {
    return b2 - (32 - BITS2);
  }
}
