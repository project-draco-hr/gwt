{
  int a1=getM(a);
  int a2=getH(a);
  int a0=getL(a);
  if (RUN_IN_JVM) {
    a.h=a2 >>> 1;
    a.m=(a1 >>> 1) | ((a2 & 0x1) << (BITS - 1));
    a.l=(a0 >>> 1) | ((a1 & 0x1) << (BITS - 1));
  }
 else {
    setH(a,a2 >>> 1);
    setM(a,(a1 >>> 1) | ((a2 & 0x1) << (BITS - 1)));
    setL(a,(a0 >>> 1) | ((a1 & 0x1) << (BITS - 1)));
  }
}
