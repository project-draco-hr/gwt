{
  n&=63;
  int res0, res1, res2;
  int a2=getH(a) & MASK_2;
  if (n < BITS) {
    res2=a2 >>> n;
    res1=(getM(a) >> n) | (a2 << (BITS - n));
    res0=(getL(a) >> n) | (getM(a) << (BITS - n));
  }
 else   if (n < BITS01) {
    res2=0;
    res1=a2 >>> (n - BITS);
    res0=(getM(a) >> (n - BITS)) | (getH(a) << (BITS01 - n));
  }
 else {
    res2=0;
    res1=0;
    res0=a2 >>> (n - BITS01);
  }
  return create(res0 & MASK,res1 & MASK,res2 & MASK_2);
}
