{
  if (!hasStorageEventHandlers()) {
    return;
  }
  UncaughtExceptionHandler ueh=GWT.getUncaughtExceptionHandler();
  for (  StorageEvent.Handler handler : storageEventHandlers) {
    if (ueh != null) {
      try {
        handler.onStorageChange(event);
      }
 catch (      Throwable t) {
        ueh.onUncaughtException(t);
      }
    }
 else {
      handler.onStorageChange(event);
    }
  }
}
