{
  if (disableCastChecking) {
    return;
  }
  for (  JDeclaredType declaredType : program.getModuleDeclaredTypes()) {
    createCompleteCastMap(declaredType);
  }
  for (  JArrayType arrayType : program.getAllArrayTypes()) {
    if (program.typeOracle.isInstantiatedType(arrayType)) {
      createCompleteCastMap(arrayType);
    }
  }
  program.initTypeInfo(castMaps,new JCastMap(SourceOrigin.UNKNOWN,program.getTypeJavaLangObject(),(Set)ImmutableSortedSet.orderedBy(HasName.BY_NAME_COMPARATOR).add(program.getTypeJavaLangObject()).add(program.getTypeJavaLangCloneable()).add(program.getTypeJavaIoSerializable()).build()));
}
