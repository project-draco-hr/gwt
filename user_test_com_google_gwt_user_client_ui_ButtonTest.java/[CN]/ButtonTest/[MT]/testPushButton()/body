{
  FormPanel f=new FormPanel();
  f.setAction("javascript:''");
  RootPanel.get().add(f);
  Button b=new Button();
  f.setWidget(b);
  final H2 h=new H2();
  f.addSubmitHandler(h);
  delayTestFinish(5000);
  new Timer(){
    @Override public void run(){
      assertFalse(h.submitted);
      finishTest();
    }
  }
.schedule(2500);
  b.click();
}
