{
  HasName node=x.getNode();
  JReferenceType isType=node instanceof JReferenceType ? (JReferenceType)node : null;
  if (isType == null && !referencedNonTypes.contains(node)) {
    ctx.replaceMe(program.getLiteralNull());
  }
 else   if (isType != null && !program.typeOracle.isInstantiatedType(isType)) {
    ctx.replaceMe(program.getLiteralNull());
  }
}
