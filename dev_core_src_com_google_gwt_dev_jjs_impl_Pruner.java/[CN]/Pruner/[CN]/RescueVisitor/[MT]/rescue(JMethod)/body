{
  if (method != null) {
    if (!referencedNonTypes.contains(method)) {
      referencedNonTypes.add(method);
      accept(method);
      if (method instanceof JsniMethod) {
        maybeRescueJavaScriptObjectPassingIntoJava(method.getType());
      }
      return true;
    }
  }
  return false;
}
