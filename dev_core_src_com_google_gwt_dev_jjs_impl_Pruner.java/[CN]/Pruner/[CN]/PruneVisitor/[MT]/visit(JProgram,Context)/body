{
  for (Iterator it=program.getDeclaredTypes().iterator(); it.hasNext(); ) {
    JReferenceType type=(JReferenceType)it.next();
    if (referencedTypes.contains(type) || program.typeOracle.isInstantiatedType(type)) {
      accept(type);
    }
 else {
      it.remove();
      didChange=true;
    }
  }
  return false;
}
