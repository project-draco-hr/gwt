{
switch (event.getTypeInt()) {
case Event.ONMOUSEDOWN:
    if (isCloseBoxEvent(event)) {
      maybeClose=true;
      return;
    }
case Event.ONMOUSEUP:
  if (maybeClose && isCloseBoxEvent(event)) {
    maybeClose=false;
    hide();
    return;
  }
}
maybeClose=false;
super.onBrowserEvent(event);
}
