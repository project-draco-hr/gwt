{
  delayTestFinish(5000);
  req.simpleFooRequest().findAll().with("barField.userName").fire(new Receiver<List<SimpleFooProxy>>(){
    @Override public void onSuccess(    List<SimpleFooProxy> response){
      assertEquals(1,response.size());
      for (      SimpleFooProxy foo : response) {
        assertNotNull(foo.stableId());
        assertEquals("FOO",foo.getBarField().getUserName());
        finishTestAndReset();
      }
    }
  }
);
}
