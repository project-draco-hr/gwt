{
  delayTestFinish(5000);
  req.simpleFooRequest().findSimpleFooById(999L).fire(new Receiver<SimpleFooProxy>(){
    @Override public void onSuccess(    SimpleFooProxy fooProxy){
      Request<SimpleFooProxy> updReq=req.simpleFooRequest().persistAndReturnSelf(fooProxy);
      fooProxy=updReq.edit(fooProxy);
      final int oldValue=fooProxy.getNumberListField().remove(0);
      updReq.fire(new Receiver<SimpleFooProxy>(){
        @Override public void onSuccess(        SimpleFooProxy response){
          assertFalse(response.getNumberListField().contains(oldValue));
          finishTestAndReset();
        }
      }
);
    }
  }
);
}
