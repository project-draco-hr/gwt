{
  delayTestFinish(5000);
  @SuppressWarnings("deprecation") final Date date=new Date(90,0,1);
  Request<Date> procReq=simpleFooRequest().processDateList(Arrays.asList(date));
  procReq.fire(new Receiver<Date>(){
    @Override public void onSuccess(    Date response){
      assertEquals(date,response);
      finishTestAndReset();
    }
  }
);
}
