{
  delayTestFinish(DELAY_TEST_FINISH);
  req.userInformationRequest().getCurrentUserInformation("").fire(new Receiver<UserInformationProxy>(){
    @Override public void onSuccess(    UserInformationProxy response){
      response=checkSerialization(response);
      assertEquals("Dummy Email",response.getEmail());
      assertEquals("Dummy User",response.getName());
      assertEquals("",response.getLoginUrl());
      assertEquals("",response.getLogoutUrl());
      finishTestAndReset();
    }
  }
);
}
