{
  SimpleFooRequest context=simpleFooRequest();
  assertFalse(context.isChanged());
  SimpleFooProxy foo=context.create(SimpleFooProxy.class);
  assertFalse(context.isChanged());
  foo.setCharField('c');
  assertTrue(context.isChanged());
  foo.setCharField(null);
  assertFalse(context.isChanged());
}
