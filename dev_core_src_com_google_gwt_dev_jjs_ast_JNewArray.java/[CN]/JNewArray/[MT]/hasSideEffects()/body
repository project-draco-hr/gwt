{
  if (initializers != null) {
    for (int i=0, c=initializers.size(); i < c; ++i) {
      if (initializers.get(i).hasSideEffects()) {
        return true;
      }
    }
  }
  if (dims != null) {
    for (int i=0, c=dims.size(); i < c; ++i) {
      if (dims.get(i).hasSideEffects()) {
        return true;
      }
    }
  }
  return false;
}
