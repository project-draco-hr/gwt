{
  if (initializers != null) {
    for (int i=0, c=initializers.size(); i < c; ++i) {
      if (((JExpression)initializers.get(i)).hasSideEffects()) {
        return true;
      }
    }
  }
  if (dims != null) {
    for (int i=0, c=dims.size(); i < c; ++i) {
      if (((JExpression)dims.get(i)).hasSideEffects()) {
        return true;
      }
    }
  }
  return false;
}
