{
  if (initializers != null) {
    for (    JExpression initializer : initializers) {
      if (initializer.hasSideEffects()) {
        return true;
      }
    }
  }
  if (dims != null) {
    for (    JExpression dim : dims) {
      if (dim.hasSideEffects()) {
        return true;
      }
    }
  }
  return false;
}
