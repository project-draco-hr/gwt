{
  SourceInfo nullSourceInfo=new MockSourceInfo();
  JsInvocation defineSeedInvocation=new JsInvocation(nullSourceInfo);
  defineSeedInvocation.getArguments().add(new JsNameRef(nullSourceInfo,barConstructorName));
