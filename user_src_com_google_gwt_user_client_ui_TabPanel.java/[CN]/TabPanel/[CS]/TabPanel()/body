{
  VerticalPanel panel=new VerticalPanel();
  panel.add(tabBar);
  panel.add(deck);
  panel.setCellHeight(deck,"100%");
  tabBar.setWidth("100%");
  tabBar.addTabListener(this);
  initWidget(panel);
  setStyleName("gwt-TabPanel");
  deck.setStyleName("gwt-TabPanelBottom");
  Roles.getTabpanelRole().set(deck.getElement());
}
