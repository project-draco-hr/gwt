{
  String fieldName=writer.declareFieldIfNeeded(elem);
  if (fieldName != null) {
    String token=writer.declareDomField(fieldName);
    if (elem.hasAttribute("id")) {
      writer.die(String.format("Cannot declare id=\"%s\" and %s=\"%s\" on the same element",elem.consumeRawAttribute("id"),writer.getUiFieldAttributeName(),fieldName));
    }
    elem.setAttribute("id",token);
  }
  return null;
}
