{
  V previous;
  if (key instanceof String) {
    previous=removeStringValue(stringMap,(String)key);
  }
 else   if (key == null) {
    previous=nullSlot;
    nullSlot=(V)UNDEFINED;
  }
 else {
    previous=removeHashValue(hashCodeMap,key,key.hashCode());
  }
  if (previous == UNDEFINED) {
    return null;
  }
 else {
    --size;
    return previous;
  }
}
