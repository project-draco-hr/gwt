{
  if (initialCapacity < 0 || loadFactor <= 0) {
    throw new IllegalArgumentException("initial capacity was negative or load factor was non-positive");
  }
  if (initialCapacity == 0) {
    initialCapacity=1;
  }
  if (loadFactor > 0.9f) {
    loadFactor=0.9f;
  }
  this.loadFactor=loadFactor;
  realloc(initialCapacity);
}
