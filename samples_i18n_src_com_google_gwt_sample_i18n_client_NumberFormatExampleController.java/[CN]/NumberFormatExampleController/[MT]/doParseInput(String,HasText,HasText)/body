{
  if (!"".equals(toParse)) {
    try {
      double x=Double.parseDouble(toParse);
      String s=activeFormat.format(x);
      output.setText(s);
    }
 catch (    NumberFormatException e) {
      String errMsg=constants.failedToParseInput();
      error.setText(errMsg);
    }
  }
 else {
    output.setText("<None>");
  }
}
