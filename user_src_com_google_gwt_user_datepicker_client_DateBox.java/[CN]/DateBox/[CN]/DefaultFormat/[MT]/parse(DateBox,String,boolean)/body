{
  Date date=null;
  try {
    if (dateText.length() > 0) {
      date=dateTimeFormat.parse(dateText);
    }
  }
 catch (  IllegalArgumentException exception) {
    try {
      date=new Date(dateText);
    }
 catch (    IllegalArgumentException e) {
      if (reportError) {
        dateBox.addStyleName(DATE_BOX_FORMAT_ERROR);
      }
      return null;
    }
  }
  return date;
}
