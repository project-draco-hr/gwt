{
  StringBuilder out=new StringBuilder();
  for (  BindingProperty prop : getOrderedProps()) {
    if (out.length() > 0) {
      out.append(",");
    }
    String name=prop.getName();
    out.append(name);
    out.append("=");
    out.append(getString(name,"(missing)"));
  }
  return out.toString();
}
