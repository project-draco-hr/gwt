{
  StringBuilder out=new StringBuilder();
  out.append("SoftProps(");
  for (int i=0, j=orderedProps.length; i < j; i++) {
    if (out.length() > 0) {
      out.append(" ");
    }
    out.append(orderedProps[i].getName());
    out.append(" = ");
    out.append(orderedPropValues[i]);
  }
  out.append(")");
  return out.toString();
}
