def fixReservedKeyWords(self, name):
    if (name in ['continue', 'delete', 'for', 'while', 'do', 'class', 'switch', 'try', 'catch', 'finally', 'int', 'float', 'short', 'double', 'char', 'byte', 'long', 'implements', 'extends', 'throws', 'case', 'if', 'else', 'throw', 'instanceof', 'true', 'false', 'default', 'null', 'abstract', 'package', 'super', 'public', 'protected', 'private', 'volatile', 'transient', 'final', 'synchronized', 'import', 'enum', 'boolean', 'assert']):
        return ('_' + name)
    return name
