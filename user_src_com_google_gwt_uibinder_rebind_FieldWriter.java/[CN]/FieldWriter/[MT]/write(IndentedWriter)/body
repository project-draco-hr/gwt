{
  if (written) {
    return true;
  }
  for (  FieldWriter f : needs) {
    f.write(w);
  }
  if (initializer == null) {
    if (type.findConstructor(new JType[0]) == null) {
      return false;
    }
    initializer=String.format("(%1$s) GWT.create(%1$s.class)",getFullTypeName());
  }
  w.write("%s %s = %s;",getFullTypeName(),name,initializer);
  this.written=true;
  return true;
}
