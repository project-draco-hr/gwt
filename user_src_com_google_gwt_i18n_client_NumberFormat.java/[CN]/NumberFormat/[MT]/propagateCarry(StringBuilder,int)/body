{
  boolean carry=true;
  while (carry && i >= 0) {
    char digit=digits.charAt(i);
    if (digit == '9') {
      digits.setCharAt(i--,'0');
    }
 else {
      digits.setCharAt(i,(char)(digit + 1));
      carry=false;
    }
  }
  if (carry) {
    digits.insert(0,'1');
    ++decimalPosition;
    ++digitsLength;
  }
}
