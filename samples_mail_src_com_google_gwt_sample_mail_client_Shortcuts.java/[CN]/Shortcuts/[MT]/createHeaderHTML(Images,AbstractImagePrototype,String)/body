{
  boolean isTop=(nextHeaderIndex == 0);
  String cssId=computeHeaderId(nextHeaderIndex);
  nextHeaderIndex++;
  String captionHTML="<table class='caption' cellpadding='0' cellspacing='0'>" + "<tr><td class='lcaption'>" + imageProto.getHTML() + "</td><td class='rcaption'><b style='white-space:nowrap'>"+ caption+ "</b></td></tr></table>";
  return "<table id='" + cssId + "' align='left' cellpadding='0' cellspacing='0'"+ (isTop ? " class='is-top'" : "")+ "><tbody>"+ "<tr><td class='box-00'>"+ images.leftCorner().getHTML()+ "</td>"+ "<td class='box-10'>&nbsp;</td>"+ "<td class='box-20'>"+ images.rightCorner().getHTML()+ "</td>"+ "</tr><tr>"+ "<td class='box-01'>&nbsp;</td>"+ "<td class='box-11'>"+ captionHTML+ "</td>"+ "<td class='box-21'>&nbsp;</td>"+ "</tr></tbody></table>";
}
