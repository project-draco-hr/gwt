{
{
    ListBox lb=new ListBox();
    lb.addItem("a");
    lb.addItem("c");
    lb.insertItem("b",1);
    assertEquals("a",lb.getItemText(0));
    assertEquals("b",lb.getItemText(1));
    assertEquals("c",lb.getItemText(2));
  }
{
    ListBox lb=new ListBox();
    lb.addItem("b");
    lb.addItem("c");
    lb.insertItem("a",0);
    assertEquals("a",lb.getItemText(0));
    assertEquals("b",lb.getItemText(1));
    assertEquals("c",lb.getItemText(2));
  }
{
    ListBox lb=new ListBox();
    lb.addItem("a");
    lb.addItem("b");
    lb.insertItem("c",-1);
    assertEquals("a",lb.getItemText(0));
    assertEquals("b",lb.getItemText(1));
    assertEquals("c",lb.getItemText(2));
  }
{
    ListBox lb=new ListBox();
    lb.addItem("a");
    lb.addItem("b");
    lb.insertItem("c",2);
    assertEquals("a",lb.getItemText(0));
    assertEquals("b",lb.getItemText(1));
    assertEquals("c",lb.getItemText(2));
  }
}
