{
  StringBuilder sb=new StringBuilder("{");
  boolean first=true;
  String[] keys=computeKeys();
  for (  String key : keys) {
    if (first) {
      first=false;
    }
 else {
      sb.append(", ");
    }
    sb.append(JsonUtils.escapeValue(key));
    sb.append(":");
    sb.append(get(key));
  }
  sb.append("}");
  return sb.toString();
}
