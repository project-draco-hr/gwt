{
  String x="hi";
  assertEquals("hi",x);
  assertEquals("hi",x.toString());
  x=new String();
  assertEquals("",x);
  x=new String(x);
  assertEquals("",x);
  x=new String("hi");
  assertEquals("hi",x);
  assertEquals('i',x.charAt(1));
  assertEquals("hiyay",x.concat("yay"));
  assertEquals("hihi",x + x);
  assertEquals("blahcom.google.gwt.dev.jjs.test.MiscellaneousTestabctruefalsenullc27",("blah" + this + String.valueOf(new char[]{'a','b','c'})+ true+ false+ null+ 'c'+ 27));
}
