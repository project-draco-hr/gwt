{
  if (instance == null) {
    writeString(null);
    return;
  }
  int objIndex=getIndexForObject(instance);
  if (objIndex >= 0) {
    writeInt(-(objIndex + 1));
    return;
  }
  saveIndexForObject(instance);
  String typeSignature=getObjectTypeSignature(instance);
  writeString(typeSignature);
  serialize(instance,typeSignature);
}
