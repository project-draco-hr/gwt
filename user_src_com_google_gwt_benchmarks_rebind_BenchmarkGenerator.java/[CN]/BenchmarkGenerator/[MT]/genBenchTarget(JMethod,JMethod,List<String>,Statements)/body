{
  Statements statements=new StatementsList();
  List<Statements> statementsList=statements.getStatements();
  if (beginMethod != null) {
    statementsList.add(new Statement(new MethodCall(beginMethod.getName(),paramNames)));
  }
  statementsList.add(test);
  if (endMethod != null) {
    statementsList.add(new Statement(new MethodCall(endMethod.getName(),null)));
  }
  return statements;
}
