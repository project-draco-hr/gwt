{
  print(".append(");
  if (htmlContext.getType() == HtmlContext.Type.TEXT) {
    emitTextContextParameterExpression(formalParameterName,parameterType);
  }
 else   if (htmlContext.getType() == HtmlContext.Type.ATTRIBUTE || htmlContext.getType() == HtmlContext.Type.ATTRIBUTE_START) {
    emitAttributeContextParameterExpression(logger,htmlContext,formalParameterName,parameterType);
  }
 else {
    throw new IllegalStateException("unknown HTML context for formal template parameter " + formalParameterName + ": "+ htmlContext);
  }
  println(")");
}
