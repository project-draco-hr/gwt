{
  assertFalse(!true);
  assertTrue(!false);
  assertTrue(!(noOptimizeTrue() == noOptimizeFalse()));
  assertFalse(!(noOptimizeTrue() != noOptimizeFalse()));
  assertFalse(!(3 < 4));
  assertFalse(!(3 <= 4));
  assertTrue(!(3 > 4));
  assertTrue(!(3 >= 4));
  assertTrue(!(4 < 3));
  assertTrue(!(4 <= 3));
  assertFalse(!(4 > 3));
  assertFalse(!(4 >= 3));
  assertTrue(!!noOptimizeTrue());
  assertFalse(!!noOptimizeFalse());
}
