{
  assertTrue(TRUE ? TRUE : FALSE);
  assertFalse(FALSE ? TRUE : FALSE);
  assertFalse(TRUE ? FALSE : TRUE);
  assertTrue(FALSE ? FALSE : TRUE);
  assertTrue(true ? TRUE : FALSE);
  assertFalse(false ? TRUE : FALSE);
  assertFalse(true ? FALSE : TRUE);
  assertTrue(false ? FALSE : TRUE);
  assertTrue(TRUE ? true : FALSE);
  assertFalse(FALSE ? true : FALSE);
  assertFalse(TRUE ? false : TRUE);
  assertTrue(FALSE ? false : TRUE);
  assertTrue(TRUE ? TRUE : false);
  assertFalse(FALSE ? TRUE : false);
  assertFalse(TRUE ? FALSE : true);
  assertTrue(FALSE ? FALSE : true);
}
