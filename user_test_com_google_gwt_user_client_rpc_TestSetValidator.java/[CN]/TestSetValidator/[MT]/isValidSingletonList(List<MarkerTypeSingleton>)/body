{
  if (list == null || list.size() != 1) {
    return false;
  }
  Object value=list.get(0);
  if (!(value instanceof MarkerTypeSingleton)) {
    return false;
  }
  MarkerTypeSingleton singleton=(MarkerTypeSingleton)value;
  if (!"singleton".equals(singleton.getValue())) {
    return false;
  }
  return true;
}
