{
  assertEquals(msg + " (text-align property value is incorrect)",expected.getTextAlignString(),label.getElement().getStyle().getTextAlign());
  assertEquals(msg + " (getHorizontalAlignment return value is incorrect)",expected,label.getHorizontalAlignment());
  assertEquals(msg + " (getAutoHorizontalAlignment return value is incorrect)",expectedAuto,label.getAutoHorizontalAlignment());
}
