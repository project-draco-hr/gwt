def test_sequence(self):
    sequence = SEQUENCE('A', 'BB', 'C')
    self._run_test(grammar=sequence, text='ABBC', expected=['A', 'BB', 'C'])
    self._run_test(grammar=sequence, text='BBAC', expected=None)
    sequence = ['A', 'BB', 'C']
    self._run_test(grammar=sequence, text='ABBC', expected=['A', 'BB', 'C'])
    self._run_test(grammar=sequence, text='BBAC', expected=None)
