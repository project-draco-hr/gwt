def __init__(self, ast=None):
    IDLDictNode.__init__(self, ast)
    self.id = None
    if (not ast):
        return
    for arg in self._find_all(ast, 'AnnotationArg'):
        name = self._find_first(arg, 'Id')
        value = self._find_first(arg, 'AnnotationArgValue')
        self[name] = value
