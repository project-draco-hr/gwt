{
  assertNotNull(ctx.create(shouldWork));
  try {
    if (GWT.isClient()) {
      ctx.create(shouldFail);
      fail();
    }
 else {
      assertNotNull(ctx.create(shouldFail));
    }
  }
 catch (  IllegalArgumentException expected) {
    if (!GWT.isClient()) {
      fail("Expect the create call to always work in RFSource implementation");
    }
  }
}
