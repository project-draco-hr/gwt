{
  if (req instanceof HttpServletRequest) {
    HttpServletRequest request=(HttpServletRequest)req;
    String pathInfo=request.getRequestURI();
    logger.log(TreeLogger.TRACE,"Request for: " + pathInfo);
    ModuleDef moduleDef=autogenScripts.get(pathInfo);
    if (moduleDef != null) {
      if (req.getParameter("compiled") == null) {
        try {
          hostedModeLink(logger.branch(TreeLogger.TRACE,"Request for '" + pathInfo + "' maps to script generator for module '"+ moduleDef.getName()+ "'"),moduleDef);
        }
 catch (        UnableToCompleteException e) {
        }
      }
    }
  }
  chain.doFilter(req,resp);
}
