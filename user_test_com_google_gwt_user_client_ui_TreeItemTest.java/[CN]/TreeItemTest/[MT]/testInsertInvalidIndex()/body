{
  TreeItem item=new TreeItem();
  item.addItem("a");
  item.addItem("b");
  item.addItem("c");
  try {
    item.insertItem(-1,"illegal");
    fail("Expected IndexOutOfBoundsException");
  }
 catch (  IndexOutOfBoundsException e) {
  }
  try {
    item.insertItem(4,"illegal");
    fail("Expected IndexOutOfBoundsException");
  }
 catch (  IndexOutOfBoundsException e) {
  }
}
