{
  TreeItem item=new TreeItem();
  TreeItem a=item.addItem("a");
  item.addItem("b");
  item.addItem("c");
  item.insertItem(2,a);
  assertNull(a.getElement().getNextSiblingElement());
  try {
    item.insertItem(3,a);
    fail("Expected IndexOutOfBoundsException");
  }
 catch (  IndexOutOfBoundsException e) {
  }
}
