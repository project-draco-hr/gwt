{
  if (type.getSort() != Type.OBJECT) {
    return true;
  }
  if (valueTypes.contains(type)) {
    return true;
  }
  logger=logger.setType(type);
  List<Type> types=getSupertypes(logger,type);
  for (  Type t : types) {
    if (valueTypes.contains(t)) {
      valueTypes.add(type);
      return true;
    }
  }
  return false;
}
