{
  return new ValueStoreListViewAdapter<K>(){
    @Override protected void onRangeChanged(    ListView<Values<K>> view){
      Range range=view.getRange();
      Delegate myDelegate=getDelegate();
      if (myDelegate != null) {
        myDelegate.onRangeChanged(range.getStart(),range.getLength());
      }
    }
  }
;
}
