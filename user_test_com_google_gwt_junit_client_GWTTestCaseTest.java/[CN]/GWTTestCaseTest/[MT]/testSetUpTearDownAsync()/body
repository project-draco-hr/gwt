{
  assertSame(IS_SETUP,setupTeardownFlag);
  delayTestFinish(1000);
  new Timer(){
    @Override public void run(){
      assertSame(IS_SETUP,setupTeardownFlag);
      finishTest();
      if (setupTeardownFlag != IS_TORNDOWN) {
        recordOutofBandError("Bad async success tearDown behavior not catchable by JUnit");
      }
    }
  }
.schedule(1);
}
