{
  SourceInfo info=x.getSourceInfo();
  if (!instrumentableLines.containsEntry(info.getFileName(),info.getStartLine())) {
    return;
  }
  JsInvocation update=new JsInvocation(info,jsProgram.getIndexedFunction("CoverageUtil.cover"),new JsStringLiteral(info,info.getFileName()),new JsNumberLiteral(info,info.getStartLine()));
  ctx.replaceMe(new JsBinaryOperation(info,JsBinaryOperator.COMMA,update,x));
}
