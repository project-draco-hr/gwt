{
  logger.log(TreeLogger.TRACE,"Generating a script selection script for module " + moduleName);
  StandardLinkerContext context=new StandardLinkerContext(logger,getModuleDef(logger,moduleName),null,null,new JJSOptions());
  HostedModeLinker linker=new HostedModeLinker();
  return linker.generateSelectionScript(logger,context,context.getArtifacts());
}
