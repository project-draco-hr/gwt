{
  List<String> tickers=new ArrayList<String>();
  if (query.length() > 0) {
    query=query.toUpperCase();
    int count=0;
    for (    String ticker : stockTickers) {
      if (match(ticker,query)) {
        tickers.add(ticker);
        count++;
        if (count > MAX_RESULTS_TO_RETURN) {
          break;
        }
      }
    }
  }
  return tickers;
}
