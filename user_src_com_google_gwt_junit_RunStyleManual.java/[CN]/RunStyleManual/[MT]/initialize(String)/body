{
  numClients=1;
  if (args != null) {
    try {
      numClients=Integer.parseInt(args);
    }
 catch (    NumberFormatException e) {
      getLogger().log(TreeLogger.ERROR,"Error parsing argument \"" + args + "\"",e);
      return false;
    }
  }
  shell.setNumClients(numClients);
  return true;
}
