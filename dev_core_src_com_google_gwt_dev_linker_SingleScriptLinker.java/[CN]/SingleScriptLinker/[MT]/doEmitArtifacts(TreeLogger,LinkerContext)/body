{
  if (context.getCompilations().size() != 1) {
    logger=logger.branch(TreeLogger.ERROR,"The module must have exactly one distinct" + " permutation when using the " + getDescription() + " Linker.",null);
    int count=0;
    for (    CompilationResult result : context.getCompilations()) {
      logger.log(TreeLogger.INFO,"Permutation " + ++count + ": "+ result.toString(),null);
    }
    throw new UnableToCompleteException();
  }
  super.doEmitArtifacts(logger,context);
}
