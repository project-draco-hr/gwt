{
  addSnippetClassDecl("static class A  { ","  final static int f1 = 1 + 1;","  static int f2 = 2;","  final static int f3;","  static { f3 = 3; }","}","static class B  { ","  final static int f1 = 1 + A.f1;","  static int f2 = 2;","  final static int f3;","  static { f3 = 3; }","}");
  optimizeInto("int i = A.f1;","int i = 1 + 1;");
  optimizeInto("int i = B.f1;","int i = 1 + 1 + 1;");
}
