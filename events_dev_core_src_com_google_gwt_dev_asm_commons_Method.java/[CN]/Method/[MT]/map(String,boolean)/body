{
  if ("".equals(type)) {
    return type;
  }
  StringBuffer sb=new StringBuffer();
  int index=0;
  while ((index=type.indexOf("[]",index) + 1) > 0) {
    sb.append('[');
  }
  String t=type.substring(0,type.length() - sb.length() * 2);
  String desc=(String)DESCRIPTORS.get(t);
  if (desc != null) {
    sb.append(desc);
  }
 else {
    sb.append('L');
    if (t.indexOf('.') < 0) {
      if (!defaultPackage) {
        sb.append("java/lang/");
      }
      sb.append(t);
    }
 else {
      sb.append(t.replace('.','/'));
    }
    sb.append(';');
  }
  return sb.toString();
}
