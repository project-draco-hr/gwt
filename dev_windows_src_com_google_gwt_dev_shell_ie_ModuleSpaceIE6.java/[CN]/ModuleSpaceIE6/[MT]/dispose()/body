{
  if (staticDispatch != null) {
    final Variant staticDispatchToDispose=staticDispatch;
    staticDispatch=null;
    Display.getCurrent().asyncExec(new Runnable(){
      public void run(){
        if (!staticDispatchProxy.isDisposed()) {
          staticDispatchToDispose.dispose();
        }
      }
    }
);
  }
  if (window != null) {
    window.dispose();
  }
  super.dispose();
}
