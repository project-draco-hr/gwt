{
  Variant result=null;
  try {
    result=invokeNative(name,jthis,types,args,true);
    if (result.getType() == COM.VT_EMPTY && isExceptionActive()) {
      return null;
    }
 else     if (result.getType() == COM.VT_NULL) {
      return null;
    }
    return result.getString();
  }
  finally {
    if (result != null) {
      result.dispose();
    }
  }
}
