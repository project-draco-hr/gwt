{
  Variant result=null;
  try {
    result=invokeNative(name,jthis,types,args,true);
    if (result.getType() == COM.VT_EMPTY && isExceptionActive()) {
      return null;
    }
 else {
      return SwtOleGlue.convertVariantToObject(Object.class,result,"Returning from method '" + name + "'");
    }
  }
  finally {
    if (result != null) {
      result.dispose();
    }
  }
}
