{
  Variant result=null;
  try {
    result=invokeNative(name,jthis,types,args,true);
    if (result.getType() == COM.VT_EMPTY && isExceptionActive()) {
      return null;
    }
    if (result.getType() == COM.VT_NULL) {
      return null;
    }
    return HandleIE6.createHandle(returnType,result.getDispatch().getAddress());
  }
  finally {
    if (result != null) {
      result.dispose();
    }
  }
}
