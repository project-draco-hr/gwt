{
  boolean needsReceiver=true;
  for (  AbstractRequest<?> request : state.invocations) {
    if (request.hasReceiver()) {
      needsReceiver=false;
      break;
    }
  }
  if (needsReceiver) {
    doFire(new Receiver<Void>(){
      @Override public void onSuccess(      Void response){
      }
    }
);
  }
 else {
    doFire(null);
  }
}
