{
  String packageName;
  String className;
  int pos=typeName.lastIndexOf('.');
  if (pos >= 0) {
    packageName=typeName.substring(0,pos);
    className=typeName.substring(pos + 1);
  }
 else {
    packageName="";
    className=typeName;
  }
  StaticCompilationUnitProvider cup=new StaticCompilationUnitProvider(packageName,className,code.toString().toCharArray());
  return cup;
}
