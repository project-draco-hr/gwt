{
  if (value > -129 && value < 128) {
    int rebase=value + 128;
    if (boxedValues == null) {
      boxedValues=new LongEmul[256];
    }
    LongEmul result=boxedValues[rebase];
    if (result == null) {
      result=boxedValues[rebase]=create(value);
    }
    return result;
  }
  return create(value);
}
