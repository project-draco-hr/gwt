{
  int signa=getH(a) >> (BITS2 - 1);
  int signb=getH(b) >> (BITS2 - 1);
  if (signa == 0) {
    if (signb != 0 || getH(a) > getH(b) || (getH(a) == getH(b) && getM(a) > getM(b)) || (getH(a) == getH(b) && getM(a) == getM(b) && getL(a) >= getL(b))) {
      return true;
    }
 else {
      return false;
    }
  }
 else {
    if (signb == 0 || getH(a) < getH(b) || (getH(a) == getH(b) && getM(a) < getM(b)) || (getH(a) == getH(b) && getM(a) == getM(b) && getL(a) < getL(b))) {
      return false;
    }
 else {
      return true;
    }
  }
}
