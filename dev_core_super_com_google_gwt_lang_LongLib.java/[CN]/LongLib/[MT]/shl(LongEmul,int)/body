{
  n&=63;
  int res0, res1, res2;
  if (n < BITS) {
    res0=getL(a) << n;
    res1=(getM(a) << n) | (getL(a) >> (BITS - n));
    res2=(getH(a) << n) | (getM(a) >> (BITS - n));
  }
 else   if (n < BITS01) {
    res0=0;
    res1=getL(a) << (n - BITS);
    res2=(getM(a) << (n - BITS)) | (getL(a) >> (BITS01 - n));
  }
 else {
    res0=0;
    res1=0;
    res2=getL(a) << (n - BITS01);
  }
  return create(res0 & MASK,res1 & MASK,res2 & MASK_2);
}
