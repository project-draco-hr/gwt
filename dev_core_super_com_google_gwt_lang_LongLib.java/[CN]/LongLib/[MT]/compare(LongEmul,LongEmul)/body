{
  int signA=sign(a);
  int signB=sign(b);
  if (signA != signB) {
    return signB - signA;
  }
  int a2=getH(a);
  int b2=getH(b);
  if (a2 != b2) {
    return a2 - b2;
  }
  int a1=getM(a);
  int b1=getM(b);
  if (a1 != b1) {
    return a1 - b1;
  }
  int a0=getL(a);
  int b0=getL(b);
  return a0 - b0;
}
