  try {
    new Float(wrongString());
    fail();
  }
 catch (  NumberFormatException e) {
  }
  assertEquals(new Float("1e50"),new Float("Infinity"));
  assertEquals(new Float("-1e50"),new Float("-Infinity"));
  assertEquals(new Float("1e-50"),new Float("0.0"));
  assertEquals(new Float("-1e-50"),new Float("-0.0"));
}
