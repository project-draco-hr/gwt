{
  assertTrue(0 == Float.parseFloat("0"));
  assertTrue(-1.5 == Float.parseFloat("-1.5"));
  assertTrue(3.0 == Float.parseFloat("3."));
  assertTrue(0.5 == Float.parseFloat(".5"));
  assertTrue("Can't parse MAX_VALUE",Float.MAX_VALUE == Float.parseFloat(String.valueOf(Float.MAX_VALUE)));
  assertTrue("Can't parse MIN_VALUE",Float.MIN_VALUE == Float.parseFloat(String.valueOf(Float.MIN_VALUE)));
}
