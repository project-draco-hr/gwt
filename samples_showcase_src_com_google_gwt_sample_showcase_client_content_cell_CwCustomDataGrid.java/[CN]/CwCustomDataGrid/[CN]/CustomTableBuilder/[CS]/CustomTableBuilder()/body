{
  super(dataGrid);
  Style style=dataGrid.getResources().style();
  rowStyle=style.evenRow();
  selectedRowStyle=" " + style.selectedRow();
  cellStyle=style.cell() + " " + style.evenRowCell();
  selectedCellStyle=" " + style.selectedRowCell();
  Date today=new Date();
  todayMonth=today.getMonth();
}
