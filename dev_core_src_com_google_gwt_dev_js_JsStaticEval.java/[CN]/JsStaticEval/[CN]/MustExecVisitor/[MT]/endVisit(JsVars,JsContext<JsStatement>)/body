{
  JsVars strippedVars=new JsVars(x.getSourceInfo().makeChild("Simplified execution"));
  boolean mustReplace=false;
  for (  JsVar var : x) {
    JsVar strippedVar=new JsVar(var.getSourceInfo().makeChild("Simplified execution"),var.getName());
    strippedVars.add(strippedVar);
    if (var.getInitExpr() != null) {
      mustReplace=true;
    }
  }
  if (mustReplace) {
    mustExec.add(strippedVars);
  }
 else {
    mustExec.add(x);
  }
}
