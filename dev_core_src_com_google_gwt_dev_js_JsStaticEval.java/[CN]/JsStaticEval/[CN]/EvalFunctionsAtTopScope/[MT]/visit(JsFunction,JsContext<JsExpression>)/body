{
  if (x.getName() != null && !dontMove.contains(x)) {
    ListIterator<JsStatement> itr=itrStack.peek();
    itr.previous();
    itr.add(x.makeStmt());
    itr.next();
    ctx.replaceMe(x.getName().makeRef(x.getSourceInfo().makeChild("Shuffled evaluation order")));
  }
  scopeStack.push(x.getBody());
  return true;
}
