{
  if (arg1 instanceof JsValueLiteral && arg2 instanceof JsValueLiteral) {
    if (arg1 instanceof JsNumberLiteral && arg2 instanceof JsNumberLiteral) {
      double value=((JsNumberLiteral)arg1).getValue() + ((JsNumberLiteral)arg2).getValue();
      ctx.replaceMe(program.getNumberLiteral(value));
    }
 else {
      StringBuilder result=new StringBuilder();
      if (appendLiteral(result,(JsValueLiteral)arg1) && appendLiteral(result,(JsValueLiteral)arg2)) {
        ctx.replaceMe(program.getStringLiteral(original.getSourceInfo(),result.toString()));
      }
    }
  }
}
