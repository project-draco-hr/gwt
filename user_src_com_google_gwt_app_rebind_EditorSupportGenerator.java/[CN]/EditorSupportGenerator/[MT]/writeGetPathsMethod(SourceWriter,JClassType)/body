{
  sw.indent();
  sw.println("public String[] getPaths() {");
  sw.indent();
  sw.println("Set<String> rtn = new HashSet<String>();");
  for (  JField field : recordType.getFields()) {
    if (field.getType().getQualifiedSourceName().equals(Property.class.getName())) {
      sw.println("rtn.add(" + recordType.getName() + "."+ field.getName()+ ".getName());");
    }
  }
  sw.println("return rtn.toArray(new String[rtn.size()]);");
  sw.outdent();
  sw.println("}");
  sw.outdent();
}
