{
  RequestProperty foundProp=getProperty(property.getPropertyName());
  if (foundProp == null && !"".equals(property.getPropertyName())) {
    add(property);
  }
 else {
    for (    RequestProperty p : property) {
      if (foundProp == null) {
        add(p);
      }
 else {
        foundProp.mergeProperty(p);
      }
    }
  }
  return foundProp;
}
