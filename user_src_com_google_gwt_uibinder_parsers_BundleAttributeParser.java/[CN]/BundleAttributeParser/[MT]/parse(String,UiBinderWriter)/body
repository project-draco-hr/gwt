{
  StringBuilder b=new StringBuilder();
  String[] values=attribute.split(" ");
  boolean first=true;
  for (  String value : values) {
    if (first) {
      first=false;
    }
 else {
      b.append(" + \" \" + ");
    }
    b.append(bundleInstance() + "." + parenthesizeDots(value)+ "()");
  }
  return b.toString();
}
