{
  StringBuffer sb=new StringBuffer();
  sb.append("new Class[]{");
  JParameter[] params=method.getParameters();
  for (int i=0; i < params.length; ++i) {
    if (i > 0) {
      sb.append(", ");
    }
    JType type=params[i].getType();
    String typeName=type.getQualifiedSourceName();
    sb.append(typeName);
    sb.append(".class");
  }
  sb.append("}");
  String classes=sb.toString();
  return classes;
}
