{
  int num=size.intValue();
  for (int i=0; i < num; i++) {
    if (where == Position.NONE) {
      v.add("hello");
    }
 else     if (where == Position.BEGIN) {
      v.add(0,"hello");
    }
 else     if (where == Position.END) {
      v.add(v.size(),"hello");
    }
 else     if (where == Position.VARIED) {
      v.add(index,"hello");
      index+=PRIME;
      index%=v.size();
    }
  }
}
