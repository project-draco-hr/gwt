{
  long length=byteCounter << 3;
  engineUpdate((byte)128);
  while (wordOff != 0) {
    engineUpdate((byte)0);
  }
  Arrays.fill(blockBuffer,blockOff,64,0);
  if (blockOff > 14) {
    computeBlock();
    Arrays.fill(blockBuffer,0);
  }
  blockBuffer[14]=(int)(length >>> 32);
  blockBuffer[15]=enforceOverflow((int)length);
  computeBlock();
}
