{
  StringBuffer call=new StringBuffer(name + "(");
  if (arguments != null) {
    call.append(" ");
    for (int i=0; i < arguments.size(); ++i) {
      call.append(arguments.get(i));
      if (i < arguments.size() - 1) {
        call.append(", ");
      }
    }
    call.append(" ");
  }
  call.append(")");
  super.code=call.toString();
}
