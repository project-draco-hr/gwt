{
  if (ascending) {
    if (sortAscDecorator == null) {
      sortAscDecorator=new IconCellDecorator<SafeHtml>(resources.cellTableSortAscending(),new SafeHtmlCell());
    }
    return sortAscDecorator;
  }
 else {
    if (sortDescDecorator == null) {
      sortDescDecorator=new IconCellDecorator<SafeHtml>(resources.cellTableSortDescending(),new SafeHtmlCell());
    }
    return sortDescDecorator;
  }
}
