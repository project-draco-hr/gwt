{
  for (  Header<?> header : headers) {
    if (header != null && header.getCell().dependsOnSelection()) {
      createHeaders(false);
      break;
    }
  }
  for (  Header<?> footer : footers) {
    if (footer != null && footer.getCell().dependsOnSelection()) {
      createHeaders(true);
      break;
    }
  }
}
