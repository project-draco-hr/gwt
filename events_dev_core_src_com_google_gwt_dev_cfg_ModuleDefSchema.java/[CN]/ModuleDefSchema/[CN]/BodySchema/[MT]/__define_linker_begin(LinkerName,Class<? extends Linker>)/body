{
  if (!Linker.class.isAssignableFrom(linker)) {
    logger.log(TreeLogger.ERROR,"A linker must extend " + Linker.class.getName(),null);
    throw new UnableToCompleteException();
  }
  if (linker.getAnnotation(LinkerOrder.class) == null) {
    logger.log(TreeLogger.ERROR,"Linkers must be annotated with the " + LinkerOrder.class.getName() + " annotation",null);
    throw new UnableToCompleteException();
  }
  moduleDef.defineLinker(name.name,linker);
  return null;
}
