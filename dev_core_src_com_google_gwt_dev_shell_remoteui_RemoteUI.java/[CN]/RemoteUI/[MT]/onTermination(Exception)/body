{
  getTopLogger().log(TreeLogger.INFO,"Remote UI connection terminated due to exception: " + e);
  getTopLogger().log(TreeLogger.INFO,"Shutting down development mode server.");
  try {
    transportSocket.close();
  }
 catch (  IOException e1) {
  }
  ((DoneCallback)getCallback(DoneEvent.getType())).onDone();
}
