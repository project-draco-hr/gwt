{
  Properties properties=new Properties();
  for (  ConfigurationProperty configurationPropertyFromArray : propertyArray) {
    ConfigurationProperty configurationProperty=properties.createConfiguration(configurationPropertyFromArray.getName(),configurationPropertyFromArray.allowsMultipleValues());
    for (    String value : configurationPropertyFromArray.getValues()) {
      configurationProperty.addValue(value);
    }
  }
  return properties;
}
