{
  StringBuffer sb=new StringBuffer();
  TreeLogger.Type[] values=TreeLogger.Type.values();
  for (int i=0, c=values.length; i < c; ++i) {
    if (i > 0) {
      sb.append(", ");
    }
    if (i + 1 == c) {
      sb.append("or ");
    }
    sb.append(values[i].name());
  }
  return sb.toString();
}
