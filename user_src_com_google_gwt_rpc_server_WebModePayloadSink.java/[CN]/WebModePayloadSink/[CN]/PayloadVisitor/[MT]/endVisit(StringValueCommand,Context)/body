{
  if (hasBackRef(x)) {
    if (!isStarted(x)) {
      String escaped=EscapeUtil.escape(x.getValue());
      push(begin(x));
      eq();
      quote();
      push(escaped);
      quote();
      commit(x,false);
    }
 else {
      push(makeBackRef(x));
    }
  }
 else {
    String escaped=EscapeUtil.escape(x.getValue());
    quote();
    push(escaped);
    quote();
  }
}
