{
  JClassType classType=type.isClassOrInterface();
  if (classType == null) {
    return true;
  }
  for (  String valueType : VALUE_TYPE_NAMES) {
    JClassType found=oracle.findType(valueType);
    if (found != null && found.isAssignableFrom(classType)) {
      return true;
    }
  }
  return false;
}
