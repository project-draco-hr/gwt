{
  getReportEditView().setEmployeePickerValues(Collections.<EmployeeRecord>emptyList());
  requests.employeeRequest().findEmployeeEntries(0,50).with(EmployeeRenderer.instance().getPaths()).fire(new Receiver<List<EmployeeRecord>>(){
    public void onSuccess(    List<EmployeeRecord> response,    Set<SyncResult> syncResults){
      List<EmployeeRecord> values=new ArrayList<EmployeeRecord>();
      values.add(null);
      values.addAll(response);
      getReportEditView().setEmployeePickerValues(values);
    }
  }
);
  super.start(display,eventBus);
}
