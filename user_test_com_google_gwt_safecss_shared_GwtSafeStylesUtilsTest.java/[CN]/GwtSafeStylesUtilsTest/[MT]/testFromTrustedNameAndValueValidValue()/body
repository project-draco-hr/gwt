{
  if (GWT.isProdMode()) {
    return;
  }
  for (  String s : VALID_STYLE_VALUES) {
    try {
      SafeStyles styles=SafeStylesUtils.fromTrustedNameAndValue("name",s);
      assertEquals("name" + ":" + s + ";",styles);
    }
 catch (    Exception e) {
      fail("Unexpected exception thrown for valid style value: '" + s + "'.\n"+ e.getMessage());
    }
  }
}
