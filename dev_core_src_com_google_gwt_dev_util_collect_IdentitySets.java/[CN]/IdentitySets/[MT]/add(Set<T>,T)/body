{
switch (set.size()) {
case 0:
    return new IdentitySingletonSet<T>(toAdd);
case 1:
{
    if (set.contains(toAdd)) {
      return set;
    }
    Set<T> result=new IdentityHashSet<T>();
    result.add(set.iterator().next());
    result.add(toAdd);
    return result;
  }
default :
set.add(toAdd);
return set;
}
}
