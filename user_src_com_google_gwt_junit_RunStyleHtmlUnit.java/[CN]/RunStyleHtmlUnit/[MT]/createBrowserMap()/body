{
  Map<String,BrowserVersion> browserMap=new HashMap<String,BrowserVersion>();
  for (  BrowserVersion browser : new BrowserVersion[]{BrowserVersion.FIREFOX_17,BrowserVersion.INTERNET_EXPLORER_8,BrowserVersion.INTERNET_EXPLORER_9,BrowserVersion.CHROME}) {
    browserMap.put(browser.getNickname(),browser);
  }
  return Collections.unmodifiableMap(browserMap);
}
