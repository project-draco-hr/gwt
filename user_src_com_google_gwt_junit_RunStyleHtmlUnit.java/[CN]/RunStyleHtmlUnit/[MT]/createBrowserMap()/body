{
  Map<String,BrowserVersion> browserMap=new HashMap<String,BrowserVersion>();
  for (  BrowserVersion browser : new BrowserVersion[]{BrowserVersion.FIREFOX_3,BrowserVersion.FIREFOX_3_6,BrowserVersion.INTERNET_EXPLORER_6,BrowserVersion.INTERNET_EXPLORER_7}) {
    browserMap.put(browser.getNickname(),browser);
  }
  return Collections.unmodifiableMap(browserMap);
}
