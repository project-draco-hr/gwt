{
  int count=(byteLength() - byteOffset()) / BYTES_PER_ELEMENT;
  if (begin < 0) {
    begin+=count;
    if (begin < 0) {
      begin=0;
    }
  }
 else   if (begin > count) {
    begin=count;
  }
  if (end < 0) {
    end+=count;
    if (end < 0) {
      end=0;
    }
  }
 else   if (end > count) {
    end=count;
  }
  if (end < begin) {
    end=begin;
  }
  return new Float32ArrayImpl(arrayBuf,begin * BYTES_PER_ELEMENT,end * BYTES_PER_ELEMENT);
}
