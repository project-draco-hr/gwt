{
  sb.append("<div class='playerScoreBox'>");
  sb.append("<b>Name: </b>");
  sb.append(value.getDisplayName());
  sb.append("<br><b>Net Worth: </b>");
  sb.append(StockSample.getFormattedPrice(value.getNetWorth()));
