{
  super(getter);
  this.shim=bean.as();
  Method found=null;
  String name=BeanMethod.GET.inferName(getter);
  for (  Method m : getter.getDeclaringClass().getMethods()) {
    if (BeanMethod.SET.matches(m)) {
      if (BeanMethod.SET.inferName(m).equals(name) && getter.getReturnType().isAssignableFrom(m.getParameterTypes()[0])) {
        found=m;
        break;
      }
    }
  }
  setter=found;
}
