{
  EntityManager em=entityManager();
  try {
    Query query=queryReportsBySearch(em,employeeId,department,startsWith,null,true);
    if (query == null) {
      return REPORT_COUNT;
    }
    long count=((Number)query.getSingleResult()).longValue();
    return count;
  }
  finally {
    em.close();
  }
}
