{
  nodes.add(node);
  graph.addNode(node);
  for (Iterator<Exit> i=currentExits.iterator(); i.hasNext(); ) {
    Exit exit=i.next();
    if (exit.isNormal()) {
      i.remove();
      addEdge(exit,node);
    }
  }
  if (node instanceof CfgSimpleNode<?>) {
    addNormalExit(node);
  }
  return node;
}
