{
  CfgEdge methodIn=new CfgEdge();
  graph.addGraphInEdge(methodIn);
  accept(codeBlock);
  graph.addIn(nodes.get(0),methodIn);
  addNode(endNode);
  for (  Exit exit : currentExits) {
switch (exit.reason) {
case CONTINUE:
case BREAK:
case CASE_ELSE:
case CASE_THEN:
      throw new IllegalArgumentException("Unhandled exit: " + exit.reason);
case NORMAL:
case RETURN:
case THROW:
{
      addEdge(exit,endNode);
      break;
    }
}
}
return graph;
}
