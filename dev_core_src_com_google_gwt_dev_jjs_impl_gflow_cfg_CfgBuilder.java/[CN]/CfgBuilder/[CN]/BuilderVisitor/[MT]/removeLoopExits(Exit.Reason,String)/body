{
  List<Exit> result=new ArrayList<Exit>();
  for (Iterator<Exit> i=currentExits.iterator(); i.hasNext(); ) {
    Exit exit=i.next();
    if (exit.reason == reason && (exit.getLabel() == null || exit.getLabel().equals(label))) {
      i.remove();
      result.add(exit);
    }
  }
  return result;
}
