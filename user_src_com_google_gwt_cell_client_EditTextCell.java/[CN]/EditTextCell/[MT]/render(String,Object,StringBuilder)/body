{
  ViewData viewData=getViewData(key);
  if (viewData != null && !viewData.isEditing() && value != null && value.equals(viewData.getText())) {
    clearViewData(key);
    viewData=null;
  }
  if (viewData != null) {
    if (viewData.isEditing()) {
      sb.append("<input type='text' value='" + viewData.getText() + "'></input>");
    }
 else {
      sb.append(viewData.getText());
    }
  }
 else   if (value != null) {
    sb.append(value);
  }
}
