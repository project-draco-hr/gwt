{
  String input="function a1(arg, x) { arg.x = x; return arg; }" + "function b1() { var x=a1(function (){}, 10); } b1();";
  verifyNoChange(input);
  String input2="function a1(arg, x) { arg.x = x; return arg; }" + "function b1() { var x=a1(function blah(){}, 10); } b1();";
  verifyNoChange(input2);
}
