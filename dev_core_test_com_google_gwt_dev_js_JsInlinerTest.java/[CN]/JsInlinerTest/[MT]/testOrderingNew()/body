{
  StringBuffer code=new StringBuffer();
  code.append("var x;");
  code.append("function foo() { alert('x = ' + x); }");
  code.append("function callee(arg) { new foo(); return arg; }");
  code.append("function caller() { callee((x=1,2)); }");
  code.append("caller();");
  compare(code.toString(),code.toString());
}
