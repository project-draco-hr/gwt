{
  StringBuffer code=new StringBuffer();
  code.append("var x;");
  code.append("function clinit() { x = 1; clinit = null; }");
  code.append("function callee(arg) { alert(x); return arg; }");
  code.append("function caller() { callee((clinit(),2)); }");
  code.append("caller();");
  verifyNoChange(code.toString());
}
