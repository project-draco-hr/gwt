{
  if (obj == this) {
    return true;
  }
  if (!(obj instanceof Map)) {
    return false;
  }
  Map<?,?> otherMap=(Map<?,?>)obj;
  if (size() != otherMap.size()) {
    return false;
  }
  for (  Entry<?,?> entry : otherMap.entrySet()) {
    if (!containsEntry(entry)) {
      return false;
    }
  }
  return true;
}
