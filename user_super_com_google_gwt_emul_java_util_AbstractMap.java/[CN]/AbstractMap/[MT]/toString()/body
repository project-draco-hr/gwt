{
  StringBuilder sb=new StringBuilder("{");
  boolean comma=false;
  for (  Entry<K,V> entry : entrySet()) {
    if (comma) {
      sb.append(", ");
    }
 else {
      comma=true;
    }
    sb.append(toString(entry.getKey()));
    sb.append("=");
    sb.append(toString(entry.getValue()));
  }
  sb.append("}");
  return sb.toString();
}
