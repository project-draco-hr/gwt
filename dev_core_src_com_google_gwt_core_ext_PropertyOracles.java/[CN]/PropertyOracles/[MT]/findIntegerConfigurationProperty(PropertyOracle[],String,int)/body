{
  int toReturn=def;
  for (  PropertyOracle oracle : propertyOracles) {
    try {
      com.google.gwt.core.ext.ConfigurationProperty property=oracle.getConfigurationProperty(name);
      toReturn=Integer.parseInt(property.getValues().get(0));
    }
 catch (    Exception e) {
      break;
    }
  }
  return toReturn;
}
