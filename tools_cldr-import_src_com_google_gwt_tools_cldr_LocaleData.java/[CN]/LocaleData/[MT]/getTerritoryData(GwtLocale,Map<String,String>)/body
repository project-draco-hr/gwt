{
  if (map == null) {
    return null;
  }
  for (  GwtLocale search : locale.getCompleteSearchList()) {
    String region=search.getRegion();
    if (region == null) {
      region="001";
    }
    String value=map.get(region);
    if (value != null) {
      return value;
    }
  }
  return null;
}
