{
  TypeReference returnType=meth.returnType;
  if (containsLong(returnType,scope)) {
    warn(meth,"Return value of type '" + returnType + "' is an opaque, non-numeric value in JS code");
  }
  if (meth.arguments != null) {
    for (    Argument arg : meth.arguments) {
      if (containsLong(arg.type,scope)) {
        warn(arg,"Parameter '" + String.valueOf(arg.name) + "': '"+ arg.type+ "' is an opaque, non-numeric value in JS code");
      }
    }
  }
}
