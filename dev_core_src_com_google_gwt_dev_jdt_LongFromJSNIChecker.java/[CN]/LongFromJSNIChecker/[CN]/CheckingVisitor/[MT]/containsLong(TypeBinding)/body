{
  if (type instanceof BaseTypeBinding) {
    BaseTypeBinding btb=(BaseTypeBinding)type;
    if (btb.id == TypeIds.T_long) {
      return true;
    }
  }
  if (type instanceof ArrayBinding) {
    ArrayBinding ab=(ArrayBinding)type;
    if (containsLong(ab.elementsType())) {
      return true;
    }
  }
  return false;
}
