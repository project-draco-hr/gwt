{
  JMethod method=map.nameToMethod(x.getName());
  if (!(method instanceof JConstructor)) {
    return;
  }
  JConstructor constructor=(JConstructor)method;
  boolean fragmentExpandsConstructorLiveness=!alreadyLoadedPredicate.isLive(constructor) && livenessPredicate.isLive(constructor);
  if (fragmentExpandsConstructorLiveness) {
    liveConstructorCount++;
  }
 else {
    ctx.removeMe();
  }
}
