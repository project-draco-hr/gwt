{
  String defaultExcludes[]=new String[]{"**/*~","**/#*#","**/.#*","**/%*%","**/._*","**/CVS","**/CVS/**","**/.cvsignore","**/SCCS","**/SCCS/**","**/vssver.scc","**/.svn","**/.svn/**","**/.DS_Store"};
