{
  if (x == null) {
    return "null";
  }
  StringBuffer b=new StringBuffer("[");
  for (int i=0; i < x.length; i++) {
    if (i != 0) {
      b.append(", ");
    }
    if (x[i] == null) {
      b.append("null");
    }
 else {
      b.append(x[i].toString());
    }
  }
  b.append("]");
  return b.toString();
}
