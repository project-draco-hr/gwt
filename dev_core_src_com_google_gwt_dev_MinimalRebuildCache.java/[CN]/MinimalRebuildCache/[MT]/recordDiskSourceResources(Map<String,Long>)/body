{
  recordModifiedResources(currentModifiedByDiskSourcePath,lastModifiedByDiskSourcePath,modifiedDiskSourcePaths,deletedDiskSourcePaths);
  deletedCompilationUnitNames.clear();
  deletedCompilationUnitNames.addAll(resourcePathsToCompilationUnitNames(deletedDiskSourcePaths));
  modifiedCompilationUnitNames.clear();
  modifiedCompilationUnitNames.addAll(resourcePathsToCompilationUnitNames(modifiedDiskSourcePaths));
}
