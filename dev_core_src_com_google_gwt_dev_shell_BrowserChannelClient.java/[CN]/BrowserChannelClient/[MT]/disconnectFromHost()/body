{
  if (logger.isLoggable(TreeLogger.DEBUG)) {
    logger.log(TreeLogger.DEBUG,"disconnecting channel " + this);
  }
  if (!isConnected()) {
    if (logger.isLoggable(TreeLogger.DEBUG)) {
      logger.log(TreeLogger.DEBUG,"Disconnecting already disconnected channel " + this);
    }
    return false;
  }
  new QuitMessage(this).send();
  endSession();
  connected=false;
  return true;
}
