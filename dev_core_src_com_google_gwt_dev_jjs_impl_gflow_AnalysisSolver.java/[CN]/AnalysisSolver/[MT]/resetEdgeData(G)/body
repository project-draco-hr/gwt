{
  for (  N node : graph.getNodes()) {
    for (    E e : graph.getInEdges(node)) {
      graph.setEdgeData(e,null);
    }
    for (    E e : graph.getOutEdges(node)) {
      graph.setEdgeData(e,null);
    }
  }
  for (  E e : graph.getGraphOutEdges()) {
    graph.setEdgeData(e,null);
  }
  for (  E e : graph.getGraphInEdges()) {
    graph.setEdgeData(e,null);
  }
}
