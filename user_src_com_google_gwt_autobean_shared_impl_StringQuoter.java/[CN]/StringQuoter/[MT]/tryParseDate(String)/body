{
  try {
    return new Date(Long.parseLong(date));
  }
 catch (  NumberFormatException ignored) {
  }
  if (date.endsWith("Z")) {
    date=date.substring(0,date.length() - 1) + "+0000";
  }
  try {
    return ISO8601.parse(date);
  }
 catch (  ParseException ignored) {
  }
  try {
    return RFC2822.parse(date);
  }
 catch (  ParseException ignored) {
  }
  return null;
}
