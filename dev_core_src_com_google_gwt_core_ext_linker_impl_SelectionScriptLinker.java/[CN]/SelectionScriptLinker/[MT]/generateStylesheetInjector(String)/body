{
  String hrefExpr="'" + stylesheetUrl + "'";
  if (isRelativeURL(stylesheetUrl)) {
    hrefExpr="base + " + hrefExpr;
  }
  return "if (!__gwt_stylesLoaded['" + stylesheetUrl + "']) {\n           "+ "  var l = $doc.createElement('link');\n                          "+ "  __gwt_stylesLoaded['"+ stylesheetUrl+ "'] = l;\n             "+ "  l.setAttribute('rel', 'stylesheet');\n                         "+ "  l.setAttribute('href', "+ hrefExpr+ ");\n                    "+ "  $doc.getElementsByTagName('head')[0].appendChild(l);\n         "+ "}\n";
}
