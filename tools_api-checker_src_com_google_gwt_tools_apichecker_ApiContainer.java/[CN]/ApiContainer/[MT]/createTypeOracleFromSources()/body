{
  numFilesCount=0;
  TypeOracleBuilder builder=new TypeOracleBuilder(new CacheManager(null,null,ApiCompatibilityChecker.DISABLE_CHECKS));
  for (Iterator<File> i=sourceTrees.iterator(); i.hasNext(); ) {
    addCompilationUnitsInPath(builder,i.next());
  }
  typeOracle=builder.build(logger);
  logger.log(TreeLogger.INFO,"API " + name + ", Finished with building typeOracle, added "+ numFilesCount+ " files",null);
}
