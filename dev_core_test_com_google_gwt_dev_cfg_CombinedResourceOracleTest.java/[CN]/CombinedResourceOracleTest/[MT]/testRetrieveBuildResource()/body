{
  LibraryGroupBuildResourceOracle buildResourceOracle=createLibraryGroupBuildResourceOracle();
  MockResourceOracle mockResourceOracle=new MockResourceOracle(JavaResourceBase.FOO);
  CombinedResourceOracle combinedResourceOracle=new CombinedResourceOracle(buildResourceOracle,mockResourceOracle);
  assertEquals(Sets.newHashSet("test/Foo.java","Tab.ui.xml","User.gwt.xml","Core.gwt.xml"),combinedResourceOracle.getPathNames());
}
