{
  assertEquals("unicodeWrapWithKnownDir(Direction, String)",RLE + en + PDF+ LRM,ltrFormatter.unicodeWrapWithKnownDir(RTL,en));
  assertEquals("unicodeWrapWithKnownDir(Direction, String, boolean)",RLE + en + longHtmlTag+ PDF+ LRM,ltrFormatter.unicodeWrapWithKnownDir(RTL,en + longHtmlTag,true));
  assertEquals("unicodeWrapWithKnownDir(Direction, String, boolean, boolean)",RLE + en + longHtmlTag+ PDF,ltrFormatter.unicodeWrapWithKnownDir(RTL,en + longHtmlTag,true,false));
}
