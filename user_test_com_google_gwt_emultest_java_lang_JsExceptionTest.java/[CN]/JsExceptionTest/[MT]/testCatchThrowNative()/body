{
  Object e;
  e=makeJSO();
  assertJsException(e,catchJava(createNativeThrower(e)));
  e="testing";
  assertJsException(e,catchJava(createNativeThrower(e)));
  e=null;
  assertJsException(e,catchJava(createNativeThrower(e)));
  e=new Object();
  assertJsException(e,catchJava(createNativeThrower(e)));
}
