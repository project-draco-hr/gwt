{
  final MediaElement element=getElement();
  if (element == null) {
    return;
  }
  int waitMillis=5000;
  delayTestFinish(3 * waitMillis);
  assertEquals("Default playback rate should be 1.0",1.0,element.getDefaultPlaybackRate());
  element.play();
  new Timer(){
    @Override public void run(){
      double rate=2.0;
      element.setPlaybackRate(rate);
      assertEquals("Should be able to change playback rate",rate,element.getPlaybackRate());
      rate=1.0;
      element.setPlaybackRate(rate);
      assertEquals("Should be able to change playback rate",rate,element.getPlaybackRate());
      finishTest();
    }
  }
.schedule(waitMillis);
}
