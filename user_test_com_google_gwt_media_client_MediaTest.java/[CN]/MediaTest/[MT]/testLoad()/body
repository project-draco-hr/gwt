{
  final MediaBase media=getMedia();
  if (media == null) {
    return;
  }
  delayTestFinish(20 * 1000);
  new Timer(){
    @Override public void run(){
      MediaError error=media.getError();
      if (error != null) {
        fail("Media error (" + error.getCode() + ")");
      }
      finishTest();
    }
  }
.schedule(15 * 1000);
  media.load();
}
