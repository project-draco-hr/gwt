{
  MediaElement element=getElement();
  if (element == null) {
    return;
  }
  element.setVolume(0.5);
  assertEquals("Volume should be at one-half loudness.",0.5,element.getVolume());
  element.setVolume(0.75);
  assertEquals("Volume should be at three-quarters loudness.",0.75,element.getVolume());
}
