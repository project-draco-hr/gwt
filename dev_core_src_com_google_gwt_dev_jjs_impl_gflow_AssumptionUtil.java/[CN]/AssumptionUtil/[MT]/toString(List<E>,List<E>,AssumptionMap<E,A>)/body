{
  StringBuilder result=new StringBuilder();
  for (  E e : inEdges) {
    if (result.length() != 0) {
      result.append("; ");
    }
    result.append(e);
    result.append("=");
    result.append(assumptionMap.getAssumption(e));
  }
  for (  E e : outEdges) {
    if (result.length() != 0) {
      result.append("; ");
    }
    result.append(e);
    result.append("=");
    result.append(assumptionMap.getAssumption(e));
  }
  return result.toString();
}
