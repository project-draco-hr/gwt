{
  if (body.length() == 0) {
    return null;
  }
  if (generatedFile == null) {
    try {
      File f=File.createTempFile(String.format("uiBinder_%s_%s",packageName,className),"." + getCssFileExtension());
      f.deleteOnExit();
      BufferedWriter out=new BufferedWriter(new FileWriter(f));
      out.write(body);
      out.close();
      generatedFile=f;
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
    ResourceGeneratorUtil.addNamedFile(getBodyFileName(),generatedFile);
  }
  return generatedFile;
}
