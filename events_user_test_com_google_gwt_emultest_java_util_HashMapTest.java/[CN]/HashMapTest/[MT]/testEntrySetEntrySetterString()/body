{
  HashMap hashMap=new HashMap();
  hashMap.put("A","B");
  Set entrySet=hashMap.entrySet();
  Entry entry=(Entry)entrySet.iterator().next();
  entry.setValue("C");
  assertEquals("C",hashMap.get("A"));
  hashMap.put("A","D");
  assertEquals("D",entry.getValue());
  assertEquals(1,hashMap.size());
}
