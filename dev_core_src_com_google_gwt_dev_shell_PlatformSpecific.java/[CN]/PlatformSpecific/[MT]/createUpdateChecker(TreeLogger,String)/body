{
  try {
    for (int i=0; i < updaterClassNames.length; i++) {
      try {
        Class<? extends CheckForUpdates> clazz=Class.forName(updaterClassNames[i]).asSubclass(CheckForUpdates.class);
        Constructor<? extends CheckForUpdates> ctor=clazz.getDeclaredConstructor(new Class[]{TreeLogger.class,String.class});
        CheckForUpdates checker=ctor.newInstance(new Object[]{logger,entryPoint});
        return checker;
      }
 catch (      Exception e) {
      }
    }
  }
 catch (  Throwable e) {
  }
  return null;
}
