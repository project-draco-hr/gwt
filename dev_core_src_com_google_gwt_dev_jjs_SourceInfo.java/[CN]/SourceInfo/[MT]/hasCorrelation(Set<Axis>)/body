{
  if (!correlations.isEmpty()) {
    for (    Axis a : axes) {
      if (correlations.containsKey(a)) {
        return true;
      }
    }
  }
  if (parent != null && parent.hasCorrelation(axes)) {
    return true;
  }
  for (  SourceInfo info : additionalAncestors) {
    if (info.hasCorrelation(axes)) {
      return true;
    }
  }
  return false;
}
