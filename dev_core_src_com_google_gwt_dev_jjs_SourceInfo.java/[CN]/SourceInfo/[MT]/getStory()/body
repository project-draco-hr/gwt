{
  StringBuilder toReturn=new StringBuilder();
  if (caller != null) {
    SourceInfo si=this;
    while (si != null) {
      if (si.mutation != null) {
        toReturn.append(si.mutation + " by " + si.caller+ "\n  ");
      }
      si=si.parent;
    }
  }
  for (  SourceInfo root : getRoots()) {
    toReturn.append(root.fileName + ":" + root.startLine+ "\n");
  }
  return toReturn.toString();
}
