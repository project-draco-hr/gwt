{
  if (!GWT.isScript()) {
    return;
  }
  delayTestFinish(TIMEOUT);
  GWT.runAsync(new RunAsyncCallback(){
    public void onFailure(    Throwable reason){
      fail();
    }
    public void onSuccess(){
      DeferredCommand.addCommand(new Command(){
        public void execute(){
          checkMetrics();
          finishTest();
        }
      }
);
    }
  }
);
}
