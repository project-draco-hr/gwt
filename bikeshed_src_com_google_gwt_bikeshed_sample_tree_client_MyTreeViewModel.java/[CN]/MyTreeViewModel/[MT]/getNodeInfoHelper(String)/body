{
  if (value.endsWith("...")) {
    ListModel<String> listModel=new StringListModel(value.toString());
    return new DefaultNodeInfo<String>(listModel,new ButtonCell(),new ValueUpdater<String>(){
      public void update(      String value){
        Window.alert("Clicked: " + value);
      }
    }
);
  }
 else {
    ListModel<Integer> listModel=new IntegerListModel(value.length());
    return new DefaultNodeInfo<Integer>(listModel,INTEGER_CELL,new ValueUpdater<Integer>(){
      public void update(      Integer value){
        Window.alert("Integer = " + value);
      }
    }
);
  }
}
