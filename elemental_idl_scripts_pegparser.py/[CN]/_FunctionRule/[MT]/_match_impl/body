def _match_impl(self, state, pos):
    'Simply invokes the sub rule'
    (nextPos, ast) = self._sub_rule.match(state, pos)
    if (nextPos is not None):
        if (not self._func.__name__.startswith('_')):
            ast = (self._func.__name__, ast)
        return (nextPos, ast)
    return (None, None)
