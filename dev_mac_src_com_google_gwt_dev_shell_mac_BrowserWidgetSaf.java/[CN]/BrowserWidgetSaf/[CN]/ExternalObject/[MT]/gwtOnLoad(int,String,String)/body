{
  if (moduleName == null) {
    return handleUnload(scriptObject);
  }
  TreeLogger logger=getHost().getLogger().branch(TreeLogger.DEBUG,"Loading an instance of module '" + moduleName + "'");
  try {
    if (!HostedHtmlVersion.validHostedHtmlVersion(logger,version)) {
      return false;
    }
    Integer key=new Integer(scriptObject);
    ModuleSpaceHost msh=getHost().createModuleSpaceHost(logger,BrowserWidgetSaf.this,moduleName);
    final int globalContext=globalContexts.get(scriptObject).intValue();
    ModuleSpace moduleSpace=new ModuleSpaceSaf(logger,msh,scriptObject,globalContext,moduleName,key);
    attachModuleSpace(logger,moduleSpace);
    return true;
  }
 catch (  Throwable e) {
    logger.log(TreeLogger.ERROR,"Failure to load module '" + moduleName + "'",e);
    return false;
  }
}
