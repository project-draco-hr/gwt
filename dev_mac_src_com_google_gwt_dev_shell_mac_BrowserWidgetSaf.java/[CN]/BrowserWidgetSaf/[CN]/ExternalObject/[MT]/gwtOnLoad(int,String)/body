{
  try {
    if (moduleName == null) {
      handleUnload(scriptObject);
      return true;
    }
    Integer key=new Integer(scriptObject);
    ModuleSpaceHost msh=getHost().createModuleSpaceHost(BrowserWidgetSaf.this,moduleName);
    final int globalContext=globalContexts.get(scriptObject).intValue();
    ModuleSpace moduleSpace=new ModuleSpaceSaf(msh,scriptObject,globalContext,moduleName,key);
    attachModuleSpace(moduleSpace);
    return true;
  }
 catch (  Throwable e) {
    getHost().getLogger().log(TreeLogger.ERROR,"Failure to load module '" + moduleName + "'",e);
    return false;
  }
}
