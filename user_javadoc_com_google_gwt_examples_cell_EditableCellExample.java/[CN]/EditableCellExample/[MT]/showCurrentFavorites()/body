{
  if (favorites.size() > 0) {
    String text="You favorite contacts are ";
    boolean first=true;
    for (    Contact contact : favorites) {
      if (!first) {
        text+=", ";
      }
 else {
        first=false;
      }
      text+=contact.name;
    }
    Window.alert(text);
  }
 else {
    Window.alert("You have not selected any favorites.");
  }
}
