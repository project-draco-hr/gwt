{
  if (loadEventsInitialized && html != null) {
    String moduleName=GWT.getModuleName();
    String listener="__gwt_CellBasedWidgetImplLoadListeners[\"" + moduleName + "\"]();";
    html=html.replaceAll("(<img)([\\s/>])","<img onload='" + listener + "' onerror='"+ listener+ "'$2");
  }
  return html;
}
