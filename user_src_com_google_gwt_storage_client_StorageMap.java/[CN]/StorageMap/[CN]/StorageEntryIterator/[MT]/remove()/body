{
  if (index >= 0 && index < size()) {
    if (removed) {
      throw new IllegalStateException("Cannot remove() Entry - already removed!");
    }
    storage.removeItem(storage.key(index));
    removed=true;
    index--;
  }
 else {
    throw new IllegalStateException("Cannot remove() Entry - index=" + index + ", size="+ size());
  }
}
