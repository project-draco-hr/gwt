{
  Pattern blockCommentPattern=JsSourceUtils.BLOCK_COMMENT_PATTERN;
  assertMatches("/* */",blockCommentPattern);
  assertMatches("/* /* */",blockCommentPattern);
  assertMatches("/* -- */",blockCommentPattern);
  assertMatches("/* \" */",blockCommentPattern);
  assertMatches("/* \" \" */",blockCommentPattern);
  assertDoesNotMatch("/* */ */",blockCommentPattern);
}
