{
  Pattern jsRegExpPattern=JsSourceUtils.JS_REG_EXP_PATTERN;
  assertMatches("/ /",jsRegExpPattern);
  assertMatches("/ /g",jsRegExpPattern);
  assertMatches("/ /gi",jsRegExpPattern);
  assertMatches("/\"\\\"\"/",jsRegExpPattern);
  assertMatches("/ --/",jsRegExpPattern);
  assertMatches("/ \\//",jsRegExpPattern);
  assertDoesNotMatch("//",jsRegExpPattern);
  assertDoesNotMatch("///",jsRegExpPattern);
}
