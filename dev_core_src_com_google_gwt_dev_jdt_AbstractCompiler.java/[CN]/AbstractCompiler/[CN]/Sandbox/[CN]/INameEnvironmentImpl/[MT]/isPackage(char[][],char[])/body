{
  final char[] pathChars=CharOperation.concatWith(parentPkg,pkg,'.');
  String packageName=String.valueOf(pathChars);
  if (isPackage(packageName)) {
    return true;
  }
 else   if (isPackage(getClassLoader(),packageName)) {
    rememberPackage(packageName);
    return true;
  }
 else {
    return false;
  }
}
