{
  Splittable n=Splittable.NULL;
  if (GWT.isScript()) {
    assertNull(n);
  }
 else {
    assertNotNull(n);
  }
  assertFalse("boolean",n.isBoolean());
  assertFalse("indexed",n.isIndexed());
  assertFalse("keyed",n.isKeyed());
  assertFalse("string",n.isString());
  assertEquals("null",n.getPayload());
}
