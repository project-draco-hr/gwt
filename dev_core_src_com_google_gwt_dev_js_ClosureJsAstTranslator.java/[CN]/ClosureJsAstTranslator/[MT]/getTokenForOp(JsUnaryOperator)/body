{
switch (op) {
case BIT_NOT:
    return Token.BITNOT;
case DEC:
  return Token.DEC;
case DELETE:
return Token.DELPROP;
case INC:
return Token.INC;
case NEG:
return Token.NEG;
case POS:
return Token.POS;
case NOT:
return Token.NOT;
case TYPEOF:
return Token.TYPEOF;
case VOID:
return Token.VOID;
}
throw new IllegalStateException();
}
