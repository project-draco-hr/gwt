{
  if (visitor.visit(this,ctx)) {
    visitor.acceptWithInsertRemove(initializers);
    if (testExpr != null) {
      testExpr=visitor.accept(testExpr);
    }
    visitor.acceptWithInsertRemove(increments);
    if (body != null) {
      body=visitor.accept(body);
    }
  }
  visitor.endVisit(this,ctx);
}
