{
  delayTestFinish(DELAY_TEST_FINISH);
  SimpleFooRequest ctx=simpleFooRequest();
  ctx.add(3,5).to(new Receiver<Integer>(){
    @Override public void onSuccess(    Integer response){
      assertTrue(8 == response);
    }
  }
);
  ctx.add(4.0,5.0).to(new Receiver<Double>(){
    @Override public void onSuccess(    Double response){
      assertTrue(9.0 == response);
    }
  }
);
  ctx.fire(new Receiver<Void>(){
    @Override public void onSuccess(    Void response){
      finishTestAndReset();
    }
  }
);
}
