{
  delayTestFinish(DELAY_TEST_FINISH);
  simpleFooRequest().fetchDoubleReference().with("fooField","selfOneToManyField").fire(new Receiver<SimpleFooProxy>(){
    @Override public void onSuccess(    SimpleFooProxy response){
      response=checkSerialization(response);
      assertNotNull(response.getFooField());
      assertSame(response.getFooField(),response.getSelfOneToManyField().get(0));
      finishTestAndReset();
    }
  }
);
}
