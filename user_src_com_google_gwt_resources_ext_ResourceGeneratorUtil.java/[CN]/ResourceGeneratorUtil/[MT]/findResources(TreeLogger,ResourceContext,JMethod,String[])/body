{
  Locator locator=new ResourceOracleLocator(context.getGeneratorContext().getResourcesOracle());
  URL[] toReturn=findResources(logger,locator,context,method,defaultSuffixes,false);
  if (toReturn == null) {
    locator=new ClassLoaderLocator(Thread.currentThread().getContextClassLoader());
    toReturn=findResources(logger,locator,context,method,defaultSuffixes,true);
  }
  return toReturn;
}
