{
  if (x.getOp() != JBinaryOperator.ASG) {
    return;
  }
  JExpression boxed=autoboxUtils.undoUnbox(x.getLhs());
  if (boxed == null) {
    return;
  }
  JClassType boxedType=(JClassType)boxed.getType();
  ctx.replaceMe(new JBinaryOperation(program,x.getSourceInfo(),boxedType,JBinaryOperator.ASG,boxed,autoboxUtils.box(x.getRhs(),boxedType)));
}
