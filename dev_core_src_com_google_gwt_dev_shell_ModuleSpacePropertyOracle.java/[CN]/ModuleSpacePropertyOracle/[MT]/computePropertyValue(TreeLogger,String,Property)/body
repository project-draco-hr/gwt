{
  String value;
  value=prop.getActiveValue();
  if (value == null) {
    String scriptFnName=makeScriptFnName(prop);
    try {
      value=space.invokeNativeString(scriptFnName,null,Empty.CLASSES,Empty.OBJECTS);
    }
 catch (    RuntimeException e) {
      String msg="Error while executing the JavaScript provider for property '" + propertyName + "'";
      logger.log(TreeLogger.ERROR,msg,e);
      throw new BadPropertyValueException(propertyName,"<failed to compute>");
    }
  }
  return value;
}
