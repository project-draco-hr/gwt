{
  StringBuffer sb=new StringBuffer();
  sb.append("function ");
  sb.append(name);
  sb.append('(');
  boolean first=true;
  for (  String paramName : paramNames) {
    if (first) {
      first=false;
    }
 else {
      sb.append(", ");
    }
    sb.append(paramName);
  }
  sb.append("} {\n");
  sb.append(source);
  sb.append("}\n");
  return sb.toString();
}
