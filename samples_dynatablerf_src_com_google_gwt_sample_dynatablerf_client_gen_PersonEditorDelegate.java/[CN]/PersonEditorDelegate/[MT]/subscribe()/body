{
  if (eventBus == null) {
    return null;
  }
  return eventBus.addHandler(PersonProxyChanged.TYPE,new PersonProxyChanged.Handler(){
    public void onPersonChanged(    PersonProxyChanged event){
      if (object.getId().equals(event.getRecord().getId())) {
        factory.personRequest().findPerson(object.getId()).fire(new Receiver<PersonProxy>(){
          public void onSuccess(          PersonProxy response,          Set<SyncResult> syncResults){
            pushValues(response);
          }
        }
);
      }
    }
  }
);
}
