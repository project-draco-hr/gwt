{
  if (value == null) {
    sb.append("null");
    return;
  }
  Iterator<?> it=((Collection<?>)value).iterator();
  sb.append("[");
  if (it.hasNext()) {
    elementDecoder.encode(sb,it.next());
    while (it.hasNext()) {
      sb.append(",");
      elementDecoder.encode(sb,it.next());
    }
  }
  sb.append("]");
}
