{
  Class<?> type=ctx.getType();
  if (value != null) {
    if (value.equals(ValueCodex.getUninitializedFieldValue(type))) {
      return false;
    }
  }
  Splittable split;
  split=encodeValue(value);
  sb.append(",\"").append(propertyName).append("\":").append(split.getPayload());
  return false;
}
