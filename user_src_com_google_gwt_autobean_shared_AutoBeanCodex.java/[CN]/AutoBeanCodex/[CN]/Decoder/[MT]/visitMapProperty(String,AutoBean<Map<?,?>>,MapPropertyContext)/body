{
  if (data.isNull(propertyName)) {
    return false;
  }
  Map<?,?> map;
  if (ValueCodex.canDecode(ctx.getKeyType())) {
    map=decodeValueKeyMap(data.get(propertyName),ctx.getKeyType(),ctx.getValueType());
  }
 else {
    map=decodeObjectKeyMap(data.get(propertyName),ctx.getKeyType(),ctx.getValueType());
  }
  ctx.set(map);
  return false;
}
