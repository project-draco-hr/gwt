{
  EntityManager em=entityManager();
  try {
    Query query=em.createQuery("select o from Expense o where o.reportId =:reportId");
    query.setParameter("reportId",reportId);
    List<Expense> expenseList=query.getResultList();
    expenseList.size();
    return expenseList;
  }
  finally {
    em.close();
  }
}
