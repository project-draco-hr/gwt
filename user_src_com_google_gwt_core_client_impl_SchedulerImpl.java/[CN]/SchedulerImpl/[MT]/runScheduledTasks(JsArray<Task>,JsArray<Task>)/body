{
  while (tasks.length() > 0) {
    Task t=tasks.shift();
    try {
      if (t.isRepeating()) {
        if (t.executeRepeating()) {
          rescheduled.push(t);
        }
      }
 else {
        t.executeScheduled();
      }
    }
 catch (    RuntimeException e) {
      if (GWT.getUncaughtExceptionHandler() != null) {
        GWT.getUncaughtExceptionHandler().onUncaughtException(e);
      }
    }
  }
}
