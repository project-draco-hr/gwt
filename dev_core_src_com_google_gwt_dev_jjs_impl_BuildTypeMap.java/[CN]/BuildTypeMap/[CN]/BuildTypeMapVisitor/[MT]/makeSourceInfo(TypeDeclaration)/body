{
  CompilationResult compResult=typeDecl.compilationResult;
  int[] indexes=compResult.lineSeparatorPositions;
  String fileName=String.valueOf(compResult.fileName);
  int startLine=ProblemHandler.searchLineNumber(indexes,typeDecl.sourceStart);
  return new JSourceInfo(typeDecl.sourceStart,typeDecl.bodyEnd,startLine,fileName);
}
