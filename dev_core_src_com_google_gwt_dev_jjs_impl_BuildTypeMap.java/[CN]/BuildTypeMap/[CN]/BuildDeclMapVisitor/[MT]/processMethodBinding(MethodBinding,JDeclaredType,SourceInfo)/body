{
  JType returnType=(JType)typeMap.get(b.returnType);
  JMethod newMethod=program.createMethod(info,String.valueOf(b.selector),enclosingType,returnType,b.isAbstract(),b.isStatic(),b.isFinal(),b.isPrivate(),b.isNative());
  addThrownExceptions(b,newMethod);
  if (b.isSynthetic()) {
    newMethod.setSynthetic();
  }
  typeMap.put(b,newMethod);
  return newMethod;
}
