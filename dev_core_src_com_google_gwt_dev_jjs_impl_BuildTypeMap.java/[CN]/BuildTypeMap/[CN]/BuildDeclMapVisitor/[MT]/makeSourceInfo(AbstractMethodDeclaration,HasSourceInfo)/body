{
  int startLine=Util.getLineNumber(methodDecl.sourceStart,currentSeparatorPositions,0,currentSeparatorPositions.length - 1);
  SourceOrigin toReturn=SourceOrigin.create(methodDecl.sourceStart,methodDecl.bodyEnd,startLine,currentFileName);
  if (enclosing != null) {
    return enclosing.getSourceInfo().makeChild(toReturn);
  }
  return toReturn;
}
