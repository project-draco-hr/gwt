{
  JType type=(JType)typeMap.get(binding.type);
  JField field=program.createField(enclosingType.getSourceInfo().makeChild("Field " + String.valueOf(binding.name)),binding.name,enclosingType,type,false,Disposition.FINAL);
  if (binding.matchingField != null) {
    typeMap.put(binding.matchingField,field);
  }
  typeMap.put(binding,field);
  return field;
}
