{
  int startLine=Util.getLineNumber(stmt.sourceStart,currentSeparatorPositions,0,currentSeparatorPositions.length - 1);
  SourceOrigin toReturn=SourceOrigin.create(stmt.sourceStart,stmt.sourceEnd,startLine,currentFileName);
  if (enclosing != null) {
    return enclosing.getSourceInfo().makeChild(toReturn);
  }
  return toReturn;
}
