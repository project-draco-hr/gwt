{
  CompilationResult compResult=methodDecl.compilationResult;
  int[] indexes=compResult.lineSeparatorPositions;
  String fileName=String.valueOf(compResult.fileName);
  int startLine=Util.getLineNumber(methodDecl.sourceStart,indexes,0,indexes.length - 1);
  return new SourceInfo(methodDecl.sourceStart,methodDecl.bodyEnd,startLine,fileName);
}
