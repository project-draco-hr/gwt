{
  JType type=(JType)typeMap.get(binding.type);
  JField field;
  if (binding.declaringClass.isEnum()) {
    field=program.createEnumField(info,binding.name,(JEnumType)enclosingType,(JClassType)type,binding.original().id);
  }
 else {
    field=program.createField(info,binding.name,enclosingType,type,binding.isStatic(),binding.isFinal(),hasInitializer);
  }
  typeMap.put(binding,field);
  return field;
}
