{
  try {
    String computeScriptBase=Utility.getFileFromClassPath("com/google/gwt/dev/codeserver/computeScriptBase.js");
    String contents=Utility.getFileFromClassPath("com/google/gwt/dev/codeserver/stub.nocache.js");
    File file=new File(options.getModuleBaseDir() + "/" + module.getName()+ "/"+ module.getName()+ ".nocache.js");
    file.deleteOnExit();
    file.getParentFile().mkdirs();
    Map<String,String> replacements=new HashMap<String,String>();
    replacements.put("__COMPUTE_SCRIPT_BASE__",computeScriptBase);
    replacements.put("__MODULE_NAME__",module.getName());
    replacements.put("__SUPERDEV_PORT__","" + codeServerPort);
    Utility.writeTemplateFile(file,contents,replacements);
  }
 catch (  IOException e) {
    logger.log(TreeLogger.ERROR,"Unable to create nocache script ",e);
    throw new UnableToCompleteException();
  }
}
