{
  assertEquals("UriUtils.fromString(\"hi \" + foo.bar().baz() + \" friend\")",parserForHtml.parse(null,"hi {foo.bar.baz} friend"));
  assertNotNull(logger.warned);
  logger.warned=null;
  assertEquals("UriUtils.fromString(\"hi \" + foo.bar().baz() + \" friend \" + boo.bahh() + \" baz\")",parserForHtml.parse(null,"hi {foo.bar.baz} friend {boo.bahh} baz"));
  assertNotNull(logger.warned);
}
