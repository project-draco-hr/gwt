{
  assertEquals("UriUtils.fromSafeConstant(\"hi mom\")",parserForHtml.parse(null,"hi mom"));
  assertEquals("UriUtils.fromSafeConstant(\"hi mom\")",parserForWidgets.parse(null,"hi mom"));
  assertEquals("UriUtils.fromSafeConstant(\"hi {foo.bar.baz} friend\")",parserForHtml.parse(null,"hi {{foo.bar.baz} friend"));
  assertEquals("UriUtils.fromSafeConstant(\"hi {foo.bar.baz} friend\")",parserForWidgets.parse(null,"hi {{foo.bar.baz} friend"));
}
