{
  try {
    new Float("0.0e");
    fail("constructor");
  }
 catch (  NumberFormatException e) {
  }
  try {
    Float.parseFloat("0.0e");
    fail("parse");
  }
 catch (  NumberFormatException e) {
  }
  try {
    Float.valueOf("0x0e");
    fail("valueOf");
  }
 catch (  NumberFormatException e) {
  }
}
