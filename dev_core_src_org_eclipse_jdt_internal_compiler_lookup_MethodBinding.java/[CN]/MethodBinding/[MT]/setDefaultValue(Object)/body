{
  MethodBinding originalMethod=this.original();
  originalMethod.tagBits|=TagBits.DefaultValueResolved;
  AnnotationHolder holder=this.declaringClass.retrieveAnnotationHolder(this,false);
  if (holder == null)   setAnnotations(null,null,defaultValue);
 else   setAnnotations(holder.getAnnotations(),holder.getParameterAnnotations(),defaultValue);
}
