{
  TypeVariableBinding[] vars=method.typeVariables;
  if (this.typeVariables == vars)   return true;
  int length=this.typeVariables.length;
  if (length != vars.length)   return false;
  for (int i=0; i < length; i++)   if (this.typeVariables[i] != vars[i] && this.typeVariables[i].erasure() != vars[i].erasure())   return false;
  return true;
}
