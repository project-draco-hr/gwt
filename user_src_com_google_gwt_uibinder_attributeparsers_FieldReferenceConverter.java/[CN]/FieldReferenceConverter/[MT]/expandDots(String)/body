{
  StringBuilder b=new StringBuilder();
  String[] segments=value.split("[.]");
  for (  String segment : segments) {
    segment=cssConverter.convertName(segment);
    if (b.length() == 0) {
      b.append(segment);
    }
 else {
      b.append(".").append(segment).append("()");
    }
  }
  return b.toString();
}
