{
  int[] nibbles=ReverseNibbles.reverseNibbles;
  return (nibbles[i >>> 28]) | (nibbles[(i >> 24) & 0xf] << 4) | (nibbles[(i >> 20) & 0xf] << 8)| (nibbles[(i >> 16) & 0xf] << 12)| (nibbles[(i >> 12) & 0xf] << 16)| (nibbles[(i >> 8) & 0xf] << 20)| (nibbles[(i >> 4) & 0xf] << 24)| (nibbles[i & 0xf] << 28);
}
