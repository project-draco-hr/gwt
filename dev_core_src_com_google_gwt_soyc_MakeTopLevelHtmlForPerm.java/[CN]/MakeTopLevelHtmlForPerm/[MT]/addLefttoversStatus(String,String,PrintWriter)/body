{
  outFile.println("<ul class=\"soyc-exclusive\">");
  if (globalInformation.dependencies != null) {
    outFile.println("<li><a href=\"" + dependenciesFileName("total",packageName) + "#"+ className+ "\">See why it's live</a></li>");
    for (int sp=1; sp <= globalInformation.getNumSplitPoints(); sp++) {
      outFile.println("<li><a href=\"" + dependenciesFileName("sp" + sp,packageName) + "#"+ className+ "\">See why it's not exclusive to s.p. #"+ sp+ " ("+ globalInformation.getSplitPointToLocation().get(sp)+ ")</a></li>");
    }
  }
  outFile.println("</ul>");
}
