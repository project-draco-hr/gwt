{
  try {
    gwtInstallCode(code);
  }
 catch (  RuntimeException e) {
    String textIntro=code;
    if (textIntro != null && textIntro.length() > MAX_LOG_LENGTH) {
      textIntro=textIntro.substring(0,MAX_LOG_LENGTH) + "...";
    }
    onLoadError(new HttpInstallFailure(url,textIntro,e),false);
  }
}
