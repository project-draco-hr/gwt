{
  assertEquals("function a(){}\n;function b(){}\nb.prototype={};a();b();",optimize("function a(){};function b(){} b.prototype={}; a(); b();"));
  assertEquals("function defineClass(a,b){}\n;function a(){}\n;function b(){}\ndefineClass(a,b);a();b();",optimize("function defineClass(a,b){};function a(){};function b(){}" + " defineClass(a,b); a(); b();"));
