{
  if (stat instanceof JsVars) {
    for (    JsVar var : (JsVars)stat) {
      String lit=map.stringLiteralForName(var.getName());
      if (lit != null) {
        return true;
      }
      JField field=map.nameToField(var.getName());
      if (field != null) {
        return true;
      }
    }
  }
  return false;
}
