{
{
    Long long1=10L;
    Long long2=long1;
    long2+=5;
    assertEquals(10L,(long)long1);
    assertEquals(15L,(long)long2);
  }
{
    Long long1=10L;
    Long long2=long1;
    long2+=1;
    assertEquals(10,(long)long1);
    assertEquals(11,(long)long2);
  }
{
    Long long1=10L;
    Long long2=long1;
    long2-=1;
    assertEquals(10,(long)long1);
    assertEquals(9,(long)long2);
  }
{
    Long long1=10L;
    Long long2=long1;
    long2*=2;
    assertEquals(10,(long)long1);
    assertEquals(20,(long)long2);
  }
{
    Long long1=10L;
    Long long2=long1;
    long2/=2;
    assertEquals(10,(long)long1);
    assertEquals(5,(long)long2);
  }
{
    Long long1=10L;
    Long long2=long1;
    long2%=3;
    assertEquals(10,(long)long1);
    assertEquals(1,(long)long2);
  }
{
    Long long1=10L;
    Long long2=long1;
    long2<<=1;
    assertEquals(10,(long)long1);
    assertEquals(20,(long)long2);
  }
{
    Long long1=10L;
    Long long2=long1;
    long2>>=1;
    assertEquals(10,(long)long1);
    assertEquals(5,(long)long2);
  }
{
    Long long1=10L;
    Long long2=long1;
    long2>>>=1;
    assertEquals(10,(long)long1);
    assertEquals(5,(long)long2);
  }
{
    Long long1=10L;
    Long long2=long1;
    long2&=8;
    assertEquals(10,(long)long1);
    assertEquals(8,(long)long2);
  }
{
    Long long1=10L;
    Long long2=long1;
    long2|=1;
    assertEquals(10,(long)long1);
    assertEquals(11,(long)long2);
  }
{
    Long long1=10L;
    Long long2=long1;
    long2^=1;
    assertEquals(10,(long)long1);
    assertEquals(11,(long)long2);
  }
}
