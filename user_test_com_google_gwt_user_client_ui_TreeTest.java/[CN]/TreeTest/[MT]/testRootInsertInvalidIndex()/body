{
  Tree t=createTree();
  t.addItem("a");
  t.addItem("b");
  t.addItem("c");
  try {
    t.insertItem(-1,"illegal");
    fail("Expected IndexOutOfBoundsException");
  }
 catch (  IndexOutOfBoundsException e) {
  }
  try {
    t.insertItem(4,"illegal");
    fail("Expected IndexOutOfBoundsException");
  }
 catch (  IndexOutOfBoundsException e) {
  }
}
