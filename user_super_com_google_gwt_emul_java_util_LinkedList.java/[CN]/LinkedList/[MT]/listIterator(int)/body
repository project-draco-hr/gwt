{
  if (index < 0 || index > size) {
    indexOutOfBounds(index,size);
  }
  Node<E> node;
  if (index >= size >> 1) {
    node=header;
    for (int i=size; i > index; --i) {
      node=node.prev;
    }
  }
 else {
    node=header.next;
    for (int i=0; i < index; ++i) {
      node=node.next;
    }
  }
  return new ListIteratorImpl(index,node);
}
