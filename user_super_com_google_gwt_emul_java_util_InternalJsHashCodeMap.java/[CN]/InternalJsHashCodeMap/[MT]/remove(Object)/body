{
  String hashCode=hash(key);
  Entry<K,V>[] chain=getChainOrEmpty(hashCode);
  for (int i=0; i < chain.length; i++) {
    Entry<K,V> entry=chain[i];
    if (host.equals(key,entry.getKey())) {
      if (chain.length == 1) {
        removeChain(hashCode);
      }
 else {
        splice(chain,i);
      }
      size--;
      return entry.getValue();
    }
  }
  return null;
}
