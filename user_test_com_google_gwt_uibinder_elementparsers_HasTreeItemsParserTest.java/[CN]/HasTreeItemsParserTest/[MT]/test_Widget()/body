{
  StringBuffer b=new StringBuffer();
  b.append("<g:Tree>");
  b.append("  <g:Button text='1'/>");
  b.append("  <g:Button text='2'/>");
  b.append("</g:Tree>");
  tester.parse(b.toString());
  assertStatements("fieldName.addItem(<g:Button text='1'>);","fieldName.addItem(<g:Button text='2'>);");
}
