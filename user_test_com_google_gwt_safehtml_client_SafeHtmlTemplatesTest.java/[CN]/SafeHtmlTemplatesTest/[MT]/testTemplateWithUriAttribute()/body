{
  Assert.assertEquals("<span><a href=\"" + GOOD_URL_ESCAPED + "\"><b>"+ HTML_MARKUP+ "</b></a></span>",templates.templateWithUriAttribute(GOOD_URL,SafeHtmlUtils.fromSafeConstant(HTML_MARKUP)).asString());
  Assert.assertEquals("<span><a href=\"#\"><b>" + HTML_MARKUP + "</b></a></span>",templates.templateWithUriAttribute(BAD_URL,SafeHtmlUtils.fromSafeConstant(HTML_MARKUP)).asString());
}
