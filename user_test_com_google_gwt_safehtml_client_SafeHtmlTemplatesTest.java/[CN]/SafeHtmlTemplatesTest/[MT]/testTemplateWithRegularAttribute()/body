{
  Assert.assertEquals("<div id=\"" + GOOD_URL_ESCAPED + "\">"+ HTML_MARKUP+ "</div>",templates.templateWithRegularAttribute(GOOD_URL,SafeHtmlUtils.fromSafeConstant(HTML_MARKUP)).asString());
  Assert.assertEquals("<div id=\"" + BAD_URL_ESCAPED + "\">"+ HTML_MARKUP+ "</div>",templates.templateWithRegularAttribute(BAD_URL,SafeHtmlUtils.fromSafeConstant(HTML_MARKUP)).asString());
}
