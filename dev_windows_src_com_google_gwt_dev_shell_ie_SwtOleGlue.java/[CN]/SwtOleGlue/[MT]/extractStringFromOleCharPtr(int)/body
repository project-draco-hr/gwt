{
  int size=COM.SysStringByteLen(pOleChar);
  if (size > 8192) {
    size=8192;
  }
  char[] buffer=new char[(size + 1) / 2];
  COM.MoveMemory(buffer,pOleChar,size);
  String s=new String(buffer);
  if (s.indexOf('\0') != -1) {
    return s.substring(0,s.indexOf('\0'));
  }
 else {
    return s;
  }
}
