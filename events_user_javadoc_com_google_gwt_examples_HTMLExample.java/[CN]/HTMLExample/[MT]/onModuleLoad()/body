{
  Label lbl=new Label("This is just text.  It will not be interpreted " + "as <html>.");
  HTML html=new HTML("This is <b>HTML</b>.  It will be interpreted as such if you specify " + "the <span style='font-family:fixed'>asHTML</span> flag.",true);
  VerticalPanel panel=new VerticalPanel();
  panel.add(lbl);
  panel.add(html);
  RootPanel.get().add(panel);
}
