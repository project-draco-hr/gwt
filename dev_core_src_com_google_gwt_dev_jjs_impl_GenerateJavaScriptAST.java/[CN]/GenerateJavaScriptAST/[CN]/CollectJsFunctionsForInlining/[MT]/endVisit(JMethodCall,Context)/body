{
  JMethod target=x.getTarget();
  if (target.isInliningAllowed() && (target.isNative() || program.getIndexedTypes().contains(target.getEnclosingType()) || target.getInliningMode() == InliningMode.FORCE_INLINE)) {
    JsFunction function=jsFunctionsByJavaMethodBody.get(currentMethod.getBody());
    if (function != null && function.getBody() != null) {
      functionsForJsInlining.add(function);
    }
  }
}
