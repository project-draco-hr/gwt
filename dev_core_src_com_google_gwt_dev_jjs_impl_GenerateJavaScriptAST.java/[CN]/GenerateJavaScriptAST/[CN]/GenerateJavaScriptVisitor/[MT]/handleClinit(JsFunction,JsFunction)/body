{
  clinitFunc.setExecuteOnce(true);
  clinitFunc.setImpliedExecute(superClinit);
  List<JsStatement> statements=clinitFunc.getBody().getStatements();
  SourceInfo sourceInfo=clinitFunc.getSourceInfo();
  JsFunction emptyFunctionFn=incremental ? getObjectConstructorFunction() : indexedFunctions.get("JavaClassHierarchySetupUtil.emptyMethod");
  JsExpression asg=createAssignment(clinitFunc.getName().makeRef(sourceInfo),emptyFunctionFn.getName().makeRef(sourceInfo));
  statements.add(0,asg.makeStmt());
}
