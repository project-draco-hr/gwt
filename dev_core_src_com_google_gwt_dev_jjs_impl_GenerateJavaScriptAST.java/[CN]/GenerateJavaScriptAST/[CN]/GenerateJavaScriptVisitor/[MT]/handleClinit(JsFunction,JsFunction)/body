{
  clinitFunc.setExecuteOnce(true);
  clinitFunc.setImpliedExecute(superClinit);
  List<JsStatement> statements=clinitFunc.getBody().getStatements();
  SourceInfo sourceInfo=clinitFunc.getSourceInfo().makeChild("clinit reassignment");
  JsExpression asg=createAssignment(clinitFunc.getName().makeRef(sourceInfo),nullMethodName.makeRef(sourceInfo));
  statements.add(0,asg.makeStmt());
}
