{
  String name=method.getName();
  if (name.length() > 3 && name.startsWith("set") && Character.isUpperCase(name.charAt(3)) && method.getParams().size() == 1) {
    String propName=Character.toLowerCase(name.charAt(3)) + name.substring(4);
    return propName;
  }
 else   if (method.getParams().size() == 1) {
    return name;
  }
  return null;
}
