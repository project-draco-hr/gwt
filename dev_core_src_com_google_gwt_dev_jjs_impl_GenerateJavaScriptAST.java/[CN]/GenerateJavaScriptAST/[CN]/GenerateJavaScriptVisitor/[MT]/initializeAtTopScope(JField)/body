{
  if (x.getLiteralInitializer() == null) {
    return false;
  }
  if (x.isFinal() || x.isStatic()) {
    return true;
  }
  JDeclaredType current=x.getEnclosingType().getSuperClass();
  while (current != null) {
    if (canObserveSubclassFields.contains(current)) {
      return false;
    }
    current=current.getSuperClass();
  }
  return true;
}
