{
  if (alreadyRan.contains(x)) {
    return false;
  }
  if (program.getTypeClassLiteralHolder() == x) {
    return false;
  }
  if (program.immortalCodeGenTypes.contains(x)) {
    return false;
  }
  if (x.getSuperClass() != null && !alreadyRan.contains(x)) {
    accept(x.getSuperClass());
  }
  return super.visit(x,ctx);
}
