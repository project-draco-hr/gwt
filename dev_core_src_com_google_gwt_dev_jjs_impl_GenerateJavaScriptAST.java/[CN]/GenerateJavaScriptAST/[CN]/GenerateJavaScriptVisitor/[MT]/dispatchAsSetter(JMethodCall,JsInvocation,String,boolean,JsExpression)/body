{
  JsExpression result;
  JsNameRef property=new JsNameRef(x.getSourceInfo(),setter);
  property.setQualifier(isFluent ? globalTemp.makeRef(x.getSourceInfo()) : qualExpr);
  result=createAssignment(property,jsInvocation.getArguments().get(0));
  if (isFluent) {
    result=createCommaExpression(createAssignment(globalTemp.makeRef(x.getSourceInfo()),qualExpr),createCommaExpression(result,globalTemp.makeRef(x.getSourceInfo())));
  }
  return result;
}
