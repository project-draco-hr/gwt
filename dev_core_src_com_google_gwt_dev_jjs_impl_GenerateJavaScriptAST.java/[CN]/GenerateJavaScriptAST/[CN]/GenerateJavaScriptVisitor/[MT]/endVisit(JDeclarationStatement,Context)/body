{
  if (x.getInitializer() == null) {
    pop();
    push(null);
    return;
  }
  JsExpression initializer=(JsExpression)pop();
  JsNameRef localRef=(JsNameRef)pop();
  JVariable target=x.getVariableRef().getTarget();
  if (target instanceof JField) {
    JField field=(JField)target;
    if (initializeAtTopScope(field)) {
      push(null);
      return;
    }
  }
  JsBinaryOperation binOp=new JsBinaryOperation(x.getSourceInfo(),JsBinaryOperator.ASG,localRef,initializer);
  push(binOp.makeStmt());
}
