{
  String name=method.getName();
  if (name.length() > 3 && name.startsWith("has") && Character.isUpperCase(name.charAt(3)) && method.getParams().size() == 0 && method.getType() == JPrimitiveType.BOOLEAN) {
    String propName=Character.toLowerCase(name.charAt(3)) + name.substring(4);
    return propName;
  }
  return null;
}
