{
  if (!crossClassTargets.contains(x)) {
    return null;
  }
  if (!x.isStatic() || program.isStaticImpl(x)) {
    return null;
  }
  JReferenceType enclosingType=x.getEnclosingType();
  if (!typeOracle.hasClinit(enclosingType)) {
    return null;
  }
  if (x == enclosingType.methods.get(0)) {
    return null;
  }
  JMethod clinitMethod=enclosingType.methods.get(0);
  SourceInfo sourceInfo=x.getSourceInfo().makeChild("clinit call");
  JsInvocation jsInvocation=new JsInvocation(sourceInfo);
  jsInvocation.setQualifier(names.get(clinitMethod).makeRef(sourceInfo));
  return jsInvocation;
}
