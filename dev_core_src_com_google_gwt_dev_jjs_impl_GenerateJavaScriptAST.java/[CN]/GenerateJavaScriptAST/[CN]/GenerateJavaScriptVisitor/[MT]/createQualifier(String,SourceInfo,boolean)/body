{
  JsNameRef ref=null;
  if (namespace.isEmpty() || qualifyWithWnd) {
    ref=new JsNameRef(sourceInfo,"$wnd");
  }
  for (  String part : namespace.split("\\.")) {
    JsNameRef newRef=new JsNameRef(sourceInfo,part);
    if (ref != null) {
      newRef.setQualifier(ref);
    }
    ref=newRef;
  }
  return ref;
}
