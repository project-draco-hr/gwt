{
  assertEquals(0.0,Double.parseDouble("0"));
  assertEquals(-1.5,Double.parseDouble("-1.5"));
  assertEquals(3.0,Double.parseDouble("3."));
  assertEquals(0.5,Double.parseDouble(".5"));
  assertEquals("parse of 2.98e8",2.98e8,Double.parseDouble("2.98e8"));
  assertEquals("parse of -2.98e-8",-2.98e-8,Double.parseDouble("-2.98e-8"));
  assertEquals("parse of 2.08E+8",+2.98E+8,Double.parseDouble("+2.98E+8"));
  assertTrue("Can't parse MIN_VALUE",Double.MIN_VALUE == Double.parseDouble(String.valueOf(Double.MIN_VALUE)));
  assertTrue("Can't parse MAX_VALUE",Double.MAX_VALUE == Double.parseDouble(String.valueOf(Double.MAX_VALUE)));
}
