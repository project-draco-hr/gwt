{
class Foo {
    native String a();
    native String b();
    String c(){
      return a();
    }
  }
  Foo f=new Foo();
  assertEquals(f.a(),"blah");
  assertEquals(f.b(),"blah");
  assertEquals(f.c(),"blah");
  Foo fo=new Foo(){
    native String a();
    native String b();
    native String c();
  }
;
  assertEquals(fo.a(),"oblah");
  assertEquals(fo.b(),"oblah");
  assertEquals(fo.c(),"oblah");
}
