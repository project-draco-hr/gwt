{
  if (left < 0) {
    left=0;
  }
  if (top < 0) {
    top=0;
  }
  leftPosition=left;
  topPosition=top;
  left-=Document.get().getBodyOffsetLeft();
  top-=Document.get().getBodyOffsetTop();
  Element elem=getElement();
  DOM.setStyleAttribute(elem,"left",left + "px");
  DOM.setStyleAttribute(elem,"top",top + "px");
}
