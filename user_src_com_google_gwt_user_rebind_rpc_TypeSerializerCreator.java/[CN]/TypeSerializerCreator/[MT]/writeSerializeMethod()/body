{
  srcWriter.print(SERIALIZE_METHOD_SIGNATURE);
  srcWriter.println(" /*-" + '{');
{
    String serializerTypeName=getTypeSerializerClassName();
    srcWriter.indent();
    srcWriter.println("var methodTable = @" + serializerTypeName + "::methodMap[typeSignature];");
    srcWriter.println("if (!methodTable) {");
    srcWriter.indentln("@" + serializerTypeName + "::raiseSerializationException(Ljava/lang/String;)(typeSignature);");
    srcWriter.println("}");
    srcWriter.println("methodTable[2](streamWriter, instance);");
    srcWriter.outdent();
  }
  srcWriter.println("}-*/;");
  srcWriter.println();
}
