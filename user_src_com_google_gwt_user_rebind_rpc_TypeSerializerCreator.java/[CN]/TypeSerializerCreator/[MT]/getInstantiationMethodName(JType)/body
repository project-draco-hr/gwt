{
  JArrayType arrayType=type.isArray();
  if (arrayType != null) {
    JType leafType=arrayType.getLeafType();
    return "create_" + leafType.getQualifiedSourceName().replace('.','_') + "_Array_Rank_"+ arrayType.getRank();
  }
  return "create_" + serializationOracle.getFieldSerializerName(type).replace('.','_');
}
