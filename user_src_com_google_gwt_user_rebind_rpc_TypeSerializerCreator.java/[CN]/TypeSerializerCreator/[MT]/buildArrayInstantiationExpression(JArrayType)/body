{
  String expression="[rank]";
  JType componentType=array.getComponentType();
  while (true) {
    array=componentType.isArray();
    if (array == null) {
      break;
    }
    expression+="[0]";
    componentType=array.getComponentType();
  }
  expression=componentType.getQualifiedSourceName() + expression;
  return expression;
}
