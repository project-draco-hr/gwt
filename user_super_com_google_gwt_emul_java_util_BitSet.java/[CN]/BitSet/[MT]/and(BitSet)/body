{
  if (this == set) {
    return;
  }
  int limit=Math.min(array.length,set.array.length);
  int index=0;
  for (; index < limit; index++) {
    int word=wordAt(array,index);
    if (word != 0) {
      array[index]=word & wordAt(set.array,index);
    }
  }
  Arrays.fill(array,index,array.length,0);
}
