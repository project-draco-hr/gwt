{
  Type type=Type.getType(annotData.getDesc());
  try {
    Class<?> clazz=Class.forName(type.getClassName(),false,Thread.currentThread().getContextClassLoader());
    if (!Annotation.class.isAssignableFrom(clazz)) {
      logger.log(TreeLogger.ERROR,"Type " + type.getClassName() + " is not an annotation");
      return null;
    }
    return clazz.asSubclass(Annotation.class);
  }
 catch (  ClassNotFoundException e) {
    logger.log(TreeLogger.WARN,"Ignoring unresolvable annotation type " + type.getClassName());
    return null;
  }
}
