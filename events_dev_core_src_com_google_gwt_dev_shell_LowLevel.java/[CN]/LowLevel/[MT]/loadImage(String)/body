{
  ClassLoader cl=LowLevel.class.getClassLoader();
  InputStream is=cl.getResourceAsStream(LowLevel.PACKAGE_PATH + name);
  if (is != null) {
    try {
      Image image=new Image(null,is);
      return image;
    }
  finally {
      try {
        is.close();
      }
 catch (      IOException e) {
      }
    }
  }
 else {
    return new Image(null,1,1);
  }
}
