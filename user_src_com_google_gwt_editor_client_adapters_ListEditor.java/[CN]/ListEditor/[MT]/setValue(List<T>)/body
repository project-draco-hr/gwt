{
  if ((list == null && value == null) || (list != null && list.isSameValue(value))) {
    list.refresh();
    return;
  }
  if (list != null) {
    list.detach();
  }
  if (value == null) {
    list=null;
  }
 else {
    list=new ListEditorWrapper<T,E>(value,chain,editorSource);
    list.attach();
  }
}
