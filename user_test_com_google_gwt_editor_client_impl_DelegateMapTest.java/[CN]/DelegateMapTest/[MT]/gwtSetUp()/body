{
  Address a=new Address();
  a.setCity("city");
  a.setStreet("street");
  Person m=new Person();
  m.setName("manager");
  person=new Person();
  person.setName("name");
  person.setAddress(a);
  person.setManager(m);
  editor=new PersonEditorWithCoAddressEditorView();
  driver=GWT.create(PersonEditorWithCoAddressEditorViewDriver.class);
  driver.initialize(editor);
  driver.edit(person);
  map=driver.getDelegateMap();
}
