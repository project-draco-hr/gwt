{
  boolean error=true;
  for (  String possibleValue : prop.getAllowedValues(prop.getRootCondition())) {
    if (possibleValue.equals(value.token)) {
      error=false;
      break;
    }
  }
  if (error) {
    logger.log(TreeLogger.ERROR,"The fallback value '" + value.token + "' was not previously defined for property '"+ prop.getName()+ "'");
    throw new UnableToCompleteException();
  }
  prop.setFallback(value.token);
  return null;
}
