{
  RootScopeHandler handler=(RootScopeHandler)handlersByScope.get(scope);
  if (handler == null) {
    JsScope parent=scope.getParent();
    if (parent == null) {
      handler=new RootScopeHandler(scope);
      handlersByScope.put(scope,handler);
    }
 else {
      handler=findHandler(parent);
    }
  }
  return handler;
}
