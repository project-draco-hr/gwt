{
  if (this.serializerClass != null && this.serializerClass != serializerClass) {
    logger.log(TreeLogger.ERROR,"Type " + serializeeClass.getQualifiedSourceName() + " has more than one custom field serializer, "+ this.serializerClass.getQualifiedSourceName()+ " and "+ serializerClass.getQualifiedSourceName(),null);
    return false;
  }
  this.serializerClass=serializerClass;
  return true;
}
