{
  StackPanel p=new StackPanel();
  assertEquals(-1,p.getSelectedIndex());
  Label a=new Label("a");
  Label b=new Label("b");
  Label c=new Label("c");
  Label d=new Label("d");
  p.add(a);
  p.add(b,"b");
  p.add(c);
  p.add(d,"d");
  assertEquals(0,p.getSelectedIndex());
  p.showStack(2);
  assertEquals(2,p.getSelectedIndex());
  p.showStack(-1);
  assertEquals(-1,p.getSelectedIndex());
}
