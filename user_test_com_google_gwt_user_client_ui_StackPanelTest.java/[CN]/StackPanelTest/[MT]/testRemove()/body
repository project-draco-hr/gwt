{
  StackPanel p=new StackPanel();
  Label a=new Label("a");
  Label b=new Label("b");
  Label c=new Label("c");
  Label d=new Label("d");
  p.add(a);
  p.add(b,"b");
  p.add(c);
  p.add(d,"d");
  assertEquals("a b c d",curContents(p));
  p.remove(c);
  assertEquals("a b d",curContents(p));
  p.remove(1);
  assertEquals("a d",curContents(p));
  assertFalse(p.remove(b));
  p.remove(a);
  assertEquals("d",curContents(p));
  p.remove(a);
  assertEquals("d",curContents(p));
}
