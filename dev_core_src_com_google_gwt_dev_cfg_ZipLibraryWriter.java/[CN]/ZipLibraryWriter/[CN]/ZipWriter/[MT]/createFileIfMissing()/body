{
  if (!zipFile.exists()) {
    try {
      zipFile.createNewFile();
      if (!zipFile.canWrite()) {
        throw new CompilerIoException("Created new library file " + zipFile.getPath() + " but am unable to write to it.");
      }
    }
 catch (    IOException e) {
      throw new CompilerIoException("Failed to create new library file " + zipFile.getPath() + ".",e);
    }
  }
}
