{
  Widget oldWidget=widgets[index];
  if (oldWidget == w) {
    return;
  }
  if (w != null) {
    w.removeFromParent();
  }
  if (oldWidget != null) {
    orphan(oldWidget);
    DOM.removeChild(elements[index],oldWidget.getElement());
  }
  widgets[index]=w;
  if (w != null) {
    DOM.appendChild(elements[index],w.getElement());
    adopt(w);
  }
}
