{
  Grid r=new Grid(4,1);
  assertEquals(4,r.getRowCount());
  assertEquals(1,r.getColumnCount());
  r.resizeRows(0);
  assertEquals(0,r.getRowCount());
  r.resizeColumns(0);
  assertEquals(0,r.getColumnCount());
  r.resize(3,2);
  assertEquals(3,r.getRowCount());
  assertEquals(2,r.getColumnCount());
}
