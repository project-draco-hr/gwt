{
  final RichTextArea area=new RichTextArea();
  BasicFormatter formatter=area.getBasicFormatter();
  RootPanel.get().add(area);
  if (formatter != null) {
    try {
      formatter.toggleBold();
      if (!GWT.isScript()) {
        fail("Expected AssertionError");
      }
    }
 catch (    AssertionError e) {
      return;
    }
    if (!GWT.isScript()) {
      fail("Expected AssertionError");
    }
  }
}
