{
  JParameter[] methodParams=method.getParameters();
  for (int i=0; i < methodParams.length; ++i) {
    JParameter methodParam=methodParams[i];
    String paramName=methodParam.getName();
    String paramValue=(String)params.get(paramName);
    if (paramValue == null) {
      String msg="Could not find the meta data attribute " + BENCHMARK_PARAM_META + " for the parameter "+ paramName+ " on method "+ method.getName();
      logger.log(TreeLogger.ERROR,msg,null);
      throw new UnableToCompleteException();
    }
  }
}
