{
  Boolean viewData=getViewData(key);
  if (viewData != null && viewData.equals(value)) {
    clearViewData(key);
    viewData=null;
  }
  sb.append("<input type=\"checkbox\"");
  if (value != null) {
    boolean isChecked=(viewData != null) ? viewData : value;
    if (isChecked) {
      sb.append(" checked");
    }
  }
  sb.append("/>");
}
