{
  TreeLogger branch=logger.branch(TreeLogger.TRACE,"Tracing compile failure path for type '" + typeSourceName + "'");
  if (logErrorChain(branch,logLevel,typeSourceName,compilerContext.getGlobalCompilationErrorsIndex())) {
    return;
  }
  if (hint) {
    logHints(logger,compilerContext,typeSourceName);
  }
}
