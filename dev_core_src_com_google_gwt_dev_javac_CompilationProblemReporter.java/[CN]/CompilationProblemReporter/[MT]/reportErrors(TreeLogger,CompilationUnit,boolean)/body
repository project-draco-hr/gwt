{
  CategorizedProblem[] problems=unit.getProblems();
  if (problems == null || problems.length == 0) {
    return false;
  }
  TreeLogger branch=CompilationProblemReporter.reportErrors(logger,unit.getProblems(),unit.getResourceLocation(),unit.isError(),new SourceFetcher(){
    @Override public String getSource(){
      return unit.getSource();
    }
  }
,unit.getTypeName(),suppressErrors);
  return branch != null;
}
