{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  ScaffoldRecordPlace other=(ScaffoldRecordPlace)obj;
  if (!id.equals(other.id)) {
    return false;
  }
  if (!operation.equals(other.operation)) {
    return false;
  }
  return true;
}
