{
  addSnippetImport("jsinterop.annotations.JsMethod");
  addSnippetImport("jsinterop.annotations.JsOptional");
  addSnippetClassDecl("public static class Buggy {","  @JsMethod public void m(@JsOptional int i, @JsOptional byte b) {}","}");
  assertBuggyFails("Line 6: JsOptional parameter 'b' in method 'void EntryPoint.Buggy.m(int, byte)' cannot be " + "of primitive type.","Line 6: JsOptional parameter 'i' in method 'void EntryPoint.Buggy.m(int, byte)' cannot be " + "of primitive type.");
}
