{
  addSnippetImport("com.google.gwt.core.client.js.JsType");
  addSnippetClassDecl("@JsType(prototype = \"S\") public static class Super {","  public Super(int x) {","  }","}","@JsType(prototype = \"x\") public static class Buggy extends Super {","  public Buggy(int n) {","    super(n++);","  }","}");
  assertBuggySucceeds();
}
