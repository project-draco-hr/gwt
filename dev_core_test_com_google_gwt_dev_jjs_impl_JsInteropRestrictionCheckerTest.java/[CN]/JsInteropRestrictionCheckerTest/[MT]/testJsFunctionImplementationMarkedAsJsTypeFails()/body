{
  addAll(jsFunctionInterface);
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetClassDecl("@JsType","public static class Buggy implements MyJsFunctionInterface {","  public int foo(int x) { return 0; }","}");
  assertBuggyFails("'test.EntryPoint$Buggy' cannot be both a JsFunction implementation and a JsType at the " + "same time.");
}
