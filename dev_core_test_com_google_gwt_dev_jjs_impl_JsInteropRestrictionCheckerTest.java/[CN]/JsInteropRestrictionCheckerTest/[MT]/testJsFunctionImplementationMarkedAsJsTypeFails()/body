{
  addAll(jsFunctionInterface);
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetClassDecl("@JsType","public static final class Buggy implements MyJsFunctionInterface {","  public int foo(int x) { return 0; }","}");
  assertBuggyFails("Line 5: 'EntryPoint.Buggy' cannot be both a JsFunction implementation and a JsType " + "at the same time.");
}
