{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetImport("jsinterop.annotations.JsFunction");
  addSnippetClassDecl("public static String someString() { return \"hello\"; }","@JsFunction public interface Buggy {","  static String s = someString();","  void m();","}");
