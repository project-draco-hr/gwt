{
  addSnippetImport("com.google.gwt.core.client.js.JsType");
  addSnippetImport("com.google.gwt.core.client.js.JsFunction");
  addSnippetClassDecl("public static String someString() { return \"hello\"; }","@JsFunction public interface Buggy {","  static String s = someString();","  void m();","}");
  assertBuggyFails("JsFunction 'test.EntryPoint$Buggy' cannot have static initializer.");
}
