{
  addSnippetImport("com.google.gwt.core.client.js.JsType");
  addSnippetImport("com.google.gwt.core.client.js.JsNoExport");
  addSnippetClassDecl("@JsType(prototype = \"B\") static class Buggy {","  @JsNoExport public int x;","}");
  assertBuggyFails("Native JsType member 'test.EntryPoint$Buggy.x' is not public or has @JsNoExport.");
}
