{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetImport("jsinterop.annotations.JsIgnore");
  addSnippetClassDecl("@JsType(isNative=true) static class Buggy {","  @JsIgnore public int x;","}");
  assertBuggyFails("Line 6: Native JsType member 'int EntryPoint.Buggy.x' is not public or has @JsIgnore.");
}
