{
  addSnippetImport("com.google.gwt.core.client.js.JsType");
  addSnippetClassDecl("@JsType(prototype = \"x\") public static class Buggy {","  public static String s = \"hello\";","  static {  s += \"hello\"; }","}");
  assertBuggyFails("Native JsType 'test.EntryPoint$Buggy' cannot have static initializer.");
}
