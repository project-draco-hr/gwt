{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetClassDecl("@JsType(isNative=true) public static class Buggy {","  public static String s = \"hello\";","  static {  s += \"hello\"; }","}");
  assertBuggyFails("Native JsType 'test.EntryPoint$Buggy' cannot have static initializer.");
}
