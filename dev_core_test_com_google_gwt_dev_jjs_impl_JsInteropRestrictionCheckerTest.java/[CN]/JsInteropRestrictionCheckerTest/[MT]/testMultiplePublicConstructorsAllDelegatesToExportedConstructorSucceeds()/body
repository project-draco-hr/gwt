{
  addSnippetImport("com.google.gwt.core.client.js.JsExport");
  addSnippetImport("com.google.gwt.core.client.js.JsNoExport");
  addSnippetClassDecl("@JsExport","public static class Buggy {","  public Buggy() {}","  @JsNoExport","  public Buggy(int a) {","    this();","  }","}");
  assertBuggySucceeds();
}
