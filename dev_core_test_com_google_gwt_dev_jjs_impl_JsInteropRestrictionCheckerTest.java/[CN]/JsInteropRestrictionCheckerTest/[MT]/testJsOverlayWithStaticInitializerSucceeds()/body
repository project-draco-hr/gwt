{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetImport("jsinterop.annotations.JsOverlay");
  addSnippetClassDecl("@JsType(isNative=true) public static class Buggy {","  @JsOverlay public final static Object f1 = new Object();","  @JsOverlay public static int f2 = 2;","}");
  assertBuggySucceeds();
}
