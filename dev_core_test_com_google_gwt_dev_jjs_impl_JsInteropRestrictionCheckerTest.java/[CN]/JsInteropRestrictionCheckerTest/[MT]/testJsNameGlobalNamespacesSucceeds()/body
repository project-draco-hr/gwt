{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetImport("jsinterop.annotations.JsMethod");
  addSnippetImport("jsinterop.annotations.JsProperty");
  addSnippetImport("jsinterop.annotations.JsPackage");
  addSnippetClassDecl("@JsType(namespace = JsPackage.GLOBAL) public static class Buggy {","   @JsMethod(namespace = JsPackage.GLOBAL) public static void m() {}","   @JsProperty(namespace = JsPackage.GLOBAL) public static int  n;","}");
  assertBuggySucceeds();
}
