{
  addSnippetImport("com.google.gwt.core.client.js.JsType");
  addSnippetClassDecl("@JsType","public static class Buggy {","  public void show() {}","  public final int show = 0;","}");
  assertCompileFails("new Buggy();","Method 'test.EntryPoint$Buggy.show()V' can't be exported in type " + "'test.EntryPoint$Buggy' because the member name 'show' is already taken.");
}
