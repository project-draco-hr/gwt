{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetClassDecl("@JsType","public static class Buggy {","  public void show() {}","  public final int show = 0;","}");
  assertBuggyFails("'test.EntryPoint$Buggy.show()V' can't be exported in type " + "'test.EntryPoint$Buggy' because the name 'show' is already taken.");
}
