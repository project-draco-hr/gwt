{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetClassDecl("@JsType","public static class Buggy {","  public void show(int x) {}","  public void show() {}","}");
  assertBuggyFails("Line 7: 'void EntryPoint.Buggy.show()' and 'void EntryPoint.Buggy.show(int)' cannot both " + "use the same JavaScript name 'show'.");
}
