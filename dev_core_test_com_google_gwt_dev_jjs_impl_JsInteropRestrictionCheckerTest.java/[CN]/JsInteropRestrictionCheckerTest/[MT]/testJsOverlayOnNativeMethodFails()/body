{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetImport("jsinterop.annotations.JsOverlay");
  addSnippetClassDecl("@JsType(isNative=true) public static class Buggy {","  @JsOverlay public final native void m();","}");
  assertBuggyFails("JsOverlay method 'test.EntryPoint$Buggy.m()V' cannot be non-final, static, nor native.");
}
