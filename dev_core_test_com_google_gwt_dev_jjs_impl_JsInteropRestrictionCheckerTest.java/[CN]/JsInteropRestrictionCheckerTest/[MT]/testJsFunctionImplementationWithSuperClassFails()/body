{
  addAll(jsFunctionInterface);
  addSnippetClassDecl("public static class BaseClass {}","public static class Buggy extends BaseClass implements MyJsFunctionInterface {","  public int foo(int x) { return 0; }","}");
  assertBuggyFails("Line 4: JsFunction implementation 'EntryPoint.Buggy' cannot " + "extend a class.");
}
