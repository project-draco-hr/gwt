{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetClassDecl("@JsType","public static class Buggy {","  public Buggy() {}","  private Buggy(int a) {","    new Buggy();","  }","}");
  assertBuggyFails("Line 6: Constructor 'EntryPoint.Buggy.EntryPoint$Buggy()' can be a JsConstructor only if " + "all constructors in the class are delegating to it.");
}
