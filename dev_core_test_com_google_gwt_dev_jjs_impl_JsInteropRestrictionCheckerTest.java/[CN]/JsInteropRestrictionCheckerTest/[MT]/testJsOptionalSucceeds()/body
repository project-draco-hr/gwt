{
  addSnippetImport("jsinterop.annotations.JsConstructor");
  addSnippetImport("jsinterop.annotations.JsMethod");
  addSnippetImport("jsinterop.annotations.JsOptional");
  addSnippetClassDecl("public static class Buggy {","  @JsConstructor public Buggy(@JsOptional Object a) {}","  @JsMethod public void fun(int a, Object b, @JsOptional String c) {}","  @JsMethod public void bar(int a, @JsOptional Object b, @JsOptional String c) {}","  @JsMethod public void baz(@JsOptional String a, @JsOptional Object b) {}","}");
  assertBuggySucceeds();
}
