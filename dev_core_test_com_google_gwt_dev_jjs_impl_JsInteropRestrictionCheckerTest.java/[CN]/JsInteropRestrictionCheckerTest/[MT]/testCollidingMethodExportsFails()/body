{
  addSnippetImport("com.google.gwt.core.client.js.JsExport");
  addSnippetClassDecl("public static class Buggy {","  @JsExport(\"show\")","  public static void show() {}","  @JsExport(\"show\")","  public static void display() {}","}");
  assertBuggyFails("Member 'test.EntryPoint$Buggy.display()V' can't be exported " + "because the global name 'test.EntryPoint.Buggy.show' is already taken.");
}
