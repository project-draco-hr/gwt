{
  addSnippetImport("com.google.gwt.core.client.js.JsExport");
  addSnippetClassDecl("public static class Buggy {","  @JsExport(\"show\")","  public static void show() {}","  @JsExport(\"show\")","  public static final int display = 0;","}");
  assertBuggyFails("Member 'test.EntryPoint$Buggy.show()V' can't be exported because the " + "global name 'test.EntryPoint.Buggy.show' is already taken.");
}
