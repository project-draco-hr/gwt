{
  addSnippetImport("jsinterop.annotations.JsMethod");
  addSnippetImport("jsinterop.annotations.JsProperty");
  addSnippetClassDecl("public static class Buggy {","  @JsMethod","  public static void show() {}","  @JsProperty","  public static final int show = 0;","}");
  assertBuggyFails("'test.EntryPoint$Buggy.show()V' can't be exported because the " + "global name 'test.EntryPoint.Buggy.show' is already taken.");
}
