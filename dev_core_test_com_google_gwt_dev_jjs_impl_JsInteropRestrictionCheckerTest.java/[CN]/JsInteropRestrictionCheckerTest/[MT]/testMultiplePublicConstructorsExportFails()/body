{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetClassDecl("@JsType","public static class Buggy {","  public Buggy() {}","  public Buggy(int a) {","    this();","  }","}");
  assertBuggyFails("Line 5: More than one JsConstructor exists for EntryPoint.Buggy.","Line 7: 'EntryPoint.Buggy.EntryPoint$Buggy(int)' cannot be exported because the global " + "name 'test.EntryPoint.Buggy' is already taken by " + "'EntryPoint.Buggy.EntryPoint$Buggy()'.");
}
