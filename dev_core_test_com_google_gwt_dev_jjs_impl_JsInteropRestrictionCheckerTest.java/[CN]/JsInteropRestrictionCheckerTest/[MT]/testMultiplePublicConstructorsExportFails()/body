{
  addSnippetImport("com.google.gwt.core.client.js.JsExport");
  addSnippetClassDecl("@JsExport","public static class Buggy {","  public Buggy() {}","  public Buggy(int a) {}","}");
  assertCompileFails("new Buggy();","Member 'test.EntryPoint$Buggy.EntryPoint$Buggy(I) <init>' can't be " + "exported because the global name 'test.EntryPoint.Buggy' is already taken.");
}
