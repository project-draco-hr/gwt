{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetClassDecl("@JsType","public static class Buggy {","  public Buggy() {}","  public Buggy(int a) {","    this();","  }","}");
  assertBuggyFails("More than one JsConstructor exists for test.EntryPoint$Buggy.","'test.EntryPoint$Buggy.EntryPoint$Buggy(I) <init>' can't be " + "exported because the global name 'test.EntryPoint.Buggy' is already taken.");
}
