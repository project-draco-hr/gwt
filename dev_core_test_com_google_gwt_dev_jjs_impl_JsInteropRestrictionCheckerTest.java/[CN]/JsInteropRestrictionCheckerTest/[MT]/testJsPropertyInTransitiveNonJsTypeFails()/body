{
  addSnippetImport("com.google.gwt.core.client.js.JsType");
  addSnippetImport("com.google.gwt.core.client.js.JsProperty");
  addSnippetClassDecl("@JsType","public static interface ParentExported {","}","public static interface Exported extends ParentExported {","  @JsProperty","  public int getX();","}","public static class Buggy {} // Unrelated class");
  assertBuggyFails("Method 'getX' can't be a JsProperty since interface " + "'test.EntryPoint$Exported' is not a JsType.");
}
