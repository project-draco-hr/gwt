{
  addSnippetImport("com.google.gwt.core.client.js.JsType");
  addSnippetClassDecl("public static interface IBuggyParent {}","@JsType","public static interface IBuggy extends IBuggyParent {}","public static class Buggy {} // Unrelated class");
  assertBuggySucceeds("JsType interface 'test.EntryPoint$IBuggy' extends non-JsType " + "interface 'test.EntryPoint$IBuggyParent'. This is not recommended.");
}
