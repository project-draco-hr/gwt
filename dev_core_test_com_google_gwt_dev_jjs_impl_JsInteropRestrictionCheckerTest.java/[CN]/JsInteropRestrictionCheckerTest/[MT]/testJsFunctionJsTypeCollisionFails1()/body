{
  addAll(buggyInterfaceBothJsFunctionAndJsType);
  addSnippetClassDecl("public static class Buggy implements MyBuggyInterface2 {","  public int foo(int x) { return x; }","}");
  assertCompileFails("new Buggy();","'test.EntryPoint$Buggy' cannot be annotated as (or extend) both a @JsFunction and a " + "@JsType at the same time.","'test.MyBuggyInterface2' cannot be annotated as (or extend) both a @JsFunction and a " + "@JsType at the same time.");
}
