{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetImport("jsinterop.annotations.JsIgnore");
  addSnippetClassDecl("@JsType(isNative=true) static class Buggy {","  @JsIgnore public native void m();","}");
  assertBuggyFails("Native JsType member 'test.EntryPoint$Buggy.m()V' is not public or has @JsIgnore.");
}
