{
  addSnippetImport("com.google.gwt.core.client.js.JsType");
  addSnippetImport("com.google.gwt.core.client.js.JsProperty");
  addSnippetClassDecl("@JsType","public static interface IBuggy {","  @JsProperty boolean hasX();","  @JsProperty int x();","  @JsProperty void x(int x);","}","public static class Buggy implements IBuggy {","  public boolean hasX() {return false;}","  public int x() {return 0;}","  public void x(int x) {};","}");
  assertCompileSucceeds("new Buggy();");
}
