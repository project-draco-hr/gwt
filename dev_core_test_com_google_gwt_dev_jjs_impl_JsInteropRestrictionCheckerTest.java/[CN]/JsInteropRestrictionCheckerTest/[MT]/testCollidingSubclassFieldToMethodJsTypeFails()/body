{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetClassDecl("@JsType","public static class ParentBuggy {","  public int foo = 55;","}","@JsType","public static class Buggy extends ParentBuggy {","  public void foo(int a) {}","}");
  assertBuggyFails("Line 10: 'void EntryPoint.Buggy.foo(int)' and 'int EntryPoint.ParentBuggy.foo' cannot " + "both use the same JavaScript name 'foo'.");
}
