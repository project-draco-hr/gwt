{
  addSnippetImport("jsinterop.annotations.JsProperty");
  addSnippetClassDecl("public static class ParentBuggy {","  @JsProperty public int getFoo() { return 0; }","}","public static class Buggy extends ParentBuggy {","  @JsProperty(name = \"bar\") public int getFoo() { return 0; }","}");
  assertBuggyFails("Line 8: 'int EntryPoint.Buggy.getFoo()' " + "cannot be assigned a different JavaScript name than the method it overrides.");
}
