{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetImport("jsinterop.annotations.JsFunction");
  addSnippetClassDecl("@JsFunction @JsType","public interface Buggy {","  void foo();","}");
  assertBuggyFails("'test.EntryPoint$Buggy' cannot be both a JsFunction and a JsType at the same time.");
}
