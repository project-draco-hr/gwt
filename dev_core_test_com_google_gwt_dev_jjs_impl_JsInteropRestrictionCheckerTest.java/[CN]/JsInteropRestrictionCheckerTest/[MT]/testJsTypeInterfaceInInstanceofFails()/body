{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetClassDecl("@JsType(isNative=true) public interface IBuggy {}","@JsType public static class Buggy {","  public Buggy() { if (new Object() instanceof IBuggy) {} }","}");
  assertBuggyFails("Line 6: Cannot do instanceof against native JsType interface " + "'EntryPoint.IBuggy'.");
