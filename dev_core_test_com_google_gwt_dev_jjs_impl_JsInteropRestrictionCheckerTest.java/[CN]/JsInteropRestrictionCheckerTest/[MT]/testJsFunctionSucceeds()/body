{
  addSnippetImport("jsinterop.annotations.JsFunction");
  addSnippetImport("jsinterop.annotations.JsOverlay");
  addSnippetClassDecl("@JsFunction","public interface Function {","  int getFoo();","}","public static final class Buggy implements Function {","  public int getFoo() { return 0; }","  public final void blah() {}","  public void blat() {}","  private void bleh() {}","  static void blet() {","    new Function() {","       public int getFoo() { return 0; }","    }.getFoo();","  }","  String x = someString();","  static int y;","}","public static String someString() { return \"hello\"; }");
  assertBuggySucceeds();
}
