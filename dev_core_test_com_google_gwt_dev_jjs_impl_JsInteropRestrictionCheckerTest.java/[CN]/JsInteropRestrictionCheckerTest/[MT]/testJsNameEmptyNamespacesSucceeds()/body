{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetImport("jsinterop.annotations.JsMethod");
  addSnippetImport("jsinterop.annotations.JsProperty");
  addSnippetClassDecl("@JsType(namespace = \"\") public static class Buggy {","   @JsMethod(namespace = \"\") public static void m() {}","   @JsProperty(namespace = \"\") public static int  n;","}");
  assertBuggySucceeds();
}
