{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetImport("jsinterop.annotations.JsIgnore");
  addSnippetClassDecl("@JsType(isNative=true) static class Buggy {","  @JsIgnore public Buggy() { }","}");
  assertBuggyFails("Native JsType member 'test.EntryPoint$Buggy.EntryPoint$Buggy() <init>' " + "is not public or has @JsIgnore.");
}
