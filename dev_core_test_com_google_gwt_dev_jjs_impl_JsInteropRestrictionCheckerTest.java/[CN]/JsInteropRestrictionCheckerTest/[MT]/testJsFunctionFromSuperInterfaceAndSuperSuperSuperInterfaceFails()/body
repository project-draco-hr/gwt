{
  addAll(jsFunctionSubInterface,jsFunctionInterface1,jsFunctionSubSubInterface,jsFunctionInterface3);
  addSnippetClassDecl("public static class Buggy implements MyJsFunctionInterface1, ","MyJsFunctionSubSubInterface {","  public int foo(int x) { return 0; }","}");
  assertCompileFails("'test.EntryPoint$Buggy' implements more than one JsFunction interfaces: " + "[interface MyJsFunctionInterface3 , interface MyJsFunctionInterface1 ]");
}
