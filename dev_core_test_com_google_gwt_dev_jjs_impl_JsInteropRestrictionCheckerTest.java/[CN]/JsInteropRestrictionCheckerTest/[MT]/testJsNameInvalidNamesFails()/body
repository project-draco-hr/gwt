{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetImport("jsinterop.annotations.JsMethod");
  addSnippetImport("jsinterop.annotations.JsProperty");
  addSnippetClassDecl("@JsType(name = \"a.b.c\") public static class Buggy {","   @JsMethod(name = \"34s\") public void m() {}","   @JsProperty(name = \"s^\") public int  m;","   @JsProperty(name = \"\") public int n;","}");
  assertBuggyFails("Line 6: 'EntryPoint.Buggy' has invalid name 'a.b.c'.","Line 7: 'void EntryPoint.Buggy.m()' has invalid name '34s'.","Line 8: 'int EntryPoint.Buggy.m' has invalid name 's^'.","Line 9: 'int EntryPoint.Buggy.n' cannot have an empty name.");
}
