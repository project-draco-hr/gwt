{
  addSnippetImport("jsinterop.annotations.JsProperty");
  addSnippetClassDecl("public static class Buggy {","  @JsProperty","  public static final int show = 0;","  @JsProperty(name = \"show\")","  public static final int display = 0;","}");
  assertBuggyFails("Line 8: 'int EntryPoint.Buggy.display' cannot be exported because the global " + "name 'test.EntryPoint.Buggy.show' is already taken.");
}
