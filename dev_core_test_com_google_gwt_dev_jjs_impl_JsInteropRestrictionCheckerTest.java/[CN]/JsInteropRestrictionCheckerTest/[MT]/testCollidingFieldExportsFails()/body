{
  addSnippetImport("com.google.gwt.core.client.js.JsExport");
  addSnippetClassDecl("public static class Buggy {","  @JsExport(\"show\")","  public static final int show = 0;","  @JsExport(\"show\")","  public static final int display = 0;","}");
  assertBuggyFails("Member 'test.EntryPoint$Buggy.display' can't be exported because the " + "global name 'show' is already taken.");
}
