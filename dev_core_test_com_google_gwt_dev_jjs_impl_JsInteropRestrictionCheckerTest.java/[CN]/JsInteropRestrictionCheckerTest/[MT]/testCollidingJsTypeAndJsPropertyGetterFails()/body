{
  addSnippetImport("jsinterop.annotations.JsType");
  addSnippetImport("jsinterop.annotations.JsProperty");
  addSnippetClassDecl("@JsType","public static interface IBuggy {","  boolean x(boolean foo);","  @JsProperty","  int getX();","}","public static class Buggy implements IBuggy {","  public boolean x(boolean foo) {return false;}","  public int getX() {return 0;}","}");
  assertBuggyFails("Line 9: 'int EntryPoint.IBuggy.getX()' and 'boolean EntryPoint.IBuggy.x(boolean)' " + "cannot both use the same JavaScript name 'x'.","Line 13: 'int EntryPoint.Buggy.getX()' and 'boolean EntryPoint.Buggy.x(boolean)' " + "cannot both use the same JavaScript name 'x'.");
}
