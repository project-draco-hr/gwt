{
  addSnippetImport("com.google.gwt.core.client.js.JsType");
  addSnippetImport("com.google.gwt.core.client.js.JsProperty");
  addSnippetClassDecl("@JsType","public static interface IBuggy {","  @JsProperty","  void setX(boolean x);","  @JsProperty","  void setX(int x);","}","public static class Buggy implements IBuggy {","  public void setX(boolean x) {}","  public void setX(int x) {}","}");
  assertBuggyFails("There can't be more than one setter for JsProperty 'x' in type 'test.EntryPoint$IBuggy'.","There can't be more than one setter for JsProperty 'x' in type 'test.EntryPoint$Buggy'.");
}
