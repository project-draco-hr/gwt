{
  addSnippetImport("com.google.gwt.core.client.js.JsType");
  addSnippetImport("com.google.gwt.core.client.js.JsProperty");
  addSnippetClassDecl("@JsType","public static interface IBuggy {","  @JsProperty","  void x(int x);","  @JsProperty","  void setX(int x);","}","public static class Buggy implements IBuggy {","  public void x(int x) {}","  public void setX(int x) {}","}");
  assertCompileFails("There can't be more than one setter for JsProperty 'x' in type " + "'test.EntryPoint$IBuggy'.","There can't be more than one setter for JsProperty 'x' in type " + "'test.EntryPoint$Buggy'.");
}
