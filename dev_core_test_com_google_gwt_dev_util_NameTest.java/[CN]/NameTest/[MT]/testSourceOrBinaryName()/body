{
  assertEquals("org.test.Foo.Bar",SourceOrBinaryName.toSourceName("org.test.Foo.Bar"));
  assertEquals("org.test.Foo.Bar",SourceOrBinaryName.toSourceName("org.test.Foo$Bar"));
  assertEquals("org.test.Foo.Bar$",SourceOrBinaryName.toSourceName("org.test.Foo.Bar$"));
  assertEquals("org.test.Foo.Bar$",SourceOrBinaryName.toSourceName("org.test.Foo$Bar$"));
}
