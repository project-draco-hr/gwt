{
  JsFunction function=JsUtils.isFunction(qualifier);
  if (function != null) {
    Integer count=invocationCount.get(function);
    if (count == null) {
      assert(!removingCounts);
      count=1;
    }
 else {
      if (removingCounts) {
        count-=1;
      }
 else {
        count+=1;
      }
    }
    invocationCount.put(function,count);
  }
}
