{
  this.program=program;
  this.whitelist=whitelist;
  invocationCountingVisitor.accept(program);
  JsName defineClass=getFunctionName(program,"JavaClassHierarchySetupUtil.defineClass");
  JsName defineClassProto=getFunctionName(program,"JavaClassHierarchySetupUtil.defineClassWithPrototype");
  safeToInlineAtTopLevel=defineClass != null ? ImmutableSet.of(defineClass,defineClassProto) : ImmutableSet.<JsName>of();
}
