{
  ArrayList<String> result=Lists.newArrayList();
  int length=path.length();
  int begin=0;
  for (int i=0; i < length; i++) {
    if (path.charAt(i) == '/') {
      result.add(path.substring(begin,i));
      begin=i + 1;
    }
  }
  if (begin < length) {
    result.add(path.substring(begin));
  }
  return result.toArray(new String[result.size()]);
}
