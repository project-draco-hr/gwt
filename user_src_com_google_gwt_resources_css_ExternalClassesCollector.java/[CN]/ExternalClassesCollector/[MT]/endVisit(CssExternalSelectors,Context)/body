{
  if (matchAll) {
    return;
  }
  for (  String selector : x.getClasses()) {
    if (selector.equals(GLOB_STRING)) {
      matchAll=true;
      return;
    }
 else     if (selector.endsWith(GLOB_STRING)) {
      globs.add(selector.substring(0,selector.length() - 1));
    }
 else {
      externalClasses.add(selector);
    }
  }
}
