{
  if (matchAll) {
    return allClasses;
  }
  glob:   for (  String glob : globs) {
    for (    String clazz : allClasses.tailSet(glob)) {
      if (clazz.startsWith(glob)) {
        externalClasses.add(clazz);
      }
 else {
        continue glob;
      }
    }
  }
  return externalClasses;
}
