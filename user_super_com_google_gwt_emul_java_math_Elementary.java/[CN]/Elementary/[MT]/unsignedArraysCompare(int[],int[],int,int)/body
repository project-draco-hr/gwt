{
  if (aSize > bSize) {
    return 1;
  }
 else   if (aSize < bSize) {
    return -1;
  }
 else {
    int i;
    for (i=aSize - 1; i >= 0 && a[i] == b[i]; i--) {
    }
    return i < 0 ? BigInteger.EQUALS : ((a[i] & 0xFFFFFFFFL) < (b[i] & 0xFFFFFFFFL) ? BigInteger.LESS : BigInteger.GREATER);
  }
}
