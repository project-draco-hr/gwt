{
  this.task=task;
  TaskEditView view=clientFactory.getTaskEditView();
  if (task == null) {
    view.setEditing(false);
    view.setDueDate(null);
    view.setName("");
    view.setNotes("");
  }
 else {
    view.setEditing(true);
    view.setDueDate(task.getDueDate());
    view.setName(task.getName());
    view.setNotes(task.getNotes());
  }
}
