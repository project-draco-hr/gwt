{
  if (BINDER_URI.equals(elem.getNamespaceUri()) && LOCAL_NAME.equals(elem.getLocalName())) {
    String fieldRef=elem.consumeStringAttribute("from");
    if (fieldRef == null) {
      logger.die(elem,"Attribute 'from' not found.");
    }
    return "\" + " + fieldRef + " + \"";
  }
  return null;
}
