{
  if (primaryParent == null) {
    return;
  }
  for (  String key : keySet()) {
    if (primaryParent.getObjectAux(key,true) == null) {
      for (int i=0; i < alternativeParents.size(); i++) {
        AbstractResource alt=alternativeParents.get(i);
        if (alt.getObjectAux(key,true) != null) {
          break;
        }
      }
      throw new IllegalArgumentException(key + " is not a valid resource key as it does not occur in the default version of " + this+ " nor in any of "+ alternativeParents);
    }
  }
}
