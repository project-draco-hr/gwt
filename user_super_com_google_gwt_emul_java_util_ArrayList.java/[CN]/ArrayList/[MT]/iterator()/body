{
  return new Iterator<E>(){
    int i=0, last=-1;
    @Override public boolean hasNext(){
      return i < array.length;
    }
    @Override public E next(){
      checkElement(hasNext());
      last=i++;
      return array[last];
    }
    @Override public void remove(){
      checkState(last != -1);
      ArrayList.this.remove(i=last);
      last=-1;
    }
  }
;
}
