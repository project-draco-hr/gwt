{
  checkNotNull(filter);
  int length=array.length;
  if (length == 0) {
    return false;
  }
  E[] newArray=ArrayHelper.createFrom(array,length);
  int i=0;
  for (  E e : array) {
    if (!filter.test(e)) {
      newArray[i++]=e;
    }
  }
  if (i == length) {
    return false;
  }
  ArrayHelper.setLength(newArray,i);
  array=newArray;
  return true;
}
