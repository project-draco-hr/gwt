{
  super.onBrowserEvent(event);
  Item item=findItem(DOM.eventGetTarget(event));
switch (DOM.eventGetType(event)) {
case Event.ONCLICK:
{
      commitSelection();
      break;
    }
case Event.ONMOUSEOVER:
{
    if (item != null) {
      setSelection(item);
    }
    break;
  }
case Event.ONMOUSEOUT:
{
  if (item != null) {
    setSelection(null);
  }
  break;
}
}
}
