{
  touching=false;
  if (touchDelegate != null) {
    touchDelegate.onTouchEnd(e);
  }
  if (!tracking || dragDelegate == null) {
    return;
  }
  Touch touch=getLastTouch();
  Point touchCoordinate=new Point(touch.getPageX(),touch.getPageY());
  if (dragging) {
    endTime=e.getTimeStamp();
    endTouchPosition=touchCoordinate;
    dragDelegate.onDragEnd(e);
    if ((Math.abs(endTouchPosition.x - startTouchPosition.x) > CLICK_BUST_THRESHOLD) || (Math.abs(endTouchPosition.y - startTouchPosition.y) > CLICK_BUST_THRESHOLD)) {
      bustNextClick=true;
    }
  }
  endTracking();
}
