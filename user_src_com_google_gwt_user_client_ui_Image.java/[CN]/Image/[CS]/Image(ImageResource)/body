{
  if (resource.isStandalone()) {
    changeState(new UnclippedState(this,resource.getSafeUri(),resource.getWidth(),resource.getHeight()));
  }
 else {
    changeState(new ClippedState(this,resource.getSafeUri(),resource.getLeft(),resource.getTop(),resource.getWidth(),resource.getHeight()));
  }
  setStyleName("gwt-Image");
}
