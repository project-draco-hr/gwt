{
  this.left=left;
  this.top=top;
  this.width=width;
  this.height=height;
  this.url=url;
  image.replaceElement(impl.createStructure(url,left,top,width,height));
  image.sinkEvents(Event.ONCLICK | Event.ONDBLCLICK | Event.MOUSEEVENTS| Event.ONMOUSEWHEEL| Event.ONLOAD| Event.TOUCHEVENTS| Event.GESTUREEVENTS);
}
