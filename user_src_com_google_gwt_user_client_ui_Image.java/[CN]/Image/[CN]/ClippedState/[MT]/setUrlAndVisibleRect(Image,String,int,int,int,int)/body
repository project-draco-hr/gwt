{
  if (!url.equals(url) || this.left != left || this.top != top || this.width != width || this.height != height) {
    this.url=url;
    this.left=left;
    this.top=top;
    this.width=width;
    this.height=height;
    impl.adjust(image.getElement(),url,left,top,width,height);
    fireSyntheticLoadEvent(image);
  }
}
