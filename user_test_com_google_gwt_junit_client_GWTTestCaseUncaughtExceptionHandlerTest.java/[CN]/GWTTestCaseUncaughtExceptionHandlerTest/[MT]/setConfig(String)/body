{
  if ("addsCustomHandlerInGwtSetup".equals(confToken)) {
    addsCustomHandlerInGwtSetup=true;
  }
 else   if ("throwsInGwtSetUp".equals(confToken)) {
    throwsInGwtSetUp=true;
  }
 else   if ("throwsInGwtTearDown".equals(confToken)) {
