{
  BeanHelper.writeInterface(context,logger,bean);
  sw.print("private final " + bean.getFullyQualifiedValidatorName() + " ");
  sw.print(bean.getValidatorInstanceName());
  sw.println(" = ");
  sw.indent();
  sw.indent();
  sw.print(bean.getFullyQualifiedValidatorName());
  sw.println(".INSTANCE;");
  sw.outdent();
  sw.outdent();
}
