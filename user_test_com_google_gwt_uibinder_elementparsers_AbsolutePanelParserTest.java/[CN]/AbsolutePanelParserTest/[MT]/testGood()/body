{
  StringBuffer b=new StringBuffer();
  b.append("<g:AbsolutePanel>");
  b.append("  <g:at left='1' top='2'>");
  b.append("    <g:Button/>");
  b.append("  </g:at>");
  b.append("  <g:Label/>");
  b.append("  <g:at left='10' top='20'>");
  b.append("    <g:Label/>");
  b.append("  </g:at>");
  b.append("</g:AbsolutePanel>");
  tester.parse(b.toString());
  assertStatements("fieldName.add(<g:Button>, 1, 2);","fieldName.add(<g:Label>);","fieldName.add(<g:Label>, 10, 20);");
}
