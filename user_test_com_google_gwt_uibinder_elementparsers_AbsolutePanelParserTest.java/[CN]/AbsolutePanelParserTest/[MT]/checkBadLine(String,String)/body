{
  StringBuffer b=new StringBuffer();
  b.append("<g:AbsolutePanel>");
  b.append("  " + badLine);
  b.append("</g:AbsolutePanel>");
  try {
    tester.parse(b.toString());
    fail();
  }
 catch (  UnableToCompleteException e) {
    String died=tester.logger.died;
    assertTrue(died,died.contains(expectedDied));
  }
}
