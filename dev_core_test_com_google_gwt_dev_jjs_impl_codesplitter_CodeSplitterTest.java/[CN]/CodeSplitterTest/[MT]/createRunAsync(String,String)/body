{
  StringBuilder code=new StringBuilder();
  code.append("GWT.runAsync(" + cast + "new "+ "RunAsyncCallback() {\n");
  code.append("  public void onFailure(Throwable reason) {}\n");
  code.append("  public void onSuccess() {\n");
  code.append("    " + body);
  code.append("  }\n");
  code.append("});\n");
  return code.toString();
}
