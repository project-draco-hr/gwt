{
  if (entityData == null || entityData.entityInstance == null) {
    return null;
  }
  Class<?> entityClass=getEntityFromRecordAnnotation(entityKey.record);
  Object entityInstance=entityClass.getMethod("find" + entityClass.getSimpleName(),Long.class).invoke(null,new Long(entityKey.id));
  if (entityInstance == null) {
    return WriteOperation.DELETE;
  }
  if (hasChanged(beforeDataMap.get(entityKey).serializedEntity,serializeEntity(entityInstance,entityKey.record))) {
    return WriteOperation.UPDATE;
  }
  return null;
}
