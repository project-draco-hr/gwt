{
  CustomFieldSerializerTestServiceAsync service=getServiceAsync();
  delayTestFinishForRpc();
  service.echo(CustomFieldSerializerTestSetFactory.createUnserializableSubclass(),new AsyncCallback(){
    @Override public void onFailure(    Throwable caught){
      assertTrue("Should be a SerializationException",caught instanceof SerializationException);
      finishTest();
    }
    @Override public void onSuccess(    Object result){
      fail("Class UnserializableSubclass should not be serializable");
    }
  }
);
}
