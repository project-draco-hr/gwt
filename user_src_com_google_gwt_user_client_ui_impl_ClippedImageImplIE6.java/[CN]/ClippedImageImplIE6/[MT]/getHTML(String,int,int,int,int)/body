{
  if (!isIE6()) {
    return super.getHTML(url,left,top,width,height);
  }
  String clipperStyle="overflow: hidden; width: " + width + "px; height: "+ height+ "px; padding: 0px; zoom: 1";
  String imgStyle="filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='" + url + "',sizingMethod='crop'); margin-left: "+ -left+ "px; margin-top: "+ -top+ "px; border: none";
  String clippedImgHtml="<gwt:clipper style=\"" + clipperStyle + "\"><img onload='this.__gwtLastOnLoadEvent=\"load\";' src='"+ moduleBaseUrlProtocol+ "' onerror='if(window.__gwt_transparentImgHandler)window.__gwt_transparentImgHandler(this);else this.src=\""+ GWT.getModuleBaseURL()+ "clear.cache.gif\"' style=\""+ imgStyle+ "\" width="+ (left + width)+ " height="+ (top + height)+ " border='0'></gwt:clipper>";
  return clippedImgHtml;
}
