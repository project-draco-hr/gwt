{
  Place newPlace=event.getNewPlace();
  if (!(newPlace instanceof EntityListPlace)) {
    return;
  }
  if (entitiesView.getParent() == null) {
    panel.clear();
    panel.add(entitiesView);
  }
  if (newPlace == Places.EMPLOYEE_LIST) {
    EntityList<EmployeeRef> list=new EntityList<EmployeeRef>("Employees",entitiesView,employeeColumns);
    requests.employeeRequest().findAllEmployees().forProperties(employeeColumns).to(list).fire();
  }
 else   if (newPlace == Places.REPORT_LIST) {
    EntityList<ReportRef> list=new EntityList<ReportRef>("Reports",entitiesView,reportColumns);
    requests.reportRequest().findAllReports().forProperties(reportColumns).to(list).fire();
  }
}
