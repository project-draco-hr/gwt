{
  JParameter[] p1=m1.getParameters();
  JParameter[] p2=m2.getParameters();
  if (p1.length != p2.length) {
    return false;
  }
  for (int i=0; i < p1.length; i++) {
    JType type1=p1[i].getType();
    JType type2=p2[i].getType();
    if (!type1.equals(type2)) {
      return false;
    }
  }
  return true;
}
