{
  stream.prepareToWrite();
  if (responseType != void.class) {
    try {
      stream.serializeValue(responseObj,responseType);
    }
 catch (    SerializationException e) {
      responseObj=e;
      isException=true;
    }
  }
  String bufferStr=(isException ? "{EX}" : "{OK}") + stream.toString();
  return bufferStr;
}
