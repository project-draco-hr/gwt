{
  if (!started && curTime >= startTime) {
    started=true;
    onStart();
  }
  if (curTime >= startTime + duration) {
    started=false;
    onComplete();
    return true;
  }
 else   if (curTime >= startTime) {
    double progress=(curTime - startTime) / duration;
    onUpdate(interpolate(progress));
  }
  return false;
}
