{
  final DisclosurePanel panel=createTestPanel();
  assertTrue(DOM.getStyleAttribute(DOM.getParent(panel.getContent().getElement()),"display").equalsIgnoreCase("none"));
  panel.setOpen(true);
  Timer t=new Timer(){
    @Override public void run(){
      assertTrue(DOM.getStyleAttribute(DOM.getParent(panel.getContent().getElement()),"display").trim().equals(""));
      finishTest();
    }
  }
;
  t.schedule(450);
  delayTestFinish(500);
}
