{
  if (storage == null) {
    return;
  }
  storage.clear();
  assertEquals(0,storage.getLength());
  storage.setItem("abc","def");
  assertEquals(1,storage.getLength());
  storage.setItem("ghi","jkl");
  assertEquals(2,storage.getLength());
  storage.clear();
  assertEquals(0,storage.getLength());
}
