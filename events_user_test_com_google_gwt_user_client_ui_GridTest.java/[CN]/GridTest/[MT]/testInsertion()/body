{
  Grid r=new Grid(4,3);
  assertEquals(4,r.getRowCount());
  assertEquals(3,r.getColumnCount());
  int index=r.insertRow(2);
  assertEquals(index,2);
  assertEquals(5,r.getRowCount());
  assertEquals(3,r.getColumnCount());
  assertEquals(5,r.getDOMRowCount());
  assertEquals(3,r.getDOMCellCount(2));
}
