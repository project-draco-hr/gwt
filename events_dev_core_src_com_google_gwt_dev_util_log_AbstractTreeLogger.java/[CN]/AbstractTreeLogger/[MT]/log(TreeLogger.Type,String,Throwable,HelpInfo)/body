{
  if (msg == null) {
    msg="(Null log message)";
  }
  if (causedByOutOfMemory(caught)) {
    branch(TreeLogger.ERROR,msg,caught);
    return;
  }
  int childIndex=allocateNextChildIndex();
  if (isLoggable(type)) {
    commitMyBranchEntryInMyParentLogger();
    doLog(childIndex,type,msg,caught,helpInfo);
  }
}
