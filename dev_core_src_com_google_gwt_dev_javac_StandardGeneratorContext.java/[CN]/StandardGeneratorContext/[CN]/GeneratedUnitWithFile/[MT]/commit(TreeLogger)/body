{
  super.commit(logger);
  BufferedOutputStream fos=null;
  try {
    fos=new BufferedOutputStream(new FileOutputStream(file));
    fos.write(Util.getBytes(source));
  }
 catch (  IOException e) {
    logger.log(TreeLogger.WARN,"Error writing out generated unit at '" + file.getAbsolutePath() + "': "+ e);
  }
 finally {
    Utility.close(fos);
  }
}
