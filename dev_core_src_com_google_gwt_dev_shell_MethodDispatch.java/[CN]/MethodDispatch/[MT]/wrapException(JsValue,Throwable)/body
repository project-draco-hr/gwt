{
  ModuleSpace.setThrownJavaException(t);
  Object jsoException=ModuleSpace.getJavaScriptExceptionException(classLoader,t);
  if (jsoException != null) {
    JsValueGlue.set(returnValue,classLoader,jsoException.getClass(),jsoException);
  }
 else {
    JsValueGlue.set(returnValue,classLoader,t.getClass(),t);
  }
}
