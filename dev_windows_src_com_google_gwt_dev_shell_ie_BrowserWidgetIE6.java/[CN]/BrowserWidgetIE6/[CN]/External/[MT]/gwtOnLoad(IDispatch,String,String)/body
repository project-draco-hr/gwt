{
  if (moduleName == null) {
    return handleUnload(frameWnd);
  }
  TreeLogger logger=getHost().getLogger().branch(TreeLogger.DEBUG,"Loading an instance of module '" + moduleName + "'");
  try {
    if (!validHostedHtmlVersion(version)) {
      throw new HResultException(COM.E_INVALIDARG);
    }
    int moduleID=++nextModuleID;
    Integer key=new Integer(moduleID);
    setIntProperty(frameWnd,"__gwt_module_id",moduleID);
    ModuleSpaceHost msh=getHost().createModuleSpaceHost(logger,BrowserWidgetIE6.this,moduleName);
    ModuleSpaceIE6 moduleSpace=new ModuleSpaceIE6(logger,msh,frameWnd,moduleName,key);
    attachModuleSpace(logger,moduleSpace);
    return true;
  }
 catch (  Throwable e) {
    logger.log(TreeLogger.ERROR,"Failure to load module '" + moduleName + "'",e);
    return false;
  }
}
