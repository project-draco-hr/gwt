{
  super(viewModel);
  setStyleName("gwt-StandardTreeView");
  setAnimation(SlideAnimation.create());
  sinkEvents(Event.ONCLICK | Event.ONCHANGE | Event.MOUSEEVENTS);
  StandardTreeNodeView<T> root=new StandardTreeNodeView<T>(this,null,null,getElement(),rootValue);
  rootNode=root;
  root.setOpen(true);
}
