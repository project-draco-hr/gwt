  assertConstraintDefinitionException(ConstraintDefinitionWithWrongMessageTypeFactory.TestValidator.class,"Unable to create a validator for org.hibernate.jsr303.tck.tests" + ".constraints" + ".invalidconstraintdefinitions.InvalidConstraintDefinitionsTest"+ ".DummyEntityInvalidMessageType because "+ "org.hibernate.jsr303.tck.tests.constraints"+ ".invalidconstraintdefinitions.InvalidMessageType "+ "contains Constraint annotation, "+ "but the message parameter is not of type java.lang.String.");
