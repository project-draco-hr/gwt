  assertConstraintDefinitionException(ConstraintDefinitionWithWrongDefaultPayloadValueFactory.TestValidator.class,"Unable to create a validator for org.hibernate.jsr303.tck.tests" + ".constraints.invalidconstraintdefinitions" + ".InvalidConstraintDefinitionsTest"+ ".DummyEntityInvalidDefaultPayload "+ "because org.hibernate.jsr303.tck.tests.constraints"+ ".invalidconstraintdefinitions.InvalidDefaultPayload contains "+ "Constraint annotation, but the payload parameter default "+ "value is not the empty array.");
