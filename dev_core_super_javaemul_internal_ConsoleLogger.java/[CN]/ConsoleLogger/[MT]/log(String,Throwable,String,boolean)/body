{
  groupStart(label + t.toString(),expanded);
  log(level,getBackingError(t));
  Throwable cause=t.getCause();
  if (cause != null) {
    log(level,cause,"Caused by: ",false);
  }
  for (  Throwable suppressed : t.getSuppressed()) {
    log(level,suppressed,"Suppressed: ",false);
  }
  groupEnd();
}
