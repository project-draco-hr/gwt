{
  if (value == null) {
    return new DefaultNodeInfo<String>(departments,departmentCell,selectionModel,null);
  }
 else   if (isAllDepartment(value)) {
    return null;
  }
 else   if (value instanceof String) {
    EmployeeListViewAdapter adapter=new EmployeeListViewAdapter((String)value);
    return new DefaultNodeInfo<EmployeeRecord>(adapter,employeeCell,selectionModel,null);
  }
  return null;
}
