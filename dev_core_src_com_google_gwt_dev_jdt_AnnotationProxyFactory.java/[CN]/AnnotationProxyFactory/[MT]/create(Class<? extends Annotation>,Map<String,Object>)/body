{
  AnnotationProxyInvocationHandler annotationInvocationHandler=new AnnotationProxyInvocationHandler(identifierToValue,annotationClass);
  Annotation proxy=(Annotation)Proxy.newProxyInstance(AnnotationProxyFactory.class.getClassLoader(),new Class<?>[]{java.lang.annotation.Annotation.class,annotationClass},annotationInvocationHandler);
  annotationInvocationHandler.setProxy(proxy);
  return proxy;
}
