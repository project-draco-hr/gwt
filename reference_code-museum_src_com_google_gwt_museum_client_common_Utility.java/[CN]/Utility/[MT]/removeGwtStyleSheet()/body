{
  HeadElement headElem=getHeadElement();
  NodeList<Node> children=headElem.getChildNodes();
  for (int i=0; i < children.getLength(); i++) {
    Node node=children.getItem(i);
    if (node.getNodeType() == Node.ELEMENT_NODE) {
      Element elem=Element.as(node);
      if (elem.getTagName().equalsIgnoreCase("link") && elem.getPropertyString("rel").equalsIgnoreCase("stylesheet") && elem.getPropertyString("href").contains("standard.css")) {
        headElem.removeChild(elem);
        return;
      }
    }
  }
}
