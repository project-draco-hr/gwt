{
  for (int i=0; i < remoteTokens.length; ++i) {
    int remoteToken=remoteTokens[i];
    BrowserManager mgr=browserManagers[i];
    if (remoteToken > 0) {
      try {
        mgr.keepAlive(remoteToken,PING_KEEPALIVE_MS);
      }
 catch (      Exception e) {
        shell.getTopLogger().log(TreeLogger.WARN,"Unexpected exception keeping remote browser alive",e);
        return true;
      }
    }
  }
  return false;
}
