{
  if (versionString == null) {
    return;
  }
  int part=0;
  int v=0;
  int len=versionString.length();
  for (int i=0; i < len; ++i) {
    char ch=versionString.charAt(i);
    if (ch == '.') {
      if (part >= version.length) {
        throw new NumberFormatException();
      }
      version[part++]=v;
      v=0;
    }
 else     if (Character.isDigit(ch)) {
      int digit=Character.digit(ch,10);
      if (digit < 0) {
        throw new NumberFormatException();
      }
      v=v * 10 + digit;
    }
  }
  version[part++]=v;
}
