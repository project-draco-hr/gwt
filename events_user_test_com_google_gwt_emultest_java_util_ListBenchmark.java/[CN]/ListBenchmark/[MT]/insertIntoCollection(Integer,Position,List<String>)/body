{
  int num=size.intValue();
  for (int i=0; i < num; i++) {
    if (where == Position.IMPLICIT_END) {
      l.add("hello");
    }
 else     if (where == Position.BEGIN) {
      l.add(0,"hello");
    }
 else     if (where == Position.EXPLICIT_END) {
      l.add(l.size(),"hello");
    }
 else     if (where == Position.VARIED) {
      l.add(index,"hello");
      index+=PRIME;
      index%=l.size();
    }
  }
}
