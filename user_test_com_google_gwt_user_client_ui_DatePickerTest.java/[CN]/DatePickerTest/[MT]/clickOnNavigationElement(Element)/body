{
  e.dispatchEvent(Document.get().createMouseOverEvent(1,0,0,0,0,false,false,false,false,NativeEvent.BUTTON_LEFT,null));
  e.dispatchEvent(Document.get().createMouseDownEvent(1,0,0,0,0,false,false,false,false,NativeEvent.BUTTON_LEFT));
  e.dispatchEvent(Document.get().createMouseUpEvent(1,0,0,0,0,false,false,false,false,NativeEvent.BUTTON_LEFT));
}
