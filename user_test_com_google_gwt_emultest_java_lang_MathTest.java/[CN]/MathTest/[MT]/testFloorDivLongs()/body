{
  assertEquals(0L,Math.floorDiv(0L,1L));
  assertEquals(1L,Math.floorDiv(4L,3L));
  assertEquals(-2L,Math.floorDiv(4L,-3L));
  assertEquals(-2L,Math.floorDiv(-4L,3L));
  assertEquals(1L,Math.floorDiv(-4L,-3L));
  assertEquals(Long.MIN_VALUE,Math.floorDiv(Long.MIN_VALUE,-1));
  try {
    Math.floorDiv(1L,0L);
    fail();
  }
 catch (  ArithmeticException expected) {
  }
}
