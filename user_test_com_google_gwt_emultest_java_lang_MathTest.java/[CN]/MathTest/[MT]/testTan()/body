{
  double v=Math.tan(0.0);
  assertEquals(0.0,v,1e-7);
  v=Math.tan(-0.0);
  assertEquals(-0.0,v,1e-7);
  v=Math.tan(Double.NaN);
  assertTrue(Double.isNaN(v));
  v=Math.tan(Double.NEGATIVE_INFINITY);
  assertTrue(Double.isNaN(v));
  v=Math.tan(Double.POSITIVE_INFINITY);
  assertTrue(Double.isNaN(v));
}
