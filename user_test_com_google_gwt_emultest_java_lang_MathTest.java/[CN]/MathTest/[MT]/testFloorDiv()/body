{
  assertEquals(0,Math.floorDiv(0,1));
  assertEquals(1,Math.floorDiv(4,3));
  assertEquals(-2,Math.floorDiv(4,-3));
  assertEquals(-2,Math.floorDiv(-4,3));
  assertEquals(1,Math.floorDiv(-4,-3));
  assertEquals(Integer.MIN_VALUE,Math.floorDiv(Integer.MIN_VALUE,-1));
  try {
    Math.floorDiv(1,0);
    fail();
  }
 catch (  ArithmeticException expected) {
  }
}
