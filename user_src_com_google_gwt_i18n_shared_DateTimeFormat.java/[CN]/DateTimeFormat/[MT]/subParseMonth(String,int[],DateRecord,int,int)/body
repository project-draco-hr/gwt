{
  if (value < 0) {
    value=matchString(text,start,dateTimeFormatInfo.monthsFull(),pos);
    if (value < 0) {
      value=matchString(text,start,dateTimeFormatInfo.monthsShort(),pos);
    }
    if (value < 0) {
      return false;
    }
    cal.setMonth(value);
    return true;
  }
 else   if (value > 0) {
    cal.setMonth(value - 1);
    return true;
  }
  return false;
}
