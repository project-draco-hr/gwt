{
  i=4;
  i=i + j;
  assertEquals(6,i);
  i=i - j;
  assertEquals(4,i);
  i=i * j;
  assertEquals(8,i);
  i=i / j;
  assertEquals(4,i);
  i=i % j;
  assertEquals(0,i);
  i=7;
  i=i & j;
  assertEquals(2,i);
  i=0;
  i=i | j;
  assertEquals(2,i);
  i=7;
  i=i ^ j;
  assertEquals(5,i);
  i=i << j;
  assertEquals(20,i);
  i=i >> j;
  assertEquals(5,i);
  i=i >>> j;
  assertEquals(1,i);
}
