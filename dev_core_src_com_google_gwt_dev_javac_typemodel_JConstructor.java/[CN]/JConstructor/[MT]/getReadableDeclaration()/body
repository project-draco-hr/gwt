{
  String[] names=TypeOracle.modifierBitsToNames(getModifierBits());
  StringBuilder sb=new StringBuilder();
  for (  String name : names) {
    sb.append(name);
    sb.append(" ");
  }
  if (getTypeParameters().length > 0) {
    toStringTypeParams(sb);
    sb.append(" ");
  }
  sb.append(getName());
  toStringParamsAndThrows(sb);
  return sb.toString();
}
