{
  Set<JReferenceType> toReturn=new HashSet<JReferenceType>();
  List<JReferenceType> q=new LinkedList<JReferenceType>();
  q.add(type);
  while (!q.isEmpty()) {
    JReferenceType t=q.remove(0);
    if (toReturn.add(t)) {
      if (t.extnds != null) {
        q.add(t.extnds);
      }
      q.addAll(t.implments);
    }
  }
  return toReturn;
}
