{
  if (type instanceof JDeclaredType) {
    for (    JMethod meth : ((JDeclaredType)type).getMethods()) {
      if (isExportedMethod(meth)) {
        return true;
      }
    }
    for (    JField field : ((JDeclaredType)type).getFields()) {
      if (isExportedField(field)) {
        return true;
      }
    }
  }
  return false;
}
