{
  Set<JInterfaceType> toReturn=new IdentityHashSet<JInterfaceType>();
  List<JInterfaceType> q=new LinkedList<JInterfaceType>();
  q.addAll(type.getImplements());
  while (!q.isEmpty()) {
    JInterfaceType t=q.remove(0);
    if (toReturn.add(t)) {
      q.addAll(t.getImplements());
    }
  }
  return toReturn;
}
