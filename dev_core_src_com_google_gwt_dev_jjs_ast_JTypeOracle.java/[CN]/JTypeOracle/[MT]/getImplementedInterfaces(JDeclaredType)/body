{
  Multimap<String,String> implementedInterfaces=(type instanceof JClassType) ? implementedInterfacesByClass : superInterfacesByInterface;
  return Collections2.transform(implementedInterfaces.get(type.getName()),new Function<String,JInterfaceType>(){
    @Override public JInterfaceType apply(    String typeName){
      JReferenceType referenceType=referenceTypesByName.get(typeName);
      assert(referenceType instanceof JInterfaceType);
      return (JInterfaceType)referenceType;
    }
  }
);
}
