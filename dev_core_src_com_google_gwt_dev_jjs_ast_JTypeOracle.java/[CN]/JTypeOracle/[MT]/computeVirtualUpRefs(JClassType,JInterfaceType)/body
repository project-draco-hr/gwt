{
  outer:   for (  JMethod intfMethod : intf.getMethods()) {
    for (    JMethod classMethod : type.getMethods()) {
      if (methodsDoMatch(intfMethod,classMethod)) {
        continue outer;
      }
    }
    for (JClassType superType=type.getSuperClass(); !superType.getName().equals(standardTypes.javaLangObject); superType=superType.getSuperClass()) {
      for (      JMethod superMethod : superType.getMethods()) {
        if (methodsDoMatch(intfMethod,superMethod)) {
          Multimap<JClassType,JMethod> classToMethodsMultimap=getOrCreateMultimap(virtualUpRefMap,superMethod);
          classToMethodsMultimap.put(type,intfMethod);
          continue outer;
        }
      }
    }
  }
}
