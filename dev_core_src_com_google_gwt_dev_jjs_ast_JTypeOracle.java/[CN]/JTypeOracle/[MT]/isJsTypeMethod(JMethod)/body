{
  if (!isJsInteropEnabled()) {
    return false;
  }
  if (!x.isNoExport() && isJsType(x.getEnclosingType())) {
    return true;
  }
  for (  JMethod overriddenMethod : getOverriddenMethodsOf(x)) {
    if (!overriddenMethod.isNoExport() && isJsType(overriddenMethod.getEnclosingType())) {
      return true;
    }
  }
  return false;
}
