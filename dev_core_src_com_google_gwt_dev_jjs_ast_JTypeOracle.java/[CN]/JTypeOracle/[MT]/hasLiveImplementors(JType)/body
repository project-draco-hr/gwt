{
  if (!hasWholeWorldKnowledge) {
    return true;
  }
  if (type instanceof JInterfaceType && isImplementedMap.get(type.getName()) != null) {
    for (    JReferenceType impl : getTypes(isImplementedMap,type.getName())) {
      if (isInstantiatedType((JClassType)impl)) {
        return true;
      }
    }
  }
  return false;
}
