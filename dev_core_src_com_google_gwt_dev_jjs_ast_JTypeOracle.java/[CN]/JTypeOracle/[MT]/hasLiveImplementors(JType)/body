{
  if (!hasWholeWorldKnowledge || !optimize) {
    return true;
  }
  if (type instanceof JInterfaceType) {
    for (    JReferenceType impl : getTypes(classesByImplementingInterface.get(type.getName()))) {
      if (isInstantiatedType((JClassType)impl)) {
        return true;
      }
    }
  }
  return false;
}
