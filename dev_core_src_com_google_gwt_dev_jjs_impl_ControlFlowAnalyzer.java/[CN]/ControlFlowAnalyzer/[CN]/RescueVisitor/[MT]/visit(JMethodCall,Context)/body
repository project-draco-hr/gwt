{
  JMethod method=call.getTarget();
  if (method.isStatic() || program.isJavaScriptObject(method.getEnclosingType()) || instantiatedTypes.contains(method.getEnclosingType())) {
    rescue(method);
  }
 else {
    if (!liveFieldsAndMethods.contains(method)) {
      methodsLiveExceptForInstantiability.add(method);
    }
  }
  return true;
}
