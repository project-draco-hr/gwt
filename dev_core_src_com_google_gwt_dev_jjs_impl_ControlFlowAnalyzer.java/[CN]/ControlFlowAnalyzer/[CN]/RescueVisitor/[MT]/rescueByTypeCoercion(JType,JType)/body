{
  if (!canBeInstantiatedInJavaScript(targetType)) {
    return;
  }
  rescue((JReferenceType)targetType,true);
  if (program.typeOracle.isSingleJsoImpl(targetType)) {
    if (program.getTypeJavaLangObject() == expressionType || program.typeOracle.canBeJavaScriptObject(expressionType)) {
      JClassType jsoImplementor=program.typeOracle.getSingleJsoImpl((JReferenceType)targetType);
      rescue(jsoImplementor,true);
    }
  }
}
