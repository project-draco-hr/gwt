{
  try {
    if (new File(loc).exists()) {
      return true;
    }
    URL url=new URL(loc);
    String s=url.toExternalForm();
    if (s.startsWith("file:") || s.startsWith("jar:file:") || s.startsWith("zip:file:")) {
      return true;
    }
  }
 catch (  MalformedURLException e) {
  }
  return false;
}
