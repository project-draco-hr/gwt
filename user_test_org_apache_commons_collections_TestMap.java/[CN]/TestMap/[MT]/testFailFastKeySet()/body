{
  if (!isAddRemoveModifiable()) {
    return;
  }
  if (!isFailFastExpected()) {
    return;
  }
  resetFull();
  Iterator it=map.keySet().iterator();
  final Object val=it.next();
  map.remove(val);
  try {
    it.next();
    fail();
  }
 catch (  ConcurrentModificationException expected) {
  }
  resetFull();
  it=map.keySet().iterator();
  it.next();
  map.clear();
  try {
    it.next();
    fail();
  }
 catch (  ConcurrentModificationException expected) {
  }
}
