{
  JType oldExceptions[]=oldMethod.getThrows();
  JType newExceptions[]=newMethod.getThrows();
  ArrayList<ApiChange> ret=new ArrayList<ApiChange>();
  for (  JType newException : newExceptions) {
    boolean isSubclass=false;
    for (    JType oldException : oldExceptions) {
      if (ApiDiffGenerator.isFirstTypeAssignableToSecond(newException,oldException)) {
        isSubclass=true;
        break;
      }
    }
    if (!isSubclass) {
      ret.add(new ApiChange(ApiChange.Status.EXCEPTIONS_ERROR,"unhandled exception in new code " + newException.toString()));
    }
  }
  return ret;
}
