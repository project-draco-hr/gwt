{
  this.type=type;
  JClassType classOrInterface=type.isClassOrInterface();
  if (classOrInterface != null) {
    autoSerializable=classOrInterface.isAssignableTo(isSerializableClass) || classOrInterface.isAssignableTo(serializableClass);
    manualSerializer=findCustomFieldSerializer(typeOracle,classOrInterface);
  }
}
