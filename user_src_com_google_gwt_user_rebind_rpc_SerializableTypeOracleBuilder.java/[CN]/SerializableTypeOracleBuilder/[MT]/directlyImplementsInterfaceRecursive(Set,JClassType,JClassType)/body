{
  if (clazz == intf) {
    return true;
  }
  JClassType[] intfImpls=clazz.getImplementedInterfaces();
  for (int i=0; i < intfImpls.length; ++i) {
    JClassType intfImpl=intfImpls[i];
    if (!seen.contains(intfImpl)) {
      seen.add(intfImpl);
      if (directlyImplementsInterfaceRecursive(seen,intfImpl,intf)) {
        return true;
      }
    }
  }
  return false;
}
