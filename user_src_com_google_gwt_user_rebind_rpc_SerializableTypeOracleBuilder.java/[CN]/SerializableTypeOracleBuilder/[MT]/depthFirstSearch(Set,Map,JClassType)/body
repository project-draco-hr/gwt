{
  if (seen.contains(type)) {
    return;
  }
  seen.add(type);
  List children=(List)adjList.get(type);
  if (children != null) {
    Iterator it=children.iterator();
    while (it.hasNext()) {
      JClassType child=(JClassType)it.next();
      if (!seen.contains(child)) {
        depthFirstSearch(seen,adjList,child);
      }
    }
  }
}
