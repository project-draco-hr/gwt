{
  JBound bounds=wildcard.getBounds();
  boolean success;
  if (bounds.isLowerBound() != null) {
    markAsUninstantiableAndLog(logger,isSpeculative,"In order to produce smaller client-side code, 'Object' is not allowed; '" + wildcard.getQualifiedSourceName() + "' includes Object.",tic);
    success=false;
  }
 else {
    JClassType firstBound=bounds.getFirstBound();
    success=checkTypeInstantiable(logger,firstBound,isSpeculative);
  }
  tic.setInstantiable(success);
  return success;
}
