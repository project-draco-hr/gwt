{
  if (field.isStatic() || field.isTransient()) {
    return false;
  }
  if (hasGwtTransientAnnotation(field)) {
    return false;
  }
  if (field.isFinal()) {
    TreeLogger.Type logLevel;
    if (isManuallySerializable(field.getEnclosingType())) {
      logLevel=TreeLogger.DEBUG;
    }
 else {
      logLevel=TreeLogger.WARN;
    }
    logger.branch(suppressNonStaticFinalFieldWarnings ? TreeLogger.DEBUG : logLevel,"Field '" + field.toString() + "' will not be serialized because it is final",null);
    return false;
  }
  return true;
}
