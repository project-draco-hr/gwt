{
  if (field.isStatic() || field.isTransient()) {
    return false;
  }
  if (field.isAnnotationPresent(GwtTransient.class)) {
    return false;
  }
  if (field.isFinal()) {
    logger.branch(suppressNonStaticFinalFieldWarnings ? TreeLogger.DEBUG : TreeLogger.WARN,"Field '" + field.toString() + "' will not be serialized because it is final",null);
    return false;
  }
  return true;
}
