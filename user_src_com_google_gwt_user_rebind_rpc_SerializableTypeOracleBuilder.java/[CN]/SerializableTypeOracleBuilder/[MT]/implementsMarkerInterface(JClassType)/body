{
  if (classOrInterface == getMarkerInterface()) {
    return true;
  }
  JClassType[] intfs=classOrInterface.getImplementedInterfaces();
  for (int i=0; i < intfs.length; ++i) {
    JClassType intf=intfs[i];
    assert(intf != null);
    if (implementsMarkerInterface(intf)) {
      return true;
    }
  }
  return false;
}
