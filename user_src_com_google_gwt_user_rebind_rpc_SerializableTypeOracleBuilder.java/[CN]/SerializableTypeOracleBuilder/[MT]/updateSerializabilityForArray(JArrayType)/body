{
  assert(isSerializable(isArray));
  JType compType=isArray.getComponentType();
  assert(compType != null);
  if (!isSerializable(compType) && !hasSerializableSubtypes(compType)) {
    addReasonForUnserializability(isArray,MessageFormat.format(NO_SERIALIZABLE_COMPONENT_TYPE,new String[]{compType.getQualifiedSourceName()}));
    return true;
  }
  return false;
}
