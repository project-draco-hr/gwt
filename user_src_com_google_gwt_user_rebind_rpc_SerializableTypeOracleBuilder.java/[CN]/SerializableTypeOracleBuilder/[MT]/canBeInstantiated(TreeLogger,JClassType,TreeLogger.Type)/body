{
  if (type.isEnum() == null) {
    if (type.isAbstract()) {
      return false;
    }
    if (!type.isDefaultInstantiable() && !isManuallySerializable(type)) {
      logger.log(logLevel,type.getParameterizedQualifiedSourceName() + " was not default instantiable (it must have a zero-argument constructor or no constructors at all)",null);
      return false;
    }
  }
 else {
  }
  return true;
}
