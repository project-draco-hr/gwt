{
  if (isDefinedInJREEmulation(classOrInterface)) {
    return;
  }
  JMethod[] methods=classOrInterface.getMethods();
  for (int i=0; i < methods.length; ++i) {
    JMethod method=methods[i];
    if (method.isNative()) {
      logger.branch(TreeLogger.WARN,MessageFormat.format("Method ''{0}'' is native, calling this method in server side code will result in an UnsatisfiedLinkError",new String[]{method.toString()}),null);
    }
  }
}
