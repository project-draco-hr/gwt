{
  JMethod[] methods=classOrInterface.getMethods();
  for (int i=0; i < methods.length; ++i) {
    if (methods[i].isNative()) {
      logger.branch(TreeLogger.WARN,MessageFormat.format("Method ''{0}'' is native, calling this method in server side code will result in an UnsatisfiedLinkError",new String[]{methods[i].toString()}),null);
    }
  }
}
