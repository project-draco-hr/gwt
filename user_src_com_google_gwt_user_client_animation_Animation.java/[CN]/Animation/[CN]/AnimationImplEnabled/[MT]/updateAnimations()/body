{
  long curTime=(new Date()).getTime();
  for (int i=0; i < animations.size(); i++) {
    Animation animation=animations.get(i);
    if (animation.update(curTime)) {
      animations.remove(i);
      i--;
    }
  }
  if (animations.size() > 0) {
    animationTimer.schedule(DEFAULT_FRAME_DELAY);
  }
}
