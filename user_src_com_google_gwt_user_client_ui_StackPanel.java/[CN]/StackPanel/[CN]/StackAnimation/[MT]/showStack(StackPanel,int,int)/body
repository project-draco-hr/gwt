{
  cancel();
  if (oldIndex < 0) {
    panel.setStackContentVisible(newIndex,true);
    return;
  }
  curPanel=panel;
  this.oldIndex=oldIndex;
  this.newIndex=newIndex;
  newTD=DOM.getFirstChild(DOM.getChild(curPanel.body,(2 * newIndex) + 1));
  if (panel.isAnimationEnabled) {
    run(250);
  }
 else {
    onInstantaneousRun();
  }
}
