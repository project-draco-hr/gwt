{
  while (elem != null && elem != getElement()) {
    String expando=elem.getPropertyString("__index");
    if (expando != null) {
      int ownerHash=elem.getPropertyInt("__owner");
      if (ownerHash == hashCode()) {
        return Integer.parseInt(expando);
      }
 else {
        return -1;
      }
    }
    elem=DOM.getParent(elem);
  }
  return -1;
}
