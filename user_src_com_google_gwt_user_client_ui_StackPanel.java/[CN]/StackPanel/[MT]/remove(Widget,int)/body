{
  boolean removed=super.remove(child);
  if (removed) {
    int rowIndex=2 * index;
    Element tr=DOM.getChild(body,rowIndex);
    body.removeChild(tr);
    tr=DOM.getChild(body,rowIndex);
    body.removeChild(tr);
    if (visibleStack == index) {
      visibleStack=-1;
    }
 else     if (visibleStack > index) {
      --visibleStack;
    }
    updateIndicesFrom(index);
  }
  return removed;
}
