{
  out.print(indent);
  if (type.needsAttention()) {
    out.print("[");
    out.print(type.getLabel());
    out.print("] ");
  }
  out.println(msg);
  if (helpInfo != null) {
    URL url=helpInfo.getURL();
    if (url != null) {
      out.print(indent);
      out.println("For additional info see: " + url.toString());
    }
  }
  if (caught != null) {
    caught.printStackTrace(out);
  }
}
