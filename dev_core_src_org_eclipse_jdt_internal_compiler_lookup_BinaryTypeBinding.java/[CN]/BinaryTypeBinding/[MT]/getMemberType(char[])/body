{
  for (int i=this.memberTypes.length; --i >= 0; ) {
    ReferenceBinding memberType=this.memberTypes[i];
    if (memberType instanceof UnresolvedReferenceBinding) {
      char[] name=memberType.sourceName;
      int prefixLength=this.compoundName[this.compoundName.length - 1].length + 1;
      if (name.length == (prefixLength + typeName.length))       if (CharOperation.fragmentEquals(typeName,name,prefixLength,true))       return this.memberTypes[i]=resolveType(memberType,this.environment,false);
    }
 else     if (CharOperation.equals(typeName,memberType.sourceName)) {
      return memberType;
    }
  }
  return null;
}
