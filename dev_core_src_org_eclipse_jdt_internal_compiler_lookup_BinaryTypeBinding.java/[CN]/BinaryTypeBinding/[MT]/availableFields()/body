{
  if ((this.tagBits & TagBits.AreFieldsComplete) != 0)   return fields;
  if ((this.tagBits & TagBits.AreFieldsSorted) == 0) {
    int length=this.fields.length;
    if (length > 1)     ReferenceBinding.sortFields(this.fields,0,length);
    this.tagBits|=TagBits.AreFieldsSorted;
  }
  FieldBinding[] availableFields=new FieldBinding[fields.length];
  int count=0;
  for (int i=0; i < fields.length; i++) {
    try {
      availableFields[count]=resolveTypeFor(fields[i]);
      count++;
    }
 catch (    AbortCompilation a) {
    }
  }
  if (count < availableFields.length)   System.arraycopy(availableFields,0,availableFields=new FieldBinding[count],0,count);
  return availableFields;
}
