{
  StringBuffer sb=new StringBuffer();
  sb.append(method.getName());
  sb.append('(');
  JParameter[] params=method.getParameters();
  for (int i=0; i < params.length; ++i) {
    if (i > 0) {
      sb.append(", ");
    }
    sb.append(params[i].getType().getQualifiedSourceName());
  }
  sb.append(')');
  return sb.toString();
}
