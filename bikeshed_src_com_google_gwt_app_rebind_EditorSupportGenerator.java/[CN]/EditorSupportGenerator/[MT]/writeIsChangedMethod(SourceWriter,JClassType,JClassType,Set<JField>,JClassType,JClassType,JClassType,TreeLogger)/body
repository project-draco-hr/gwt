{
  sw.indent();
  sw.println("public boolean isChanged(" + viewType.getName() + " view) {");
  sw.indent();
  for (  JField uiField : uiPropertyFields) {
    JField property=recordType.getField(uiField.getName());
    if (property != null) {
      String getter=findGetterMethod(property,uiField,takesValueType,hasTextType,stringType,logger);
      if (getter != null) {
        sw.println(String.format("view.getDeltaValueStore().set(%s.%s, view.getValue(), view.%s.%s());",recordType.getName(),property.getName(),uiField.getName(),getter));
      }
    }
  }
  sw.println("return view.getDeltaValueStore().isChanged();");
  sw.outdent();
  sw.println("}");
  sw.outdent();
}
