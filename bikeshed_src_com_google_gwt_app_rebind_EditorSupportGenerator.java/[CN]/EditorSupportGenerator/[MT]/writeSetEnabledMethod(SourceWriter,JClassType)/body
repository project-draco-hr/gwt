{
  sw.indent();
  sw.println("public void setEnabled(" + viewType.getName() + " view, boolean enabled) {");
  sw.indent();
  sw.println("// Note that we require package protection, just like UiBinder does.");
  for (  Entry<JField,JClassType> uiFieldEntry : uiPropertyFields.entrySet()) {
    if (uiFieldEntry.getValue() != takesValueType) {
      continue;
    }
    sw.println("view." + uiFieldEntry.getKey().getName() + ".setEnabled(enabled);");
  }
  sw.outdent();
  sw.println("}");
  sw.outdent();
}
