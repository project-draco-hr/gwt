{
  UnitTestTreeLogger.Builder builder=new UnitTestTreeLogger.Builder();
  builder.expect(Type.ERROR,"No @javax.validation.constraints.Size(message={javax.validation.constraints.Size.message}," + " min=0, max=2147483647, payload=[], groups=[]) " + "ConstraintValidator for type class java.lang.Integer",ValidationException.class);
  builder.setLowestLogLevel(Type.INFO);
  UnitTestTreeLogger testLogger=builder.createLogger();
  assertModuleFails(testLogger,getFullyQaulifiedModuleName(getClass(),UnexpectedTypeValidatorFactory.class.getSimpleName()),UnexpectedTypeValidator.class,Bar.class);
}
