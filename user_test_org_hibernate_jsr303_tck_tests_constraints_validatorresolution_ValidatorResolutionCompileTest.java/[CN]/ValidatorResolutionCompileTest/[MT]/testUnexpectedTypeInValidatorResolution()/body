  assertBeanValidatorFailsToCompile(UnexpectedTypeValidator.class,Bar.class,ValidationException.class,Pattern.compile("No \\@javax\\.validation\\.constraints\\.Size\\(" + "((\\s)*(message=\\{javax\\.validation\\.constraints\\.Size\\.message\\}|" + "min=0|max=2147483647|payload=\\[\\]|groups=\\[\\])(,)?){5}"+ "\\) "+ "ConstraintValidator for type class java\\.lang\\.Integer"));
