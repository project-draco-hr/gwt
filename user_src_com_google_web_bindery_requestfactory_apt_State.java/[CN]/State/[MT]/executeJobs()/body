{
  while (!jobs.isEmpty()) {
    Job job=jobs.remove(0);
    if (verbose) {
      messager.printMessage(Kind.NOTE,String.format("Scanning %s",elements.getBinaryName(job.element)));
    }
    try {
      job.scanner.scan(job.element,this);
    }
 catch (    HaltException ignored) {
    }
catch (    Throwable e) {
      StringWriter sw=new StringWriter();
      e.printStackTrace(new PrintWriter(sw));
      poison(job.element,sw.toString());
    }
  }
  for (  TypeElement proxyElement : proxiesRequiringMapping) {
    if (!getClientToDomainMap().containsKey(proxyElement)) {
      poison(proxyElement,"A proxy must be annotated with %s, %s, or %s",ProxyFor.class.getSimpleName(),ProxyForName.class.getSimpleName(),JsonRpcProxy.class.getSimpleName());
    }
  }
}
