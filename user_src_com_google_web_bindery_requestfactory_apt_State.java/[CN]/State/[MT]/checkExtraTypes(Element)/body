{
  (new ExtraTypesScanner<Void>(){
    @Override public Void visitExecutable(    ExecutableElement x,    State state){
      checkForAnnotation(x,state);
      return null;
    }
    @Override public Void visitType(    TypeElement x,    State state){
      checkForAnnotation(x,state);
      return null;
    }
    @Override protected void scanExtraType(    TypeElement extraType){
      maybeScanProxy(extraType);
    }
  }
).scan(x,this);
}
