{
  TypeElement element=elements.getTypeElement("com.google.web.bindery.requestfactory.shared." + simpleName);
  if (element == null) {
    poison(null,"Unable to find RequestFactory built-in type. " + "Is requestfactory-[client|server].jar on the classpath?");
    return null;
  }
  return types.getDeclaredType(element);
}
