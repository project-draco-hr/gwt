{
  StringBuilder sb=new StringBuilder();
  sb.append(method.getEnclosingType().getName());
  sb.append("::");
  sb.append(method.getName());
  sb.append('(');
  for (  JType type : method.getOriginalParamTypes()) {
    sb.append(type.getJsniSignatureName());
  }
  sb.append(')');
  return sb.toString();
}
