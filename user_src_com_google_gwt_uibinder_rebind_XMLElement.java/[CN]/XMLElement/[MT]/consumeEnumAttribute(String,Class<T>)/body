{
  String strValue=consumeRawAttribute(attr);
  T value=null;
  try {
    if (strValue != null) {
      value=Enum.valueOf(type,strValue);
    }
  }
 catch (  IllegalArgumentException e) {
  }
  if (value == null) {
    logger.die(String.format("Error parsing \"%s\" attribute of \"%s\" " + "as a %s enum",attr,this,type.getSimpleName()));
  }
  return value;
}
