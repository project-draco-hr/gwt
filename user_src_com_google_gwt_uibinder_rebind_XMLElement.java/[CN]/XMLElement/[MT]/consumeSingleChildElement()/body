{
  XMLElement ret=null;
  for (  XMLElement child : consumeChildElements()) {
    if (ret != null) {
      logger.die("%s may only contain a single child element, but found " + "%s and %s.",this,ret,child);
    }
    ret=child;
  }
  if (ret == null) {
    logger.die("%s must have a single child element",this);
  }
  return ret;
}
