{
  XMLAttribute attribute=getAttribute(name);
  if (attribute == null) {
    if (required) {
      failRequired(name);
    }
    return defaultValue;
  }
  String value=attribute.consumeRawValue();
  if ("".equals(value)) {
    if (required) {
      failRequired(name);
    }
    return defaultValue;
  }
  return getParser(attribute,types).parse(value);
}
