{
  assert(lhsWildcard != rhsWildcard);
  assert(lhsWildcard != null && rhsWildcard != null);
  JBound lhsBound=lhsWildcard.getBounds();
  JBound rhsBound=rhsWildcard.getBounds();
  if (lhsBound.isUpperBound() != null && rhsBound.isUpperBound() != null) {
    return areClassTypesAssignable(lhsBound.getFirstBound(),rhsBound.getFirstBound());
  }
 else   if (lhsBound.isLowerBound() != null && rhsBound.isLowerBound() != null) {
    return areClassTypesAssignable(rhsBound.getFirstBound(),lhsBound.getFirstBound());
  }
  return false;
}
