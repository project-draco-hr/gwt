{
  if (serverWindow.hasBrowserWindowsOpen()) {
    boolean closeWindows=true;
    if (System.getProperty("gwt.shell.endquick") == null) {
      closeWindows=DialogBase.confirmAction((Shell)e.widget,"Closing the development shell will close " + "all hosted mode browsers.  Continue?","Confirm close");
    }
    if (closeWindows) {
      serverWindow.closeAllBrowserWindows();
      e.doit=true;
    }
 else {
      e.doit=false;
    }
  }
}
