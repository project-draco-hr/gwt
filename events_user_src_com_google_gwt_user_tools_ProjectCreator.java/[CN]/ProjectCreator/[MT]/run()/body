{
  try {
    if (ant == null && eclipse == null) {
      System.err.println("Please specify either -ant or -eclipse.");
      printHelp();
      return false;
    }
    createProject(eclipse,ant,outDir,overwrite,ignore,classPathHandler.getExtraClassPathList());
    return true;
  }
 catch (  IOException e) {
    System.err.println(e.getClass().getName() + ": " + e.getMessage());
    return false;
  }
}
