{
  try {
    if (moduleName == null) {
      handleUnload(scriptObject);
      return true;
    }
    if (!validHostedHtmlVersion(version)) {
      return false;
    }
    Object key=new Integer(scriptObject);
    ModuleSpaceHost msh=getHost().createModuleSpaceHost(BrowserWidgetMoz.this,moduleName);
    ModuleSpace moduleSpace=new ModuleSpaceMoz(msh,scriptObject,moduleName,key);
    attachModuleSpace(moduleSpace);
    return true;
  }
 catch (  Throwable e) {
    getHost().getLogger().log(TreeLogger.ERROR,"Failure to load module '" + moduleName + "'",e);
    return false;
  }
}
