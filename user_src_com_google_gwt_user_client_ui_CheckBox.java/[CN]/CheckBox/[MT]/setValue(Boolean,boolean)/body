{
  if (value == null) {
    throw new IllegalArgumentException("value must not be null");
  }
  Boolean oldValue=getValue();
  inputElem.setChecked(value);
  inputElem.setDefaultChecked(value);
  if (value.equals(oldValue)) {
    return;
  }
  if (fireEvents) {
    ValueChangeEvent.fire(this,value);
  }
}
