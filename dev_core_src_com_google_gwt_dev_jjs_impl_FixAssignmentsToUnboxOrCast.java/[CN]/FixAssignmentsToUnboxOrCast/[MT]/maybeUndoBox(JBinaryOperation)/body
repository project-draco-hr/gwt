{
  JExpression lhs=x.getLhs();
  JExpression boxed=autoboxUtils.undoUnbox(lhs);
  if (boxed == null) {
    return x;
  }
  JClassType boxedType=(JClassType)boxed.getType();
  return new JBinaryOperation(x.getSourceInfo(),boxedType,x.getOp(),boxed,autoboxUtils.box(x.getRhs(),(JPrimitiveType)lhs.getType()));
}
