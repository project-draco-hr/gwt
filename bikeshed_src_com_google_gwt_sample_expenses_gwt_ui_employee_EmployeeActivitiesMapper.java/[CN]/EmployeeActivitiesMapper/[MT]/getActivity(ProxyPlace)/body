{
switch (place.getOperation()) {
case DETAILS:
    return new EmployeeDetailsActivity((EmployeeRecord)place.getProxy(),requests,placeController);
case EDIT:
  return new EmployeeEditActivity((EmployeeRecord)place.getProxy(),requests,placeController,false);
case CREATE:
return new EmployeeEditActivity((EmployeeRecord)place.getProxy(),requests,placeController,true);
}
throw new IllegalArgumentException("Unknown operation " + place.getOperation());
}
