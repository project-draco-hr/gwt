{
  while ((node != null) && (node != table)) {
    if (Element.is(node)) {
      Element elem=Element.as(node);
      String tagName=elem.getTagName();
      if ("td".equalsIgnoreCase(tagName) || "th".equalsIgnoreCase(tagName)) {
        return elem.cast();
      }
    }
    node=node.getParentNode();
  }
  return null;
}
