{
  StringBuffer b=new StringBuffer();
  b.append("<g:Grid>");
  b.append("  <g:row>");
  b.append("    <g:customCell></g:customCell>");
  b.append("  </g:row>");
  b.append("</g:Grid>");
  try {
    tester.parse(b.toString());
    fail();
  }
 catch (  UnableToCompleteException exception) {
    assertNotNull(tester.logger.died);
  }
}
