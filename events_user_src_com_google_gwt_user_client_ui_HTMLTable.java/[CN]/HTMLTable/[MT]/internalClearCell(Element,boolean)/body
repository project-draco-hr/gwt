{
  Element maybeChild=DOM.getFirstChild(td);
  Widget widget=null;
  if (maybeChild != null) {
    widget=widgetMap.getWidget(maybeChild);
  }
  if (widget != null) {
    remove(widget);
    return true;
  }
 else {
    if (clearInnerHTML) {
      DOM.setInnerHTML(td,"");
    }
    return false;
  }
}
