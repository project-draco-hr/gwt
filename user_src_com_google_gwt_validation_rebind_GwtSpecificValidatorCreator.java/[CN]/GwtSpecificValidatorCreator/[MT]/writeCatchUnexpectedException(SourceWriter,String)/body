{
  sw.outdent();
  sw.println("} catch (IllegalArgumentException e) {");
  sw.indent();
  sw.println("throw e;");
  sw.outdent();
  sw.println("} catch (ValidationException e) {");
  sw.indent();
  sw.println("throw e;");
  sw.outdent();
  sw.println("} catch (Exception e) {");
  sw.indent();
  sw.println("throw new ValidationException(" + message + ", e);");
  sw.outdent();
  sw.println("}");
}
