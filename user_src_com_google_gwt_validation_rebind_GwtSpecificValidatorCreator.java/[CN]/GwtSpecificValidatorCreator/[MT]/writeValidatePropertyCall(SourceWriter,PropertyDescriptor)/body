{
  String propertyName=p.getPropertyName();
  sw.print(validateMethodName(p));
  sw.print("(context, ");
  sw.print("violations, ");
  sw.print("object, ");
  sw.print("object.");
  if (hasGetter(p)) {
    sw.print(asGetter(p) + "()");
  }
 else {
    sw.print(propertyName);
  }
  sw.print(", ");
  sw.println("groups);");
}
