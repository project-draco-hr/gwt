{
  int n=heap.size();
  E value=heap.get(node);
  while (node * 2 < n) {
    int childNode=2 * node + 1;
    if ((childNode + 1 < n) && (cmp.compare(heap.get(childNode + 1),heap.get(childNode)) < 0)) {
      childNode++;
    }
    heap.set(node,heap.get(childNode));
    node=childNode;
  }
  heap.set(node,value);
}
