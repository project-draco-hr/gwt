{
  StringBuilder buf=new StringBuilder();
  StringGenerator gen=StringGenerator.create(buf,true);
  gen.appendStringLiteral("Hello ");
  gen.appendStringLiteral("there");
  gen.completeString();
  assertEquals("new com.google.gwt.safehtml.shared.SafeHtmlBuilder().appendHtmlConstant(" + "\"Hello there\").toSafeHtml()",buf.toString());
}
