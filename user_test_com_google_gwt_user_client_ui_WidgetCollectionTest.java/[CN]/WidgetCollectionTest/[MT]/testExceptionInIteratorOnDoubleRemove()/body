{
  WidgetCollection wc=new Container().collection;
  wc.add(new Button("a"));
  wc.add(new Button("b"));
  Iterator<Widget> iter=wc.iterator();
  iter.next();
  iter.remove();
  try {
    iter.remove();
    fail("expected IllegalStateException");
  }
 catch (  IllegalStateException expected) {
  }
}
