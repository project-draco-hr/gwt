{
  HasOneWidget panel=createPanel();
  Label l=new Label("L");
  Label l2=new Label("L2");
  assertNull(panel.getWidget());
  panel.setWidget(l);
  assertEquals(l,panel.getWidget());
  panel.setWidget(l2);
  assertEquals(l2,panel.getWidget());
  panel.setWidget(null);
  assertNull(panel.getWidget());
}
