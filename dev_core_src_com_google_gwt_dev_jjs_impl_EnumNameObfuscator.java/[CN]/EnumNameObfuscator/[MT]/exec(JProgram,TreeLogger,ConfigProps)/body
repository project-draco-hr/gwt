{
  String enumObfucationProperty=configProps.getString(EnumNameObfuscator.ENUM_NAME_OBFUSCATION_PROPERTY,"false");
  boolean closureMode=enumObfucationProperty.equals("closure");
  if (enumObfucationProperty.equals("false")) {
    return;
  }
  List<String> blacklistedEnums=configProps.getCommaSeparatedStrings(EnumNameObfuscator.ENUM_NAME_OBFUSCATION_BLACKLIST_PROPERTY);
  new EnumNameCallChecker(jprogram,logger,blacklistedEnums).accept(jprogram);
  new EnumNameReplacer(jprogram,logger,blacklistedEnums,closureMode).exec();
}
