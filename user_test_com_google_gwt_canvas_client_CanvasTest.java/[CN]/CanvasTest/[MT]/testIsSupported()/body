{
  if (canvas1 == null) {
    assertFalse("isSupported() should be false when createIfSupported() returns null",Canvas.isSupported());
  }
 else {
    assertTrue("isSupported() should be true when createIfSupported() returns non-null",Canvas.isSupported());
  }
  if (isFirefox35OrLater()) {
    assertTrue(Canvas.isSupported());
    assertTrue(Canvas.isSupported());
  }
  if (isIE6()) {
    assertFalse(Canvas.isSupported());
    assertFalse(Canvas.isSupported());
  }
}
