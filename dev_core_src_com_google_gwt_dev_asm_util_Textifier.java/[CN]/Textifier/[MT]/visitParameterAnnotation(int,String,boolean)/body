{
  buf.setLength(0);
  buf.append(tab2).append('@');
  appendDescriptor(FIELD_DESCRIPTOR,desc);
  buf.append('(');
  text.add(buf.toString());
  Textifier t=createTextifier();
  text.add(t.getText());
  text.add(visible ? ") // parameter " : ") // invisible, parameter ");
  text.add(new Integer(parameter));
  text.add("\n");
  return t;
}
