{
synchronized (byteCodeCache) {
    if (getByteCode(binaryTypeName) == null) {
      byteCodeCache.put(binaryTypeName,byteCode,(!byteCode.isTransient()));
      logger.log(TreeLogger.SPAM,"Cached bytecode for " + binaryTypeName,null);
      return true;
    }
 else {
      logger.log(TreeLogger.SPAM,"Bytecode not re-cached for " + binaryTypeName,null);
      return false;
    }
  }
}
