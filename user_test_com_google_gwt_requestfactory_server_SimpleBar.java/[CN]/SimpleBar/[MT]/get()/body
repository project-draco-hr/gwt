{
  HttpServletRequest req=RequestFactoryServlet.getThreadLocalRequest();
  if (req == null) {
    return jreTestSingleton;
  }
 else {
    SimpleBar value=(SimpleBar)req.getSession().getAttribute(SimpleBar.class.getCanonicalName());
    if (value == null) {
      value=reset();
    }
    return value;
  }
}
