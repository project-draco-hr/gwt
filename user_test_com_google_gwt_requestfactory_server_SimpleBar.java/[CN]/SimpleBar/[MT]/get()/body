{
  HttpServletRequest req=RequestFactoryServlet.getThreadLocalRequest();
  if (req == null) {
    return jreTestSingleton;
  }
 else {
    Map<String,SimpleBar> value=(Map<String,SimpleBar>)req.getSession().getAttribute(SimpleBar.class.getCanonicalName());
    if (value == null) {
      value=reset();
    }
    return value;
  }
}
