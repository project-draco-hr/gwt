{
  if (daysFilter[day] == included) {
    return;
  }
  daysFilter[day]=included;
  if (pendingRefresh == null) {
    pendingRefresh=new ScheduledCommand(){
      public void execute(){
        pendingRefresh=null;
        dynaTable.refresh();
      }
    }
;
    Scheduler.get().scheduleDeferred(pendingRefresh);
  }
}
