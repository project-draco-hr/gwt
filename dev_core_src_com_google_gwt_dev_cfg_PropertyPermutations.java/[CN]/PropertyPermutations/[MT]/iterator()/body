{
  return new Iterator(){
    public boolean hasNext(){
      return iterPermIndex < values.length;
    }
    public Object next(){
      if (!hasNext()) {
        throw new NoSuchElementException();
      }
      return values[iterPermIndex++];
    }
    public void remove(){
      throw new UnsupportedOperationException("remove");
    }
    private int iterPermIndex;
  }
;
}
