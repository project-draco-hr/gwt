{
  assertValidatorFailsToCompile(GroupSequenceContainingDefaultValidator.class,GroupDefinitionException.class,Pattern.compile("Unable to create a validator for " + "org.hibernate.jsr303.tck.tests.constraints.groups." + "DefaultGroupRedefinitionTest.AddressWithDefaultInGroupSequence "+ "because 'Default.class' cannot appear in default group "+ "sequence list.",Pattern.LITERAL));
