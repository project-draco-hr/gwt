{
  int i=0;
  try {
    while (i < 20) {
      StringBuffer x=new StringBuffer();
      for (int j=i; j >= 0; j--) {
        x.append(toRepeat);
      }
      addCheckpoint(x.toString());
      numberFactory.create(x.toString());
      i++;
    }
  }
 catch (  NumberFormatException e) {
  }
  return i;
}
