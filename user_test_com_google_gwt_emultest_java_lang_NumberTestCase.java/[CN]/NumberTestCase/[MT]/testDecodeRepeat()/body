{
  assertEquals("digits",numberFactory.maxDigits(),determineLongestAcceptable(DIGIT));
  assertEquals("decimals",numberFactory.maxDecimalPoints(),determineLongestAcceptable(DIGIT_WITH_DECIMAL));
  assertEquals("minus signs",numberFactory.maxMinusSigns(),determineLongestAcceptable(DIGIT_WITH_MINUS));
  assertEquals("minus signs",numberFactory.maxExponents(),determineLongestAcceptable(DIGIT_WITH_EXPONENT));
}
