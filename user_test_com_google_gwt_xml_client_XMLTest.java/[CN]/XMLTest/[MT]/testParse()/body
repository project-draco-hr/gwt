{
  Document d=XMLParser.parse("<!--hello-->   <a spam=\"ham\">\n  <?pi hello ?>dfgdfg  <b/>\t</a>");
  XMLParser.removeWhitespace(d);
  assertEquals("<!--hello--><a spam=\"ham\"><?pi hello ?>dfgdfg  <b/></a>",d.toString());
  try {
    XMLParser.parse("<<<");
    fail();
  }
 catch (  DOMParseException e) {
  }
}
