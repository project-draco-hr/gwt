{
  if (in.peek() == '*') {
    addToString(in.read());
    if (in.peek() != ')') {
      reportSyntaxError("msg.jsni.expected.char",new String[]{")"});
    }
    addToString(in.read());
    return true;
  }
  do {
    int c=in.read();
    if (c == ')') {
      addToString(c);
      return true;
    }
    in.unread();
  }
 while (jsniMatchParamTypeSignature());
  return false;
}
