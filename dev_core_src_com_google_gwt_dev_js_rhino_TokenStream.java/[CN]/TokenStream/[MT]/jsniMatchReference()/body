{
  if (!jsniMatchQualifiedTypeName('.',':')) {
    return ERROR;
  }
  int c=in.read();
  if (c != ':') {
    in.unread();
    reportSyntaxError("msg.jsni.expected.char",new String[]{":"});
    return ERROR;
  }
  addToString(c);
  if (!jsniMatchMethodSignatureOrFieldName()) {
    return ERROR;
  }
  this.string=new String(stringBuffer,0,stringBufferTop);
  return NAME;
}
