{
  assertThat("s == null","boolean","String s = null;").evaluatesInto("true");
  assertThat("s != null","boolean","String s = null;").evaluatesInto("false");
  assertThat("null == s","boolean","String s = null;").evaluatesInto("true");
  assertThat("null != s","boolean","String s = null;").evaluatesInto("false");
}
