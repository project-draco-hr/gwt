{
  TreeLogger linkLogger=logger.branch(TreeLogger.DEBUG,"Linking module '" + module.getName() + "'");
  StandardLinkerContext linkerStack=new StandardLinkerContext(linkLogger,module,options);
  linkerStacks.put(module.getName(),linkerStack);
  ArtifactSet artifacts=linkerStack.invokeLink(linkLogger);
  produceOutput(linkLogger,linkerStack,artifacts,module);
}
