{
  if (isAttached()) {
switch (keyCode) {
case KeyboardListener.KEY_DOWN:
      shiftSelection(getColumnsPerRow());
    break;
case KeyboardListener.KEY_UP:
  shiftSelection(-getColumnsPerRow());
break;
case KeyboardListener.KEY_LEFT:
shiftSelection(-1);
break;
case KeyboardListener.KEY_RIGHT:
shiftSelection(1);
break;
case KeyboardListener.KEY_ENTER:
commitSelection();
break;
default :
return false;
}
return true;
}
 else {
return false;
}
}
