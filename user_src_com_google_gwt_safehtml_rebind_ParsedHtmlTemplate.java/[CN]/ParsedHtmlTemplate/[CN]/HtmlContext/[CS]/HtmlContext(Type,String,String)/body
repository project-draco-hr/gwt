{
  Preconditions.checkArgument((type != Type.UNDEFINED) || ((tag == null) && (attribute == null)),"tag and attribute must be null for context \"UNDEFINED\"");
  Preconditions.checkArgument((type != Type.TEXT) || ((tag == null) && (attribute == null)),"tag and attribute must be null for context \"TEXT\"");
  this.type=type;
  this.tag=tag;
  this.attribute=attribute;
}
