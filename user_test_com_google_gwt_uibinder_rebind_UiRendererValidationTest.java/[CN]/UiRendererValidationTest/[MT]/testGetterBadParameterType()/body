{
  initGetterTest();
  declaredMethods.append("    public DivElement getRoot(String bar);");
  init(UI_XML,generateRendererResource(declaredMethods));
  assertParseFailure("Expected failure due to getter parameter not of type Element.","Getter getRoot must have exactly one parameter of type assignable to " + "com.google.gwt.dom.client.Element");
}
