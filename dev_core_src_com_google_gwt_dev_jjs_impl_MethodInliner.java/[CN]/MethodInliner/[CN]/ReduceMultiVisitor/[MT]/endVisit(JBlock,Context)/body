{
  for (Iterator it=x.statements.iterator(); it.hasNext(); ) {
    JStatement stmt=(JStatement)it.next();
    if (stmt instanceof JExpressionStatement) {
      JExpression expr=((JExpressionStatement)stmt).getExpr();
      if (!expr.hasSideEffects()) {
        it.remove();
        didChange=true;
      }
    }
  }
}
