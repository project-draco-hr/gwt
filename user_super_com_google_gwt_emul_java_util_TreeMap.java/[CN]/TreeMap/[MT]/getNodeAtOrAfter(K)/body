{
  Node<K,V> foundNode=null;
  Node<K,V> node=root;
  while (node != null) {
    int c=cmp.compare(key,node.key);
    if (c == 0) {
      return node;
    }
 else     if (c > 0) {
      node=node.child[RIGHT];
    }
 else {
      foundNode=node;
      node=node.child[LEFT];
    }
  }
  return foundNode;
}
