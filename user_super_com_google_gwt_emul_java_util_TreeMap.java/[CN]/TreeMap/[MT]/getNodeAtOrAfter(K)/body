{
  if (root == null) {
    return null;
  }
  Node<K,V> foundNode=null;
  Node<K,V> node=root;
  while (true) {
    int c=cmp.compare(key,node.key);
    if (c == 0) {
      return node;
    }
 else     if (c > 0) {
      node=node.child[RIGHT];
      if (node == null) {
        return foundNode;
      }
    }
 else {
      foundNode=node;
      Node<K,V> nextNode=node.child[LEFT];
      if (nextNode == null) {
        return node;
      }
      node=nextNode;
    }
  }
}
