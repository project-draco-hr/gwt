{
  addSnippetImport("com.google.gwt.core.client.JavaScriptObject");
  addSnippetClassDecl("static class Buggy {","  native void jsniMethod(Object o) /*-{","    @java.lang.Double::new(D)();","    @java.lang.Boolean::new(Z)();","  }-*/;","}");
  assertCompileSucceeds("new Buggy().jsniMethod(null);");
}
