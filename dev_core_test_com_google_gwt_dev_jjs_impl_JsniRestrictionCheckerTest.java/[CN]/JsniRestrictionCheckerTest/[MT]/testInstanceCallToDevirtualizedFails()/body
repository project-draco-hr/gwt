{
  addSnippetImport("com.google.gwt.core.client.JavaScriptObject");
  addSnippetClassDecl("static class Buggy {","  native void jsniMethod(Object o) /*-{","    new Object().@java.lang.Double::doubleValue()();","  }-*/;","}");
  assertCompileFails("new Buggy().jsniMethod(null);","Line 6: JSNI method test.EntryPoint$Buggy.jsniMethod(Ljava/lang/Object;)V calls method " + "java.lang.Double.doubleValue()D. Instance methods on java.lang.Double cannot be " + "called from JSNI.");
}
