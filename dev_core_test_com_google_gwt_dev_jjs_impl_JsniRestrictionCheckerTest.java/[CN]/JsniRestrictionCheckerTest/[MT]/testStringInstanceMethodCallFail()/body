{
  addSnippetClassDecl("static class Buggy {","  static String foo;","  native void jsniMethod(Object o) /*-{","    \"Hello\".@java.lang.String::length()();","  }-*/;","}");
  assertCompileFails("new Buggy().jsniMethod(null);","Line 6: JSNI method test.EntryPoint$Buggy.jsniMethod(Ljava/lang/Object;)V calls method " + "java.lang.String.length()I. Instance methods on java.lang.String cannot be called " + "from JSNI.");
}
