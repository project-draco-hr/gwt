{
  Throwable rethrow=null;
  try {
    Field victimField=victimClass.getDeclaredField(fieldName);
    victimField.setAccessible(true);
    victimField.set(victimObject,value);
    return;
  }
 catch (  IllegalArgumentException e) {
    rethrow=e;
  }
catch (  SecurityException e) {
    rethrow=e;
  }
catch (  IllegalAccessException e) {
    rethrow=e;
  }
catch (  NoSuchFieldException e) {
    rethrow=e;
  }
  throw new RuntimeException("Unable to clobber field '" + fieldName + "' from class "+ victimClass.getName(),rethrow);
}
