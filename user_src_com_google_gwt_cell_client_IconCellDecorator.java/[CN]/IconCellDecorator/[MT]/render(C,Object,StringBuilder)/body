{
  sb.append("<div style='position:relative;padding-left:");
  sb.append(imageWidth);
  sb.append("px;'>");
  if (isIconUsed(value)) {
    sb.append(getIconHtml(value));
  }
 else {
    sb.append(placeHolderHtml);
  }
  sb.append("<div>");
  cell.render(value,key,sb);
  sb.append("</div></div>");
}
