{
  if (!a.getName().equals(b.getName())) {
    return false;
  }
  JType[] bParamTypes=b.getErasedParameterTypes();
  if (aParamTypes.length != bParamTypes.length) {
    return false;
  }
  for (int i=0; i < aParamTypes.length; ++i) {
    if (aParamTypes[i] != bParamTypes[i]) {
      return false;
    }
  }
  return true;
}
