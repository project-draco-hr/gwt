{
  if (!init()) {
    disconnectFromHost();
    return false;
  }
  logger.log(TreeLogger.DEBUG,"sending " + MessageType.LOAD_MODULE + " message, userAgent: "+ htmlUnitSessionHandler.getUserAgent());
  ReturnMessage returnMessage=null;
synchronized (htmlUnitSessionHandler.getHtmlPage()) {
    new LoadModuleMessage(this,url,tabKey,sessionKey,moduleName,htmlUnitSessionHandler.getUserAgent()).send();
    returnMessage=reactToMessages(htmlUnitSessionHandler,true);
    if (shouldDisconnect) {
      disconnectFromHost();
    }
  }
  logger.log(TreeLogger.DEBUG,"loaded module, returnValue: " + returnMessage.getReturnValue() + ", isException: "+ returnMessage.isException());
  return !returnMessage.isException();
}
