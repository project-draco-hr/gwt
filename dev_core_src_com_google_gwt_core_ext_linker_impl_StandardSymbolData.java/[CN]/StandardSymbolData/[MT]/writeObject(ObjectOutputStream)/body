{
  out.writeObject(className);
  if (isClass()) {
    out.write(0);
  }
 else   if (isField()) {
    out.write(1);
    out.writeUTF(memberName);
  }
 else {
    out.write(2);
    out.writeUTF(memberName);
    out.writeUTF(methodSig);
  }
  out.writeInt(sourceLine);
  out.writeObject(sourceUri);
  out.writeUTF(symbolName);
  out.writeInt(typeId);
  out.writeObject(castableTypeMap);
  out.writeInt(typeId);
  out.writeInt(fragmentNumber);
}
