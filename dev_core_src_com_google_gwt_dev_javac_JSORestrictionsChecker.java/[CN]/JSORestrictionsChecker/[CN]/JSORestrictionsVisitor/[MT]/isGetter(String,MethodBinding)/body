{
  if (methodBinding.parameters.length > 0 || methodBinding.returnType == TypeBinding.VOID) {
    return false;
  }
  if (startsWithCamelCase(methodName,"set")) {
    return false;
  }
  if ((startsWithCamelCase(methodName,"is") || startsWithCamelCase(methodName,"has")) && methodBinding.returnType != TypeBinding.BOOLEAN) {
    return false;
  }
  return true;
}
