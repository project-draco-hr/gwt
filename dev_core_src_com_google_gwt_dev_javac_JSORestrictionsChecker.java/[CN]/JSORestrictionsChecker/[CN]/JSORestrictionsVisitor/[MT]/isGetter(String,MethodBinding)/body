{
  if (name.length() > 3 && name.startsWith("get") && Character.isUpperCase(name.charAt(3)) && mb.returnType == TypeBinding.VOID && mb.parameters.length == 0) {
    return true;
  }
 else   if (name.length() > 3 && name.startsWith("is") && Character.isUpperCase(name.charAt(2)) && mb.returnType == TypeBinding.BOOLEAN && mb.parameters.length == 0) {
    return true;
  }
 else   if (mb.parameters.length == 0 && mb.returnType != TypeBinding.VOID) {
    return true;
  }
  return false;
}
