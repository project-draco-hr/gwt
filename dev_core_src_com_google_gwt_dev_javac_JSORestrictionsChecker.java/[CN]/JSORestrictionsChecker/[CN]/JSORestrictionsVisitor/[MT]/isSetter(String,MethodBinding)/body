{
  if (methodBinding.parameters.length != 1) {
    return false;
  }
  if (startsWithCamelCase(methodName,"get") || startsWithCamelCase(methodName,"is") || startsWithCamelCase(methodName,"has")) {
    return false;
  }
  if (methodBinding.returnType != TypeBinding.VOID && methodBinding.returnType != methodBinding.declaringClass) {
    return false;
  }
  return true;
}
