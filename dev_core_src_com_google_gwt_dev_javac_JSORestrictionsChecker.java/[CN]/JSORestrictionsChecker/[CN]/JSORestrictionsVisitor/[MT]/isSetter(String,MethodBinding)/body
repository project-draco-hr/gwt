{
  if (mb.returnType == TypeBinding.VOID || mb.returnType == mb.declaringClass) {
    if (name.length() > 3 && name.startsWith("set") && Character.isUpperCase(name.charAt(3)) && mb.parameters.length == 1) {
      return true;
    }
 else     if (mb.parameters.length == 1) {
      return true;
    }
  }
  return false;
}
