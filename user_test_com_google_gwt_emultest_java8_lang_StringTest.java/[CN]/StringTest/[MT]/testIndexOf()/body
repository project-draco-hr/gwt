{
  String haystack="abcdefghi";
  assertEquals(haystack.indexOf("q"),-1);
  assertEquals(haystack.indexOf('q'),-1);
  assertEquals(haystack.indexOf("a"),0);
  assertEquals(haystack.indexOf('a'),0);
  assertEquals(haystack.indexOf('a',1),-1);
  assertEquals(haystack.indexOf("bc"),1);
  assertEquals(haystack.indexOf(""),0);
}
