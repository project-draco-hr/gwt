{
{
    assertNull(returnNull());
    String a=returnNull() + returnNull();
    assertEquals("nullnull",a);
    String s=returnNull();
    s+=returnNull();
    assertEquals("nullnull",s);
  }
{
    String a=null + (String)null;
    assertEquals("nullnull",a);
    String s=null;
    s+=null;
    assertEquals("nullnull",s);
  }
}
