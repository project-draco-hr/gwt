{
  if ((x.getOperator() == JsBinaryOperator.COMMA) && (x.getArg1() instanceof JsInvocation)) {
    JsName clinit=getClinitFromInvocation((JsInvocation)x.getArg1());
    if ((clinit != null) && called.contains(clinit)) {
      ctx.replaceMe(x.getArg2());
      accept(x.getArg2());
      return false;
    }
  }
  return true;
}
