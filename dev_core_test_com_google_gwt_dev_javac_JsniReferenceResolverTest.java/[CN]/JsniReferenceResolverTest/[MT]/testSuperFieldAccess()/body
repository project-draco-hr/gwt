{
  MockJavaResource buggy=JavaResourceBase.createMockJavaResource("Buggy","class Buggy extends Super {","  native void jsniMeth() /*-{","    this.@Buggy::x; ","   }-*/;","}");
  MockJavaResource extra=JavaResourceBase.createMockJavaResource("Super","class Super {","  public long x = -1;","}");
  shouldGenerateError(buggy,extra,3,"Referencing field 'Buggy.x': unable to resolve field in class 'Buggy'");
}
