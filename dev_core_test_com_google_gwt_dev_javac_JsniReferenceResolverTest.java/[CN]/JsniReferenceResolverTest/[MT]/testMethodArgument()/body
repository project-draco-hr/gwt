{
  MockJavaResource buggy=JavaResourceBase.createMockJavaResource("Buggy","class Buggy {","  void print(long x) { }","  native void jsniMeth() /*-{ this.@Buggy::print(J)(0); }-*/;","}");
  shouldGenerateError(buggy,3,"Parameter 1 of method 'Buggy.print': type 'long' may not be passed out of JSNI code");
}
