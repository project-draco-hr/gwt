{
  MockJavaResource buggy=JavaResourceBase.createMockJavaResource("Buggy","@Deprecated","class DeprecatedClass {","  @Deprecated static void foo(){}","}","class Buggy {","  native void jsniMethod() /*-{","    @DeprecatedClass::foo();","  }-*/;","}");
  shouldGenerateWarnings(buggy,warning(7,"Referencing deprecated class 'DeprecatedClass'"),warning(7,"Referencing method 'DeprecatedClass.foo': " + "method 'DeprecatedClass.foo()' is deprecated"));
}
