{
  MockJavaResource buggy=JavaResourceBase.createMockJavaResource("Buggy","class Buggy extends Extra {","  public int m(String x) { return -1; }","  native void jsniMeth() /*-{","    this.@Buggy::m(*)(\"hello\"); }-*/;","}");
