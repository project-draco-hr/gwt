{
  MockJavaResource buggy=JavaResourceBase.createMockJavaResource("some.Buggy","package some;","import other.Super;","class Buggy extends Super {","  native void jsniMeth() /*-{","    this.@Buggy::m(*)(\"hello\"); }-*/;","}");
  MockJavaResource extra=JavaResourceBase.createMockJavaResource("other.Super","package other;","public class Super {","  static long along = 3;","  int m(Object x) { return -1; }","}");
  shouldGenerateError(buggy,extra,5,"Referencing method 'Buggy.m(*)': unable to resolve method");
}
