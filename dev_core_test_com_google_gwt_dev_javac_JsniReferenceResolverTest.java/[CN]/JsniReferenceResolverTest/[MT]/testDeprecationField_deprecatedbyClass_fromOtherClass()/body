{
  MockJavaResource buggy=JavaResourceBase.createMockJavaResource("Buggy","@Deprecated","class DeprecatedClass {","  @Deprecated static int bar;","}","class Buggy {","  native void jsniMethod() /*-{","    @DeprecatedClass::bar;","  }-*/;","}");
  shouldGenerateWarnings(buggy,warning(7,"Referencing deprecated class 'DeprecatedClass'"),warning(7,"Referencing field 'DeprecatedClass.bar': field 'DeprecatedClass.bar' is deprecated"));
}
