{
  MockJavaResource buggy=JavaResourceBase.createMockJavaResource("Buggy","class Buggy {","  @Deprecated static void foo(){}","  native void jsniMethod() /*-{","    @Buggy::foo();","  }-*/;","}");
  shouldGenerateWarning(buggy,4,"Referencing method 'Buggy.foo': method 'Buggy.foo()' is deprecated");
}
