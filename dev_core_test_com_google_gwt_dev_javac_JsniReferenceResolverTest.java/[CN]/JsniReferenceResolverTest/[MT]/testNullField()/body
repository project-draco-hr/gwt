{
  MockJavaResource buggy=JavaResourceBase.createMockJavaResource("Buggy","class Buggy {","  static native Object main() /*-{","    return @null::nullField;","  }-*/;","}");
  shouldGenerateNoWarning(buggy);
  buggy=JavaResourceBase.createMockJavaResource("Buggy","class Buggy {","  static native Object main() /*-{","    return @null::foo;","  }-*/;","}");
  shouldGenerateError(buggy,3,"Referencing field 'null.foo': 'nullField' is the only legal field reference for 'null'");
}
