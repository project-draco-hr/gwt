{
  if (!isRemoveSupported())   return;
  resetFull();
  Set entrySet=map.entrySet();
  assertTrue(map.size() > 0);
  assertTrue(entrySet.size() > 0);
  entrySet.clear();
  assertTrue(map.size() == 0);
  assertTrue(entrySet.size() == 0);
  resetFull();
  entrySet=map.entrySet();
  assertTrue(map.size() > 0);
  assertTrue(entrySet.size() > 0);
  map.clear();
  assertTrue(map.size() == 0);
  assertTrue(entrySet.size() == 0);
}
