{
  StringBuilder toReturn=new StringBuilder();
  for (Iterator<Value> i=values.iterator(); i.hasNext(); ) {
    Value value=i.next();
    if (value.isSpaceRequired()) {
      toReturn.append("\" \" +");
    }
    toReturn.append(value.getExpression());
    if (i.hasNext()) {
      toReturn.append("+ ");
    }
  }
  return toReturn.toString();
}
