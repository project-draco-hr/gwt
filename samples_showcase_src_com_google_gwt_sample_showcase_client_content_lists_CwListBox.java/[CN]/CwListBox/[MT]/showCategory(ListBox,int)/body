{
  listBox.clear();
  String[] listData=null;
switch (category) {
case 0:
    listData=constants.cwListBoxCars();
  break;
case 1:
listData=constants.cwListBoxSports();
break;
case 2:
listData=constants.cwListBoxVacations();
break;
}
for (int i=0; i < listData.length; i++) {
listBox.addItem(listData[i]);
}
}
