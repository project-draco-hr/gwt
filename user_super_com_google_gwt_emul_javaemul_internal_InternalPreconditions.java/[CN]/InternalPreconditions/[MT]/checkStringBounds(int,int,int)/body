{
  if (start < 0) {
    throw new StringIndexOutOfBoundsException("fromIndex: " + start + " < 0");
  }
  if (end > size) {
    throw new StringIndexOutOfBoundsException("toIndex: " + end + " > size "+ size);
  }
  if (end < start) {
    throw new StringIndexOutOfBoundsException("fromIndex: " + start + " > toIndex: "+ end);
  }
}
