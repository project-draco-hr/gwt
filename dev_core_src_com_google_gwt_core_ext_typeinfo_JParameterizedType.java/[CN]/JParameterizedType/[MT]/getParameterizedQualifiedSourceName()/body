{
  StringBuffer sb=new StringBuffer();
  sb.append(parameterized.getQualifiedSourceName());
  sb.append('<');
  boolean needComma=false;
  for (  JType typeArg : typeArgs) {
    if (needComma) {
      sb.append(", ");
    }
 else {
      needComma=true;
    }
    sb.append(typeArg.getParameterizedQualifiedSourceName());
  }
  sb.append('>');
  return sb.toString();
}
