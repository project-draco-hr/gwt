{
  int count=0;
  for (  String arrayElement[] : baseArrays) {
    count+=arrayElement.length;
  }
  String retArray[]=new String[count];
  count=0;
  for (  String arrayElement[] : baseArrays) {
    for (    String element : arrayElement) {
      retArray[count++]=element;
    }
  }
  return retArray;
}
