{
  if (inConditionalRule() && "external".equals(node.getName().getValue())) {
    if (lenient) {
      errorManager.reportWarning(new GssError("You should not define a external at-rule inside" + " a  ConditionalNode that will be evaluated at runtime. This will be disallowed in " + "the next version of GWT.",node.getSourceCodeLocation()));
    }
 else {
      errorManager.report(new GssError("You cannot define a external at-rule inside a " + "ConditionalNode that will be evaluated at runtime.",node.getSourceCodeLocation()));
    }
  }
  return super.enterUnknownAtRule(node);
}
