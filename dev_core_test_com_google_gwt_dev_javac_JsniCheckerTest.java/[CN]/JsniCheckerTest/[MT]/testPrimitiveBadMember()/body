{
  StringBuffer code=new StringBuffer();
  code.append("class Buggy {\n");
  code.append("  native void jsniMethod() /*-{\n");
  code.append("    @Z::blah;\n");
  code.append("  }-*/;\n");
  code.append("}\n");
  shouldGenerateError(code,3,"Referencing member 'Z.blah': 'class' is the only legal reference for primitive types");
}
