{
  StringBuffer code=new StringBuffer();
  code.append("class Buggy {\n");
  code.append("  native void jsniMethod() /*-{\n");
  code.append("    @Buggy::x;\n");
  code.append("  }-*/;\n");
  code.append("}\n");
  shouldGenerateError(code,3,"JSNI Referencing field 'Buggy.x': unable to resolve field, expect subsequent failures");
}
