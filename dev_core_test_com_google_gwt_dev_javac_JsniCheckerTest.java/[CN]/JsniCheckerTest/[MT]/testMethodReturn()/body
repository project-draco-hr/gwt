{
  MockJavaResource buggy=JavaResourceBase.createMockJavaResource("Buggy","class Buggy {","  long m() { return -1; }","  native void jsniMeth() /*-{","    $wnd.alert(this.@Buggy::m()()); }-*/;","}");
  shouldGenerateError(buggy,4,"Referencing method 'Buggy.m': return type 'long' is not safe to access in JSNI code");
}
