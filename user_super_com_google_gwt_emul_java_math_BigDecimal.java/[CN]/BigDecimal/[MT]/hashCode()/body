{
  if (hashCode != 0) {
    return hashCode;
  }
  if (bitLength < 54) {
    long longValue=(long)smallValue;
    hashCode=(int)(longValue & 0xffffffff);
    hashCode=33 * hashCode + (int)((longValue >> 32) & 0xffffffff);
    hashCode=17 * hashCode + (int)scale;
    return hashCode;
  }
  hashCode=17 * intVal.hashCode() + (int)scale;
  return hashCode;
}
