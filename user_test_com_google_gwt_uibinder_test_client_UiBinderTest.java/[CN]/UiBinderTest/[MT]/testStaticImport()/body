{
  assertEquals(Constants.CONST_FOO,widgetUi.bracedParagraph.getAttribute("foo"));
  assertEquals(Constants.Inner.CONST_BAR + " " + Constants.Inner.CONST_BAZ,widgetUi.bracedParagraph.getAttribute("bar"));
  assertEquals(Constants.MyEnum.ENUM_1.name() + " " + Constants.MyEnum.ENUM_2.name(),widgetUi.bracedParagraph.getAttribute("enum"));
}
