{
  assertEquals("<b>click me</b>",widgetUi.pushButton.getUpFace().getHTML().toLowerCase(Locale.ROOT));
  assertTrue(widgetUi.pushButton.getUpHoveringFace().getHTML().contains(">Click ME!<"));
  assertEquals("<b>click me!</b>",widgetUi.pushButton.getUpHoveringFace().getHTML().toLowerCase(Locale.ROOT));
}
