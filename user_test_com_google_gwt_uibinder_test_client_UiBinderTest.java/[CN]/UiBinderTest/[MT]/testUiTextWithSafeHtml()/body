{
  assertEquals("<b>this text should be bold!</b>",StringCase.toLower(widgetUi.htmlWithComputedSafeHtml.getHTML()));
  assertEquals("&lt;b&gt;this text won't be bold!&lt;/b&gt;",StringCase.toLower(widgetUi.htmlWithComputedText.getHTML()).replaceAll(">","&gt;"));
  assertEquals("<b>this text won't be bold!</b>",StringCase.toLower(widgetUi.labelWithComputedText.getText()));
}
