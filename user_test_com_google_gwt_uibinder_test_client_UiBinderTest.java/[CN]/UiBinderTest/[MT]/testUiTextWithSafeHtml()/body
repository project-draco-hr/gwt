{
  assertEquals("<b>this text should be bold!</b>",widgetUi.htmlWithComputedSafeHtml.getHTML().toLowerCase());
  assertEquals("&lt;b&gt;this text won't be bold!&lt;/b&gt;",widgetUi.htmlWithComputedText.getHTML().toLowerCase().replaceAll(">","&gt;"));
  assertEquals("<b>this text won't be bold!</b>",widgetUi.labelWithComputedText.getText().toLowerCase());
}
