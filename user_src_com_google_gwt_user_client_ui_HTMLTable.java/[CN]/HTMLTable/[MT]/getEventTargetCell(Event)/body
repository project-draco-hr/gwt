{
  Element td=DOM.eventGetTarget(event);
  for (; td != null; td=DOM.getParent(td)) {
    if (td.getPropertyString("tagName").equalsIgnoreCase("td")) {
      Element tr=DOM.getParent(td);
      Element body=DOM.getParent(tr);
      if (body == bodyElem) {
        return td.cast();
      }
    }
    if (td == bodyElem) {
      return null;
    }
  }
  return null;
}
