{
  Element td=Element.as(event.getEventTarget());
  for (; td != null; td=td.getParentElement()) {
    if (td.getTagName().equalsIgnoreCase("td")) {
      Element tr=td.getParentElement();
      Element body=tr.getParentElement();
      if (body == bodyElem) {
        return td.cast();
      }
    }
    if (td == bodyElem) {
      return null;
    }
  }
  return null;
}
