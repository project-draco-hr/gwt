{
  prepareColumn(col);
  prepareColumnGroup();
  int num=DOM.getChildCount(columnGroup);
  if (num <= col) {
    Element colElement=null;
    for (int i=num; i <= col; i++) {
      colElement=DOM.createElement("col");
      DOM.appendChild(columnGroup,colElement);
    }
    return colElement;
  }
  return DOM.getChild(columnGroup,col);
}
