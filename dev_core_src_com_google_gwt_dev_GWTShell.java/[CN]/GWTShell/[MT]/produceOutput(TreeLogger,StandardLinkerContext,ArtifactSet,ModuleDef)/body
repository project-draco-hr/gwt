{
  TreeLogger linkLogger=logger.branch(TreeLogger.DEBUG,"Linking module '" + module.getName() + "'");
  File outputDir=options.getShellPublicGenDir(module);
  outputDir.mkdirs();
  OutputFileSetOnDirectory outFileSet=new OutputFileSetOnDirectory(outputDir,"");
  linkerStack.produceOutput(linkLogger,artifacts,false,outFileSet);
  outFileSet.close();
}
