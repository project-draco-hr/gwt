{
  JClassType type=oracle.findType(typeName);
  if (type == null) {
    die("In %s, unknown type %s",elem,typeName);
  }
  String fieldName=getFieldName(elem);
  if (fieldName == null) {
    fieldName=("f_" + elem.getLocalName() + (++fieldIndex));
  }
  fieldManager.registerField(type,fieldName);
  return fieldName;
}
