{
  fieldManager.registerFieldOfGeneratedType(bundleClass.getPackageName(),bundleClass.getClassName(),bundleClass.getFieldName());
  String rootField=new UiBinderParser(this,messages,fieldManager,oracle,bundleClass).parse(elem);
  StringWriter stringWriter=new StringWriter();
  IndentedWriter niceWriter=new IndentedWriter(new PrintWriter(stringWriter));
  writeBinder(niceWriter,rootField);
  return stringWriter.toString();
}
