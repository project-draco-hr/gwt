{
  insideMediaAtRule=true;
  out.print("@media");
  boolean isFirst=true;
  for (  String m : x.getMedias()) {
    if (isFirst) {
      out.print(" ");
      isFirst=false;
    }
 else {
      comma();
    }
    out.print(m);
  }
  spaceOpt();
  out.print("{");
  out.newlineOpt();
  out.indentIn();
  return true;
}
