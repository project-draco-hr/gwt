{
  this.program=program;
  setCheckMethod=program.getIndexedMethod(RuntimeConstants.ARRAY_SET_CHECK);
  initializeUnidimensionalArrayMethod=program.getIndexedMethod(RuntimeConstants.ARRAY_INITIALIZE_UNIDIMENSIONAL_ARRAY);
  initializeMultidimensionalArrayMethod=program.getIndexedMethod(RuntimeConstants.ARRAY_INITIALIZE_MULTIDIMENSIONAL_ARRAY);
  stampJavaTypeInfoMethod=program.getIndexedMethod(RuntimeConstants.ARRAY_STAMP_JAVA_TYPE_INFO);
}
