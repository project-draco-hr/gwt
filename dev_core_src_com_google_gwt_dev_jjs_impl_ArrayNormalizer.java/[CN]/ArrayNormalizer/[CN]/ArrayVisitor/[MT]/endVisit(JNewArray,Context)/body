{
  JArrayType type=x.getArrayType();
  if (x.initializers != null) {
    processInitializers(x,ctx,type);
  }
 else {
    int realDims=0;
    for (    JExpression dim : x.dims) {
      if (dim instanceof JAbsentArrayDimension) {
        break;
      }
      ++realDims;
    }
    assert(realDims >= 1);
    if (realDims == 1) {
      processDim(x,ctx,type);
    }
 else {
      processDims(x,ctx,type,realDims);
    }
  }
}
