{
  if (maybePushBackRef(x)) {
    Object instance;
    try {
      instance=CommandSerializationUtil.allocateInstance(x.getTargetClass());
      push(x,instance);
      return true;
    }
 catch (    InstantiationException e) {
      halt(new SerializationException("Unable to create instance",e));
    }
  }
  return false;
}
