{
  if (runStyle instanceof RunStyleHtmlUnit && (bannedPlatforms.contains(Platform.HtmlUnitUnknown) || bannedPlatforms.contains(Platform.HtmlUnitLayout) || bannedPlatforms.contains(Platform.HtmlUnitBug))) {
    return true;
  }
  if (developmentMode) {
    if (bannedPlatforms.contains(Platform.Devel)) {
      return true;
    }
  }
 else {
    if (bannedPlatforms.contains(Platform.Prod)) {
      return true;
    }
  }
  return false;
}
