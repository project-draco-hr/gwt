{
  if (runStyle instanceof RunStyleHtmlUnit && (bannedPlatforms.contains(Platform.HtmlUnitBug) || bannedPlatforms.contains(Platform.HtmlUnit))) {
    return true;
  }
  if (developmentMode) {
    if (bannedPlatforms.contains(Platform.Devel)) {
      return true;
    }
  }
 else {
    if (bannedPlatforms.contains(Platform.Prod)) {
      return true;
    }
  }
  return false;
}
