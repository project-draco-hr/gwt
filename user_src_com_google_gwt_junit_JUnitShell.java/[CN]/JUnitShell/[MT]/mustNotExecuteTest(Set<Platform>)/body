{
  if (!Collections.disjoint(bannedPlatforms,runStyle.getPlatforms())) {
    return true;
  }
  if (developmentMode) {
    if (bannedPlatforms.contains(Platform.Devel)) {
      return true;
    }
  }
 else {
    if (bannedPlatforms.contains(Platform.Prod)) {
      return true;
    }
  }
  return false;
}
