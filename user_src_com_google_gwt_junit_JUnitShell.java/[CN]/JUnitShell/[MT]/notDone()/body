{
  if (!messageQueue.haveAllClientsRetrievedCurrentTest() && testBeginTimeout < System.currentTimeMillis()) {
    double elapsed=(System.currentTimeMillis() - testBeginTime) / 1000.0;
    throw new TimeoutException("The browser did not contact the server within " + TEST_BEGIN_TIMEOUT_MILLIS + "ms.\n"+ messageQueue.getUnretrievedClients()+ "\n Actual time elapsed: "+ elapsed+ " seconds.\n");
  }
  if (messageQueue.hasResult(currentModule.getName())) {
    return false;
  }
  return !runStyle.wasInterrupted();
}
