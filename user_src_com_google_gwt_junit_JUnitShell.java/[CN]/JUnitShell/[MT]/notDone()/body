{
  if (messageQueue.hasNextTestName(testCaseClassName) && testBeginTimout < System.currentTimeMillis()) {
    throw new TimeoutException("The browser did not contact the server within " + TEST_BEGIN_TIMEOUT_MILLIS + "ms.");
  }
  if (messageQueue.hasResult(testCaseClassName)) {
    return false;
  }
  return !runStyle.wasInterrupted();
}
