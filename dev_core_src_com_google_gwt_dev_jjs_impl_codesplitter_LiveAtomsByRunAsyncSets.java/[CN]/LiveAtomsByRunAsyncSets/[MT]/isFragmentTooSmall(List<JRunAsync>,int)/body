{
  BitSet fragmentSubset=new BitSet();
  for (  JRunAsync runAsync : fragmentRunAsyncs) {
    fragmentSubset.set(getIdForRunAsync(runAsync));
  }
  int size=0;
  for (  BitSet subset : payloadSizeBySubset.elementSet()) {
    if (isSubset(subset,fragmentSubset)) {
      size+=payloadSizeBySubset.count(subset);
      if (size >= minSize) {
        return false;
      }
    }
  }
  return true;
}
