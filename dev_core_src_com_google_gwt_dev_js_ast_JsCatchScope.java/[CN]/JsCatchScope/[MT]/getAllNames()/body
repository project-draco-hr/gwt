{
  return new Iterator(){
    private boolean didIterate=false;
    public boolean hasNext(){
      return !didIterate;
    }
    public Object next(){
      if (didIterate) {
        throw new NoSuchElementException();
      }
      didIterate=true;
      return name;
    }
    public void remove(){
      throw new UnsupportedOperationException();
    }
  }
;
}
