{
  final StringBufferDisplayList dl=new StringBufferDisplayList();
  dl.begin();
  for (int i=0; i < times; i++) {
    dl.translate(50,50);
    dl.rotate(45);
    dl.moveTo(0,0);
    dl.lineTo(100,0);
    dl.lineTo(100,100);
    dl.lineTo(0,100);
    dl.lineTo(0,0);
    dl.stroke();
    dl.fill();
  }
  pretendToUse(dl.end().length());
}
