{
  Ref mine=entry.getValue();
  boolean result;
  if ((mine == null) != (theirs == null)) {
    result=false;
  }
 else   if (mine == null && theirs == null) {
    return true;
  }
 else   if (structurallySame(mine,theirs,cachedStructurallySame)) {
    entry.setValue(new DirectRef(theirs));
    result=true;
  }
 else {
    result=false;
  }
  return result;
}
