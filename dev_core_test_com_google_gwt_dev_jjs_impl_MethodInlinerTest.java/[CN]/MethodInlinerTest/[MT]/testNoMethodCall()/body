{
  addSnippetClassDecl("static int fun1(int a) { return a; }");
  addSnippetClassDecl("static int fun2(int a, int b) { return a + b; }");
  Result result=optimize("void","");
  assertEquals("static int fun1(int a){\n" + "  return a;\n}",result.findMethod("fun1").toSource());
  assertEquals("static int fun2(int a, int b){\n" + "  return a + b;\n}",result.findMethod("fun2").toSource());
}
