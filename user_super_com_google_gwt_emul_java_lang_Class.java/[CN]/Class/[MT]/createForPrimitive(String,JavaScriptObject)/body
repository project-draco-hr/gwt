{
  Class<?> clazz=new Class<Object>();
  if (clazz.isClassMetadataEnabled()) {
    clazz.packageName="";
    clazz.compoundName=className;
    clazz.typeId=primitiveTypeId;
  }
 else {
    synthesizePrimitiveNamesFromTypeId(clazz,primitiveTypeId);
  }
  clazz.modifiers=PRIMITIVE;
  return clazz;
}
