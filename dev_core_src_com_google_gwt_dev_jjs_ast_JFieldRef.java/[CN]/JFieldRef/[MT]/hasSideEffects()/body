{
  if (field.isStatic() && (!field.isFinal() || field.constInitializer == null)) {
    if (program.typeOracle.checkClinit(enclosingType,field.getEnclosingType())) {
      return true;
    }
  }
  JExpression expr=instance;
  if (expr == null) {
    return false;
  }
  return expr.hasSideEffects();
}
