{
  BitSet liveSet=map.get(atom);
  if (liveSet == null) {
    liveSet=new BitSet();
    liveSet.set(splitPoint);
    map.put(atom,liveSet);
    return true;
  }
 else {
    if (liveSet.get(splitPoint)) {
      return false;
    }
 else {
      liveSet.set(splitPoint);
      return true;
    }
  }
}
