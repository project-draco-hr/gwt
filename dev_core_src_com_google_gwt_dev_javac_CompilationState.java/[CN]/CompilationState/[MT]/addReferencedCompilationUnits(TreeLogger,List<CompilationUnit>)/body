{
  Event referencedUnitsAddEvent=SpeedTracerLogger.start(DevModeEventType.COMP_STATE_ADD_REFERENCED_UNITS);
  try {
    logger=logger.branch(TreeLogger.DEBUG,"Adding '" + referencedUnits.size() + "' new referenced units");
    referencedUnitsAddEvent.addData("# new referenced units","" + referencedUnits.size());
    assimilateUnits(logger,referencedUnits,false);
  }
  finally {
    referencedUnitsAddEvent.end();
  }
}
