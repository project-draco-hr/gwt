{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  Either other=(Either)obj;
  if (left != null && other.left != null) {
    return left.equals(other.left);
  }
 else   if (right != null && other.right != null) {
    return right.equals(other.right);
  }
  return false;
}
