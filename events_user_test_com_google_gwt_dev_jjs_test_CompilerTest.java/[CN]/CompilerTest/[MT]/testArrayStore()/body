{
  Object[][] oaa;
  oaa=new Object[4][4];
  oaa[0][0]="foo";
  assertEquals(oaa[0][0],"foo");
  oaa=new Object[4][];
  oaa[0]=new Object[4];
  oaa[0][0]="bar";
  assertEquals(oaa[0][0],"bar");
  Apple[] apple=TRUE ? new Granny[3] : new Apple[3];
  Apple g=TRUE ? (Apple)new Granny() : (Apple)new Fuji();
  Apple a=apple[0]=g;
  assertEquals(g,a);
  byte[] bytes=new byte[10];
  bytes[0]=(byte)'1';
  assertEquals(49,bytes[0]);
}
