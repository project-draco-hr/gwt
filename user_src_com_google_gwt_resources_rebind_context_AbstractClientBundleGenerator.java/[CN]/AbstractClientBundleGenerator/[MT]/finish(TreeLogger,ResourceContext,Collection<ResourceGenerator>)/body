{
  boolean fail=false;
  for (  ResourceGenerator rg : generators) {
    try {
      rg.finish(logger.branch(TreeLogger.DEBUG,"Finishing ResourceGenerator"),context);
    }
 catch (    UnableToCompleteException e) {
      fail=true;
    }
  }
  if (fail) {
    throw new UnableToCompleteException();
  }
}
