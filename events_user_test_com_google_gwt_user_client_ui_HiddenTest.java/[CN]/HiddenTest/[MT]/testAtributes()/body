{
  Hidden x=new Hidden("test");
  x.setDefaultValue("myDefaultValue");
  assertEquals("myDefaultValue",x.getDefaultValue());
  x.setValue("myValue");
  assertEquals("myValue",x.getValue());
  x.setID("myID");
  assertEquals("myID",x.getID());
  x.setName("myName");
  assertEquals("myName",x.getName());
}
