{
  UIObject.setVisible(container,false);
  DOM.setStyleAttribute(container,"height","100%");
  Element element=w.getElement();
  if (DOM.getStyleAttribute(element,"width").equals("")) {
    w.setWidth("100%");
  }
  if (DOM.getStyleAttribute(element,"height").equals("")) {
    w.setHeight("100%");
  }
  w.setVisible(false);
}
