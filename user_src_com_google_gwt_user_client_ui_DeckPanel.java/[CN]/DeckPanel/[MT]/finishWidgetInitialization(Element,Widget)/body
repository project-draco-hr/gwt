{
  UIObject.setVisible(container,false);
  container.getStyle().setProperty("height","100%");
  Element element=w.getElement();
  if (element.getStyle().getProperty("width").equals("")) {
    w.setWidth("100%");
  }
  if (element.getStyle().getProperty("height").equals("")) {
    w.setHeight("100%");
  }
  w.setVisible(false);
}
