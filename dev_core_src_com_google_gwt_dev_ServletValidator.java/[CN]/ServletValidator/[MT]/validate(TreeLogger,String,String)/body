{
  if (containsServletClass(servletClass)) {
    if (!containsServletMapping(servletClass,servletPath)) {
      String servletName=getNameForClass(servletClass);
      assert(servletName != null);
      logger.log(TreeLogger.WARN,generateMissingMappingMessage(servletClass,servletPath,servletName));
    }
  }
 else {
    logger.log(TreeLogger.WARN,generateMissingServletMessage(servletClass,servletPath));
  }
}
