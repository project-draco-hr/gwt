{
  if (!isRemoveModifiable())   return;
  resetEmpty();
  Map m2=makeFullMap();
  map.putAll(m2);
  confirmed.putAll(m2);
  verify();
  resetEmpty();
  m2=new HashMap();
  Object[] keys=getSampleKeys();
  Object[] values=getSampleValues();
  for (int i=0; i < keys.length; i++) {
    m2.put(keys[i],values[i]);
  }
  map.putAll(m2);
  confirmed.putAll(m2);
  verify();
}
