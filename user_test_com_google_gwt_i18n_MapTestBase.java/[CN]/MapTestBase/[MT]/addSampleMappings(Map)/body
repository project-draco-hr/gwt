{
  Object[] keys=getSampleKeys();
  Object[] values=getSampleValues();
  for (int i=0; i < keys.length; i++) {
    try {
      m.put(keys[i],values[i]);
    }
 catch (    NullPointerException exception) {
      assertTrue("NullPointerException only allowed to be thrown " + "if either the key or value is null.",keys[i] == null || values[i] == null);
      assertTrue("NullPointerException on null key, but " + "useNullKey is not overridden to return false.",keys[i] == null || !useNullKey());
      assertTrue("NullPointerException on null value, but " + "useNullValue is not overridden to return false.",values[i] == null || !useNullValue());
      assertTrue("Unknown reason for NullPointer.",false);
    }
  }
  assertEquals("size must reflect number of mappings added.",keys.length,m.size());
}
