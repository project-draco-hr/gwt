{
  String typeName=statement.replace(" />","").replace("<","").replace(">","").replace("\n","");
  statementRangesBuilder.addStartPosition(sb.length());
  LinkedHashMap<Range,SourceInfo> sourceInfosByRange=Maps.<Range,SourceInfo>newLinkedHashMap();
  sourceInfosByRange.put(new Range(0,statement.length(),lines,0,lines + 1,statement.length()),SourceOrigin.create(0,statement.length(),0,typeName));
  jsSourceMapBuilder.append(new JsSourceMap(sourceInfosByRange,statement.length(),1));
  sb.append(statement);
  statementRangesBuilder.addEndPosition(sb.length());
  lines++;
}
