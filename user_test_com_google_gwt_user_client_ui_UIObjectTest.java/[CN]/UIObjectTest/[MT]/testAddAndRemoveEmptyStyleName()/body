{
  MyObject o=new MyObject();
  o.setStyleName("base");
  try {
    o.addStyleName("");
    fail();
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    o.addStyleName(" ");
    fail();
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    o.removeStyleName("");
    fail();
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    o.removeStyleName(" ");
    fail();
  }
 catch (  IllegalArgumentException e) {
  }
  assertEquals("base",o.getStyleName());
}
