{
  try {
    Widget w=new Widget(){
    }
;
    w.getElement();
    fail("Expected assertion failure");
  }
 catch (  AssertionError e) {
    assertEquals(UIObject.MISSING_ELEMENT_ERROR,e.getMessage());
  }
  try {
    Composite c=new Composite(){
    }
;
    c.getElement();
    fail("Expected assertion failure");
  }
 catch (  AssertionError e) {
    assertEquals(UIObject.MISSING_ELEMENT_ERROR,e.getMessage());
  }
}
