{
  MyObject o=new MyObject();
  o.setStylePrimaryName("primaryStyle");
  assertEquals("primaryStyle",o.getStyleName());
  doDependentAndSecondaryStyleTest(o,true);
  assertEquals("primaryStyle",o.getStyleName());
  doDependentAndSecondaryStyleTest(o,false);
  assertEquals("primaryStyle",o.getStyleName());
}
