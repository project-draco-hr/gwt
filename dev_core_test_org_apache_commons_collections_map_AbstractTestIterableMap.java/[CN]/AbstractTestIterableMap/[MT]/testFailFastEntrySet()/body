{
  if (isRemoveSupported() == false)   return;
  resetFull();
  Iterator it=map.entrySet().iterator();
  Map.Entry val=(Map.Entry)it.next();
  map.remove(val.getKey());
  try {
    it.next();
    fail();
  }
 catch (  ConcurrentModificationException ex) {
  }
  resetFull();
  it=map.entrySet().iterator();
  it.next();
  map.clear();
  try {
    it.next();
    fail();
  }
 catch (  ConcurrentModificationException ex) {
  }
}
