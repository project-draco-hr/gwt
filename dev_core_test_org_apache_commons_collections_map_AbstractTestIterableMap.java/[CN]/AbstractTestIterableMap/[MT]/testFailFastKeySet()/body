{
  if (isRemoveSupported() == false)   return;
  resetFull();
  Iterator it=map.keySet().iterator();
  Object val=it.next();
  map.remove(val);
  try {
    it.next();
    fail();
  }
 catch (  ConcurrentModificationException ex) {
  }
  resetFull();
  it=map.keySet().iterator();
  it.next();
  map.clear();
  try {
    it.next();
    fail();
  }
 catch (  ConcurrentModificationException ex) {
  }
}
