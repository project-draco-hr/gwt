{
  boolean first=true;
  for (  Object messageId : exceptions.keySet()) {
    if (exceptions.get(messageId) != selected) {
      continue;
    }
    if (first) {
      first=false;
      sb.append(selected ? '+' : '-');
      sb.append("msg(s) ");
    }
    sb.append(messageId);
    sb.append(' ');
  }
}
