{
  boolean first=true;
  for (  Message message : exceptions.keySet()) {
    if (exceptions.get(message) != selected) {
      continue;
    }
    if (first) {
      first=false;
      sb.append(selected ? '+' : '-');
      sb.append("msg(s) ");
    }
    sb.append(message.id);
    sb.append(' ');
  }
}
