{
  if (plusRowExceptions.contains(index)) {
    return true;
  }
  if (minusRowExceptions.contains(index)) {
    return false;
  }
  int id=object.id;
  if (plusIdExceptions.contains(id)) {
    return true;
  }
  if (minusIdExceptions.contains(id)) {
    return false;
  }
  return isDefaultSelected(object);
}
