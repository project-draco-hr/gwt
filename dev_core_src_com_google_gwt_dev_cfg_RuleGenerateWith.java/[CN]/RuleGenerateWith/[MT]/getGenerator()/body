{
  if (generator == null) {
    try {
      generator=generatorClass.newInstance();
    }
 catch (    InstantiationException e) {
      throw new InternalCompilerException("Could not instantiate generator " + generatorClass.getSimpleName());
    }
catch (    IllegalAccessException e) {
      throw new InternalCompilerException("Could not instantiate generator " + generatorClass.getSimpleName());
    }
  }
  return generator;
}
