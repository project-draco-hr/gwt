{
  for (  PackageDescriptor childPackage : packages) {
    if (childPackage.getName().equals(packageName)) {
      return childPackage;
    }
  }
  PackageDescriptor childPackage=new PackageDescriptor(packageName,longName.length() > 0 ? longName + "." + packageName : packageName);
  this.addPackage(childPackage);
  return childPackage;
}
