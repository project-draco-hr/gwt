{
  if (!initializing) {
    return;
  }
  initializing=false;
  isReady=true;
  super.onElementInitialized();
  if (beforeInitPlaceholder != null) {
    setHTMLImpl(DOM.getInnerHTML(beforeInitPlaceholder));
    beforeInitPlaceholder=null;
  }
  if (isPendingFocus) {
    isPendingFocus=false;
    setFocus(true);
  }
}
