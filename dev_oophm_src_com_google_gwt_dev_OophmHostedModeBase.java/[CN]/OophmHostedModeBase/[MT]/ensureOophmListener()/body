{
  if (listener == null) {
    codeServerPort=options.getPortHosted();
    listener=new BrowserListener(getTopLogger(),codeServerPort,new OophmSessionHandler(browserHost));
    listener.start();
    try {
      codeServerPort=listener.getSocketPort();
    }
 catch (    UnableToCompleteException e) {
    }
  }
}
