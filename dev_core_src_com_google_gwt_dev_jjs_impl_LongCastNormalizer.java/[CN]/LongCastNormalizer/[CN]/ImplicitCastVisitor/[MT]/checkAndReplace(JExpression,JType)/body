{
  JType argType=arg.getType();
  if (targetType == argType) {
    return arg;
  }
  if (targetType != longType && argType != longType) {
    return arg;
  }
  if (arg instanceof JValueLiteral) {
    JValueLiteral coerced=longType.coerceLiteral((JValueLiteral)arg);
    if (coerced != null) {
      return coerced;
    }
  }
  JCastOperation cast=new JCastOperation(program,arg.getSourceInfo(),targetType,arg);
  return cast;
}
