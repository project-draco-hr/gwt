{
  Value val=new Value();
  val.setBoolean(true);
  BrowserChannel.writeValue(oStr,val);
  val=BrowserChannel.readValue(iStr);
  assertEquals(ValueType.BOOLEAN,val.getType());
  assertEquals(true,val.getBoolean());
  val.setBoolean(false);
  BrowserChannel.writeValue(oStr,val);
  val=BrowserChannel.readValue(iStr);
  assertEquals(ValueType.BOOLEAN,val.getType());
  assertEquals(false,val.getBoolean());
}
