{
  StringBuilder out=new StringBuilder();
  out.append("SoftProps(");
  for (int i=0, j=orderedProperties.length; i < j; i++) {
    if (out.length() > 0) {
      out.append(" ");
    }
    out.append(orderedProperties[i].getName());
    out.append(" = ");
    out.append(orderedPropertyValues[i]);
  }
  out.append(")");
  return out.toString();
}
