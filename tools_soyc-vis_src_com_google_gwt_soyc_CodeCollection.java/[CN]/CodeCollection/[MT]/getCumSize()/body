{
  cumSize=0;
  for (  String className : classes) {
    if (!GlobalInformation.classToSize.containsKey(className)) {
      System.err.println("*** NO SIZE FOUND FOR CLASS " + className + " *****");
    }
 else {
      cumSize+=GlobalInformation.classToSize.get(className);
    }
  }
  return cumSize;
}
