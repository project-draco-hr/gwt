{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  Slot<E,V> other=(Slot<E,V>)obj;
  if (!entity.getId().equals(other.entity.getId())) {
    return false;
  }
  if (!property.equals(other.property)) {
    return false;
  }
  return true;
}
