{
  scrollHeight=0;
  if (!opening) {
    scrollHeight=curItem.childSpanElem.getScrollHeight();
  }
  DOM.setStyleAttribute(curItem.childSpanElem,"overflow","hidden");
  super.onStart();
  if (opening) {
    UIObject.setVisible(curItem.childSpanElem,true);
    scrollHeight=curItem.childSpanElem.getScrollHeight();
  }
}
