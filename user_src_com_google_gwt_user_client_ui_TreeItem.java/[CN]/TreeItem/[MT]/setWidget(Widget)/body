{
  if (newWidget != null) {
    newWidget.removeFromParent();
  }
  if (widget != null && tree != null) {
    tree.orphan(widget);
  }
  DOM.setInnerHTML(contentElem,"");
  widget=newWidget;
  if (newWidget != null) {
    DOM.appendChild(contentElem,newWidget.getElement());
    if (tree != null) {
      tree.adopt(widget,this);
    }
  }
}
