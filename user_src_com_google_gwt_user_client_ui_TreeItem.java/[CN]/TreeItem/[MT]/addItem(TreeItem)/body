{
  if ((item.getParentItem() != null) || (item.getTree() != null)) {
    item.remove();
  }
  item.setTree(tree);
  item.setParentItem(this);
  children.add(item);
  DOM.setStyleAttribute(item.getElement(),"marginLeft","16px");
  DOM.appendChild(childSpanElem,item.getElement());
  if (children.size() == 1) {
    updateState();
  }
}
