{
  String[] bits=className.split("\\-");
  StringBuilder b=new StringBuilder();
  for (  String bit : bits) {
    if (b.length() == 0) {
      b.append(bit);
    }
 else {
      b.append(bit.substring(0,1).toUpperCase(Locale.ROOT));
      if (bit.length() > 1) {
        b.append(bit.substring(1));
      }
    }
  }
  String converted=b.toString();
  return converted;
}
