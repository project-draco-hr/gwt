{
  tokenList.clear();
  tokenListIndex=0;
  stringTable=null;
  int idx=0, nextIdx;
  while (-1 != (nextIdx=encodedTokens.indexOf('\uffff',idx))) {
    String current=encodedTokens.substring(idx,nextIdx);
    tokenList.add(current);
    idx=nextIdx + 1;
  }
  super.prepareToRead(encodedTokens);
  deserializeStringTable();
}
