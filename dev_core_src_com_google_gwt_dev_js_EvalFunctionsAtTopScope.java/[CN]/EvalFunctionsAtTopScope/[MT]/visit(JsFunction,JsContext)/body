{
  JsStaticEval.isFunctionDecl(currentStatement);
  if (x.getName() != null && !dontMove.contains(x) && !isVtableDeclaration(currentStatement)) {
    ListIterator<JsStatement> itr=itrStack.peek();
    itr.previous();
    itr.add(x.makeStmt());
    itr.next();
    ctx.replaceMe(x.getName().makeRef(x.getSourceInfo().makeChild(EvalFunctionsAtTopScope.class,"Shuffled evaluation order")));
  }
  scopeStack.push(x.getBody());
  return true;
}
