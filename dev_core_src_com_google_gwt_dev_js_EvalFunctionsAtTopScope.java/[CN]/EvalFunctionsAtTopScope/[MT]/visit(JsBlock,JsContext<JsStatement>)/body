{
  if (x == scopeStack.peek()) {
    ListIterator<JsStatement> itr=x.getStatements().listIterator();
    itrStack.push(itr);
    while (itr.hasNext()) {
      JsStatement stmt=itr.next();
      JsFunction func=JsStaticEval.isFunctionDecl(stmt);
      if (func != null) {
        dontMove.add(func);
      }
      accept(stmt);
      if (func != null) {
        dontMove.remove(func);
      }
    }
    itrStack.pop();
    return false;
  }
 else {
    return true;
  }
}
