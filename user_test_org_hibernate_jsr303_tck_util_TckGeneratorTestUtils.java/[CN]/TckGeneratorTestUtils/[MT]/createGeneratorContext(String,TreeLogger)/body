{
  ModuleDef module=ModuleDefLoader.loadFromClassPath(logger,moduleName);
  File genDir=new File(System.getProperty("java.io.tmpdir"));
  ArtifactSet allGenreatedArtifacts=new ArtifactSet();
  boolean isProd=false;
  StandardGeneratorContext context=new StandardGeneratorContext(module.getCompilationState(logger),module,genDir,allGenreatedArtifacts,isProd);
  return context;
}
