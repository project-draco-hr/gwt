{
  error.setText("");
  if (!"".equals(toParse)) {
    try {
      Date x=new Date(toParse);
      String s=activeFormat.format(x);
      output.setText(s);
    }
 catch (    IllegalArgumentException e) {
      String errMsg=constants.failedToParseInput();
      error.setText(errMsg);
    }
  }
 else {
    output.setText("<None>");
  }
}
