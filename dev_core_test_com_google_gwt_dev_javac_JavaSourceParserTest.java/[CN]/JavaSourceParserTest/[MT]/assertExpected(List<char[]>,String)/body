{
  if (actual.size() != expected.length) {
    fail("Expected " + Arrays.deepToString(expected) + ", got "+ actual);
  }
  for (int i=0; i < expected.length; ++i) {
    assertTrue("index " + i + " should be "+ expected[i]+ ", got "+ Arrays.toString(actual.get(i)),Arrays.equals(actual.get(i),expected[i].toCharArray()));
  }
}
