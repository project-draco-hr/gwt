{
  currentStatementCount++;
  if (doSplits && currentStatementCount == MAX_BLOCK_SIZE) {
    newJs.append('}');
    super.endStatement(index,newJs,ends);
    currentStatementCount=0;
    statementsAddedBlockClose.add(Integer.valueOf(index));
  }
 else   if (!doSplits) {
    super.endStatement(index,newJs,ends);
  }
}
