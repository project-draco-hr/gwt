{
  if (supportsAsync()) {
    if (testIsFinished) {
      return;
    }
    if (timer == null) {
      throw new IllegalStateException("This test is not in asynchronous mode; call delayTestFinish() first");
    }
    if (mainTestHasRun) {
      reportResultsAndRunNextMethod(null);
    }
 else {
      resetAsyncState();
    }
  }
 else {
    throw new UnsupportedOperationException("This test case does not support asynchronous mode.");
  }
}
