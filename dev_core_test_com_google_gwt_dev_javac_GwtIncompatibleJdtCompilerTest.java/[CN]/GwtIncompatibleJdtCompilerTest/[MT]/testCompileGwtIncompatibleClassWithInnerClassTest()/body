{
  List<CompilationUnit> units=compile(GWTINCOMPATIBLE_ANNOTATION,GWTINCOMPATIBLE_WITH_INNERCLASS,GWTINCOMPATIBLE_WITH_INNERCLASS_TEST);
  assertOnlyLastUnitHasErrors(units,"The constructor GwtIncompatibleWithInnerClass() is not visible","GwtIncompatibleWithInnerClass.Child cannot be resolved to a type");
}
