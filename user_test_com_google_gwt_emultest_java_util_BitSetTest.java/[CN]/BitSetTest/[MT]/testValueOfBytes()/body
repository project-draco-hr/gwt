{
  BitSet set=BitSet.valueOf(new byte[0]);
  assertTrue(set.isEmpty());
  set=BitSet.valueOf(new byte[]{(byte)-128,(byte)-1,(byte)0,(byte)31,(byte)127,(byte)128,(byte)250});
  assertEquals("{7, 8, 9, 10, 11, 12, 13, 14, 15, 24, 25, 26, 27, 28, 32, 33, 34, 35, 36, 37, 38, " + "47, 49, 51, 52, 53, 54, 55}",set.toString());
}
