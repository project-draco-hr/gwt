{
  Images images=getImages();
  MockCell<String> innerCell=new MockCell<String>(true,"newValueFromInnerCell","click");
  IconCellDecorator<String> cell=new IconCellDecorator<String>(images.prettyPiccy(),innerCell){
    @Override protected boolean isIconUsed(    String value){
      return false;
    }
  }
;
  StringBuilder sb=new StringBuilder();
  cell.render("helloworld",null,sb);
  String expected="<div style='position:relative;padding-left:64px;'>";
  expected+=cell.getImageHtml(images.prettyPiccy(),HasVerticalAlignment.ALIGN_MIDDLE,true);
  expected+="<div>helloworld</div>";
  expected+="</div>";
  assertEquals(expected,sb.toString());
}
