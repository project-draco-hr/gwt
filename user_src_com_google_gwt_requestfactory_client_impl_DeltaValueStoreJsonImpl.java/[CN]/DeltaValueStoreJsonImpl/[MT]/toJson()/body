{
  StringBuffer jsonData=new StringBuffer("{");
  for (  WriteOperation writeOperation : new WriteOperation[]{WriteOperation.CREATE,WriteOperation.UPDATE}) {
    String jsonDataForOperation=getJsonForOperation(writeOperation);
    if (jsonDataForOperation.equals("")) {
      continue;
    }
    if (jsonData.length() > 1) {
      jsonData.append(",");
    }
    jsonData.append(jsonDataForOperation);
  }
  jsonData.append("}");
  return jsonData.toString();
}
