{
  File failedFile=null;
  try {
    failedFile=pendingFile;
    File thisFile=pendingFile.getCanonicalFile();
    failedFile=pendingFile;
    File thatFile=other.getCanonicalFile();
    if (thisFile.equals(thatFile)) {
      return true;
    }
 else {
      return false;
    }
  }
 catch (  IOException e) {
    logger.log(TreeLogger.ERROR,"Unable to determine canonical path of pending resource '" + failedFile.toString() + "'",e);
  }
  return false;
}
