{
  logger=logger.branch(TreeLogger.TRACE,"Writing generated resource '" + pendingFile.getAbsolutePath() + "'",null);
  if (pendingFile.exists()) {
    logger.log(TreeLogger.ERROR,"The destination file already exists; aborting the commit",null);
    throw new UnableToCompleteException();
  }
  Util.writeBytesToFile(logger,pendingFile,baos.toByteArray());
}
