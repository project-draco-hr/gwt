{
  if (pendingResourcesByOutputStream.isEmpty()) {
    return;
  }
  logger=logger.branch(TreeLogger.WARN,"The following resources will not be created because they were never committed (did you forget to call commit()?)",null);
  try {
    for (Iterator iter=pendingResourcesByOutputStream.values().iterator(); iter.hasNext(); ) {
      PendingResource pendingResource=(PendingResource)iter.next();
      logger.log(TreeLogger.WARN,pendingResource.getFile().getAbsolutePath(),null);
    }
  }
  finally {
    pendingResourcesByOutputStream.clear();
  }
}
