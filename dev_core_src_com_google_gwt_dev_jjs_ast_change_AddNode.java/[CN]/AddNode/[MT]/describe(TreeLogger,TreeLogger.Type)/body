{
  if (index < 0) {
    logger.log(type,"Add " + ChangeList.getNodeString(node) + ChangeList.getEnclosingTypeString(" to the end of",node),null);
  }
 else {
    logger.log(type,"Add " + ChangeList.getNodeString(node) + ChangeList.getEnclosingTypeString(" to",node)+ " at index "+ index,null);
  }
}
