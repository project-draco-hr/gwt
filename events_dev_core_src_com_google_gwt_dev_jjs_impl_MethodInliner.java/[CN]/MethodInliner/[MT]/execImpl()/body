{
  boolean madeChanges=false;
  while (true) {
    InliningVisitor inliner=new InliningVisitor();
    inliner.accept(program);
    if (!inliner.didChange()) {
      break;
    }
    madeChanges=true;
    for (    JMethod method : inliner.modifiedMethods) {
      DeadCodeElimination.exec(program,method);
    }
  }
  return madeChanges;
}
