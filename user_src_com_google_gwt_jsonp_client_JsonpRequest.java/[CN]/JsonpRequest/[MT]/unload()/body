{
  Scheduler.get().scheduleDeferred(new ScheduledCommand(){
    public void execute(){
      if (!canHaveMultipleRequestsForSameId) {
        unregisterCallbacks(CALLBACKS);
      }
      Node script=Document.get().getElementById(callbackId);
      if (script != null) {
        getHeadElement().removeChild(script);
      }
    }
  }
);
}
