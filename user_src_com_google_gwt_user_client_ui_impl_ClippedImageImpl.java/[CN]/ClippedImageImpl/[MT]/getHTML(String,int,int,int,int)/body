{
  String style="width: " + width + "px; height: "+ height+ "px; background: url("+ url+ ") no-repeat "+ (-left + "px ")+ (-top + "px");
  String clippedImgHtml="<img " + "onload='this.__gwtLastUnhandledEvent=\"load\";' src='" + GWT.getModuleBaseURL() + "clear.cache.gif' style='"+ style+ "' border='0'>";
  return clippedImgHtml;
}
