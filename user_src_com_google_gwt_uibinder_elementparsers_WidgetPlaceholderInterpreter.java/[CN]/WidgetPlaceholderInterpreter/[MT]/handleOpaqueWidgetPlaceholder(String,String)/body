{
  if (uiWriter.useSafeHtmlTemplates()) {
    idHolder=uiWriter.tokenForStringExpression(idHolder);
  }
 else {
    idHolder="\" + " + idHolder + " + \"";
  }
  String tag=String.format("<span id='%s'></span>",idHolder);
  String placeholder=nextPlaceholder(name,"<span></span>",tag);
  return placeholder;
}
