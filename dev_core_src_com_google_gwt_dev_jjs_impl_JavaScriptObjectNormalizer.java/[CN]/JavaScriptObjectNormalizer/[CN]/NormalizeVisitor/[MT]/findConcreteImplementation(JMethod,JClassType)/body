{
  while (concreteType != null) {
    for (    JMethod m : concreteType.getMethods()) {
      if (program.typeOracle.getAllOverrides(m).contains(method)) {
        if (!m.isAbstract()) {
          return m;
        }
      }
    }
    concreteType=concreteType.getSuperClass();
  }
  return null;
}
