{
  if (instance.hasSideEffects()) {
    SourceInfo info=instance.getSourceInfo().makeChild(JavaScriptObjectNormalizer.class,"Temporary assignment for instance with side-effects");
    JLocal local=JProgram.createLocal(info,"maybeJsoInvocation",instance.getType(),true,currentMethodBody.peek());
    multi.exprs.add(JProgram.createAssignmentStmt(info,new JLocalRef(info,local),instance).getExpr());
    instance=new JLocalRef(info,local);
  }
  return instance;
}
