{
  assertEquals(0f,Float.parseFloat("0"),0.0);
  assertEquals(-1.5f,Float.parseFloat("-1.5"),0.0);
  assertEquals(3.0f,Float.parseFloat("3."),0.0);
  assertEquals(0.5f,Float.parseFloat(".5"),0.0);
  assertEquals("Can't parse MAX_VALUE",Float.MAX_VALUE,Float.parseFloat(String.valueOf(Float.MAX_VALUE)),1e31);
  assertEquals("Can't parse MIN_VALUE",Float.MIN_VALUE,Float.parseFloat(String.valueOf(Float.MIN_VALUE)),Float.MIN_VALUE);
}
