{
  Object s=getObjectAux(key,true);
  if (s == null) {
    String msg="Cannot find '" + key + "' in "+ this;
    Set<String> keys=this.keySet();
    if (keys.size() < REPORT_KEYS_THRESHOLD) {
      msg=msg + ", keys found:\n\t" + keys;
    }
    throw new MissingResourceException(msg,key,key);
  }
  return s;
}
