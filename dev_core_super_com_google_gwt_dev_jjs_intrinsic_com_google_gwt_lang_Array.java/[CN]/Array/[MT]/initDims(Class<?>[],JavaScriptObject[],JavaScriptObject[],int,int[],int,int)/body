{
  int length=dimExprs[index];
  boolean isLastDim=(index == (count - 1));
  int elementTypeClass=isLastDim ? leafElementTypeClass : TYPE_JAVA_OBJECT;
  Array result=initializeArrayElementsWithDefaults(elementTypeClass,length);
  initValues(arrayClasses[index],castableTypeMapExprs[index],elementTypeIds[index],elementTypeClass,result);
  if (!isLastDim) {
    ++index;
    for (int i=0; i < length; ++i) {
      set(result,i,initDims(arrayClasses,castableTypeMapExprs,elementTypeIds,leafElementTypeClass,dimExprs,index,count));
    }
  }
  return result;
}
