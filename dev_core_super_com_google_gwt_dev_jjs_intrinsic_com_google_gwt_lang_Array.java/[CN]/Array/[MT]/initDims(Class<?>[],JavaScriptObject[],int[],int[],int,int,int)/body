{
  int length=dimExprs[index];
  boolean isLastDim=(index == (count - 1));
  Array result=createFromSeed(isLastDim ? seedType : NULL_SEED_TYPE,length);
  initValues(arrayClasses[index],castableTypeMapExprs[index],queryIdExprs[index],result);
  if (!isLastDim) {
    ++index;
    for (int i=0; i < length; ++i) {
      set(result,i,initDims(arrayClasses,castableTypeMapExprs,queryIdExprs,dimExprs,index,count,seedType));
    }
  }
  return result;
}
