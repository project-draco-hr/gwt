{
  assert(type != null);
  assert(subtype != null);
  return new TypePaths.TypePath(){
    public TypePaths.TypePath getParent(){
      return parent;
    }
    @Override public String toString(){
      return "'" + type.getParameterizedQualifiedSourceName() + "' is reachable as a supertype of type '"+ subtype+ "'";
    }
  }
;
}
