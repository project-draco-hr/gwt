{
  if (DOM.eventGetType(event) == Event.ONMOUSEWHEEL) {
    final Element senderElem=sender.getElement();
    int x=DOM.eventGetClientX(event) - DOM.getAbsoluteLeft(sender.getElement()) + DOM.getElementPropertyInt(senderElem,"scrollLeft") + Window.getScrollLeft();
    int y=DOM.eventGetClientY(event) - DOM.getAbsoluteTop(sender.getElement()) + DOM.getElementPropertyInt(senderElem,"scrollTop") + Window.getScrollTop();
    MouseWheelVelocity velocity=new MouseWheelVelocity(event);
    fireMouseWheel(sender,x,y,velocity);
  }
}
