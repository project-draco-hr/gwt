{
  Set<String> refs=invocation.getPropertyRefs();
  if (refs == null) {
    return Collections.emptySet();
  }
  Set<String> toReturn=new TreeSet<String>();
  for (  String raw : refs) {
    for (int idx=raw.length(); idx >= 0; idx=raw.lastIndexOf('.',idx - 1)) {
      toReturn.add(raw.substring(0,idx));
    }
  }
  return toReturn;
}
