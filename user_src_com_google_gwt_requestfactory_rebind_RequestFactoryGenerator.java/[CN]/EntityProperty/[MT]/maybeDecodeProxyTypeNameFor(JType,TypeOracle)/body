{
  JClassType cType=decodeToBaseType(proxyParam);
  if (cType != null && entityProxyType.isAssignableFrom(cType)) {
    ProxyFor pFor=proxyParam.isClassOrInterface().getAnnotation(ProxyFor.class);
    diagnosticIf(pFor == null,"Missing @ProxyFor annotation on type %s",proxyParam.getQualifiedBinaryName());
    if (pFor != null) {
      return pFor.value().getName();
    }
  }
  return proxyParam.getQualifiedBinaryName();
}
