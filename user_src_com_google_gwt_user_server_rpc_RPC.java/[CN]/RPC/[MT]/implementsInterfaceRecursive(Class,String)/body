{
  assert(clazz.isInterface());
  if (clazz.getName().equals(intfName)) {
    return true;
  }
  Class[] intfs=clazz.getInterfaces();
  for (int i=0; i < intfs.length; i++) {
    Class intf=intfs[i];
    if (implementsInterfaceRecursive(intf,intfName)) {
      return true;
    }
  }
  return false;
}
