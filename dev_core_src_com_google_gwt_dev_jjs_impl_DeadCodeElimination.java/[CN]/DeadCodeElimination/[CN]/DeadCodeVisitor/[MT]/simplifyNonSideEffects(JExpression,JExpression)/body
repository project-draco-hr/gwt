{
  List<JExpression> expressionsWithSideEffects=Lists.newArrayList(Iterables.filter(Arrays.asList(evaluateIfSideEffects),new Predicate<JExpression>(){
    @Override public boolean apply(    JExpression expression){
      return expression.hasSideEffects();
    }
  }
));
  if (expressionsWithSideEffects.isEmpty()) {
    return result;
  }
  expressionsWithSideEffects.add(result);
  return new JMultiExpression(expressionsWithSideEffects.get(0).getSourceInfo(),expressionsWithSideEffects);
}
