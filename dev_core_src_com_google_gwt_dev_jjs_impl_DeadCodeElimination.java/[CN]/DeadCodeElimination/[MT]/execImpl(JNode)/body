{
  Event optimizeEvent=SpeedTracerLogger.start(CompilerEventType.OPTIMIZE,"optimizer","DeadCodeElimination");
  boolean didChange=false;
  while (true) {
    DeadCodeVisitor deadCodeVisitor=new DeadCodeVisitor();
    deadCodeVisitor.accept(node);
    if (!deadCodeVisitor.didChange()) {
      break;
    }
    didChange=true;
  }
  optimizeEvent.end("didChange","" + didChange);
  return didChange;
}
