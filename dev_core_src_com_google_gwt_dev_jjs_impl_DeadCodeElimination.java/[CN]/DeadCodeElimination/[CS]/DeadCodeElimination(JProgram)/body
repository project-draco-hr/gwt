{
  this.program=program;
  typeClassMap=new ImmutableMap.Builder().put(program.getTypeJavaLangObject(),Object.class).put(program.getTypeJavaLangString(),String.class).put(program.getTypePrimitiveBoolean(),boolean.class).put(program.getTypePrimitiveByte(),byte.class).put(program.getTypePrimitiveChar(),char.class).put(program.getTypePrimitiveDouble(),double.class).put(program.getTypePrimitiveFloat(),float.class).put(program.getTypePrimitiveInt(),int.class).put(program.getTypePrimitiveLong(),long.class).put(program.getTypePrimitiveShort(),short.class).build();
}
