{
  ArrayList fieldList=new ArrayList();
  for (int index=0; index < fields.length; ++index) {
    Field field=fields[index];
    assert(field != null);
    int fieldModifiers=field.getModifiers();
    if (Modifier.isStatic(fieldModifiers) || Modifier.isTransient(fieldModifiers) || Modifier.isFinal(fieldModifiers)) {
      continue;
    }
    fieldList.add(field);
  }
  Field[] fieldSubset=(Field[])fieldList.toArray(new Field[fieldList.size()]);
  Comparator comparator=new Comparator(){
    public int compare(    Object o1,    Object o2){
      Field f1=(Field)o1;
      Field f2=(Field)o2;
      return f1.getName().compareTo(f2.getName());
    }
  }
;
  Arrays.sort(fieldSubset,0,fieldSubset.length,comparator);
  return fieldSubset;
}
