{
  if (value == null) {
    return;
  }
  sb.append("<table>");
  sb.append("<tr><td rowspan='3'>");
  sb.append(imageHtml);
  sb.append("</td>");
  sb.append("<td style='font-size:95%;'>");
  sb.append(value.getFullName());
  sb.append("</td>");
  sb.append("</tr>");
  sb.append("<tr><td>");
  sb.append(value.getAddress());
  sb.append("</td></tr>");
  sb.append("</table>");
}
