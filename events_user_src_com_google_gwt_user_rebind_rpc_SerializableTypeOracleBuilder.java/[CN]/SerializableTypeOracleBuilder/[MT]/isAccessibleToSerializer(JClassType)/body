{
  if (type.isPrivate() || type.isLocalType()) {
    return false;
  }
  if (isInStandardJavaPackage(type.getQualifiedSourceName())) {
    if (!type.isPublic()) {
      return false;
    }
  }
  if (type.isMemberType()) {
    return isAccessibleToSerializer(type.getEnclosingType());
  }
  return true;
}
