{
  try {
    libraryGroup=LibraryGroup.fromZipPaths(compilerOptions.getLibraryPaths());
  }
 catch (  IncompatibleLibraryVersionException e) {
    logger.log(TreeLogger.ERROR,e.getMessage());
    throw new UnableToCompleteException();
  }
  compilerContext=compilerContextBuilder.libraryGroup(libraryGroup).libraryWriter(new ZipLibraryWriter(compilerOptions.getOutputLibraryPath())).unitCache(new LibraryGroupUnitCache(libraryGroup)).build();
}
