{
  long beforeLoadModuleMs=System.currentTimeMillis();
  module=ModuleDefLoader.loadFromResources(logger,compilerContext,compilerOptions.getModuleNames().get(0),resourceLoader,false);
  compilerContext=compilerContextBuilder.module(module).build();
  long durationMs=System.currentTimeMillis() - beforeLoadModuleMs;
  logger.log(TreeLogger.INFO,String.format("%.3fs -- Parsing and loading module definition",durationMs / 1000d));
}
