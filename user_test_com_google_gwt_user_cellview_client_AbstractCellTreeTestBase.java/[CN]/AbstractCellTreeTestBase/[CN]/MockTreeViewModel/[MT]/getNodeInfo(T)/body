{
  if (value == ROOT_VALUE) {
    return new DefaultNodeInfo<String>(rootDataProvider,cell,selectionModels.get(0),null);
  }
 else   if (value instanceof String) {
    String prefix=(String)value;
    int depth=prefix.length();
    if (depth >= MAX_DEPTH) {
      throw new IllegalStateException("Prefix should never exceed " + MAX_DEPTH + " characters.");
    }
    return new DefaultNodeInfo<String>(createDataProvider(prefix),cell,selectionModels.get(depth),null);
  }
  throw new IllegalArgumentException("Unrecognized value type");
}
