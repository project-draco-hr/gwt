{
  EntityManager em=entityManager();
  try {
    Query query=em.createQuery("select o from Report o where o.reporterKey =:reporterKey");
    query.setParameter("reporterKey",employeeId);
    List<Report> reportList=query.getResultList();
    reportList.size();
    return reportList;
  }
  finally {
    em.close();
  }
}
