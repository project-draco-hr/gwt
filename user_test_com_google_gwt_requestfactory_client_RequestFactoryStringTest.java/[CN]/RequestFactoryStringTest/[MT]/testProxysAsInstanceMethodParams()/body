{
  delayTestFinish(5000);
  req.simpleFooStringRequest().findSimpleFooStringById("999x").fire(new Receiver<SimpleFooStringProxy>(){
    @Override public void onSuccess(    SimpleFooStringProxy response){
      SimpleBarProxy bar=req.create(SimpleBarProxy.class);
      RequestObject<String> helloReq=req.simpleFooStringRequest().hello(response,bar);
      bar=helloReq.edit(bar);
      bar.setUserName("BAR");
      helloReq.fire(new Receiver<String>(){
        @Override public void onSuccess(        String response){
          assertEquals("Greetings BAR from GWT",response);
          finishTestAndReset();
        }
      }
);
    }
  }
);
}
