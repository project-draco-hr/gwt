{
  delayTestFinish(5000);
  req.simpleBarRequest().findSimpleBarById("999L").fire(new Receiver<SimpleBarProxy>(){
    @Override public void onSuccess(    final SimpleBarProxy barProxy){
      req.simpleFooStringRequest().findSimpleFooStringById("999x").fire(new Receiver<SimpleFooStringProxy>(){
        @Override public void onSuccess(        SimpleFooStringProxy fooProxy){
          SimpleFooStringRequest context=req.simpleFooStringRequest();
          Request<Void> updReq=context.persist().using(fooProxy);
          fooProxy=context.edit(fooProxy);
          fooProxy.setBarField(barProxy);
          updReq.fire(new Receiver<Void>(){
            @Override public void onSuccess(            Void response){
              finishTestAndReset();
            }
          }
);
        }
      }
);
    }
  }
);
}
