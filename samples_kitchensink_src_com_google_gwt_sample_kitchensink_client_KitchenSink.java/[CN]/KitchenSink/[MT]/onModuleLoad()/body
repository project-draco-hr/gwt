{
  loadSinks();
  panel.add(list);
  panel.add(description);
  panel.setWidth("100%");
  description.setStyleName("ks-Info");
  History.addHistoryListener(this);
  RootPanel.get().add(panel);
  String initToken=History.getToken();
  if (initToken.length() > 0) {
    onHistoryChanged(initToken);
  }
 else {
    showInfo();
  }
}
