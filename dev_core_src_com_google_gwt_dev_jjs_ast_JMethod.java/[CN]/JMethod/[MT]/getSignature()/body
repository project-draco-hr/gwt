{
  if (signature == null) {
    StringBuilder sb=new StringBuilder();
    sb.append(getName());
    sb.append('(');
    for (    JType type : getOriginalParamTypes()) {
      sb.append(type.getJsniSignatureName());
    }
    sb.append(')');
    if (!isConstructor()) {
      sb.append(getOriginalReturnType().getJsniSignatureName());
    }
 else {
      sb.append(" <init>");
    }
    signature=sb.toString();
  }
  return signature;
}
