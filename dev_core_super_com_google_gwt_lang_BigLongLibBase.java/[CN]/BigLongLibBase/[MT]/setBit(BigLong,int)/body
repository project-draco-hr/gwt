{
  if (LongLib.RUN_IN_JVM) {
    if (bit < BITS) {
      a.l|=0x1 << bit;
    }
 else     if (bit < BITS01) {
      a.m|=0x1 << (bit - BITS);
    }
 else {
      a.h|=0x1 << (bit - BITS01);
    }
  }
 else {
    if (bit < BITS) {
      setBitL(a,bit);
    }
 else     if (bit < BITS01) {
      setBitM(a,bit - BITS);
    }
 else {
      setBitH(a,bit - BITS01);
    }
  }
}
