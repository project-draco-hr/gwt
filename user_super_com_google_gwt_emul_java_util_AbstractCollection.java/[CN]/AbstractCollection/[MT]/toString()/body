{
  StringBuffer sb=new StringBuffer();
  String comma=null;
  sb.append("[");
  Iterator<E> iter=iterator();
  while (iter.hasNext()) {
    if (comma != null) {
      sb.append(comma);
    }
 else {
      comma=", ";
    }
    E value=iter.next();
    sb.append(value == this ? "(this Collection)" : String.valueOf(value));
  }
  sb.append("]");
  return sb.toString();
}
