{
  BufferedReader reader=new BufferedReader(new FileReader(f));
  StringBuffer source=new StringBuffer((int)f.length());
  while (true) {
    String line=reader.readLine();
    if (line == null) {
      break;
    }
    source.append(line);
    source.append("\n");
  }
  char[] buf=new char[source.length()];
  source.getChars(0,buf.length,buf,0);
  return buf;
}
