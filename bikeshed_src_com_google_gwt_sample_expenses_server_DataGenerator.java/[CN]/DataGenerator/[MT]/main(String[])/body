{
  if (args.length < 2) {
    throw new IllegalArgumentException("Requires two arguments: #Employees and json-fileName");
  }
  int numEmployees=Integer.parseInt(args[0]);
  DataGenerator generator=new DataGenerator();
  List<Object> recordList=generator.generateData(numEmployees);
  System.out.println("Generated " + recordList.size() + " records, #employees: "+ numEmployees);
  String jsonString=generator.getJsonString(recordList);
  File output=new File(args[1]);
  Util.writeStringAsFile(output,jsonString);
  System.out.println("Wrote out the data as json into file " + output.getAbsolutePath());
}
