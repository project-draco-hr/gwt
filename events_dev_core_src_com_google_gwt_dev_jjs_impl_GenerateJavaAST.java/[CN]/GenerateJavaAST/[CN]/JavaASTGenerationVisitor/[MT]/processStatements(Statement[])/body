{
  List<JStatement> jstatements=new ArrayList<JStatement>();
  if (statements != null) {
    for (int i=0, n=statements.length; i < n; ++i) {
      JStatement jstmt=dispProcessStatement(statements[i]);
      if (jstmt != null) {
        jstatements.add(jstmt);
      }
    }
  }
  return jstatements;
}
