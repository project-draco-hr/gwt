{
  assert(name != null);
  assert(enclosingType != null);
  assert(type != null);
  if (codeGenTypes.contains(enclosingType) || enclosingType == typeJavaLangObject) {
    hasInitializer=true;
  }
  String sname=String.valueOf(name);
  JField x=new JField(this,info,sname,enclosingType,type,isStatic,isFinal,hasInitializer);
  if (indexedTypes.containsValue(enclosingType)) {
    indexedFields.put(enclosingType.getShortName() + '.' + sname,x);
  }
  enclosingType.fields.add(x);
  return x;
}
