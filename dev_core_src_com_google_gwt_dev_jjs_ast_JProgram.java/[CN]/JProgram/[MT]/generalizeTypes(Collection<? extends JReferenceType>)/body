{
  assert(types != null);
  assert(!types.isEmpty());
  Iterator<? extends JReferenceType> it=types.iterator();
  JReferenceType curType=it.next();
  while (it.hasNext()) {
    curType=generalizeTypes(curType,it.next());
    if (curType == typeJavaLangObject) {
      break;
    }
  }
  return curType;
}
