{
  JStringLiteral toReturn=stringLiteralMap.get(s);
  if (toReturn == null) {
    toReturn=new JStringLiteral(stringPoolSourceInfo.makeChild(JProgram.class,"String literal: " + s),s,getTypeJavaLangString());
    stringLiteralMap.put(s,toReturn);
  }
  toReturn.getSourceInfo().merge(sourceInfo);
  return toReturn;
}
