{
  JClassLiteral leafTypeClassLiteral=new JClassLiteral(sourceInfo,leafType);
  leafTypeClassLiteral.setField(field);
  return new JMethodCall(sourceInfo,null,getIndexedMethod("Class.getClassLiteralForArray"),new JFieldRef(sourceInfo,null,field,field.getEnclosingType()),getLiteralInt(dimensions));
}
