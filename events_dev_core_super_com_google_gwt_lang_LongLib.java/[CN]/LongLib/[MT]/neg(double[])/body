{
  if (eq(a,MIN_VALUE)) {
    return MIN_VALUE;
  }
  double newHigh=-a[HIGH];
  double newLow=-a[LOW];
  if (newLow > LOW_MAX) {
    newLow-=TWO_PWR_32_DBL;
    newHigh+=TWO_PWR_32_DBL;
  }
  if (newLow < LOW_MIN) {
    newLow+=TWO_PWR_32_DBL;
    newHigh-=TWO_PWR_32_DBL;
  }
  return createNormalized(newLow,newHigh);
}
