{
  out.println("<tr><td>&nbsp;&nbsp;&nbsp;&nbsp;<a href=\"" + dependenciesFileName("total",packageName) + "#"+ className+ "\">See why it's live</a></td></tr>");
  for (int sp=1; sp <= GlobalInformation.numSplitPoints; sp++) {
    out.println("<tr><td>&nbsp;&nbsp;&nbsp;&nbsp;<a href=\"" + dependenciesFileName("sp" + sp,packageName) + "#"+ className+ "\">See why it's not exclusive to s.p. #"+ sp+ " ("+ GlobalInformation.splitPointToLocation.get(sp)+ ")</a></td></tr>");
  }
}
