{
  if (selectFirstItemIfNoneSelected()) {
    return;
  }
  if (vertical) {
    selectNextItem();
  }
 else {
    if (selectedItem.getSubMenu() != null) {
      doItemAction(selectedItem,false);
    }
 else     if (parentMenu != null) {
      if (parentMenu.vertical) {
        parentMenu.selectNextItem();
      }
 else {
        parentMenu.moveDown();
      }
    }
  }
}
