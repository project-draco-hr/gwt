{
  if (item == null) {
    if ((selectedItem != null) && (shownChildMenu == selectedItem.getSubMenu())) {
      return;
    }
  }
  selectItem(item);
  if (focus && focusOnHover) {
    focus();
  }
  if (item != null) {
    if ((shownChildMenu != null) || (parentMenu != null) || autoOpen) {
      doItemAction(item,false,focusOnHover);
    }
  }
}
