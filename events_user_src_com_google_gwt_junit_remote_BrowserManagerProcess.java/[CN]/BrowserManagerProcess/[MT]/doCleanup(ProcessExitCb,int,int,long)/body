{
synchronized (this) {
    deadOrDying=true;
  }
  if (exitValue != 0) {
    logger.warning("Browser: " + token + " exited with bad status: "+ exitValue);
  }
 else {
    logger.info("Browser: " + token + " process exited normally after "+ getElapsed(intervalMs)+ "s");
  }
  cb.childExited(token,exitValue);
}
