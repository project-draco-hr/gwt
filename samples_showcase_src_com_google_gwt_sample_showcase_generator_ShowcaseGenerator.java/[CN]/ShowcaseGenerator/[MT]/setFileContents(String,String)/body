{
  BufferedWriter out=null;
  try {
    FileWriter fstream=new FileWriter(filename);
    out=new BufferedWriter(fstream);
    out.write(contents);
  }
 catch (  IOException e) {
    System.out.println("Cannot save file: " + filename);
  }
 finally {
    if (out != null) {
      try {
        out.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
