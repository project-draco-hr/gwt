{
  String[] names=TypeOracle.modifierBitsToNames(modifierBits);
  StringBuffer sb=new StringBuffer();
  for (int i=0; i < names.length; i++) {
    if (i > 0) {
      sb.append(" ");
    }
    sb.append(names[i]);
  }
  if (names.length > 0)   sb.append(" ");
  sb.append(type.getQualifiedSourceName());
  sb.append(" ");
  sb.append(getName());
  return sb.toString();
}
