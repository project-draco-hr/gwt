def tweak(fn):
    newName = ('fixed/' + fn)
    f = open(fn, 'r')
    outf = open(newName, 'w')
    seen = []
    print fn
    for line in f:
        cleaned = line.strip()
        if (len(cleaned) == 0):
            continue
        if cleaned.startswith('#'):
            []
        elif (seen.count(cleaned) == 0):
            outf.write(cleaned)
            outf.write('\n')
            seen.append(cleaned)
        else:
            []
    f.close()
