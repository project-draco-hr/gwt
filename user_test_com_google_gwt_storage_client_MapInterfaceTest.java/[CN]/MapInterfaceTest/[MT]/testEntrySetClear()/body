{
  final Map<K,V> map;
  try {
    map=makePopulatedMap();
  }
 catch (  UnsupportedOperationException e) {
    return;
  }
  Set<Entry<K,V>> entrySet=map.entrySet();
  if (supportsClear) {
    entrySet.clear();
    assertTrue(entrySet.isEmpty());
  }
 else {
    try {
      entrySet.clear();
      fail("Expected UnsupportedOperationException.");
    }
 catch (    UnsupportedOperationException e) {
    }
  }
  assertInvariants(map);
}
