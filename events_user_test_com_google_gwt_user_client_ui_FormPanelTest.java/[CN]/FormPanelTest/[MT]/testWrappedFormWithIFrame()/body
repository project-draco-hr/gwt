{
  String uid=Document.get().createUniqueId();
  HTML formAndFrame=new HTML("<form id='" + uid + "' method='get' "+ "encoding='application/x-www-form-urlencoded' action='"+ GWT.getModuleBaseURL()+ "formHandler'>"+ "<input type='text' name='tb' value='text'></input></form>");
  RootPanel.get().add(formAndFrame);
  FormPanel form=FormPanel.wrap(Document.get().getElementById(uid),true);
  delayTestFinish(5000);
  form.addFormHandler(new FormHandler(){
    public void onSubmit(    FormSubmitEvent event){
    }
    public void onSubmitComplete(    FormSubmitCompleteEvent event){
      assertTrue(event.getResults().equals("tb=text"));
      finishTest();
    }
  }
);
  form.submit();
}
