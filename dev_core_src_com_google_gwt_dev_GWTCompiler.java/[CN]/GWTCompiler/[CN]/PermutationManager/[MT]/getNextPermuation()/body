{
  if (hasNextPermutation()) {
    int permNumber=nextPerm++;
    while (nextPerm < rebindOracles.length && rebindOracles[nextPerm] == null) {
      ++nextPerm;
    }
    TreeLogger branch=logger.branch(TreeLogger.TRACE,"Analyzing permutation #" + (permNumber + 1),null);
    return new Permuation(branch,permNumber,rebindOracles[permNumber]);
  }
  return null;
}
