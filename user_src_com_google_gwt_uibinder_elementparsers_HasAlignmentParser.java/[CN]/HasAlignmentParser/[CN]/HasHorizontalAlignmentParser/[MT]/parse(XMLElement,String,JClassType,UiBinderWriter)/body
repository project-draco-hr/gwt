{
  JClassType hAlignConstantType=writer.getOracle().findType(HorizontalAlignmentConstant.class.getCanonicalName());
  String horizontalAlignment=elem.consumeAttributeWithDefault("horizontalAlignment",null,hAlignConstantType);
  if (horizontalAlignment != null) {
    writer.addStatement("%s.setHorizontalAlignment(%s);",fieldName,horizontalAlignment);
  }
}
