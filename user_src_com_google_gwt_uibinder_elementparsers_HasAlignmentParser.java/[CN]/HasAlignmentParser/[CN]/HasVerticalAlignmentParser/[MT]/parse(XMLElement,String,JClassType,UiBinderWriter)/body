{
  JClassType vAlignConstantType=writer.getOracle().findType(VerticalAlignmentConstant.class.getCanonicalName());
  String verticalAlignment=elem.consumeAttributeWithDefault("verticalAlignment",null,vAlignConstantType);
  if (verticalAlignment != null) {
    writer.addStatement("%s.setVerticalAlignment(%s);",fieldName,verticalAlignment);
  }
}
