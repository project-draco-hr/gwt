{
  StringBuffer b=new StringBuffer();
  b.append("<g:TabPanel>");
  b.append("  <g:Tab>");
  b.append("    <g:TabHTML>Foo</g:TabHTML>");
  b.append("    <g:TabHTML>Bar</g:TabHTML>");
  b.append("    <g:Button/>");
  b.append("  </g:Tab>");
  b.append("</g:TabPanel>");
  parseAndFail(b,"May have only one \"text\" attribute or <g:TabHTML>");
}
