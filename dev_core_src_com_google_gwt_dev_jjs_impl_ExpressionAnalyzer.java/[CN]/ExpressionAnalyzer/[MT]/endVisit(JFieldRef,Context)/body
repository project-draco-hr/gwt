{
  accessesField=true;
  if (!x.getTarget().isFinal()) {
    accessesFieldNonFinal=true;
  }
  if (x.hasClinit()) {
    recordMethodCall();
  }
  JExpression instance=x.getInstance();
  if (instance == null) {
    return;
  }
  if (instance instanceof JThisRef) {
    return;
  }
  canThrowException=true;
}
