{
  if (listener == null) {
    codeServerPort=options.getCodeServerPort();
    listener=new BrowserListener(getTopLogger(),bindAddress,codeServerPort,new OophmSessionHandler(getTopLogger(),browserHost));
    listener.start();
    try {
      codeServerPort=listener.getSocketPort();
    }
 catch (    UnableToCompleteException e) {
    }
  }
}
