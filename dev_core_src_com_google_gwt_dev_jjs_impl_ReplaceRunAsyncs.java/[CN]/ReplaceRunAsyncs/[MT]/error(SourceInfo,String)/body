{
  errorsFound=true;
  TreeLogger fileLogger=logger.branch(TreeLogger.ERROR,"Error in '" + info.getFileName() + "'");
  String linePrefix="";
  if (info.getStartLine() > 0) {
    linePrefix="Line " + info.getStartLine() + ": ";
  }
  fileLogger.log(TreeLogger.ERROR,linePrefix + message);
  return fileLogger;
}
