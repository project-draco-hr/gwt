{
  String aboutText=Util.getFileFromInstallPath("about.txt");
  if (aboutText != null) {
    System.err.println(aboutText);
  }
 else {
    System.err.println(About.GWT_VERSION);
  }
  System.err.println("Available main classes:");
  System.err.println(addSpaces(GWTShell.class.getName(),"runs the development shell"));
  System.err.println(addSpaces(GWTCompiler.class.getName(),"compiles a GWT module"));
}
