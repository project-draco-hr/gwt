{
  final ArrayOfInt array=Collections.arrayOfInt();
  array.insert(0,0);
  assertSamelitude(new int[]{0},array);
  array.insert(0,1);
  assertSamelitude(new int[]{1,0},array);
  array.insert(100,2);
  assertSamelitude(new int[]{1,0,2},array);
  array.insert(-1,3);
  assertSamelitude(new int[]{1,0,3,2},array);
}
