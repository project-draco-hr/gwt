{
  final TestItem[] items=new TestItem[]{new TestItem(0),new TestItem(1),new TestItem(2),new TestItem(3)};
  final ArrayOf<TestItem> array=Collections.arrayOf();
  array.push(items[2]);
  array.push(items[1]);
  array.push(items[3]);
  array.push(items[0]);
  array.sort(new CanCompare<TestItem>(){
    @Override public int compare(    TestItem a,    TestItem b){
      return a.id() - b.id();
    }
  }
);
  assertEquals(items.length,array.length());
  for (int i=0, n=array.length(); i < n; ++i) {
    assertEquals(items[i],array.get(i));
  }
}
