{
  generateStringList(pw,group + "-" + categorySuffix,null,locale,methodPrefix + width,keys);
  String redirect=localeData.getEntry(group + "-sa-" + categorySuffix+ "-redirect",locale,"redirect");
  if ("yes".equals(redirect)) {
    generateStandaloneRedirect(pw,methodPrefix + width);
  }
 else {
    generateStringList(pw,group + "-sa-" + categorySuffix,group + "-" + categorySuffix,locale,methodPrefix + width + "Standalone",keys);
  }
}
