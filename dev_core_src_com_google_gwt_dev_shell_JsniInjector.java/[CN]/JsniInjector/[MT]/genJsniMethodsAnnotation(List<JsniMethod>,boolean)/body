{
  StringBuffer sb=new StringBuffer();
  String nl=pretty ? "\n " : "";
  sb.append("@" + JSNIMETHODS_NAME + "({");
  for (  JsniMethod jsniMethod : jsniMethods) {
    sb.append(jsniMethod.toString());
    sb.append(',');
    sb.append(nl);
  }
  sb.append("})");
  return sb.toString().toCharArray();
}
