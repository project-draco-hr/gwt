{
  String query=getQuery();
  int pos=query.indexOf("?" + SERVERLESS_QUERY_PARAM + "=");
  if (pos < 0) {
    pos=query.indexOf("&" + SERVERLESS_QUERY_PARAM + "=");
  }
  if (pos < 0) {
    return null;
  }
  pos+=SERVERLESS_QUERY_PARAM.length() + 2;
  query=query.substring(pos);
  pos=query.indexOf('&');
  if (pos >= 0) {
    query=query.substring(0,pos);
  }
  return query;
}
