{
  testIsFinished=true;
  resetAsyncState();
  ExceptionWrapper ew=null;
  if (ex != null) {
    ew=new ExceptionWrapper(ex);
    if (checkPoints != null) {
      for (int i=0, c=checkPoints.size(); i < c; ++i) {
        ew.message+="\n" + checkPoints.get(i);
      }
    }
  }
  junitHost.reportResultsAndGetNextMethod(outer.getTestName(),ew,junitHostListener);
}
