{
  assertEquals(HVA + ".ALIGN_TOP",parser.parse(null,"top"));
  assertEquals(HVA + ".ALIGN_MIDDLE",parser.parse(null,"middle"));
  assertEquals(HVA + ".ALIGN_BOTTOM",parser.parse(null,"bottom"));
  assertEquals(HVA + ".ALIGN_TOP",parser.parse(null,"Top"));
  assertEquals(HVA + ".ALIGN_MIDDLE",parser.parse(null,"Middle"));
  assertEquals(HVA + ".ALIGN_BOTTOM",parser.parse(null,"Bottom"));
}
