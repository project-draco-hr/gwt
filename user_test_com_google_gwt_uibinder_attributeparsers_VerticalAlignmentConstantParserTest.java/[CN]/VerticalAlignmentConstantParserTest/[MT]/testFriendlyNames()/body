{
  assertEquals(HVA + ".ALIGN_TOP",parser.parse("top"));
  assertEquals(HVA + ".ALIGN_MIDDLE",parser.parse("middle"));
  assertEquals(HVA + ".ALIGN_BOTTOM",parser.parse("bottom"));
  assertEquals(HVA + ".ALIGN_TOP",parser.parse("Top"));
  assertEquals(HVA + ".ALIGN_MIDDLE",parser.parse("Middle"));
  assertEquals(HVA + ".ALIGN_BOTTOM",parser.parse("Bottom"));
}
