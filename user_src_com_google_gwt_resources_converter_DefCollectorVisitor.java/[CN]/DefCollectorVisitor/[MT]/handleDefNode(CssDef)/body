{
  constantNodes.add(def);
  if (!defMapping.containsKey(def.getKey())) {
    String upperCaseName=toUpperCase(def.getKey());
    if (defMapping.containsValue(upperCaseName)) {
      if (lenient) {
        treeLogger.log(Type.WARN,"Two constants have the same name [" + upperCaseName + "] "+ "after conversion. The second constant will be renamed automatically.");
        upperCaseName=renameConstant(upperCaseName);
      }
 else {
        throw new Css2GssConversionException("Two constants have the same name [" + upperCaseName + "] after conversion.");
      }
    }
    defMapping.forcePut(def.getKey(),upperCaseName);
  }
  return false;
}
