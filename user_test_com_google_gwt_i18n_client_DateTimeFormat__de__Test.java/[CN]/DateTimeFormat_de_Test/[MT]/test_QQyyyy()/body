{
  Date date=new Date(2006 - 1900,0,27);
  date.setHours(13);
  date.setMinutes(10);
  date.setSeconds(10);
  assertEquals("Q1 2006",format("QQ yyyy",date));
  date=new Date(2006 - 1900,1,27);
  date.setHours(13);
  date.setMinutes(10);
  date.setSeconds(10);
