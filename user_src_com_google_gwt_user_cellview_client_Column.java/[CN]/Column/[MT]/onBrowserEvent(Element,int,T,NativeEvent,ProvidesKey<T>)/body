{
  Object key=getKey(object,providesKey);
  Object viewData=getViewData(key);
  DelayedValueUpdater valueUpdater=(fieldUpdater == null) ? null : new DelayedValueUpdater(index,object);
  Object newViewData=cell.onBrowserEvent(elem,getValue(object),viewData,event,valueUpdater);
  if (newViewData != viewData) {
    setViewData(key,newViewData);
  }
  if (valueUpdater != null) {
    valueUpdater.flush();
  }
}
