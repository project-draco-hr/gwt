{
  runCount++;
  OutputStream barStream=context.tryCreateResource(logger,"bar.txt");
  if (barStream != null) {
    try {
      barStream.write("here's some text.".getBytes(Charsets.UTF_8));
      context.commitResource(logger,barStream).setVisibility(Visibility.Public);
    }
 catch (    IOException e) {
      return null;
    }
  }
  return null;
}
