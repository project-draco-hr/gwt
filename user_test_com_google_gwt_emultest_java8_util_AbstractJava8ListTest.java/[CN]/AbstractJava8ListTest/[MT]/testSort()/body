{
  List<String> list=createEmptyList();
  list.sort(null);
  Collections.addAll(list,"b","a","c");
  list.sort(null);
  assertEquals(asList("a","b","c"),list);
  list=createEmptyList();
  Collections.addAll(list,"b","a","c");
  list.sort(Collections.reverseOrder());
  assertEquals(asList("c","b","a"),list);
}
