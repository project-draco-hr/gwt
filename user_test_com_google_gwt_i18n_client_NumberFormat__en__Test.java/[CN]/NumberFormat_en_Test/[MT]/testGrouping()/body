{
  String str;
  str=NumberFormat.getFormat("#,###").format(1234567890);
  assertTrue(str.equals("1,234,567,890"));
  str=NumberFormat.getFormat("#,####").format(1234567890);
  assertTrue(str.equals("12,3456,7890"));
  str=NumberFormat.getFormat("#").format(1234567890);
  assertTrue(str.equals("1234567890"));
}
