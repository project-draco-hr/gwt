{
  getWidget().setHeight(height + "px");
  int contentTitleHeight=DOM.getElementPropertyInt(contentLayout.getCellFormatter().getElement(0,1),"offsetHeight");
  int bottomHeight=height - topPanel.getOffsetHeight() - 50- contentTitleHeight;
  int bottomWidth=width - 50;
  mainMenuWrapper.setSize("200px",bottomHeight + "px");
  contentWrapper.setSize((bottomWidth - 200) + "px",bottomHeight + "px");
}
