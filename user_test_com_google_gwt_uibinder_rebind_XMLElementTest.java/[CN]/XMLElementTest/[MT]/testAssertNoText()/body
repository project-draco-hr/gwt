{
  init("<doc><elm yes='true' no='false'>Blah <blah/> blah</elm></doc>");
  try {
    elm.assertNoText();
    fail();
  }
 catch (  UnableToCompleteException e) {
    assertTrue("Expect extra text",logger.died.contains("Blah"));
    assertTrue("Expect extra text",logger.died.contains("blah"));
  }
}
