{
  if (out != null) {
    try {
      out.writeBoolean(false);
      out.flush();
      out.close();
    }
 catch (    IOException e) {
    }
  }
  if (in != null) {
    try {
      in.close();
    }
 catch (    IOException e) {
    }
  }
  if (workerSocket != null) {
    try {
      workerSocket.close();
    }
 catch (    IOException e) {
    }
  }
}
