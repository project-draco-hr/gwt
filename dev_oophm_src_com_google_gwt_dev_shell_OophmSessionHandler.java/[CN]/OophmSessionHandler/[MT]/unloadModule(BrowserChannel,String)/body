{
  BrowserChannelServer serverChannel=(BrowserChannelServer)channel;
  ModuleSpace moduleSpace=moduleMap.get(serverChannel);
  if (moduleSpace == null) {
    logger.log(TreeLogger.ERROR,"Unload request without a module loaded",null);
    return;
  }
  logger.log(TreeLogger.INFO,"Unloading module " + moduleSpace.getModuleName() + " ("+ moduleName+ ")",null);
  host.unloadModule(moduleSpace.host);
  moduleSpace.dispose();
  moduleMap.remove(serverChannel);
}
