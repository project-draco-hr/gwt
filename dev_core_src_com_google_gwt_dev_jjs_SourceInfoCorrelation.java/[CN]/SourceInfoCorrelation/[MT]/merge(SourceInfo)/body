{
  for (  SourceInfo info : sourceInfos) {
    if (this == info) {
      continue;
    }
    for (    Correlation c : info.getAllCorrelations()) {
      if (!isAlreadyInAllCorrelations(c)) {
        allCorrelations.add(c);
      }
    }
    if (primaryCorrelations.size() < Axis.values().length && info instanceof SourceInfoCorrelation) {
      EnumMap<Axis,Correlation> copy=new EnumMap<Axis,Correlation>(((SourceInfoCorrelation)info).primaryCorrelations);
      copy.keySet().removeAll(primaryCorrelations.keySet());
      primaryCorrelations.putAll(copy);
    }
  }
}
