{
  if (jarFiles.length > 0) {
    try {
synchronized (jarFiles) {
        if (force || (System.currentTimeMillis() > (lastJarAccessed + 90000))) {
          for (int i=0; i < jarFiles.length; i++) {
            if (jarFiles[i] != null) {
              jarFiles[i].close();
              jarFiles[i]=null;
            }
          }
        }
      }
    }
 catch (    IOException e) {
      log("Failed to close JAR",e);
    }
  }
}
