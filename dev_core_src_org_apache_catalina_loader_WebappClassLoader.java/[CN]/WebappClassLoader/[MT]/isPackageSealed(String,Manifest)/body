{
  String path=name + "/";
  Attributes attr=man.getAttributes(path);
  String sealed=null;
  if (attr != null) {
    sealed=attr.getValue(Name.SEALED);
  }
  if (sealed == null) {
    if ((attr=man.getMainAttributes()) != null) {
      sealed=attr.getValue(Name.SEALED);
    }
  }
  return "true".equalsIgnoreCase(sealed);
}
