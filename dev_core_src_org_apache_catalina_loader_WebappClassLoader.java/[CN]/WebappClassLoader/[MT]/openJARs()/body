{
  if (started && (jarFiles.length > 0)) {
    lastJarAccessed=System.currentTimeMillis();
    if (jarFiles[0] == null) {
      try {
        for (int i=0; i < jarFiles.length; i++) {
          jarFiles[i]=new JarFile(jarRealFiles[i]);
        }
      }
 catch (      IOException e) {
        log("Failed to open JAR",e);
      }
    }
  }
}
