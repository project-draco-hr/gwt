{
  if (val == null) {
    setNull();
    return;
  }
  if (val instanceof DispatchObject) {
    value=val;
  }
 else {
    DispatchObject dispObj=dispatchObjectCache.get(val);
    if (dispObj == null) {
      dispObj=new DispatchObjectOOPHM(cl,val);
      dispatchObjectCache.put(val,dispObj);
    }
    value=dispObj;
  }
}
