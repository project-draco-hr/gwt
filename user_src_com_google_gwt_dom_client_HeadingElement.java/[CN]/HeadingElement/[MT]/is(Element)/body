{
  if (elem == null) {
    return false;
  }
  String tag=elem.getTagName().toLowerCase();
  if (tag.length() != 2) {
    return false;
  }
  if (tag.charAt(0) != 'h') {
    return false;
  }
  int n=Integer.parseInt(tag.substring(1,2));
  if (n < 1 || n > 6) {
    return false;
  }
  return true;
}
