{
  boolean seenWebKit=false;
  for (  PropertyOracle oracle : propertyOracles) {
    try {
      SelectionProperty prop=oracle.getSelectionProperty(logger,"user.agent");
      if ("safari".equals(prop.getCurrentValue())) {
        seenWebKit=true;
        break;
      }
    }
 catch (    BadPropertyValueException e) {
      seenWebKit=true;
      break;
    }
  }
  if (!seenWebKit) {
    return false;
  }
  MyVisitor v=new MyVisitor();
  v.accept(program);
  return v.didChange();
}
