{
  JsBinaryOperator op=x.getOperator();
  if (op != JsBinaryOperator.SHR && op != JsBinaryOperator.SHRU) {
    return;
  }
  SourceInfo sourceInfo=x.getSourceInfo();
  JsExpression lhs=x.getArg1();
  JsExpression rhs=x.getArg2();
  JsExpression newNode=new JsBinaryOperation(sourceInfo,op,new JsPrefixOperation(sourceInfo,JsUnaryOperator.BIT_NOT,new JsPrefixOperation(sourceInfo,JsUnaryOperator.BIT_NOT,lhs)),rhs);
  ctx.replaceMe(newNode);
}
