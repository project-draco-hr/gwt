{
  String generatedClass=generateLocaleTree(logger,context,targetClass,locale);
  if (generatedClass == null) {
    logger.log(TreeLogger.ERROR,"Failed to generate " + targetClass.getQualifiedSourceName() + " in locale "+ locale.toString());
    return null;
  }
  Set<GwtLocale> locales=localeMap.get(generatedClass);
  if (locales == null) {
    locales=new HashSet<GwtLocale>();
    localeMap.put(generatedClass,locales);
  }
  locales.add(locale);
  return generatedClass;
}
