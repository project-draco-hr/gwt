{
  String viewData=getViewData(key);
  if (viewData != null && viewData.equals(value)) {
    clearViewData(key);
    viewData=null;
  }
  sb.append("<input type='text'");
  if (viewData != null) {
    sb.append(" value='" + viewData + "'");
  }
 else   if (value != null) {
    sb.append(" value='" + value + "'");
  }
  sb.append("></input>");
}
