{
  return new Iterator(){
    boolean hasElement=widget != null;
    Widget returned=null;
    public boolean hasNext(){
      return hasElement;
    }
    public Object next(){
      if (!hasElement || (widget == null)) {
        throw new NoSuchElementException();
      }
      hasElement=false;
      return (returned=widget);
    }
    public void remove(){
      if (returned != null) {
        SimplePanel.this.remove(returned);
      }
    }
  }
;
}
