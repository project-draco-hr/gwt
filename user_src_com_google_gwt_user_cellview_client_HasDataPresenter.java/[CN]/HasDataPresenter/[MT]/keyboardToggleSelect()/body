{
  if (KeyboardSelectionPolicy.ENABLED == keyboardSelectionPolicy && selectionModel != null && keyboardSelectedRow >= 0 && keyboardSelectedRow < rowData.size()) {
    T value=rowData.get(keyboardSelectedRow);
    if (value != null) {
      selectionModel.setSelected(value,!selectionModel.isSelected(value));
    }
  }
}
