{
  int keyboardSelectedRow=getKeyboardSelectedRow();
  if (KeyboardSelectionPolicy.ENABLED == keyboardSelectionPolicy && selectionModel != null && keyboardSelectedRow >= 0 && keyboardSelectedRow < getRowDataSize()) {
    T value=getRowDataValue(keyboardSelectedRow);
    if (value != null) {
      selectionModel.setSelected(value,!selectionModel.isSelected(value));
    }
  }
}
