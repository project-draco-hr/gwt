{
  if (count == this.rowCount && isExact == this.rowCountIsExact) {
    return;
  }
  this.rowCount=count;
  this.rowCountIsExact=isExact;
  updateLoadingState();
  if (keyboardSelectedRow >= count) {
    keyboardSelectedRow=Math.max(0,count - 1);
  }
  if (updateCachedData()) {
    redraw();
  }
  RowCountChangeEvent.fire(display,count,rowCountIsExact);
}
