{
  if (KeyboardSelectionPolicy.DISABLED == keyboardSelectionPolicy) {
    return false;
  }
 else   if (keyboardSelectedRow < rowData.size() - 1) {
    return true;
  }
 else   if (!keyboardPagingPolicy.isLimitedToRange() && (keyboardSelectedRow + pageStart < rowCount - 1 || !rowCountIsExact)) {
    return true;
  }
  return false;
}
