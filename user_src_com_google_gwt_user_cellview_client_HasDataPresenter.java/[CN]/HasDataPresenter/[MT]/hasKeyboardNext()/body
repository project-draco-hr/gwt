{
  if (KeyboardSelectionPolicy.DISABLED == keyboardSelectionPolicy) {
    return false;
  }
 else   if (getKeyboardSelectedRow() < getVisibleItemCount() - 1) {
    return true;
  }
 else   if (!keyboardPagingPolicy.isLimitedToRange() && (getKeyboardSelectedRow() + getPageStart() < getRowCount() - 1 || !isRowCountExact())) {
    return true;
  }
  return false;
}
