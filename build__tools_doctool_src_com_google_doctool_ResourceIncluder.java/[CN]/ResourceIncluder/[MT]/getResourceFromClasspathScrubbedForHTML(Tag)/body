{
  String partialPath=tag.text();
  try {
    String contents;
    contents=getFileFromClassPath(partialPath);
    contents=scrubForHtml(contents);
    return contents;
  }
 catch (  IOException e) {
    System.err.println(tag.position().toString() + ": unable to include resource " + partialPath+ " for tag "+ tag);
    System.exit(1);
    return null;
  }
}
