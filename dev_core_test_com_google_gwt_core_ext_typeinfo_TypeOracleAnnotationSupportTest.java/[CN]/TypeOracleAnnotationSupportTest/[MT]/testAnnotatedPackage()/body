{
  JPackage annotatedPackage=typeOracle.getPackage("com.google.gwt.core.ext.typeinfo.test");
  assertNotNull(annotatedPackage);
  TestAnnotation realAnnotation=Class.forName("com.google.gwt.core.ext.typeinfo.test.package-info",false,TypeOracleAnnotationSupportTest.class.getClassLoader()).getAnnotation(TestAnnotation.class);
  validateAnnotation(annotatedPackage,"Package","Not assigned",realAnnotation);
}
