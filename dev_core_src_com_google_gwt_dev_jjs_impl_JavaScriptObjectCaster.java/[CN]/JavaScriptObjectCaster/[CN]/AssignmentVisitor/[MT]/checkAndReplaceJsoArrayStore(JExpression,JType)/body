{
  if (!(targetType instanceof JReferenceType)) {
    return arg;
  }
  if (((JReferenceType)targetType).isFinal()) {
    return arg;
  }
  if (!program.isJavaScriptObject(arg.getType())) {
    return arg;
  }
  JCastOperation cast=new JCastOperation(program,arg.getSourceInfo(),targetType,arg);
  return cast;
}
