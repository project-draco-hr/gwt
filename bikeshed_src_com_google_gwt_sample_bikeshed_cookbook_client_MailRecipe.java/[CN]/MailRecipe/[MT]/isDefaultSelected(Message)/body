{
switch (type) {
case NONE:
    return false;
case ALL:
  return true;
case READ:
return object.isRead();
case UNREAD:
return !object.isRead();
case SENDER:
if (search == null || search.length() == 0) {
return false;
}
return canonicalize(object.getSender()).contains(search);
case SUBJECT:
if (search == null || search.length() == 0) {
return false;
}
return canonicalize(object.getSubject()).contains(search);
default :
throw new IllegalStateException("type = " + type);
}
}
