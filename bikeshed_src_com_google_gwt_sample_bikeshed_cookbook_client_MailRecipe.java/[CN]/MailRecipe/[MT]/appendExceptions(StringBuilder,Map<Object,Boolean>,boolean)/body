{
  boolean first=true;
  for (  Map.Entry<Object,Boolean> entry : exceptions.entrySet()) {
    if (entry.getValue() != selected) {
      continue;
    }
    if (first) {
      first=false;
      sb.append(selected ? '+' : '-');
      sb.append("msg(s) ");
    }
    sb.append(entry.getKey());
    sb.append(' ');
  }
}
