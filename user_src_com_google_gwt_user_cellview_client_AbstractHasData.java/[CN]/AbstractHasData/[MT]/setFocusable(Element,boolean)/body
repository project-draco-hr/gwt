{
  if (focusable) {
    FocusImpl focusImpl=FocusImpl.getFocusImplForWidget();
    focusImpl.setTabIndex(elem,getTabIndex());
    if (accessKey != 0) {
      focusImpl.setAccessKey(elem,accessKey);
    }
  }
 else {
    elem.setTabIndex(-1);
    elem.removeAttribute("tabIndex");
    elem.removeAttribute("accessKey");
  }
}
