{
  if (wasFocused) {
    CellBasedWidgetImpl.get().resetFocus(new Scheduler.ScheduledCommand(){
      public void execute(){
        if (!hasData.resetFocusOnCell()) {
          Element elem=hasData.getKeyboardSelectedElement();
          if (elem != null) {
            elem.focus();
          }
        }
      }
    }
);
  }
}
