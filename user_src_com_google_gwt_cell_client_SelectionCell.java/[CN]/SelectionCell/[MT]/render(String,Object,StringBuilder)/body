{
  int selectedIndex=getSelectedIndex(value);
  sb.append("<select>");
  int index=0;
  for (  String option : options) {
    if (index++ == selectedIndex) {
      sb.append("<option selected='selected'>");
    }
 else {
      sb.append("<option>");
    }
    sb.append(option);
    sb.append("</option>");
  }
  sb.append("</select>");
}
