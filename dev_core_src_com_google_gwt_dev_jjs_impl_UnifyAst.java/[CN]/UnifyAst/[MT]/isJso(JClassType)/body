{
  if (type == null) {
    return false;
  }
  boolean isJso=type == program.getJavaScriptObject() || isJso(type.getSuperClass());
  if (isJso) {
    return true;
  }
  for (  JInterfaceType intf : type.getImplements()) {
    if (isJsInterface(intf)) {
      return true;
    }
  }
  return false;
}
