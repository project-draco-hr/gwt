{
  List<JDeclaredType> types=unit.getTypes();
  for (  JDeclaredType t : types) {
    program.addType(t);
  }
  for (  JDeclaredType t : types) {
    resolveType(t);
  }
  for (  JDeclaredType t : types) {
    if (t instanceof JClassType && isJso((JClassType)t)) {
      instantiate(t);
    }
  }
}
