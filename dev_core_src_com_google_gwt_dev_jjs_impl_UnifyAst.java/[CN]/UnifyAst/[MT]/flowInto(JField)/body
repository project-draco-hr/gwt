{
  if (field.isExternal()) {
    assert errorsFound;
    return;
  }
  if (field == JField.NULL_FIELD) {
    return;
  }
  if (liveFieldsAndMethods.contains(field)) {
    return;
  }
  liveFieldsAndMethods.add(field);
  field.setType(translate(field.getType()));
  if (field.isStatic()) {
    staticInitialize(field.getEnclosingType());
  }
}
