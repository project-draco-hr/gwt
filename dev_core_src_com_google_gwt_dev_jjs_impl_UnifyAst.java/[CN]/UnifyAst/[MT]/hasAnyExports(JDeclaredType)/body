{
  if (!jsInteropEnabled) {
    return false;
  }
  for (  JMethod method : t.getMethods()) {
    if (program.typeOracle.isExportedMethod(method)) {
      return true;
    }
  }
  for (  JField field : t.getFields()) {
    if (program.typeOracle.isExportedField(field)) {
      return true;
    }
  }
  return false;
}
