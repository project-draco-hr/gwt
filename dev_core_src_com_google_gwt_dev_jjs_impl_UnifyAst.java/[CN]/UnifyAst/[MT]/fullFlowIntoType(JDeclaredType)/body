{
  String typeName=type.getName();
  if (!fullFlowTypes.contains(typeName) && !typeName.endsWith("package-info")) {
    fullFlowTypes.add(type.getName());
    instantiate(type);
    for (    JField field : type.getFields()) {
      flowInto(field);
    }
    for (    JMethod method : type.getMethods()) {
      flowInto(method);
    }
  }
}
