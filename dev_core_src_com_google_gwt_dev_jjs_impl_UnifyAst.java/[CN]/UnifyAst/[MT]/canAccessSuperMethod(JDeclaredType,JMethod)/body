{
  if (method.isPrivate()) {
    return false;
  }
  if (method.isPackagePrivate()) {
    String typePackage=Shared.getPackageName(type.getName());
    String methodPackage=Shared.getPackageName(method.getEnclosingType().getName());
    return typePackage.equals(methodPackage);
  }
  return true;
}
