{
  Date date=new Date(event.getMillis());
  StringBuilder prefix=new StringBuilder();
  prefix.append("<code>");
  prefix.append(date.toString());
  prefix.append(" ");
  prefix.append(event.getLoggerName());
  prefix.append("<br/>");
  prefix.append(event.getLevel().getName());
  prefix.append(": ");
  return prefix.toString();
}
