{
  double v=Math.tanh(0.0);
  assertEquals(0.0,v);
  v=Math.tanh(1.0);
  assertEquals(0.761594155,v,1e-7);
  v=Math.tanh(-1.0);
  assertEquals(-0.761594155,v,1e-7);
  v=Math.tanh(Double.NaN);
  assertNaN(v);
  v=Math.tanh(Double.NEGATIVE_INFINITY);
  assertEquals(-1.0,v,1e-7);
  v=Math.tanh(Double.POSITIVE_INFINITY);
  assertEquals(1.0,v,1e-7);
  v=Math.tanh(-0.0);
  assertEquals(-0.0,v);
}
