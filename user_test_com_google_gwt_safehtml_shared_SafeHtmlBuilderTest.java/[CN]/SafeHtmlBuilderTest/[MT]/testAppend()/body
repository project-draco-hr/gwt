{
  SafeHtml html=new SafeHtmlString(FOOBARBAZ_HTML);
  SafeHtmlBuilder b=new SafeHtmlBuilder().appendHtmlConstant("Yabba dabba &amp; doo\n").appendEscaped("What's up so&so\n").append(html);
  String expected="Yabba dabba &amp; doo\n" + "What&#39;s up so&amp;so\n" + FOOBARBAZ_HTML;
  assertEquals(expected,b.toSafeHtml().asString());
}
