{
  RootPanel.get().add(w);
  assertNotSunkAfterAttach(w,ProgressEvent.getType().getName(),isProgressEventSunk(e));
  w.addProgressHandler(new ProgressHandler(){
    public void onProgress(    ProgressEvent event){
    }
  }
);
  assertSunkAfterAddHandler(w,ProgressEvent.getType().getName(),isProgressEventSunk(e));
}
