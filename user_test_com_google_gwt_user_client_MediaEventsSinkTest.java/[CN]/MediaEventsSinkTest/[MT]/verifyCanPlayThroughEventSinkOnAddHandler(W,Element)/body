{
  RootPanel.get().add(w);
  assertNotSunkAfterAttach(w,CanPlayThroughEvent.getType().getName(),isCanPlayThroughEventSunk(e));
  w.addCanPlayThroughHandler(new CanPlayThroughHandler(){
    public void onCanPlayThrough(    CanPlayThroughEvent event){
    }
  }
);
  assertSunkAfterAddHandler(w,CanPlayThroughEvent.getType().getName(),isCanPlayThroughEventSunk(e));
}
