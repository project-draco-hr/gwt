{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  RecordKey other=(RecordKey)obj;
  if (!id.equals(other.id)) {
    return false;
  }
  if (!schema.equals(other.schema)) {
    return false;
  }
  return true;
}
