{
  try {
    ui.setCallback(RestartServerEvent.getType(),this);
    TreeLogger serverLogger=ui.getWebServerLogger(getWebServerName(),null);
    serverLogger.log(TreeLogger.INFO,"Starting HTTP on port " + getPort(),null);
    server=options.getServletContainerLauncher().start(serverLogger,getPort(),options.getWarDir());
    assert(server != null);
    return server.getPort();
  }
 catch (  BindException e) {
    System.err.println("Port " + getPort() + " is already is use; you probably still have another session active");
  }
catch (  Exception e) {
    System.err.println("Unable to start embedded HTTP server");
    e.printStackTrace();
  }
  ui.setCallback(RestartServerEvent.getType(),null);
  return -1;
}
