{
  MockXhrLoadingStrategy xls=new MockXhrLoadingStrategy(new MockXhr(500,"Server Error",false,false),new MockXhr(404,"Not Found",false,false,"Cache-Control","no-cache"),new MockXhr(200,"Ok",true,true,"Cache-Control","no-cache"));
  xls.startLoadingFragment(1,new LoadErrorHandler(){
    public void loadFailed(    Throwable reason){
      fail();
    }
  }
);
  xls.assertDone();
}
