{
  MockXhrLoadingStrategy xls=new MockXhrLoadingStrategy(new MockXhr(0,"Could not connect",false,false),new MockXhr(200,"Ok",true,true,"Cache-Control","no-cache"));
  xls.startLoadingFragment(1,new LoadTerminatedHandler(){
    public void loadTerminated(    Throwable reason){
      fail();
    }
  }
);
  xls.assertDone();
}
