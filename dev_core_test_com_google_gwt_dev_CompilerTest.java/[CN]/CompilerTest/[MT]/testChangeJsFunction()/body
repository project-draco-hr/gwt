  MockJavaResource fooResource=JavaResourceBase.createMockJavaResource("com.foo.Foo","package com.foo;","import com.google.gwt.core.client.js.JsExport;","@JsExport public class Foo implements IFoo {","  @Override public int foo(int x) { return 0; }","}");
