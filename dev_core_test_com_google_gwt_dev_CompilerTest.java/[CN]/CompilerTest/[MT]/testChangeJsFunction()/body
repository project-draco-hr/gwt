  compilerOptions.setJsInteropMode(OptionJsInteropMode.Mode.JS);
  MockJavaResource jsFunctionIFooResource=JavaResourceBase.createMockJavaResource("com.foo.IFoo","package com.foo;","import com.google.gwt.core.client.js.JsFunction;","@JsFunction public interface IFoo {","  int foo(int x);","}");
