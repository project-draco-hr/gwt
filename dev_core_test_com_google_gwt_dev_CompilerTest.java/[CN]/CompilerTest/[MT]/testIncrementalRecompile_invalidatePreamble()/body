{
  MinimalRebuildCache relinkMinimalRebuildCache=new MinimalRebuildCache();
  File relinkApplicationDir=Files.createTempDir();
  compileToJs(relinkApplicationDir,"com.foo.SimpleModule",Lists.newArrayList(simpleModuleResource,emptyEntryPointResource),relinkMinimalRebuildCache,null,JsOutputOption.PRETTY);
  assertEquals(0,relinkMinimalRebuildCache.getStaleTypeNames().size());
  relinkMinimalRebuildCache.markSourceFileStale("java/lang/Object.java");
  compileToJs(relinkApplicationDir,"com.foo.SimpleModule",Lists.<MockResource>newArrayList(),relinkMinimalRebuildCache,null,JsOutputOption.PRETTY);
  assertTrue(relinkMinimalRebuildCache.getProcessedStaleTypeNames().size() > 100);
  compileToJs(relinkApplicationDir,"com.foo.SimpleModule",Lists.<MockResource>newArrayList(emptyEntryPointResource),relinkMinimalRebuildCache,null,JsOutputOption.PRETTY);
  assertEquals(2,relinkMinimalRebuildCache.getProcessedStaleTypeNames().size());
}
