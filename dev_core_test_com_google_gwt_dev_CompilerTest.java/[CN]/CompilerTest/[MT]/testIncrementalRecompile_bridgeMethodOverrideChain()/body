{
  MinimalRebuildCache relinkMinimalRebuildCache=new MinimalRebuildCache();
  File relinkApplicationDir=Files.createTempDir();
  compileToJs(relinkApplicationDir,"com.foo.SimpleModule",Lists.newArrayList(simpleModuleResource,overriddenMethodChainEntryPointResource,topResource,middleResource,bottomResource),relinkMinimalRebuildCache,null,JsOutputOption.PRETTY);
  assertEquals(0,relinkMinimalRebuildCache.getStaleTypeNames().size());
  relinkMinimalRebuildCache.markSourceFileStale("com/foo/Bottom.java");
  compileToJs(relinkApplicationDir,"com.foo.SimpleModule",Lists.<MockResource>newArrayList(),relinkMinimalRebuildCache,null,JsOutputOption.PRETTY);
  assertTrue(relinkMinimalRebuildCache.getProcessedStaleTypeNames().contains("com.foo.Bottom$Value"));
}
