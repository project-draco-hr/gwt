{
  String accessModifier=null;
  if (methodBinding.isProtected()) {
    accessModifier="protected";
  }
 else   if (methodBinding.isPrivate()) {
    accessModifier="private";
  }
 else   if (methodBinding.isPublic()) {
    accessModifier="public";
  }
  return Joiner.on(" ").skipNulls().join(accessModifier,methodBinding.isStatic() ? "static" : null,getSourceName(methodBinding.declaringClass) + "." + formatMethodSignature(methodBinding));
}
