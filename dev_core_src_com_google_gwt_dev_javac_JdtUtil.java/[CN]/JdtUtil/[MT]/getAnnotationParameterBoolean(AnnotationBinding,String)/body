{
  if (a != null) {
    for (    ElementValuePair maybeValue : a.getElementValuePairs()) {
      if (maybeValue.getValue() instanceof BooleanConstant && paramName.equals(String.valueOf(maybeValue.getName()))) {
        return ((BooleanConstant)maybeValue.getValue()).booleanValue();
      }
    }
  }
  return null;
}
