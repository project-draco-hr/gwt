{
  if (annotations != null) {
    for (    Annotation a : annotations) {
      AnnotationBinding annBinding=a.getCompilerAnnotation();
      if (annBinding != null) {
        String annName=CharOperation.toString(annBinding.getAnnotationType().compoundName);
        if (nameToFind.equals(annName)) {
          return annBinding;
        }
      }
    }
  }
  return null;
}
