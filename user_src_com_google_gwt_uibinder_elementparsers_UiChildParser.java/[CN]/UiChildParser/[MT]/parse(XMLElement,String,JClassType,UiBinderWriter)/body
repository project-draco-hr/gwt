{
  this.fieldName=fieldName;
  this.writer=writer;
  OwnerFieldClass ownerFieldClass=OwnerFieldClass.getFieldClass(type,writer.getLogger(),uiBinderCtx);
  uiChildMethods=ownerFieldClass.getUiChildMethods();
  elem.consumeChildElements(new Interpreter<Boolean>(){
    public Boolean interpretElement(    XMLElement child) throws UnableToCompleteException {
      if (isValidChildElement(elem,child)) {
        handleChild(child);
        return true;
      }
      return false;
    }
  }
);
}
