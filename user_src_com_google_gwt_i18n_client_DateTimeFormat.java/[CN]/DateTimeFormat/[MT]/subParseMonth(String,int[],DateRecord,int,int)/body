{
  if (value < 0) {
    value=matchString(text,start,dateTimeConstants.months(),pos);
    if (value < 0) {
      value=matchString(text,start,dateTimeConstants.shortMonths(),pos);
    }
    if (value < 0) {
      return false;
    }
    cal.setMonth(value);
    return true;
  }
 else {
    cal.setMonth(value - 1);
    return true;
  }
}
