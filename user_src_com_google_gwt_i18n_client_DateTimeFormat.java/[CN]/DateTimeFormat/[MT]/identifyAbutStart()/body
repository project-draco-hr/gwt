{
  boolean abut=false;
  int len=patternParts.size();
  for (int i=0; i < len; i++) {
    if (isNumeric((PatternPart)patternParts.get(i))) {
      if (!abut && i + 1 < len && isNumeric((PatternPart)patternParts.get(i + 1))) {
        abut=true;
        ((PatternPart)patternParts.get(i)).abutStart=true;
      }
    }
 else {
      abut=false;
    }
  }
}
