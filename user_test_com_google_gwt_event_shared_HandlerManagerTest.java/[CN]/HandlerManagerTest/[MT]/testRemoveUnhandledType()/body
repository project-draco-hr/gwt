{
  final HandlerManager manager=new HandlerManager("bogus source");
  HandlerRegistration reg=manager.addHandler(MouseDownEvent.getType(),mouse1);
  reg.removeHandler();
  if (!GWT.isScript()) {
    try {
      reg.removeHandler();
      fail("Should have thrown assertion error");
    }
 catch (    AssertionError e) {
    }
  }
 else {
    reg.removeHandler();
  }
}
