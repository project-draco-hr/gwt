{
  Key key=method.getAnnotation(Key.class);
  if (key != null) {
    return key.value();
  }
  String[][] id=method.getMetaData(LocalizableGenerator.GWT_KEY);
  if (id.length > 0) {
    warnOnMetadata(method);
    if (id[0].length == 0) {
      logger.log(TreeLogger.WARN,method + " had a mislabeled gwt.key, using default key",null);
    }
 else {
      String tag=id[0][0];
      return tag;
    }
  }
  return AnnotationsResource.getKey(logger,keyGenerator,method,isConstants);
}
