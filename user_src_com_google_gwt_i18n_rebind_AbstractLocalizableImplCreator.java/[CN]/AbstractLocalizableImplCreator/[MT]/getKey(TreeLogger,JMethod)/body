{
  String[][] id=method.getMetaData(LocalizableGenerator.GWT_KEY);
  if (id.length > 0) {
    if (id[0].length == 0) {
      logger.log(TreeLogger.WARN,method + " had a mislabeled gwt.key, using method name as key",null);
    }
 else {
      String tag=id[0][0];
      return tag;
    }
  }
  return method.getName();
}
