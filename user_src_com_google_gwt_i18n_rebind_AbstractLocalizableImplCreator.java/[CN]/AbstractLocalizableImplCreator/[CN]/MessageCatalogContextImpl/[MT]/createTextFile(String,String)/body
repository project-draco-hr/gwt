{
  OutputStream outStr=createBinaryFile(catalogName);
  if (outStr != null) {
    try {
      return new PrintWriter(new BufferedWriter(new OutputStreamWriter(outStr,"UTF-8")),false);
    }
 catch (    UnsupportedEncodingException e) {
      error("UTF-8 not supported",e);
    }
  }
  return null;
}
