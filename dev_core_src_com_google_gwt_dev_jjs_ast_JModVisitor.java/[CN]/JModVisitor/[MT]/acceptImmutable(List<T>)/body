{
  NodeContext ctx=new NodeContext(false);
  try {
    for (int i=0, c=list.size(); i < c; ++i) {
      ctx.node=list.get(i);
      traverse(ctx.node,ctx);
      if (ctx.replaced) {
        list=Lists.set(list,i,(T)ctx.node);
        ctx.replaced=false;
      }
    }
    didChange|=ctx.didChange;
    return list;
  }
 catch (  Throwable e) {
    throw translateException(ctx.node,e);
  }
}
