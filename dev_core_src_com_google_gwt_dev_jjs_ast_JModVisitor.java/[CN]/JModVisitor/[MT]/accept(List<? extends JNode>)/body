{
  NodeContext ctx=new NodeContext();
  try {
    for (int i=0, c=list.size(); i < c; ++i) {
      (ctx.node=list.get(i)).traverse(this,ctx);
      if (ctx.replaced) {
        ((List)list).set(i,ctx.node);
        ctx.replaced=false;
      }
    }
    didChange|=ctx.didChange;
  }
 catch (  Throwable e) {
    throw translateException(ctx.node,e);
  }
}
