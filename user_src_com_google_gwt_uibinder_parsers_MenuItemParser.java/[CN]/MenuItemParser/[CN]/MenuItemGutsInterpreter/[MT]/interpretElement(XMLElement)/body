{
  if (isMenuHtml(elem)) {
    writer.warn("In %s, the MenuItemHTML element is no longer required, " + "and its contents have been inlined. This will become an error.",errorContext);
    return elem.consumeInnerHtml(htmlInterpreter);
  }
  if (isMenuBar(elem)) {
    if (menuBarField != null) {
      writer.die("In %s, only one MenuBar may be contained in a MenuItem",errorContext);
    }
    menuBarField=writer.parseWidget(elem);
    return "";
  }
  return null;
}
