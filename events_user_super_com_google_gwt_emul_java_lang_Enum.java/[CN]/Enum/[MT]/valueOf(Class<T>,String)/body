{
  if (enumType == null || name == null) {
    throw new NullPointerException("enumType and name must not be null.");
  }
  T[] constants=enumType.getEnumConstants();
  if (constants == null) {
    throw new IllegalArgumentException(enumType.getName() + " is not an enum.");
  }
  for (  T constant : constants) {
    if (constant.name().equals(name)) {
      return constant;
    }
  }
  throw new IllegalArgumentException(enumType.getName() + " does not have an enum constant named, " + name+ ".");
}
