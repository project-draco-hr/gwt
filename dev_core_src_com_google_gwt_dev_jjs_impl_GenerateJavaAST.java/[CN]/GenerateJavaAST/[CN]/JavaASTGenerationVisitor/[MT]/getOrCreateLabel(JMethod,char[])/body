{
  if (name == null) {
    return null;
  }
  String sname=String.valueOf(name);
  Map labelMap=(Map)fLabelMap.get(enclosingMethod);
  if (labelMap == null) {
    labelMap=new HashMap();
    fLabelMap.put(enclosingMethod,labelMap);
  }
  JLabel jlabel=(JLabel)labelMap.get(sname);
  if (jlabel == null) {
    jlabel=new JLabel(program,sname);
    labelMap.put(sname,jlabel);
  }
  return jlabel;
}
