{
  JStatement stmt;
  if (x instanceof Expression) {
    JExpression expr=dispProcessExpression((Expression)x);
    if (expr == null) {
      return null;
    }
    stmt=new JExpressionStatement(program,makeSourceInfo(x),expr);
  }
 else {
    stmt=(JStatement)dispatch("processStatement",x);
  }
  return stmt;
}
