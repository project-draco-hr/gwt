{
  int startLine=Util.getLineNumber(x.sourceStart,currentSeparatorPositions,0,currentSeparatorPositions.length - 1);
  SourceOrigin toReturn=SourceOrigin.create(x.sourceStart,x.sourceEnd,startLine,currentFileName);
  if (currentMethod != null) {
    return currentMethod.getSourceInfo().makeChild(toReturn);
  }
  return toReturn;
}
