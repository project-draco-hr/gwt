{
  SourceInfo sourceInfo=mapField.getSourceInfo().makeChild(JavaASTGenerationVisitor.class,"enum accessor method");
  JFieldRef mapRef=new JFieldRef(program,sourceInfo,null,mapField,type);
  JVariableRef nameRef=createVariableRef(sourceInfo,currentMethod.getParams().get(0));
  JMethod delegateTo=program.getIndexedMethod("Enum.valueOf");
  JMethodCall call=new JMethodCall(program,sourceInfo,null,delegateTo);
  call.addArgs(mapRef,nameRef);
  currentMethodBody.getBlock().addStmt(new JReturnStatement(program,sourceInfo,call));
}
