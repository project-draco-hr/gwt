{
  String wrapperTypeName=primitiveType.getWrapperTypeName();
  JClassType wrapperType=(JClassType)program.getFromTypeMap(wrapperTypeName);
  if (wrapperType == null) {
    throw new InternalCompilerException(toBox,"Cannot find wrapper type '" + wrapperTypeName + "' associated with primitive type '"+ primitiveType.getName()+ "'",null);
  }
  return box(toBox,wrapperType);
}
