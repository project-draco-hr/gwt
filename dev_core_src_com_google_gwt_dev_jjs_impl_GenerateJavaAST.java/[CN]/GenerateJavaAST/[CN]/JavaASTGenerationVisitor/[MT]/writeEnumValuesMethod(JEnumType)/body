{
  SourceInfo sourceInfo=type.getSourceInfo().makeChild(JavaASTGenerationVisitor.class,"enum values method");
  List<JExpression> initializers=new ArrayList<JExpression>();
  for (  JEnumField field : type.getEnumList()) {
    JFieldRef fieldRef=new JFieldRef(sourceInfo,null,field,type);
    initializers.add(fieldRef);
  }
  JNewArray newExpr=JNewArray.createInitializers(program,sourceInfo,program.getTypeArray(type,1),initializers);
  currentMethodBody.getBlock().addStmt(new JReturnStatement(sourceInfo,newExpr));
}
