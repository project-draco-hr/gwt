{
  List<JStatement> jstatements=new ArrayList<JStatement>();
  if (statements != null) {
    for (    Statement stmt : statements) {
      JStatement jstmt=dispProcessStatement(stmt);
      if (jstmt != null) {
        jstatements.add(jstmt);
        if (jstmt.unconditionalControlBreak()) {
          break;
        }
      }
    }
  }
  return jstatements;
}
