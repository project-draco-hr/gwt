{
  String[] parts=ident.substring(1).split("::");
  assert(parts.length == 2);
  String className=parts[0];
  JReferenceType type=program.getFromTypeMap(className);
  if (type == null) {
    reportJsniError(x,"Unresolvable native reference to type '" + className + "'",0);
    return null;
  }
  String rhs=parts[1];
  int parenPos=rhs.indexOf('(');
  if (parenPos < 0) {
    for (int i=0; i < type.fields.size(); ++i) {
      JField field=(JField)type.fields.get(i);
      if (field.getName().equals(rhs)) {
        return field;
      }
    }
    reportJsniError(x,"Unresolvable native reference to field '" + rhs + "' in type '"+ className+ "'",0);
  }
 else {
    String methodName=rhs.substring(0,parenPos);
    String almostMatches=null;
    for (int i=0; i < type.methods.size(); ++i) {
      JMethod method=(JMethod)type.methods.get(i);
      if (method.getName().equals(methodName)) {
        String jsniSig=getJsniSig(method);
        if (jsniSig.equals(rhs)) {
          return method;
        }
 else         if (almostMatches == null) {
          almostMatches="'" + jsniSig + "'";
        }
 else {
          almostMatches+=", '" + jsniSig + "'";
        }
      }
    }
    if (almostMatches == null) {
      reportJsniError(x,"Unresolvable native reference to method '" + methodName + "' in type '"+ className+ "'",0);
    }
 else {
      reportJsniError(x,"Unresolvable native reference to method '" + methodName + "' in type '"+ className+ "' (did you mean "+ almostMatches+ "?)",0);
    }
  }
  return null;
}
