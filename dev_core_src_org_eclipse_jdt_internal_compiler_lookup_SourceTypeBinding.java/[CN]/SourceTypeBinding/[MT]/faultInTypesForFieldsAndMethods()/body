{
  getAnnotationTagBits();
  ReferenceBinding enclosingType=this.enclosingType();
  if (enclosingType != null && enclosingType.isViewedAsDeprecated() && !this.isDeprecated())   this.modifiers|=ExtraCompilerModifiers.AccDeprecatedImplicitly;
  fields();
  methods();
  for (int i=0, length=this.memberTypes.length; i < length; i++)   ((SourceTypeBinding)this.memberTypes[i]).faultInTypesForFieldsAndMethods();
}
