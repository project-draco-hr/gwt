{
  for (  Field f : clazz.getFields()) {
    String fieldName;
    PropertyName annotation=f.getAnnotation(PropertyName.class);
    if (annotation != null) {
      fieldName=annotation.value();
    }
 else {
      fieldName=f.getName();
    }
    if (token.equals(fieldName)) {
      f.setAccessible(true);
      return f.get(null);
    }
  }
  throw new IllegalArgumentException("Cannot find enum " + token + " in type "+ clazz.getCanonicalName());
}
