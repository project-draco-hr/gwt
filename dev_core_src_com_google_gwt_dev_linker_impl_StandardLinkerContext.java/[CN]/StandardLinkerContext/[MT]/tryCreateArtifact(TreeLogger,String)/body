{
  File f=new File(linkerOutDir,partialPath);
  if (f.exists() || openPaths.contains(partialPath)) {
    logger.branch(TreeLogger.DEBUG,"Refusing to create artifact " + partialPath + " because it already exists or is already open.",null);
    return null;
  }
  ByteArrayOutputStream out=new ByteArrayOutputStream();
  outs.put(out,f);
  openPaths.add(partialPath);
  return out;
}
