{
  StringBuilder buf=new StringBuilder();
  if (language != null) {
    buf.append(language);
  }
  if (script != null) {
    buf.append('_');
    buf.append(script);
  }
  if (region != null) {
    buf.append('_');
    buf.append(region);
  }
  if (variant != null) {
    buf.append('_');
    buf.append(variant);
  }
  return buf.toString();
}
