{
  section=isFooter ? HtmlBuilderFactory.get().createTFootBuilder() : HtmlBuilderFactory.get().createTHeadBuilder();
  idToHeaderMap.clear();
  idToColumnMap.clear();
  rowIndex=0;
  if (!buildHeaderOrFooterImpl()) {
    return null;
  }
  while (section.getDepth() > 0) {
    section.end();
  }
  return section;
}
