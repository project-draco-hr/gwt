{
  if (i < 0 || i >= params.length) {
    return null;
  }
  String argName="arg" + i;
  seenFlag[i]=true;
  if (enablePluralOffsets && offset[i] != 0) {
    return argName + "_count";
  }
  if (isArray[i]) {
    return argName + ".length";
  }
  if (isList[i]) {
    return argName + ".size()";
  }
  return argName;
}
