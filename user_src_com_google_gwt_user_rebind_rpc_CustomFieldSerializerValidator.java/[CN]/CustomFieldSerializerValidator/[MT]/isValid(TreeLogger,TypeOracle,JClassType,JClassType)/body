{
  assert(logger != null);
  assert(serializee != null);
  assert(serializer != null);
  assert(typeOracle != null);
  logger=logger.branch(TreeLogger.SPAM,"Validating Custom Field Serializer '" + serializer.getQualifiedSourceName() + "' for class '"+ serializee.getQualifiedSourceName()+ "'",null);
  boolean isValidDeserialize=checkDeserializeMethod(logger,typeOracle,serializee,serializer);
  boolean isValidSerialize=checkSerializeMethod(logger,typeOracle,serializee,serializer);
  checkInstantiateMethod(logger,typeOracle,serializee,serializer);
  boolean isValid=isValidDeserialize && isValidSerialize;
  if (isValid) {
    logger=logger.branch(TreeLogger.SPAM,"Custom Field Serializer '" + serializer.getQualifiedSourceName() + "' is valid.",null);
  }
  return isValid;
}
