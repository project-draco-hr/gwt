{
  RequestFactoryInterfaceValidator validator=new RequestFactoryInterfaceValidator(log,new RequestFactoryInterfaceValidator.ClassLoaderLoader(classLoader));
  validator.antidote();
  validator.validateRequestFactory(binaryName);
  if (validator.isPoisoned()) {
    return false;
  }
  if (deobfuscator == null) {
    deobfuscator=validator.getDeobfuscator();
  }
 else {
    deobfuscator=new Deobfuscator.Builder().merge(deobfuscator).merge(validator.getDeobfuscator()).build();
  }
  return true;
}
