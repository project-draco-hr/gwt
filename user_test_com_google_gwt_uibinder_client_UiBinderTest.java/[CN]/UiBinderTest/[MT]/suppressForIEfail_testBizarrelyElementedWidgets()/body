{
  assertInOrder(widgetUi.getWidgetCrazyTable().getInnerHTML().toLowerCase(),"<td>they have been known</td>","<td>to write widgets</td>","<td>that masquerade</td>","<td>as table cells,</td>","<td>just like these.</td>","<td>burma shave</td>");
  assertInOrder(widgetUi.getWidgetCrazyOrderedList().getInnerHTML(),"<li>similar</li>","<li>things</li>");
  assertInOrder(widgetUi.getWidgetCrazyDefinitionList().getInnerHTML(),"<dt>Being</dt>","<dd>done</dd>","<dd>with</dd>","<dd>lists</dd>");
}
