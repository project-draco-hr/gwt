{
  if (popups == null)   return false;
  grabControl=null;
  boolean result=false;
  while (popups != null) {
    Menu menu=popups[0];
    if (menu == null)     break;
    int length=popups.length;
    System.arraycopy(popups,1,popups,0,--length);
    popups[length]=null;
    clearMenuFlags();
    runDeferredEvents();
    menu._setVisible(true);
    clearMenuFlags();
    result=true;
  }
  popups=null;
  return result;
}
