{
  if (disposeWindow != 0) {
    OS.DisposeWindow(disposeWindow);
    disposeWindow=0;
  }
  if (disposeWindowList != null) {
    for (int i=0; i < disposeWindowList.length; i++) {
      if (disposeWindowList[i] != 0) {
        OS.DisposeWindow(disposeWindowList[i]);
      }
    }
    disposeWindowList=null;
  }
}
