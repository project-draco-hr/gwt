{
  Flags.parse(args);
  final GoogleServletEngine gse=new GoogleServletEngine();
  gse.configure();
  gse.setServerType("what_the_fuck_is_all_this_random_stuff");
  gse.getPathDispatcher().addServlet("/*",new GwtResourceServlet(Server.class.getClassLoader(),IpScanners.ALLOW_ALL,"SilverComet"));
  gse.run();
}
