{
  Window.addWindowCloseListener(new WindowCloseListener(){
    public void onWindowClosed(){
      for (      RootPanel gwt : rootPanels.values()) {
        if (gwt.isAttached()) {
          gwt.onDetach();
        }
      }
    }
    public String onWindowClosing(){
      return null;
    }
  }
);
}
