{
  RootPanel gwt=(RootPanel)rootPanels.get(id);
  if (gwt != null) {
    return gwt;
  }
  Element elem=null;
  if (id != null) {
    if (null == (elem=DOM.getElementById(id))) {
      return null;
    }
  }
  if (rootPanels.size() == 0) {
    hookWindowClosing();
  }
  rootPanels.put(id,gwt=new RootPanel(elem));
  return gwt;
}
