{
  StringBuffer b=new StringBuffer();
  b.append("<g:LayoutPanel>");
  b.append("  <g:blah/>");
  b.append("</g:LayoutPanel>");
  try {
    tester.parse(b.toString());
    fail();
  }
 catch (  UnableToCompleteException e) {
    assertTrue("expect \"only g:layer\" error",tester.logger.died.contains("only <g:layer> children"));
  }
}
