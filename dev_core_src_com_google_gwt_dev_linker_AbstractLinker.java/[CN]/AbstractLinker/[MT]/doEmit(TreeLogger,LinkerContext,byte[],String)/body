{
  OutputStream out=context.tryCreateArtifact(logger,where);
  if (out != null) {
    try {
      out.write(what);
      context.commit(logger,out);
    }
 catch (    IOException e) {
      logger.log(TreeLogger.ERROR,"Unable to emit artifact",e);
      throw new UnableToCompleteException();
    }
  }
}
