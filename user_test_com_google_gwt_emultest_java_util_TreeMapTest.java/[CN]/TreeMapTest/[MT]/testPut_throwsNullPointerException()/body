{
  if (isPutSupported) {
    Map<K,V> map;
    map=createMap();
    try {
      map.put(null,getValues()[0]);
    }
 catch (    NullPointerException e) {
      assertFalse(useNullKey());
    }
    try {
      map.put(null,getValues()[0]);
      assertTrue(useNullKey());
    }
 catch (    NullPointerException e) {
      assertFalse(useNullKey());
    }
    map=createMap();
    map.put(getKeys()[0],getValues()[0]);
    try {
      map.put(null,getValues()[0]);
      assertTrue(useNullKey());
    }
 catch (    NullPointerException e) {
      assertFalse(useNullKey());
    }
  }
}
