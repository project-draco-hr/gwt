{
  if (isPutSupported) {
    K[] keys=getKeys();
    V[] values=getValues();
    Map<K,V> map=createMap();
    map.put(keys[0],values[0]);
    try {
      Map untypedMap=map;
      untypedMap.put(getConflictingKey(),values[1]);
      fail("ClassCastException expected");
    }
 catch (    ClassCastException expected) {
    }
  }
}
