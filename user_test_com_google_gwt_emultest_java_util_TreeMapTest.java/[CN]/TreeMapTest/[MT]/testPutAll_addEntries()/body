{
  if (isPutAllSupported) {
    Map<K,V> sourceMap=createMap();
    sourceMap.put(getKeys()[0],getValues()[0]);
    Map<K,V> destMap=createMap();
    destMap.putAll(sourceMap);
    sourceMap.put(getKeys()[1],getValues()[1]);
    destMap.putAll(sourceMap);
    _assertEquals(sourceMap,destMap);
  }
}
