{
  if (isPutSupported) {
    K[] keys=getKeys();
    V[] values=getValues();
    Map<K,V> map=createMap();
    assertNull(map.put(keys[0],values[0]));
    assertFalse(map.isEmpty());
    assertEquals(1,map.size());
    assertEquals(map.put(keys[0],values[1]),values[0]);
    assertEquals(1,map.size());
  }
}
