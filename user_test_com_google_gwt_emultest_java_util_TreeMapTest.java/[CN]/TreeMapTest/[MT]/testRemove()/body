{
  if (isRemoveSupported) {
    Map<K,V> map=createMap();
    if (useNullKey()) {
      assertNull(map.remove(null));
    }
    assertNull(map.remove(getKeys()[0]));
    assertNull(map.put(getKeys()[0],getValues()[0]));
    assertEquals(map.remove(getKeys()[0]),getValues()[0]);
    assertNull(map.remove(getKeys()[0]));
  }
}
