{
  if (isRemoveSupported) {
    K[] keys=getKeys();
    V[] values=getValues();
    Map<K,V> map=createMap();
    if (useNullKey()) {
      assertNull(map.remove(null));
    }
    assertNull(map.remove(keys[0]));
    assertNull(map.put(keys[0],values[0]));
    assertEquals(map.remove(keys[0]),values[0]);
    assertNull(map.remove(keys[0]));
  }
}
