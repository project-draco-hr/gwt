{
  if (isRemoveSupported) {
    K[] keys=getKeys();
    V[] values=getValues();
    Map<K,V> map=createMap();
    map.remove(getConflictingKey());
    map.put(keys[0],values[0]);
    try {
      map.remove(getConflictingKey());
      fail("ClassCastException expected");
    }
 catch (    ClassCastException expected) {
    }
  }
}
