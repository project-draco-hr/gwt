{
  if (lhsType == rhsType) {
    return true;
  }
  Set<JClassType> supertypes=getFlattenedSuperTypeHierarchy(rhsType);
  for (  JClassType supertype : supertypes) {
    if (supertype.isParameterized() == null) {
      continue;
    }
    if (supertype.isParameterized().getBaseType() == lhsType) {
      return true;
    }
  }
  return false;
}
