{
  StringBuffer sb=new StringBuffer();
  sb.append(getQualifiedSourceName());
  sb.append('<');
  boolean needComma=false;
  for (  JClassType typeParam : typeParams) {
    if (needComma) {
      sb.append(", ");
    }
 else {
      needComma=true;
    }
    sb.append(typeParam.getParameterizedQualifiedSourceName());
  }
  sb.append('>');
  return sb.toString();
}
