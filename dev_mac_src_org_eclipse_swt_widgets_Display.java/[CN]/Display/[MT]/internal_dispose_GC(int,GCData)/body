{
  if (isDisposed())   SWT.error(SWT.ERROR_DEVICE_DISPOSED);
  if (data != null) {
    int window=data.window;
    if (gcWindow == window) {
      OS.HideWindow(window);
    }
 else {
      OS.DisposeWindow(window);
    }
    data.window=0;
  }
  OS.CGContextSynchronize(context);
  OS.CGContextRelease(context);
}
