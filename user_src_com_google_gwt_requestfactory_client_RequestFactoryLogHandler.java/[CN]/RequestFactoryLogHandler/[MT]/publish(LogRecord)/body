{
  if (!isLoggable(record)) {
    return;
  }
  SerializableLogRecord slr=new SerializableLogRecord(record);
  String json=JsonLogRecordClientUtil.serializableLogRecordAsJson(slr);
  requestProvider.getLoggingRequest().logMessage(json).fire(new Receiver<String>(){
    @Override public void onSuccess(    String response){
      if (!response.isEmpty()) {
        wireLogger.severe("Remote Logging failed on server: " + response);
      }
    }
  }
);
}
