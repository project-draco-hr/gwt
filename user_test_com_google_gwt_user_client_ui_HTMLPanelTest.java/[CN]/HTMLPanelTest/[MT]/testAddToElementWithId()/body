{
  Label labelA=new Label("A"), labelB=new Label("B");
  HTMLPanel p=new HTMLPanel("<div id=\"a\"></div><div id=\"b\"></div>");
  p.add(labelA,"a");
  p.add(labelB,"b");
  assertEquals("a",DOM.getElementAttribute(DOM.getParent(labelA.getElement()),"id"));
  assertEquals("b",DOM.getElementAttribute(DOM.getParent(labelB.getElement()),"id"));
}
