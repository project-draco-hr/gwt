{
  this.classLoader=Thread.currentThread().getContextClassLoader();
  this.strategy=new LoadStrategy(){
    @Override public void load(    TreeLogger logger,    String moduleName,    ModuleDef moduleDef) throws UnableToCompleteException {
      for (      String inherit : inherits) {
        TreeLogger branch=logger.branch(TreeLogger.TRACE,"Loading inherited module '" + inherit + "'",null);
        nestedLoad(branch,inherit,moduleDef);
      }
    }
  }
;
}
