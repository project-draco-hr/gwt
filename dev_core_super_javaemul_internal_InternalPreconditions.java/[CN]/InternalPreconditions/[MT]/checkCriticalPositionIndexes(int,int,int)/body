{
  if (start < 0) {
    throw new IndexOutOfBoundsException("fromIndex: " + start + " < 0");
  }
  if (end > size) {
    throw new IndexOutOfBoundsException("toIndex: " + end + " > size "+ size);
  }
  if (start > end) {
    throw new IllegalArgumentException("fromIndex: " + start + " > toIndex: "+ end);
  }
}
