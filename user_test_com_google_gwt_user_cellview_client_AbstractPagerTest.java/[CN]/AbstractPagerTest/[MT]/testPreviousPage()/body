{
  AbstractPager pager=createPager();
  assertNull(pager.getView());
  pager.previousPage();
  HasRows view=new MockHasData<String>();
  pager.setView(view);
  view.setVisibleRange(45,20);
  view.setRowCount(105,true);
  pager.previousPage();
  assertEquals(new Range(25,20),view.getVisibleRange());
}
