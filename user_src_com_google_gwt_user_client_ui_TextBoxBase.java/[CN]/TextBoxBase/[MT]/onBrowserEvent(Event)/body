{
  int type=DOM.eventGetType(event);
  if ((type & Event.KEYEVENTS) != 0) {
    currentEvent=event;
    super.onBrowserEvent(event);
    currentEvent=null;
  }
 else   if (type == Event.ONCHANGE) {
    if (changeListeners != null) {
      changeListeners.fireChange(this);
    }
  }
 else {
    super.onBrowserEvent(event);
  }
}
