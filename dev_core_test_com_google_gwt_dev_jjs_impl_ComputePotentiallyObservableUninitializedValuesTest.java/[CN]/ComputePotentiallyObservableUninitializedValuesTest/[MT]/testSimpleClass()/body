{
  addSnippetClassDecl("static class A  { ","  int i1 = 1;","  Integer i2 = new Integer(1);","  final int fi1 = 1;","  final Integer fi2 = new Integer(1);","}");
  JProgram program=compileSnippet("void","return;");
  assertAnalysisCorrect(program,ImmutableList.<String>of(),ImmutableList.of("EntryPoint$A.i1","EntryPoint$A.i2","EntryPoint$A.fi1","EntryPoint$A.fi2"));
}
