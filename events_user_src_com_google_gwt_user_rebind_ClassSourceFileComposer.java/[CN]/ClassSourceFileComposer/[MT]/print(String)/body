{
  if (atStart) {
    for (int j=0; j < indent; ++j) {
      printWriter.print("  ");
    }
    if (inComment) {
      printWriter.print(commentIndicator);
    }
    atStart=false;
  }
  String rest=null;
  int i=s.indexOf("\n");
  if (i > -1 && i < s.length() - 1) {
    rest=s.substring(i + 1);
    s=s.substring(0,i + 1);
  }
  printWriter.print(s);
  if (rest != null) {
    atStart=true;
    print(rest);
  }
}
