{
  if (w == widget) {
    return;
  }
  if (w != null) {
    w.removeFromParent();
  }
  if (widget != null) {
    remove(widget);
  }
  widget=w;
  if (w != null) {
    layer=layout.attachChild(widget.getElement(),widget);
    layer.setTopHeight(0.0,Unit.PX,100.0,Unit.PCT);
    layer.setLeftWidth(0.0,Unit.PX,100.0,Unit.PCT);
    adopt(w);
    layout.layout();
    scheduleResize();
  }
}
