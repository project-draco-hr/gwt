{
  if (GWT.isScript() && entryDepth != 0) {
    int oldDepth=entryDepth;
    entryDepth=0;
    if (GWT.getUncaughtExceptionHandler() != null) {
      GWT.getUncaughtExceptionHandler().onUncaughtException(new IllegalStateException("Invalid entryDepth value " + oldDepth));
    }
  }
  watchdogEntryDepthTimerId=-1;
}
