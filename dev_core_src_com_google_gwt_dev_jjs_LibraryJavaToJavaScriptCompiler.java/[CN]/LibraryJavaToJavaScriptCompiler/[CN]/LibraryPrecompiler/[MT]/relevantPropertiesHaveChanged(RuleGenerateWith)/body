{
  Multimap<String,String> newConfigurationPropertyValues=compilerContext.gatherNewConfigurationPropertyValuesForGenerator(generatorRule.getName());
  Multimap<String,String> newBindingPropertyValues=compilerContext.gatherNewBindingPropertyValuesForGenerator(generatorRule.getName());
  return generatorRule.caresAboutProperties(newConfigurationPropertyValues.keySet()) || generatorRule.caresAboutProperties(newBindingPropertyValues.keySet());
}
