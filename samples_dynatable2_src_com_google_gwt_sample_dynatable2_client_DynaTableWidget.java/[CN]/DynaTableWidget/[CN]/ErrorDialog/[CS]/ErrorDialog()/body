{
  setStylePrimaryName("DynaTable-ErrorDialog");
  Button closeButton=new Button("Close",this);
  VerticalPanel panel=new VerticalPanel();
  panel.setSpacing(4);
  panel.add(body);
  panel.add(closeButton);
