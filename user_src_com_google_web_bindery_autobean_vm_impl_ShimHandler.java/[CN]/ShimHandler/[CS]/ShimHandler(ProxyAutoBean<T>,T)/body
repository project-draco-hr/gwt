{
  this.bean=bean;
  Method maybe=null;
  for (  Class<?> clazz : bean.getConfiguration().getCategories()) {
    try {
      maybe=clazz.getMethod("__intercept",AutoBean.class,Object.class);
      break;
    }
 catch (    SecurityException expected) {
    }
catch (    NoSuchMethodException expected) {
    }
  }
  interceptor=maybe;
}
