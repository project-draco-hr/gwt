{
  delayTestFinish(TEST_DELAY);
  CollectionsTestServiceAsync service=getServiceAsync();
  final List expected=new ArrayList();
  for (byte i=0; i < 10; ++i) {
    expected.add(new Byte(i));
  }
  service.getArraysAsList(expected,new AsyncCallback(){
    public void onFailure(    Throwable caught){
      assertTrue(caught.getClass().getName() + " should have been an InvocationException",caught instanceof InvocationException);
      finishTest();
    }
    public void onSuccess(    Object result){
      fail("Expected an InvocationException");
    }
  }
);
}
