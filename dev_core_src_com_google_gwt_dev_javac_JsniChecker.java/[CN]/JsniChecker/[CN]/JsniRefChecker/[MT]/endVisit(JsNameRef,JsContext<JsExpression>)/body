{
  this.errorInfo=x.getSourceInfo();
  String ident=x.getIdent();
  if (ident.charAt(0) == '@') {
    JsniRef jsniRef=JsniRef.parse(ident);
    if (jsniRef == null) {
      emitError("Malformed JSNI identifier '" + ident + "'");
    }
 else {
      checkRef(jsniRef);
    }
  }
  this.errorInfo=null;
}
