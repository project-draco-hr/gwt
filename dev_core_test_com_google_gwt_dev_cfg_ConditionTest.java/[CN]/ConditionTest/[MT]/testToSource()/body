{
  try {
    new ConditionWhenLinkerAdded("com.some.Linker").toSource();
    fail("expected linker condition source conversion to fail.");
  }
 catch (  UnsupportedOperationException e) {
  }
  assertEquals("((requestTypeName.equals(\"com.google.gwt.Foo\")))",new ConditionAll(new ConditionAny(),new ConditionWhenTypeIs("com.google.gwt.Foo")).toSource());
  assertEquals("!(((((requestTypeName.equals(\"com.google.gwt.Foo\")) && (RuntimePropertyRegistry." + "getPropertyValue(\"user.agent\").equals(\"webkit\")))) || (((requestTypeName.equals" + "(\"com.google.gwt.HasFocus\")) && (RuntimePropertyRegistry.getPropertyValue("+ "\"user.agent\").equals(\"ie9\"))))))",new ConditionNone(new ConditionAll(new ConditionWhenTypeIs("com.google.gwt.Foo"),new ConditionWhenPropertyIs("user.agent","webkit")),new ConditionAll(new ConditionWhenTypeIs("com.google.gwt.HasFocus"),new ConditionWhenPropertyIs("user.agent","ie9"))).toSource());
}
