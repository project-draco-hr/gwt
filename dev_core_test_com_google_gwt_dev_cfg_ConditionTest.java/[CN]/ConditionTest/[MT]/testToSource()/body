{
  try {
    new ConditionWhenLinkerAdded("com.some.Linker").toSource();
    fail("expected linker condition source conversion to fail.");
  }
 catch (  UnsupportedOperationException e) {
  }
  assertEquals("((requestTypeClass == com.google.gwt.Foo.class))",new ConditionAll(new ConditionAny(),new ConditionWhenTypeIs("com.google.gwt.Foo")).toSource());
  String a=new ConditionNone(new ConditionAll(new ConditionWhenTypeIs("com.google.gwt.Foo"),new ConditionWhenPropertyIs("user.agent","webkit")),new ConditionAll(new ConditionWhenTypeIs("com.google.gwt.HasFocus"),new ConditionWhenPropertyIs("user.agent","ie9"))).toSource();
  assertEquals("!(((((requestTypeClass == com.google.gwt.Foo.class) && (RuntimePropertyRegistry." + "getPropertyValue(\"user.agent\").equals(\"webkit\")))) || (((requestTypeClass == " + "com.google.gwt.HasFocus.class) && (RuntimePropertyRegistry.getPropertyValue("+ "\"user.agent\").equals(\"ie9\"))))))",a);
}
