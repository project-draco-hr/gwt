{
  assertEquals(VALID_STRONG_NAME,SourceHandler.getStrongNameFromSourcemapFilename(VALID_STRONG_NAME + SOURCEMAP_SUFFIX));
  assertNull(SourceHandler.getStrongNameFromSourcemapFilename("invalid_hash" + SOURCEMAP_SUFFIX));
  assertNull(SourceHandler.getStrongNameFromSourcemapFilename("whatever/" + VALID_STRONG_NAME + SOURCEMAP_SUFFIX));
}
