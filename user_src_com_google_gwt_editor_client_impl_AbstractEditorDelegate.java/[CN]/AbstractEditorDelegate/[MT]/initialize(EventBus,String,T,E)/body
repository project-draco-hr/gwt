{
  this.eventBus=eventBus;
  this.path=pathSoFar;
  setEditor(editor);
  setObject(object);
  if (editor instanceof ValueAwareEditor<?>) {
    valueAwareEditor=(ValueAwareEditor<T>)editor;
    valueAwareEditor.setDelegate(this);
    valueAwareEditor.setValue(object);
  }
  if (object != null) {
    attachSubEditors();
    pushValues();
  }
}
