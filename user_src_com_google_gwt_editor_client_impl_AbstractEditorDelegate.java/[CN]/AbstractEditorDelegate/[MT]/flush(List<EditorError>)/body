{
  try {
    if (valueAwareEditor != null) {
      valueAwareEditor.flush();
    }
    if (leafValueEditor != null) {
      setObject(leafValueEditor.getValue());
      return;
    }
    if (getObject() == null) {
      return;
    }
    flushSubEditors(errors);
    if (editorChain != null) {
      editorChain.collectErrors();
    }
  }
  finally {
    showErrors(errorAccumulator);
  }
}
