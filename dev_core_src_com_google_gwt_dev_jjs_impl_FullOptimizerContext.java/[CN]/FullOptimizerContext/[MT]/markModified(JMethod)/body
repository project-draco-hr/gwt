{
  methodsByModificationStep.get(modificationStepByMethod.count(modifiedMethod)).remove(modifiedMethod);
  methodsByModificationStep.get(optimizationStep).add(modifiedMethod);
  modificationStepByMethod.setCount(modifiedMethod,optimizationStep);
  callGraph.updateCallGraphOfMethod(modifiedMethod,deletedSubCallGraphs.get(optimizationStep),addedSubCallGraphs.get(optimizationStep));
  fieldReferencesGraph.updateFieldReferencesOfMethod(modifiedMethod);
}
