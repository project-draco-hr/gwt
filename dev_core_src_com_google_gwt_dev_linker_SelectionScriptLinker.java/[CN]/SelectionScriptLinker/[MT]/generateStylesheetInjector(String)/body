  if (isRelativeURL(stylesheetUrl)) {
    return "  if (!__gwt_stylesLoaded['" + stylesheetUrl + "']) {\n"+ "    __gwt_stylesLoaded['"+ stylesheetUrl+ "'] = true;\n"+ "    document.write('<link rel=\\\"stylesheet\\\" href=\\\"'+base+'"+ stylesheetUrl+ "\\\">');\n"+ "  }\n";
