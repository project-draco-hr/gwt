{
  defaultRecipe=new CellListRecipe();
  cats.add(new Category("Lists",new Recipe[]{defaultRecipe}));
  cats.add(new Category("Tables",new Recipe[]{new BasicTableRecipe(),new EditableTableRecipe()}));
  cats.add(new Category("Trees",new Recipe[]{new CellTreeRecipe(),new CellBrowserRecipe()}));
  cats.add(new Category("Other",new Recipe[]{new ValidationRecipe(),new MailRecipe()}));
}
