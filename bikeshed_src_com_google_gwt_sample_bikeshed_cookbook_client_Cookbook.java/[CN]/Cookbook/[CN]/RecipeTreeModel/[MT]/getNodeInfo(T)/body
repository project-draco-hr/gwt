{
  if (value == null) {
    return new DefaultNodeInfo<Category>(adapter,new CategoryCell(),selectionModel,null);
  }
 else   if (value instanceof Category) {
    Category category=(Category)value;
    return new DefaultNodeInfo<Recipe>(new ListViewAdapter<Recipe>(category.getRecipes()),new RecipeCell(),selectionModel,null);
  }
  return null;
}
