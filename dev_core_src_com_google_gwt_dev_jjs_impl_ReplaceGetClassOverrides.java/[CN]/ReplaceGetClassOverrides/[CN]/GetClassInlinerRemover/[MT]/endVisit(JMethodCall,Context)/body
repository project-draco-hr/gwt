{
  if (x.getTarget().getEnclosingType() == program.getJavaScriptObject()) {
    return;
  }
  if (x.getTarget() == getClassMethod || x.getTarget().getOverrides().contains(getClassMethod)) {
    ctx.replaceMe(new JFieldRef(x.getSourceInfo(),x.getInstance(),clazzField,clazzField.getEnclosingType()));
  }
}
