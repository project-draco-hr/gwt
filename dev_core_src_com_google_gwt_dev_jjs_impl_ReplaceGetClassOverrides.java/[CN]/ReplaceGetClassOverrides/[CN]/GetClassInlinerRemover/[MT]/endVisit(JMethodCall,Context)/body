{
  if (Devirtualizer.isGetClassDevirtualized(program,x.getTarget().getEnclosingType())) {
    return;
  }
  if (x.getTarget() == getClassMethod || x.getTarget().getOverriddenMethods().contains(getClassMethod)) {
    ctx.replaceMe(new JFieldRef(x.getSourceInfo(),x.getInstance(),clazzField,clazzField.getEnclosingType()));
  }
}
