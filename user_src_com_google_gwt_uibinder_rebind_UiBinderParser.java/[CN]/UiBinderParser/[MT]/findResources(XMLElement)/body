{
  binderElement.consumeChildElements(new XMLElement.Interpreter<Boolean>(){
    @Override public Boolean interpretElement(    XMLElement elem) throws UnableToCompleteException {
      if (writer.isBinderElement(elem)) {
        try {
          Resource.valueOf(elem.getLocalName().toUpperCase()).create(UiBinderParser.this,elem);
        }
 catch (        IllegalArgumentException e) {
          writer.die(elem,"Unknown tag %s, or is not appropriate as a top level element",elem.getLocalName());
        }
        return true;
      }
      return false;
    }
  }
);
}
