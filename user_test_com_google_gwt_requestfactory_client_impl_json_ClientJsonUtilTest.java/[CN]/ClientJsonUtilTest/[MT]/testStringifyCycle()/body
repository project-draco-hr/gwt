{
  String json="{\"a\":1,\"b\":\"hello\",\"c\":true," + "\"d\":null,\"e\":[1,2,3,4],\"f\":{\"x\":1}}";
  JsonMap jso=ClientJsonUtil.parse(json).cast();
  jso.put("cycle",jso);
  try {
    ClientJsonUtil.stringify(jso);
    fail("Expected JsonException for object cycle");
  }
 catch (  JsonException je) {
  }
}
