{
  try {
    return backgroundService.submit(new Runnable(){
      @Override public void run(){
        try {
          cacheDir.writeObject(entry);
        }
 catch (        UnableToCompleteException e) {
          backgroundService.shutdownNow();
        }
      }
    }
);
  }
 catch (  RejectedExecutionException ex) {
    return null;
  }
}
