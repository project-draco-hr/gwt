{
  ia=new int[]{i,j};
  assertEquals(2,ia.length);
  assertEquals(j,ia[1]);
  iaa=new int[][]{{i,j}};
  assertEquals(1,iaa.length);
  assertEquals(2,iaa[0].length);
  assertEquals(j,iaa[0][1]);
  iaa=new int[][]{{i,j},ia};
  assertEquals(2,iaa.length);
  assertEquals(2,iaa[0].length);
  assertEquals(j,iaa[0][1]);
  assertEquals(ia,iaa[1]);
}
