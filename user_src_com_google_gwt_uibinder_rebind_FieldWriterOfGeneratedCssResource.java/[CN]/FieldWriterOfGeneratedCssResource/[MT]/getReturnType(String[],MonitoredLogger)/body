{
  if (path.length == 2) {
    String maybeCssClass=path[1];
    try {
      if (css.getCssClassNames().contains(maybeCssClass)) {
        return stringType;
      }
    }
 catch (    UnableToCompleteException e) {
      logger.error("Can't interpret CSS");
    }
  }
  return super.getReturnType(path,logger);
}
