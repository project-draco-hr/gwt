{
  try {
    handleRequest(request,response);
  }
 catch (  Exception e) {
    if (e.getClass().getName().endsWith(".ClientAbortException") || e.getClass().getName().endsWith(".EofException")) {
    }
 else {
      logException("An error occured while trying to create the chart.",e,response);
    }
    return;
  }
}
