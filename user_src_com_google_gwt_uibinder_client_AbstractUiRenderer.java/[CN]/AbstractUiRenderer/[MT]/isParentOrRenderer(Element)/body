{
  if (parent == null) {
    return false;
  }
  Element root;
  try {
    root=UiRendererUtilsImpl.findRootElement(parent,RENDERED_ATTRIBUTE);
    return UiRendererUtilsImpl.isAttachedToDom(root) && UiRendererUtilsImpl.isRenderedElementSingleChild(root);
  }
 catch (  IllegalArgumentException e) {
    return false;
  }
}
