{
  String b=UIBINDER.replaceAll(CHILDREN,"<g:alimitedchild param1=\"1\"> <g:Label/> </g:alimitedchild>");
  FieldWriter w=tester.parse(b.toString());
  assertEquals(1,tester.writer.statements.size());
  assertEquals("Only call was: " + tester.writer.statements.get(0),"fieldName.addLimitedChild(<g:Label>, 1);",tester.writer.statements.get(0));
  assertNull("Parser should never set an initializer.",w.getInitializer());
}
