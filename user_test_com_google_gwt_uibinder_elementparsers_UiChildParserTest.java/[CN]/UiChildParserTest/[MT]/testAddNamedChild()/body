{
  String b=UIBINDER.replaceAll(CHILDREN,"<g:achild > <g:Label/> </g:achild>");
  FieldWriter w=tester.parse(b.toString());
  assertEquals(1,tester.writer.statements.size());
  assertEquals("fieldName.addNamedChild(<g:Label>);",tester.writer.statements.get(0));
  assertNull("Parser should never set an initializer.",w.getInitializer());
}
