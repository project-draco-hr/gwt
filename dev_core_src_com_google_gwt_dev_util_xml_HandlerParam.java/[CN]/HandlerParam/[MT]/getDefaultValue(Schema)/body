{
  Throwable caught=null;
  try {
    return (String)metaField.get(schema);
  }
 catch (  IllegalArgumentException e) {
    caught=e;
  }
catch (  IllegalAccessException e) {
    caught=e;
  }
  throw (IllegalStateException)new IllegalStateException("Unable to get attribute default value from meta field '" + metaField.getName() + "'").initCause(caught);
}
