{
  FileInputStream fileInputStream=null;
  try {
    fileInputStream=new FileInputStream(file);
    int length=(int)file.length();
    byte[] data=new byte[length];
    fileInputStream.read(data);
    return data;
  }
 catch (  IOException e) {
    return null;
  }
 finally {
    Utility.close(fileInputStream);
  }
}
