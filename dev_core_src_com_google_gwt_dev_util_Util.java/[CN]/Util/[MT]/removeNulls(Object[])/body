{
  int n=a.length;
  for (int i=0; i < a.length; i++) {
    if (a[i] == null) {
      --n;
    }
  }
  Class componentType=a.getClass().getComponentType();
  Object[] t=(Object[])Array.newInstance(componentType,n);
  int out=0;
  for (int in=0; in < t.length; in++) {
    if (a[in] != null) {
      t[out++]=a[in];
    }
  }
  return t;
}
