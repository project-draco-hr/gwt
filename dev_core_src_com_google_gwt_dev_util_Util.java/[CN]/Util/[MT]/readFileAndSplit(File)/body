{
  RandomAccessFile f=null;
  try {
    f=new RandomAccessFile(file,"r");
    int length=f.readInt();
    byte[][] results=new byte[length][];
    for (int i=0; i < length; i++) {
      int nextLength=f.readInt();
      results[i]=new byte[nextLength];
      f.read(results[i]);
    }
    return results;
  }
 catch (  IOException e) {
    return null;
  }
 finally {
    Utility.close(f);
  }
}
