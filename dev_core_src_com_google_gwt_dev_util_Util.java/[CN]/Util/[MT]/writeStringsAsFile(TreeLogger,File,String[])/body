{
  RandomAccessFile f=null;
  try {
    makePermFilename.delete();
    makePermFilename.getParentFile().mkdirs();
    f=new RandomAccessFile(makePermFilename,"rwd");
    f.writeInt(js.length);
    for (    String s : js) {
      byte[] b=getBytes(s);
      f.writeInt(b.length);
      f.write(b);
    }
    return true;
  }
 catch (  IOException e) {
    return false;
  }
 finally {
    Utility.close(f);
  }
}
