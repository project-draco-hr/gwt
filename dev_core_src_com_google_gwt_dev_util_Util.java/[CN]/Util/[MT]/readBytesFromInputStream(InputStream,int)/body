{
  try {
    byte[] bytes=new byte[byteLength];
    int byteOffset=0;
    while (byteOffset < byteLength) {
      int bytesReadCount=input.read(bytes,byteOffset,byteLength - byteOffset);
      if (bytesReadCount == -1) {
        return null;
      }
      byteOffset+=bytesReadCount;
    }
    return bytes;
  }
 catch (  IOException e) {
  }
  return null;
}
