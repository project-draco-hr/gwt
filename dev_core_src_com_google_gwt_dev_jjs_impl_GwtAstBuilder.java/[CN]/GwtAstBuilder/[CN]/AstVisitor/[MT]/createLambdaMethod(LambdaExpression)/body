{
  JMethod lambdaMethod=curMethod.method;
  JNode node=pop();
  if (node instanceof JExpression) {
    node=simplify((JExpression)node,(Expression)x.body);
  }
  JMethodBody body=(JMethodBody)curMethod.method.getBody();
  JStatement lambdaStatement=node instanceof JExpression ? (((JExpression)node).getType() == JPrimitiveType.VOID ? ((JExpression)node).makeStatement() : new JReturnStatement(node.getSourceInfo(),(JExpression)node)) : (JStatement)node;
  body.getBlock().addStmt(lambdaStatement);
  lambdaMethod.setBody(body);
  return lambdaMethod;
}
