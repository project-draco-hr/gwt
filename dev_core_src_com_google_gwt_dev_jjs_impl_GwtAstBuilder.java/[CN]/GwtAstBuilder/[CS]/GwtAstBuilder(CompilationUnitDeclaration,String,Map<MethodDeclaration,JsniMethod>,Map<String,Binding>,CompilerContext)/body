{
  this.sourceMapPath=sourceMapPath;
  this.jsniRefs=jsniRefs;
  this.jsniMethods=jsniMethods;
  this.compilerContext=compilerContext;
  this.isLegacyJsInteropEnabled=compilerContext.getOptions().getJsInteropMode() == Mode.JS;
