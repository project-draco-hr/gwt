{
  compilerContext=compilerContextBuilder.compileMonolithic(false).build();
  ModuleDef libraryOneModule=ModuleDefLoader.loadFromClassPath(TreeLogger.NULL,compilerContext,"com.google.gwt.dev.cfg.testdata.separate.libraryone.LibraryOne",false);
  assertNotNull(libraryOneModule.findSourceFile("com/google/gwt/dev/cfg/testdata/separate/libraryone/client/LibraryOne.java"));
  assertNotNull(libraryOneModule.findSourceFile("com/google/gwt/dev/cfg/testdata/separate/filesetone/client/FileSetOne.java"));
  assertNull(libraryOneModule.findSourceFile("com/google/gwt/dev/cfg/testdata/separate/librarytwo/client/LibraryTwo.java"));
}
