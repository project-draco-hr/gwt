{
  compilerContext=compilerContextBuilder.compileMonolithic(false).build();
  ModuleDefLoader.loadFromClassPath(TreeLogger.NULL,compilerContext,"com.google.gwt.dev.cfg.testdata.separate.libraryone.LibraryOne",false);
  assertEquals(Sets.newHashSet("com/google/gwt/dev/cfg/testdata/separate/filesetone/FileSetOne.gwt.xml","com/google/gwt/dev/cfg/testdata/separate/libraryone/LibraryOne.gwt.xml"),mockLibraryWriter.getBuildResourcePaths());
  assertEquals(Sets.newHashSet("com.google.gwt.core.Core","com.google.gwt.dev.cfg.testdata.separate.librarytwo.LibraryTwo"),mockLibraryWriter.getDependencyLibraryNames());
}
