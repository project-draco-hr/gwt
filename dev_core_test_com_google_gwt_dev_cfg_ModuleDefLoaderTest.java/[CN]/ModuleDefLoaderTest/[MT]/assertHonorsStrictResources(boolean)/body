{
  TreeLogger logger=TreeLogger.NULL;
  compilerContext.getOptions().setEnforceStrictResources(strictResources);
  ModuleDef emptyModule=ModuleDefLoader.loadFromClassPath(logger,compilerContext,"com.google.gwt.dev.cfg.testdata.merging.Empty");
  Resource sourceFile=emptyModule.findSourceFile("com/google/gwt/dev/cfg/testdata/merging/client/InOne.java");
  Resource publicFile=emptyModule.findPublicFile("Public.java");
  if (strictResources) {
    assertNull(sourceFile);
    assertNull(publicFile);
  }
 else {
    assertNotNull(sourceFile);
    assertNotNull(publicFile);
  }
}
