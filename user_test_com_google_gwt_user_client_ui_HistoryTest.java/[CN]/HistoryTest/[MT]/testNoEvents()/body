{
  addHistoryListenerImpl(new HistoryListener(){
{
      timer=new Timer(){
        public void run(){
          finishTest();
        }
      }
;
      timer.schedule(500);
    }
    public void onHistoryChanged(    String historyToken){
      fail("onHistoryChanged should not have been called");
    }
  }
);
  delayTestFinish(5000);
  History.newItem("testNoEvents",false);
}
