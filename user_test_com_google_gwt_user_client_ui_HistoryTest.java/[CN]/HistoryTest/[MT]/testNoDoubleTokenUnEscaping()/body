{
  final String shouldBeEncoded="abc%20abc";
  delayTestFinish(5000);
  History.newItem(shouldBeEncoded);
  History.newItem("someOtherToken");
  History.back();
  timer=new Timer(){
    @Override public void run(){
      assertEquals(shouldBeEncoded,History.getToken());
      finishTest();
    }
  }
;
  timer.schedule(200);
}
