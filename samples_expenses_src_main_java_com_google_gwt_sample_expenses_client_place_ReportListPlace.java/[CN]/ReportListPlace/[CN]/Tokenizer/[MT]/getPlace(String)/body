{
  String bits[]=token.split(SEPARATOR);
  if (bits.length != 3) {
    return null;
  }
  String reporterIdToken=bits[0];
  String search=bits[1];
  int page=Integer.valueOf(bits[2]);
  return new ReportListPlace(requests.<EmployeeProxy>getProxyId(reporterIdToken),URL.decodePathSegment(search),page);
}
