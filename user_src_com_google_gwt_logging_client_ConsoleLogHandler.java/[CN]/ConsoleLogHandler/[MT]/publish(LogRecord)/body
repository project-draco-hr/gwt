{
  if (!isSupported() || !isLoggable(record)) {
    return;
  }
  String msg=getFormatter().format(record);
  int val=record.getLevel().intValue();
  if (val >= Level.SEVERE.intValue()) {
    error(msg);
  }
 else   if (val >= Level.WARNING.intValue()) {
    warn(msg);
  }
 else   if (val >= Level.INFO.intValue()) {
    info(msg);
  }
 else {
    log(msg);
  }
}
