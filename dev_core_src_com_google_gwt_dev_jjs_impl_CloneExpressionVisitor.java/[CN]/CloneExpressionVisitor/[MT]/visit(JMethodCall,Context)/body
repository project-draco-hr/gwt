{
  JMethodCall newMethodCall=new JMethodCall(program,x.getSourceInfo(),cloneExpression(x.getInstance()),x.getTarget());
  if (!x.canBePolymorphic()) {
    newMethodCall.setCannotBePolymorphic();
  }
  newMethodCall.addArgs(cloneExpressions(x.getArgs()));
  expression=newMethodCall;
  return false;
}
