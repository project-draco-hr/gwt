{
  CellTable<String> table=new CellTable<String>();
  table.addColumn(new Column<String,String>(new TextCell()){
    @Override public String getValue(    String object){
      return object;
    }
  }
);
  table.addColumn(new Column<String,String>(new TextCell()){
    @Override public String getValue(    String object){
      return object + "-2";
    }
  }
);
  return table;
}
