{
  Integer backRef=backRefs.get(x);
  if (backRef != null) {
    if (PRETTY) {
      try {
        buffer.append(NL_CHAR);
      }
 catch (      IOException e) {
        halt(e);
      }
    }
    append(BACKREF_TYPE + String.valueOf(backRef));
    return false;
  }
 else {
    backRefs.put(x,backRefs.size());
    return true;
  }
}
