{
  assertTrue(FieldReferenceConverter.hasFieldReferences("{able} {baker}"));
  assertFalse(FieldReferenceConverter.hasFieldReferences("{{able}}"));
  assertFalse(FieldReferenceConverter.hasFieldReferences("{{able} baker { Charlie }"));
  assertFalse(FieldReferenceConverter.hasFieldReferences("\\{able} baker { Charlie }"));
}
