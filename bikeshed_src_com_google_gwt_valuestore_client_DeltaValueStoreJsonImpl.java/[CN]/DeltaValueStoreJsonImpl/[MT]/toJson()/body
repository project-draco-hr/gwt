{
  StringBuffer requestData=new StringBuffer("[");
  boolean first=true;
  for (  Map.Entry<RecordKey,ValuesImpl<?>> entry : changes.entrySet()) {
    ValuesImpl<?> impl=entry.getValue();
    if (first) {
      first=false;
    }
 else {
      requestData.append(",");
    }
    requestData.append(impl.toJson());
  }
  requestData.append("]");
  return requestData.toString();
}
