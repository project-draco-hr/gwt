{
  Object o=new Foo[3];
  assertEquals("[Lcom.google.gwt.dev.jjs.test.ClassObjectTest$Foo;",o.getClass().getName());
  assertEquals("class [Lcom.google.gwt.dev.jjs.test.ClassObjectTest$Foo;",o.getClass().toString());
  assertTrue(o.getClass().isArray());
  assertFalse(o.getClass().isEnum());
  assertFalse(o.getClass().isInterface());
  assertFalse(o.getClass().isPrimitive());
}
