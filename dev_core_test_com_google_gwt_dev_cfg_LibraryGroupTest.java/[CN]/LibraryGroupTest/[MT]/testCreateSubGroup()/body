{
  LibraryGroup looseLibraryGroup=LibraryGroup.fromLibraries(Lists.<Library>newArrayList(new MockLibrary("LibraryA"),new MockLibrary("LibraryB"),new MockLibrary("LibraryC"),new MockLibrary("LibraryD")),true);
  assertEquals(4,looseLibraryGroup.getLibraries().size());
  LibraryGroup strictLibraryGroup=looseLibraryGroup.createSubgroup(Lists.newArrayList("LibraryA","LibraryA","LibraryC"));
  assertEquals(2,strictLibraryGroup.getLibraries().size());
}
