{
  TempLocalTracker tracker=localTrackers.get(type);
  if (tracker == null) {
    tracker=new TempLocalTracker();
    localTrackers.put(type,tracker);
  }
  JLocal temp=null;
  if (reuseTemps) {
    temp=tracker.tryGetReusableLocal();
  }
  if (temp == null) {
    temp=program.createLocal(currentMethodBody.getSourceInfo(),getTempPrefix() + localCounter++,type,false,currentMethodBody);
  }
  tracker.useLocal(temp);
  return temp;
}
