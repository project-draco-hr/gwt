{
  if (!x.hasSideEffects()) {
    return x;
  }
  JLocal tempLocal=getTempLocal();
  JLocalRef tempRef=new JLocalRef(program,x.getSourceInfo(),tempLocal);
  JBinaryOperation asg=new JBinaryOperation(program,x.getSourceInfo(),x.getType(),JBinaryOperator.ASG,tempRef,x);
  multi.exprs.add(asg);
  return tempRef;
}
