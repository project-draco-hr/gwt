{
  TypeBinding resolvedType=jannotation.resolvedType;
  Class<? extends java.lang.annotation.Annotation> clazz=(Class<? extends java.lang.annotation.Annotation>)getClassLiteral(logger,resolvedType);
  if (clazz == null) {
    return false;
  }
  java.lang.annotation.Annotation annotation=(java.lang.annotation.Annotation)evaluateExpression(logger,jannotation);
  declaredAnnotations.put(clazz,annotation);
  return (annotation != null) ? true : false;
}
