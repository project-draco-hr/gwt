{
  analyze("void","int i = k; if (i == 1) { int j = i; } else { int j = i; } ").into("BLOCK -> [*]","STMT -> [*]","READ(k) -> [*]","WRITE(i, EntryPoint.k) -> [* {i = T}]","STMT -> [* {i = T}]","READ(i) -> [* {i = T}]","COND (i == 1) -> [THEN=* {i = 1}, ELSE=1 {i = T}]","BLOCK -> [* {i = 1}]","STMT -> [* {i = 1}]","READ(i) -> [* {i = 1}]","WRITE(j, i) -> [2 {i = 1, j = 1}]","1: BLOCK -> [* {i = T}]","STMT -> [* {i = T}]","READ(i) -> [* {i = T}]","WRITE(j, i) -> [* {i = T, j = T}]","2: END");
  analyze("int","int j = 0; if (foo() == 1) j = 1; return j;").into("BLOCK -> [*]","STMT -> [*]","WRITE(j, 0) -> [* {j = 0}]","STMT -> [* {j = 0}]","OPTTHROW(foo()) -> [NOTHROW=* {j = 0}, RE=2 {j = 0}]","CALL(foo) -> [* {j = 0}]","COND (EntryPoint.foo() == 1) -> [THEN=* {j = 0}, ELSE=1 {j = 0}]","STMT -> [* {j = 0}]","WRITE(j, 1) -> [* {j = 1}]","1: STMT -> [* {j = T}]","READ(j) -> [* {j = T}]","GOTO -> [* {j = T}]","2: END");
  analyze("int","int j = 0; if (foo() == 1) j = foo(); return j;").into("BLOCK -> [*]","STMT -> [*]","WRITE(j, 0) -> [* {j = 0}]","STMT -> [* {j = 0}]","OPTTHROW(foo()) -> [NOTHROW=* {j = 0}, RE=2 {j = 0}]","CALL(foo) -> [* {j = 0}]","COND (EntryPoint.foo() == 1) -> [THEN=* {j = 0}, ELSE=1 {j = 0}]","STMT -> [* {j = 0}]","OPTTHROW(foo()) -> [NOTHROW=* {j = 0}, RE=2 {j = 0}]","CALL(foo) -> [* {j = 0}]","WRITE(j, EntryPoint.foo()) -> [* {j = T}]","1: STMT -> [* {j = T}]","READ(j) -> [* {j = T}]","GOTO -> [* {j = T}]","2: END");
}
