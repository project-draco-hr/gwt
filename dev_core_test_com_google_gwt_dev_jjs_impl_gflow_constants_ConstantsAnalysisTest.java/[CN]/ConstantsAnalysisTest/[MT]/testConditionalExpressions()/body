{
  analyze("void","boolean b1 = false; boolean b2 = false; if (b1 && (b2 = true)) b1 = true;").into("BLOCK -> [* T]","STMT -> [* T]","WRITE(b1, false) -> [* {b1 = false}]","STMT -> [* {b1 = false}]","WRITE(b2, false) -> [* {b1 = false, b2 = false}]","STMT -> [* {b1 = false, b2 = false}]","READ(b1) -> [* {b1 = false, b2 = false}]","COND (b1) -> [THEN=* {b1 = false, b2 = false}, ELSE=1 {b1 = false, b2 = false}]","WRITE(b2, true) -> [* {b1 = false, b2 = true}]","1: COND (b1 && (b2 = true)) -> [THEN=* {b1 = false}, ELSE=2 {b1 = false}]","STMT -> [* {b1 = false}]","WRITE(b1, true) -> [* {b1 = true}]","2: END");
}
