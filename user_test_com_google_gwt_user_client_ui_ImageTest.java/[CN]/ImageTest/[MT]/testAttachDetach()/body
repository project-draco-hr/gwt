{
  if (isIE6()) {
    return;
  }
  final Image image=new Image("counting-forwards.png");
  RootPanel.get().add(image);
  RootPanel.get().remove(image);
  delayTestFinish(DEFAULT_TEST_TIMEOUT);
  new Timer(){
    @Override public void run(){
      finishTest();
    }
  }
.schedule(SYNTHETIC_LOAD_EVENT_TIMEOUT);
}
