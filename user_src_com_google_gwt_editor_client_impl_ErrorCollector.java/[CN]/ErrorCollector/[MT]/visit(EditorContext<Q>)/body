{
  HasEditorErrors<Q> asErrors=ctx.asHasEditorErrors();
  if (asErrors != null) {
    if (ctx.getAbsolutePath().equals(lastAddedPath)) {
      errorStack.peek().removeAll(lastAdded);
      errorStack.push(lastAdded);
    }
 else {
      errorStack.push(new ArrayList<EditorError>());
    }
  }
  return true;
}
