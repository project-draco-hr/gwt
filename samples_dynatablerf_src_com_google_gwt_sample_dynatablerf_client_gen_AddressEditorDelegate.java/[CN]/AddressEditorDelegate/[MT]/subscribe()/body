{
  if (eventBus == null) {
    return null;
  }
  return eventBus.addHandler(AddressProxyChanged.TYPE,new AddressProxyChanged.Handler(){
    public void onAddressChanged(    AddressProxyChanged event){
      if (object.getId().equals(event.getRecord().getId())) {
        factory.addressRequest().findAddress(object.getId()).fire(new Receiver<AddressProxy>(){
          public void onSuccess(          AddressProxy response,          Set<SyncResult> syncResults){
            pushValues(response);
          }
        }
);
      }
    }
  }
);
}
