{
  if (computeSVUID && !hasSVUID) {
    try {
      cv.visitField(Opcodes.ACC_FINAL + Opcodes.ACC_STATIC,"serialVersionUID","J",null,new Long(computeSVUID()));
    }
 catch (    Throwable e) {
      throw new RuntimeException("Error while computing SVUID for " + name,e);
    }
  }
  super.visitEnd();
}
