{
  UserAgent impl=GWT.create(UserAgent.class);
  String compileTimeValue=impl.getCompileTimeValue();
  String runtimeValue=impl.getRuntimeValue();
  if (!compileTimeValue.equals(runtimeValue)) {
    throw new AssertionError("Possible problem with your *.gwt.xml module file.\n" + "The compile time user.agent value (" + compileTimeValue + ") "+ "does not match the runtime user.agent value ("+ runtimeValue+ ").\n"+ "Expect more errors.");
  }
}
