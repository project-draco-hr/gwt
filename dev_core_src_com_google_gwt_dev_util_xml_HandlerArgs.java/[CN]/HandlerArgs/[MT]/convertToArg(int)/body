{
  String value=fArgValues[i];
  if (value != null) {
    AttributeConverter converter=fSchema.getAttributeConverter(fHandlerParams[i].getParamType());
    return converter.convertToArg(fSchema,fLineNumber,fElemName,fAttrNames[i],value);
  }
 else {
    return new NullPointerException("Argument " + i + " was null");
  }
}
