{
  for (int i=0; i < items.length; i++) {
    TreeItem item=items[i];
    for (int j=0; j < depth; j++) {
      result.print("   ");
    }
    result.println(item.getText());
    TreeItem[] children=item.getItems();
    if (children != null) {
      appendTreeItemText(result,children,depth + 1);
    }
  }
}
