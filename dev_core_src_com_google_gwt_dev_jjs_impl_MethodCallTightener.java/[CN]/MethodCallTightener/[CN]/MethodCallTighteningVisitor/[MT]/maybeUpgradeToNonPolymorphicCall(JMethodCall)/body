{
  JReferenceType instanceType=(JReferenceType)x.getInstance().getType();
  if (instanceType.isFinal()) {
    x=maybeTightenMethodCall(x);
    x.setCannotBePolymorphic();
    madeChanges();
  }
 else   if (!hasPotentialOverride(instanceType,x.getTarget())) {
    x.setCannotBePolymorphic();
    madeChanges();
  }
  return x;
}
