{
  assertCfg("boolean","i = 1;","if (i == 1) {","  i = 2;","  return true;","} else {","  i = 3;","  return false;","}").is("BLOCK -> [*]","STMT -> [*]","WRITE(i, 1) -> [*]","STMT -> [*]","READ(i) -> [*]","COND (EntryPoint.i == 1) -> [THEN=*, ELSE=1]","BLOCK -> [*]","STMT -> [*]","WRITE(i, 2) -> [*]","STMT -> [*]","GOTO -> [2]","1: BLOCK -> [*]","STMT -> [*]","WRITE(i, 3) -> [*]","STMT -> [*]","GOTO -> [*]","2: END");
}
