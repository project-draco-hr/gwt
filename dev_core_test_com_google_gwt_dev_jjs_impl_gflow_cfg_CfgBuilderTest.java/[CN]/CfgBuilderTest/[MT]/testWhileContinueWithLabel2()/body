{
  assertCfg("void","nextLoop: while(b3)","  while (b1) {","    if (b2) { continue nextLoop; }","    i++;","  }").is("BLOCK -> [*]","STMT -> [*]","1: READ(b3) -> [*]","COND (EntryPoint.b3) -> [THEN=*, ELSE=4]","STMT -> [*]","2: READ(b1) -> [*]","COND (EntryPoint.b1) -> [THEN=*, ELSE=1]","BLOCK -> [*]","STMT -> [*]","READ(b2) -> [*]","COND (EntryPoint.b2) -> [THEN=*, ELSE=3]","BLOCK -> [*]","STMT -> [*]","GOTO -> [1]","3: STMT -> [*]","READWRITE(i, null) -> [2]","4: END");
}
