  assertCfg("void","do { if (b1) { break; } else { do { j = 2; } while (b2); } } while (i == 1);").is("BLOCK -> [*]","STMT -> [*]","1: BLOCK -> [*]","STMT -> [*]","READ(b1) -> [*]","COND (EntryPoint.b1) -> [THEN=*, ELSE=2]","BLOCK -> [*]","STMT -> [*]","GOTO -> [4]","2: BLOCK -> [*]","STMT -> [*]","3: BLOCK -> [*]","STMT -> [*]","WRITE(j, 2) -> [*]","READ(b2) -> [*]","COND (EntryPoint.b2) -> [THEN=3, ELSE=*]","READ(i) -> [*]","COND (EntryPoint.i == 1) -> [THEN=1, ELSE=*]","4: END");
