{
  if (supportsRemove() == false) {
    return;
  }
  MapIterator it=makeFullMapIterator();
  Map map=getMap();
  Map confirmed=getConfirmedMap();
  assertEquals(true,it.hasNext());
  Object key=it.next();
  it.remove();
  confirmed.remove(key);
  verify();
  try {
    it.getValue();
    fail();
  }
 catch (  IllegalStateException ex) {
  }
  verify();
}
