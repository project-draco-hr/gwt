{
  String[] names=TypeOracle.modifierBitsToNames(modifierBits);
  StringBuffer sb=new StringBuffer();
  for (int i=0; i < names.length; i++) {
    sb.append(names[i]);
    sb.append(" ");
  }
  sb.append(returnType.getParameterizedQualifiedSourceName());
  sb.append(" ");
  sb.append(getName());
  toStringParamsAndThrows(sb);
  return sb.toString();
}
