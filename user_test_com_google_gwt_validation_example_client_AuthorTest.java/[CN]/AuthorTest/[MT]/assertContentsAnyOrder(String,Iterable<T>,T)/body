{
  List<T> expectedList=new ArrayList<T>(Arrays.asList(expected));
  message+="Expected to find " + expectedList + " but found "+ actual;
  for (  T a : actual) {
    if (expectedList.contains(a)) {
      expectedList.remove(a);
    }
 else {
      fail(message);
    }
  }
  if (!expectedList.isEmpty()) {
    fail(message);
  }
}
