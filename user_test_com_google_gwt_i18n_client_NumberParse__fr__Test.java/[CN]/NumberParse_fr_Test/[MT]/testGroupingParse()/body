{
  Number value;
  value=numberParse("#,###","1\u00a0234\u00a0567\u00a0890");
  assertTrue(value.doubleValue() == 1234567890);
  value=numberParse("#,####","12\u00a03456\u00a07890");
  assertTrue(value.doubleValue() == 1234567890);
  value=numberParse("#","1234567890");
  assertTrue(value.doubleValue() == 1234567890);
}
