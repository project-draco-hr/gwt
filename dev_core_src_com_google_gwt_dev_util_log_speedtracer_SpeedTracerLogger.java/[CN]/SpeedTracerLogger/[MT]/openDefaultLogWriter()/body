{
  final String logFile=System.getProperty("gwt.speedtracerlog");
  Writer writer=null;
  if (logFile != null) {
    try {
      writer=new FileWriter(logFile);
      return openLogWriter(writer,logFile);
    }
 catch (    IOException e) {
      System.err.println("Unable to open gwt.speedtracerlog '" + logFile + "'");
      e.printStackTrace();
    }
  }
  return null;
}
