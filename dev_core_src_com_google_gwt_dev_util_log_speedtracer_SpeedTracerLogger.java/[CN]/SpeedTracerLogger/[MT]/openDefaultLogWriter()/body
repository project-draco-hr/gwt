{
  Writer writer=null;
  if (enabled) {
    try {
      writer=new BufferedWriter(new FileWriter(logFile));
      return openLogWriter(writer,logFile);
    }
 catch (    IOException e) {
      log.log(Level.SEVERE,"Unable to open gwt.speedtracerlog '" + logFile + "'",e);
    }
  }
  return null;
}
