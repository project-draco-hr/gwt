{
  print(CHARS_DO);
  if (x.getBody() != null) {
    nestedStatementPush(x.getBody());
    accept(x.getBody());
    nestedStatementPop(x.getBody());
  }
  if (needSemi) {
    semi();
    newline();
  }
 else {
    space();
    needSemi=true;
  }
  print(CHARS_WHILE);
  lparen();
  accept(x.getTestExpr());
  rparen();
  return false;
}
