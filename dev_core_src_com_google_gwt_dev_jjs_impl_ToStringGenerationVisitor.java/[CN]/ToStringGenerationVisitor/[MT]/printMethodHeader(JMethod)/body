{
  if (x.isPrivate()) {
    print(CHARS_PRIVATE);
  }
 else {
    print(CHARS_PUBLIC);
  }
  printStaticFlag(x);
  printAbstractFlag(x);
  printNativeFlag(x);
  printMemberFinalFlag(x);
  printType(x);
  space();
  if (x.isStatic()) {
    printUniqueName(x);
  }
 else {
    printName(x);
  }
  printParameterList(x);
  if (x.thrownExceptions.size() > 0) {
    print(CHARS_THROWS);
    Iterator<JClassType> iter=x.thrownExceptions.iterator();
    if (iter.hasNext()) {
      printTypeName(iter.next());
    }
    while (iter.hasNext()) {
      print(CHARS_COMMA);
      printTypeName(iter.next());
    }
  }
}
