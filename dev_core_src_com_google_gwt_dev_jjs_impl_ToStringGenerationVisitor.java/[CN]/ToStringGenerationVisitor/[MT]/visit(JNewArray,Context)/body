{
  print(CHARS_NEW);
  printTypeName(x.getArrayType().getLeafType());
  if (x.initializers != null) {
    print('{');
    visitCollectionWithCommas(x.initializers.iterator());
    print('}');
  }
 else {
    for (int i=0; i < x.dims.size(); ++i) {
      JExpression expr=(JExpression)x.dims.get(i);
      print('[');
      accept(expr);
      print(']');
    }
  }
  return false;
}
