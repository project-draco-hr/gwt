{
  if (isInitializer(x)) {
    if (x.isStatic()) {
      print(CHARS_STATIC);
    }
    if (shouldPrintMethodBody()) {
      accept(x.getBody());
    }
 else {
      print("{ ... }");
      newlineOpt();
    }
  }
 else {
    printMethodHeader(x);
    if (shouldPrintMethodBody() && !x.isAbstract()) {
      space();
      accept(x.getBody());
    }
 else {
      semi();
      newlineOpt();
    }
  }
  return false;
}
