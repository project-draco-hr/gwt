{
  printAbstractFlag(x);
  printFinalFlag(x);
  print(CHARS_CLASS);
  printTypeName(x);
  space();
  if (x.getSuperClass() != null) {
    print(CHARS_EXTENDS);
    printTypeName(x.getSuperClass());
    space();
  }
  if (x.getImplements().size() > 0) {
    print(CHARS_IMPLEMENTS);
    for (int i=0, c=x.getImplements().size(); i < c; ++i) {
      if (i > 0) {
        print(CHARS_COMMA);
      }
      printTypeName(x.getImplements().get(i));
    }
    space();
  }
  return false;
}
