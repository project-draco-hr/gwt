{
  print(CHARS_DO);
  if (x.body != null) {
    nestedStatementPush(x.body);
    x.body.traverse(this);
    nestedStatementPop(x.body);
  }
  if (needSemi) {
    semi();
    newline();
  }
 else {
    space();
    needSemi=true;
  }
  print(CHARS_WHILE);
  lparen();
  x.getTestExpr().traverse(this);
  rparen();
  return false;
}
