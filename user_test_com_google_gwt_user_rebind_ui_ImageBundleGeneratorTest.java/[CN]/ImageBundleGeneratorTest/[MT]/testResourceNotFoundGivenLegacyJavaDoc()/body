{
  UnitTestTreeLogger.Builder b=new UnitTestTreeLogger.Builder();
  b.expectWarn(ImageBundleGenerator.MSG_JAVADOC_FORM_DEPRECATED,null);
  b.expectError(ImageBundleGenerator.msgCannotFindImageFromMetaData("from/metadata/notfound.png"),null);
  UnitTestTreeLogger logger=b.createLogger();
  try {
    getImageName(logger,new String[0],"nonexistentImg","from.metadata",new String[][]{{"notfound.png"}},null);
    fail("Should have thrown");
  }
 catch (  UnableToCompleteException e) {
  }
  logger.assertCorrectLogEntries();
}
