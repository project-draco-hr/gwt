{
  String packagePath=getClass().getPackage().getName().replace('.','/') + "/";
  URL testUrl=getClass().getClassLoader().getResource(packagePath + testName + "_test.css");
  assertNotNull("Cauld not find testUrl",testUrl);
  URL expectedUrl=getClass().getClassLoader().getResource(packagePath + testName + "_expected.css");
  assertNotNull("Cauld not find testUrl",expectedUrl);
  test(logger,testUrl,expectedUrl,visitors);
  if (reversible) {
    test(logger,expectedUrl,testUrl,visitors);
  }
}
