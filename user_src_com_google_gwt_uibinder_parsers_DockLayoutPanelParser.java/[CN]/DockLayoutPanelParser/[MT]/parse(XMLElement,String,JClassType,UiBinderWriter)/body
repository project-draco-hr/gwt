{
  if (type != getSplitLayoutPanelType(writer)) {
    JEnumType unitEnumType=writer.getOracle().findType(Unit.class.getCanonicalName()).isEnum();
    String unit=elem.consumeAttribute("unit",unitEnumType);
    writer.setFieldInitializerAsConstructor(fieldName,writer.getOracle().findType(DockLayoutPanel.class.getName()),unit);
  }
  for (  XMLElement child : elem.consumeChildElements()) {
    if (!isValidChildElement(elem,child)) {
      writer.die("In %1$s, child must be one of " + "<%2$s:north>, <%2$s:south>, <%2$s:east>, <%2$s:west> or <%2$s:center>, " + "but found %3$s",elem,elem.getPrefix(),child);
    }
    XMLElement widget=child.consumeSingleChildElement();
    String childFieldName=writer.parseElementToField(widget);
    if (requiresSize(child)) {
      String size=child.consumeDoubleAttribute("size");
      writer.addStatement("%s.%s(%s, %s);",fieldName,addMethodName(child),childFieldName,size);
    }
 else {
      writer.addStatement("%s.%s(%s);",fieldName,addMethodName(child),childFieldName);
    }
  }
}
