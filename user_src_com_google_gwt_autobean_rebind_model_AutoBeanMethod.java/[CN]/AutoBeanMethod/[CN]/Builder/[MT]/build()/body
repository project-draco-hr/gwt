{
  if (toReturn.action.equals(Action.GET) || toReturn.action.equals(Action.SET)) {
    PropertyName annotation=toReturn.method.getAnnotation(PropertyName.class);
    if (annotation != null) {
      toReturn.propertyName=annotation.value();
    }
 else {
      String name=toReturn.method.getName();
      toReturn.propertyName=Character.toLowerCase(name.charAt(3)) + (name.length() >= 5 ? name.substring(4) : "");
    }
  }
  try {
    return toReturn;
  }
  finally {
    toReturn=null;
  }
}
