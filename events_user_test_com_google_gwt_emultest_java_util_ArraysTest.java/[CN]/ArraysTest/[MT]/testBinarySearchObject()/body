{
  Object[] a1={};
  int ret=Arrays.binarySearch(a1,"");
  assertEquals(-1,ret);
  Object[] a2={"a","g","y"};
  ret=Arrays.binarySearch(a2,"c");
  assertEquals(-2,ret);
  ret=Arrays.binarySearch(a2,"y");
  assertEquals(2,ret);
  Object[] a3={"b","c","x","y"};
  ret=Arrays.binarySearch(a3,"z");
  assertEquals(-5,ret);
  ret=Arrays.binarySearch(a3,"a");
  assertEquals(-1,ret);
  ret=Arrays.binarySearch(a3,"b");
  assertEquals(0,ret);
}
