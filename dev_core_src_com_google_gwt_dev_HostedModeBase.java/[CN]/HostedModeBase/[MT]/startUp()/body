{
  if (started) {
    throw new IllegalStateException("Startup code has already been run");
  }
  started=true;
  if (!doStartup()) {
    return false;
  }
  if (runTomcat) {
    int resultPort=doStartUpServer();
    if (resultPort < 0) {
      return false;
    }
    port=resultPort;
  }
  return true;
}
