{
  if (started) {
    throw new IllegalStateException("Startup code has already been run");
  }
  started=true;
  loadRequiredNativeLibs();
  openAppWindow();
  initializeLogger();
  if (runTomcat) {
    int resultPort=startUpServer();
    if (resultPort < 0) {
      return false;
    }
    port=resultPort;
  }
  return true;
}
