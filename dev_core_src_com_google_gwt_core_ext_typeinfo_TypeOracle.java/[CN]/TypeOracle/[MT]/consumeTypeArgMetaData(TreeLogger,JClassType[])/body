{
  if (GenUtil.warnAboutMetadata()) {
    logger=logger.branch(TreeLogger.DEBUG,"Scanning source for uses of the deprecated " + TAG_TYPEARGS + " javadoc annotation; please use Java parameterized types instead",null);
  }
  for (int i=0; i < types.length; i++) {
    JClassType type=types[i];
    TreeLogger branch=logger.branch(TreeLogger.DEBUG,"Type " + type.getQualifiedSourceName(),null);
    consumeTypeArgMetaData(branch,type.getMethods());
    consumeTypeArgMetaData(branch,type.getFields());
  }
}
