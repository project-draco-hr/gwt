{
  TreeLogger branch;
  for (int i=0; i < fields.length; i++) {
    JField field=fields[i];
    String[][] tokensArray=field.getMetaData(TAG_TYPEARGS);
    if (tokensArray.length == 0) {
      continue;
    }
    try {
      String msg="Field " + field.getName();
      branch=logger.branch(TreeLogger.TRACE,msg,null);
      if (tokensArray.length > 1) {
        branch.log(TreeLogger.WARN,"Metadata error on field '" + field.getName() + "' in type '"+ field.getEnclosingType()+ "': expecting at most one "+ TAG_TYPEARGS+ " (the last one will be used)",null);
      }
      JType fieldType=field.getType();
      String[] token=tokensArray[tokensArray.length - 1];
      JType resultingType=determineActualType(branch,fieldType,token,0);
      branch.log(TreeLogger.WARN,"Deprecated use of " + TAG_TYPEARGS + " for field "+ field.getName()+ "; Please use "+ resultingType.getParameterizedQualifiedSourceName()+ " as the field's type",null);
      field.setType(resultingType);
    }
 catch (    UnableToCompleteException e) {
    }
  }
}
