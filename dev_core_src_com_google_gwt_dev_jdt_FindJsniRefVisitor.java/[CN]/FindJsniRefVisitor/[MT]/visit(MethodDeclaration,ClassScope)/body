{
  if (!methodDeclaration.isNative()) {
    return false;
  }
  String jsniCode=getJSNICode(methodDeclaration);
  if (jsniCode == null) {
    return false;
  }
  if (jsniCode.indexOf('@') < 0) {
    return false;
  }
  if (sloppy) {
    findJsniRefsSloppily(jsniCode);
  }
 else {
    findJsniRefsAccurately(methodDeclaration,jsniCode);
  }
  return false;
}
