{
  if (maxRetryCount < 0 || curRetryCount < maxRetryCount) {
    reportWarning("Retrying syncing back to junit backend. (Exception: " + caught + ")");
    curRetryCount++;
    new Timer(){
      @Override public void run(){
        syncToServer();
      }
    }
.schedule(1000);
  }
 else {
    reportFatalError("Cannot sync back to junit backend: " + caught);
  }
}
