{
  clientInfo=new ClientInfo(parseQueryParamInteger(SESSIONID_QUERY_PARAM,-1),getUserAgentProperty());
  maxRetryCount=parseQueryParamInteger(RETRYCOUNT_QUERY_PARAM,-1);
  currentBlock=checkForQueryParamTestToRun();
  if (currentBlock != null) {
    serverless=true;
    runTest();
  }
 else {
    syncToServer();
  }
}
