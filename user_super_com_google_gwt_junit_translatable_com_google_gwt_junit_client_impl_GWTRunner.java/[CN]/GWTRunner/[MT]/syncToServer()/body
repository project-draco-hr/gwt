{
  if (currentBlock == null) {
    int firstBlockIndex=parseQueryParamInteger(BLOCKINDEX_QUERY_PARAM,0);
    junitHost.getTestBlock(firstBlockIndex,getUserAgentProperty(),junitHostListener);
  }
 else {
    junitHost.reportResultsAndGetTestBlock(currentResults,currentBlock.getIndex() + 1,getUserAgentProperty(),junitHostListener);
  }
}
