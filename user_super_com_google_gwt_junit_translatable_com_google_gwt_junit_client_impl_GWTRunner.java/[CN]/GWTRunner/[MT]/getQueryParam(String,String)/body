{
  int pos=query.indexOf("?" + queryParam + "=");
  if (pos < 0) {
    pos=query.indexOf("&" + queryParam + "=");
  }
  if (pos < 0) {
    return null;
  }
  pos+=queryParam.length() + 2;
  String result=query.substring(pos);
  pos=result.indexOf('&');
  if (pos >= 0) {
    result=result.substring(0,pos);
  }
  return result;
}
