{
  if (serverless) {
    return;
  }
  TestInfo currentTest=getCurrentTest();
  currentResults.put(currentTest,result);
  ++currentBlockIndex;
  if (currentBlockIndex < currentBlock.length) {
    DeferredCommand.addCommand(new Command(){
      public void execute(){
        doRunTest();
      }
    }
);
  }
 else {
    syncToServer();
  }
}
