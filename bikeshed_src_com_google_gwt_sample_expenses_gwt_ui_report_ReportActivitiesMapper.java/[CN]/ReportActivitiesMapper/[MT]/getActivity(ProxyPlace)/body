{
switch (place.getOperation()) {
case DETAILS:
    return new ReportDetailsActivity((ReportRecord)place.getProxy(),requests,placeController);
case EDIT:
  return new ReportEditActivity((ReportRecord)place.getProxy(),requests,placeController,false);
case CREATE:
return new ReportEditActivity((ReportRecord)place.getProxy(),requests,placeController,true);
}
throw new IllegalArgumentException("Unknown operation " + place.getOperation());
}
