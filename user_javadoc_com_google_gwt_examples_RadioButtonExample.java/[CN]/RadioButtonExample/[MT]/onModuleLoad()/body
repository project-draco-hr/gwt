{
  RadioButton rb0=new RadioButton("myRadioGroup","foo");
  RadioButton rb1=new RadioButton("myRadioGroup","bar");
  RadioButton rb2=new RadioButton("myRadioGroup","baz");
  rb2.setChecked(true);
  FlowPanel panel=new FlowPanel();
  panel.add(rb0);
  panel.add(rb1);
  panel.add(rb2);
  RootPanel.get().add(panel);
}
