{
  assertEquals("abcdef","abc" + "def");
  assertEquals("abcdef","abc".concat("def"));
  assertEquals("".concat(""),"");
  char c='d';
  String s="abc";
  assertEquals("abcd","abc" + 'd');
  assertEquals("abcd","abc" + c);
  assertEquals("abcd",s + 'd');
  assertEquals("abcd",s + c);
  s+=c;
  assertEquals("abcd",s);
}
