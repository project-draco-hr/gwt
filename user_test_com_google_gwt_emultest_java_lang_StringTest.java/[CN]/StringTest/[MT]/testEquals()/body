{
  assertFalse(hideFromCompiler("ABC").equals("abc"));
  assertFalse(hideFromCompiler("abc").equals("ABC"));
  assertTrue(hideFromCompiler("abc").equals("abc"));
  assertTrue(hideFromCompiler("ABC").equals("ABC"));
  assertFalse(hideFromCompiler("AbC").equals("aBC"));
  assertFalse(hideFromCompiler("AbC").equals("aBC"));
  assertTrue(hideFromCompiler("").equals(""));
  assertFalse(hideFromCompiler("").equals(null));
}
