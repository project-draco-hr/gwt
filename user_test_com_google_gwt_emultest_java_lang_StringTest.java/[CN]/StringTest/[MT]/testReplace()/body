{
  assertEquals("axax".replace('x','a'),"aaaa");
  assertEquals("aaaa".replace('x','a'),"aaaa");
  for (char from=32; from < 250; ++from) {
    char to=(char)(from + 5);
    assertEquals(toS(to),toS(from).replace(from,to));
  }
  for (char to=32; to < 250; ++to) {
    char from=(char)(to + 5);
    assertEquals(toS(to),toS(from).replace(from,to));
  }
}
