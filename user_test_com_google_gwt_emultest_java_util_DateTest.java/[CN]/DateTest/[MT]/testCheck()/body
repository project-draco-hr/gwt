{
  if (GWT.isScript()) {
    Date d=new Date();
    mungeDateNull(d);
    try {
      d.getHours();
      fail("Expected IllegalStateException");
    }
 catch (    IllegalStateException expected) {
    }
    d=new Date();
    mungeDateUndef(d);
    try {
      d.getHours();
      fail("Expected IllegalStateException");
    }
 catch (    IllegalStateException expected) {
    }
    d=new Date();
    mungeDatePrimitive(d);
    try {
      d.getHours();
      fail("Expected IllegalStateException");
    }
 catch (    IllegalStateException expected) {
    }
  }
}
