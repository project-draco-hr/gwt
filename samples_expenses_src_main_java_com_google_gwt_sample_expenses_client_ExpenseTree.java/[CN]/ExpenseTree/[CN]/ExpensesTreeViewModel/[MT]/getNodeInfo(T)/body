{
  if (value == null) {
    return new DefaultNodeInfo<String>(departments,departmentCell,selectionModel,null);
  }
 else   if (isAllDepartment(value)) {
    return null;
  }
 else   if (value instanceof String) {
    EmployeeListDataProvider dataProvider=new EmployeeListDataProvider((String)value);
    return new DefaultNodeInfo<EmployeeProxy>(dataProvider,employeeCell,selectionModel,null);
  }
  return null;
}
