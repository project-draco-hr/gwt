{
  String name=String.valueOf(method.selector);
  StringBuilder sb=new StringBuilder();
  sb.append(name);
  sb.append("(");
  if (method.arguments != null) {
    for (    Argument param : method.arguments) {
      sb.append(param.binding.type.signature());
    }
  }
  sb.append(")");
  return sb.toString();
}
