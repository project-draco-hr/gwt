{
  if (entrySet == null) {
    HashSet set=new HashSet();
    set.add(new AbstractMapEntry<K,V>(){
      public K getKey(){
        return key;
      }
      public V getValue(){
        return value;
      }
      public V setValue(      V value){
        throw new UnsupportedOperationException();
      }
    }
);
    entrySet=unmodifiableSet(set);
  }
  return entrySet;
}
