{
  if (isInitializer(x)) {
    if (x.isStatic()) {
      print(CHARS_STATIC);
    }
    if (!shouldPrintMethodBody()) {
      print("{...}");
    }
  }
 else {
    printMethodHeader(x);
  }
  if (x.isAbstract() || !shouldPrintMethodBody()) {
    semi();
    newlineOpt();
  }
 else {
    accept(x.getBody());
  }
  return false;
}
