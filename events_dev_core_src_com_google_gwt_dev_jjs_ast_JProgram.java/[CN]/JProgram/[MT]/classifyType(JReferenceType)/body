{
  if (type instanceof JNullType) {
    return IS_NULL;
  }
 else   if (type instanceof JInterfaceType) {
    return IS_INTERFACE;
  }
 else   if (type instanceof JArrayType) {
    return IS_ARRAY;
  }
 else   if (type instanceof JClassType) {
    return IS_CLASS;
  }
  throw new InternalCompilerException("Unknown reference type");
}
