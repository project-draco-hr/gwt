{
  _do();
  _nestedPush(x.getBody(),true);
  x.getBody().traverse(this);
  _nestedPop(x.getBody());
  if (fNeedSemi) {
    _semi();
    _newlineOpt();
  }
 else {
    _spaceOpt();
    fNeedSemi=true;
  }
  _while();
  _spaceOpt();
  _lparen();
  x.getCondition().traverse(this);
  _rparen();
  return false;
}
