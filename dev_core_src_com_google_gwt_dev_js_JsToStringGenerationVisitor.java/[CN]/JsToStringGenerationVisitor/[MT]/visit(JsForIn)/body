{
  _for();
  _spaceOpt();
  _lparen();
  if (x.getIterVarName() != null) {
    _var();
    _space();
    _nameDef(x.getIterVarName());
    if (x.getIterExpr() != null) {
      _spaceOpt();
      _assignment();
      _spaceOpt();
      x.getIterExpr().traverse(this);
    }
  }
 else {
    x.getIterExpr().traverse(this);
  }
  _space();
  _in();
  _space();
  x.getObjExpr().traverse(this);
  _rparen();
  _nestedPush(x.getBody(),false);
  x.getBody().traverse(this);
  _nestedPop(x.getBody());
  return false;
}
