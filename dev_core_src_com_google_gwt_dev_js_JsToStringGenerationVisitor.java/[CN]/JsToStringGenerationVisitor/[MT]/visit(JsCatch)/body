{
  _spaceOpt();
  _catch();
  _spaceOpt();
  _lparen();
  _nameDef(x.getParameter().getName());
  JsExpression catchCond=x.getCondition();
  if (catchCond != null) {
    _space();
    _if();
    _space();
    catchCond.traverse(this);
  }
  _rparen();
  _spaceOpt();
  x.getBody().traverse(this);
