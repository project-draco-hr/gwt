{
  double dvalue=x.getValue();
  if (dvalue == 0.0 && 1.0 / dvalue == Double.NEGATIVE_INFINITY) {
    p.print("-0.");
    return false;
  }
  long lvalue=(long)dvalue;
  if (lvalue == dvalue) {
    p.print(Long.toString(lvalue));
  }
 else {
    p.print(Double.toString(dvalue));
  }
  return false;
}
