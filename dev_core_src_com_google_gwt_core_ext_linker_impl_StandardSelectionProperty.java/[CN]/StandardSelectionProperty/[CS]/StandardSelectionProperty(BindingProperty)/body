{
  if (p.getAllowedValues().length == 1) {
    activeValue=p.getAllowedValues()[0];
  }
 else {
    activeValue=null;
  }
  name=p.getName();
  provider=p.getProvider() == null ? null : p.getProvider().getBody();
  values=Collections.unmodifiableSortedSet(new TreeSet<String>(Arrays.asList(p.getDefinedValues())));
}
