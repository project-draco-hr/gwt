{
  String jsTry="try ";
  String jsCatch=" catch (e) {\n  __static[\"@" + Jsni.JAVASCRIPTHOST_NAME + "::exceptionCaught(Ljava/lang/Object;)\"](e);\n"+ "}\n";
  JsFunction func=jsniMethod.function(logger);
  if (func == null) {
    return null;
  }
  return jsTry + generateJavaScriptForHostedMode(func.getBody()) + jsCatch;
}
