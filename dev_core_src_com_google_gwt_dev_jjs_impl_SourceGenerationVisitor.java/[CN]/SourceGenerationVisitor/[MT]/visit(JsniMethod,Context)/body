{
  super.visit(x,ctx);
  space();
  print(CHARS_SLASHSTAR);
  String jsniCode=x.getFunc().getBody().toString();
  String[] splits=jsniCode.split("\r|\n");
  for (int i=0, c=splits.length; i < c; ++i) {
    if (i > 0) {
      newline();
    }
    print(splits[i]);
  }
  print(CHARS_STARSLASH);
  semi();
  return false;
}
