{
  if (id == null || "".equals(id)) {
    R selected=selectionModel.getSelectedObject();
    if (selected != null) {
      selectionModel.setSelected(selected,false);
    }
  }
 else {
    R record=idToRecord.get(id);
    if (record != null) {
      selectionModel.setSelected(record,true);
    }
  }
}
