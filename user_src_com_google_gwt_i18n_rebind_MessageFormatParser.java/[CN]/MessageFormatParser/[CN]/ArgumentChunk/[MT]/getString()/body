{
  StringBuilder buf=new StringBuilder();
  buf.append('{');
  buf.append(argNumber);
  if (format != null || subFormat != null) {
    buf.append(',');
  }
  if (format != null) {
    buf.append(format);
  }
  if (subFormat != null) {
    buf.append(',');
    buf.append(subFormat);
  }
  buf.append('}');
  return buf.toString();
}
