{
  if (dir == null) {
    return;
  }
  String path=dir.getPath();
  if (createdDirs.contains(path)) {
    return;
  }
  createdDirs.add(path);
  if (!dir.exists()) {
    mkdirs(dir.getParentFile());
    if (!dir.mkdir()) {
      throw new IOException("unable to create directory: " + dir.getAbsolutePath());
    }
  }
}
