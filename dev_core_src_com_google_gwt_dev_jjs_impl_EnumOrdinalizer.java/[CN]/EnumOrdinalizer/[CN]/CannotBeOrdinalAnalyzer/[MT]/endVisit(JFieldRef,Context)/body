{
  if (x.getField() == enumOrdinalField) {
    return;
  }
  if (x.getInstance() != null) {
    blackListIfEnumExpression(x.getInstance());
  }
 else   if (x.getField().isStatic()) {
    if (x.getField().getName().equals("$VALUES") && this.currentMethod.getEnclosingType() == x.getField().getEnclosingType()) {
      if (getEnumType(x.getField().getEnclosingType()) != null) {
        return;
      }
    }
    if (x.getField().isFinal() && (x.getField().getEnclosingType() == getEnumType(x.getField().getType()))) {
      return;
    }
    blackListIfEnum(x.getField().getEnclosingType());
  }
}
