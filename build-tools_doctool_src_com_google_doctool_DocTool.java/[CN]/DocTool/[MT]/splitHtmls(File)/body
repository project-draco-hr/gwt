{
  try {
    List args=new ArrayList();
    args.add("split");
    args.add(mergedHtmlsFile.getAbsolutePath());
    String[] argArray=(String[])args.toArray(new String[0]);
    traceCommand("SplitterJoiner",argArray);
    SplitterJoiner.main(argArray);
  }
 catch (  IOException e) {
    e.printStackTrace(fErr);
    return false;
  }
  return true;
}
