{
  if (this.open == open) {
    return;
  }
  this.open=open;
  if (open) {
    if (!nodeInfoLoaded) {
      nodeInfo=tree.getTreeViewModel().getNodeInfo(value,this);
      nodeInfoLoaded=true;
    }
    if (nodeInfo != null) {
      onOpen(nodeInfo);
    }
  }
 else {
    if (listReg != null) {
      listReg.removeHandler();
      listReg=null;
    }
    childContainer.setInnerHTML("");
    children.clear();
  }
  updateImage();
}
