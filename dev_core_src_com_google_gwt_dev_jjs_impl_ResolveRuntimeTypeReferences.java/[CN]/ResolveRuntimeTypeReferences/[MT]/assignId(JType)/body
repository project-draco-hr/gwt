{
  if (type instanceof JArrayType) {
    JArrayType arrayType=((JArrayType)type);
    JType leafType=arrayType.getLeafType();
    assignId(leafType);
    typeIdLiteralsByType.put(type,program.getStringLiteral(SourceOrigin.UNKNOWN,getTypeIdAsString(leafType) + Strings.repeat("[]",arrayType.getDims())));
    return;
  }
  assignIdImpl(type);
}
