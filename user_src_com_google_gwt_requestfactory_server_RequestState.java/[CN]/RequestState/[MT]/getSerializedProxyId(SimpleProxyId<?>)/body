{
  if (stableId.isEphemeral()) {
    return IdUtil.ephemeralId(stableId.getClientId(),service.getTypeToken(stableId.getProxyClass()));
  }
 else   if (stableId.isSynthetic()) {
    return IdUtil.syntheticId(stableId.getSyntheticId(),service.getTypeToken(stableId.getProxyClass()));
  }
 else {
    return IdUtil.persistedId(SimpleRequestProcessor.toBase64(stableId.getServerId()),service.getTypeToken(stableId.getProxyClass()));
  }
}
