{
  String typeToken=IdUtil.getTypeToken(serializedProxyId);
  SimpleProxyId<Q> id;
  if (IdUtil.isEphemeral(serializedProxyId)) {
    id=idFactory.getId(typeToken,null,IdUtil.getClientId(serializedProxyId));
  }
 else {
    id=idFactory.getId(typeToken,SimpleRequestProcessor.fromBase64(IdUtil.getServerId(serializedProxyId)));
  }
  return getBeanForPayload(id);
}
