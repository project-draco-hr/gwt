{
  String outputModuleName=module.getName();
  try {
    String stub=PageUtil.loadResource(Recompiler.class,"recompile.nocache.js");
    return "(function() {\n" + " var moduleName = '" + outputModuleName + "';\n"+ PropertiesUtil.generatePropertiesSnippet(module,compileLogger)+ stub+ "})();\n";
  }
 catch (  IOException e) {
    compileLogger.log(Type.ERROR,"Can not generate + " + outputModuleName + " + .recompile.nocache.js",e);
    throw new UnableToCompleteException();
  }
}
