{
  while (true) {
    if (len >= remainder) {
      System.arraycopy(input,offset,buffer,(int)(counter & 63L),remainder);
      transform(buffer);
      counter+=remainder;
      offset+=remainder;
      len-=remainder;
      remainder=64;
    }
 else {
      System.arraycopy(input,offset,buffer,(int)(counter & 63L),len);
      counter+=len;
      remainder-=len;
      break;
    }
  }
}
