{
  final StringBuffer b=new StringBuffer("<");
  b.append(getTagName());
  if (hasAttributes()) {
    b.append(getAttributes().toString());
  }
  if (hasChildNodes()) {
    b.append(">");
    b.append(getChildNodes().toString());
    b.append("</");
    b.append(getTagName());
    b.append(">");
  }
 else {
    b.append("/>");
  }
  return b.toString();
}
