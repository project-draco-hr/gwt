{
  assertSame(RPCServletUtils.getCharset(null),RPCServletUtils.getCharset(null));
  assertSame(RPCServletUtils.getCharset("UTF-8"),RPCServletUtils.getCharset("UTF-8"));
  assertSame(RPCServletUtils.getCharset("US-ASCII"),RPCServletUtils.getCharset("US-ASCII"));
  assertSame(RPCServletUtils.getCharset("ISO-8859-1"),RPCServletUtils.getCharset("ISO-8859-1"));
}
