{
  String curLine;
  curLine="<members>";
  htmlOut.printRawOpt(curLine);
  htmlOut.newlineOpt();
  htmlOut.indentIn();
  htmlOut.indentIn();
  curNumIndents++;
  SortedMap<String,Set<ClassMember>> packageToClasses=new TreeMap<String,Set<ClassMember>>();
  emitPackages(report,htmlOut,packageToClasses);
  emitFunctions(report,htmlOut);
  htmlOut.indentOut();
  htmlOut.indentOut();
  curNumIndents--;
  curLine="</members>";
  htmlOut.printRawOpt(curLine);
  htmlOut.newlineOpt();
}
