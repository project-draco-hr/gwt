{
  if (o == this) {
    return true;
  }
  if (!(o instanceof Set)) {
    return false;
  }
  Set<?> other=(Set<?>)o;
  if (other.size() != size()) {
    return false;
  }
  for (Iterator<?> iter=other.iterator(); iter.hasNext(); ) {
    Object otherItem=iter.next();
    if (!contains(otherItem)) {
      return false;
    }
  }
  return true;
}
