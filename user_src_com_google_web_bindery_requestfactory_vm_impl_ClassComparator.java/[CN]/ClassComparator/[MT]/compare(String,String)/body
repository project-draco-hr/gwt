{
  Class<?> class1=forName(className1);
  Class<?> class2=forName(className2);
  if (class1.equals(class2)) {
    return 0;
  }
 else   if (class1.isAssignableFrom(class2)) {
    return 1;
  }
 else   if (class2.isAssignableFrom(class1)) {
    return -1;
  }
  return className1.compareTo(className2);
}
