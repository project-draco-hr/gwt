{
switch (variant.getType()) {
case COM.VT_BOOL:
    return "boolean";
case COM.VT_I1:
case COM.VT_I2:
case COM.VT_I4:
case COM.VT_I8:
case COM.VT_UI1:
case COM.VT_UI2:
case COM.VT_UI4:
case COM.VT_R4:
case COM.VT_R8:
  return "number";
case COM.VT_BSTR:
return "string";
case COM.VT_EMPTY:
return "undefined";
case COM.VT_NULL:
return "null";
case COM.VT_DISPATCH:
return isWrappedJavaObject() ? "Java Object" : "JavaScript object";
default :
return "unexpected Variant type";
}
}
