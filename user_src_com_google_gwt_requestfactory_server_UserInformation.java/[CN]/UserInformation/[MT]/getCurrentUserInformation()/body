{
  UserInformation userInfo=null;
  if (!userInformationImplClass.isEmpty()) {
    try {
      userInfo=(UserInformation)Class.forName(userInformationImplClass).newInstance();
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
  if (userInfo == null) {
    userInfo=new UserInformationSimpleImpl();
  }
  return userInfo;
}
