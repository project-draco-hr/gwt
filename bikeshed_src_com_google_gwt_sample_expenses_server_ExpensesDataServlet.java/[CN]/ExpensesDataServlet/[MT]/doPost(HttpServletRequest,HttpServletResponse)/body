{
  response.setStatus(HttpServletResponse.SC_OK);
  MethodName methodName=getMethodName(request.getParameter("methodName"));
  PrintWriter writer=response.getWriter();
switch (methodName) {
case SYNC:
    sync(request,writer);
  break;
default :
System.err.println("POST: unknown method " + methodName);
break;
}
writer.flush();
}
