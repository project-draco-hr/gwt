{
  if (jprogram.typeOracle.isExportedField(x)) {
    boolean success=minimalRebuildCache.addExportedGlobalName(x.getQualifiedExportName(),currentType.getName());
    if (!success) {
      logError("Static field '%s' can't be exported because the global name '%s' is already " + "taken.",x.getQualifiedName(),x.getQualifiedExportName());
    }
  }
 else   if (jprogram.typeOracle.isJsTypeField(x)) {
    boolean success=addJsTypeMemberName(x.getName());
    if (!success) {
      logError("Instance field '%s' can't be exported because the member name '%s' is already " + "taken.",x.getQualifiedName(),x.getName());
    }
  }
  return false;
}
