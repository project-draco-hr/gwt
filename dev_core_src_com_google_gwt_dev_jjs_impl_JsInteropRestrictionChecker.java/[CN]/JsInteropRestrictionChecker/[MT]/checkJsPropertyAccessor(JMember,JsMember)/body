{
  if (member.getJsMemberType() == JsMemberType.UNDEFINED_ACCESSOR) {
    logError(member,"JsProperty %s should have a correct setter or getter signature.",getMemberDescription(member));
  }
  if (member.getJsMemberType() == JsMemberType.GETTER) {
    if (member.getType() != JPrimitiveType.BOOLEAN && member.getName().startsWith("is")) {
      logError(member,"JsProperty %s cannot have a non-boolean return.",getMemberDescription(member));
    }
  }
  if (newMember.setter != null && newMember.getter != null) {
    List<JParameter> setterParams=((JMethod)newMember.setter).getParams();
    if (newMember.getter.getType() != setterParams.get(0).getType()) {
      logError(member,"JsProperty setter %s and getter %s cannot have inconsistent types.",getMemberDescription(newMember.setter),getMemberDescription(newMember.getter));
    }
  }
}
