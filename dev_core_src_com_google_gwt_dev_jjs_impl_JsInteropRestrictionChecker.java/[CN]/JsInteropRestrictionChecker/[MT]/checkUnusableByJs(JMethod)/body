{
  if (!method.canBeCalledExternally() || isUnusableByJsSuppressed(method.getEnclosingType()) || isUnusableByJsSuppressed(method)) {
    return;
  }
  for (  JParameter parameter : method.getParams()) {
    if (!parameter.getType().canBeReferencedExternally() && !isUnusableByJsSuppressed(parameter)) {
      logWarning(parameter,"[unusable-by-js] Type of parameter '%s' in method %s is not usable by but exposed to" + " JavaScript.",parameter.getName(),getMemberDescription(method));
    }
  }
  if (!method.getType().canBeReferencedExternally()) {
    logWarning(method,"[unusable-by-js] Return type of %s is not usable by but exposed to JavaScript.",getMemberDescription(method));
  }
}
