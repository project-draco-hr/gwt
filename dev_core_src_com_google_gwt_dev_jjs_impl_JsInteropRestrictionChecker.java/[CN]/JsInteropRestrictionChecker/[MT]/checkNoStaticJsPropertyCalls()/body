{
  new JVisitor(){
    @Override public void endVisit(    JMethodCall x,    Context ctx){
      JMethod target=x.getTarget();
      if (x.isStaticDispatchOnly() && target.isJsPropertyAccessor()) {
        logError("Cannot call property accessor '%s' via super (%s:%d).",target.getQualifiedName(),x.getSourceInfo().getFileName(),x.getSourceInfo().getStartLine());
      }
    }
  }
.accept(jprogram);
}
