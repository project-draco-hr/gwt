{
  new JVisitor(){
    @Override public boolean visit(    JInstanceOf x,    Context ctx){
      JReferenceType type=x.getTestType();
      if (type.isJsNative() && type instanceof JInterfaceType) {
        logError("Cannot do instanceof against native JsType interface %s (%s:%d).",type.getName(),x.getSourceInfo().getFileName(),x.getSourceInfo().getStartLine());
      }
      return true;
    }
  }
.accept(jprogram);
}
