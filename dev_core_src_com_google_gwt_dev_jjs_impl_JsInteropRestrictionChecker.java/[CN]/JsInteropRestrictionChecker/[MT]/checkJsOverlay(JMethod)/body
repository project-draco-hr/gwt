{
  if (method.getEnclosingType().isJsoType()) {
    return;
  }
  String methodDescription=JjsUtils.getReadableDescription(method);
  if (!method.getEnclosingType().isJsNative()) {
    logError(method,"Method '%s' in non-native type cannot be @JsOverlay.",methodDescription);
  }
  if (!method.getOverriddenMethods().isEmpty()) {
    logError(method,"JsOverlay method '%s' cannot override a supertype method.",methodDescription);
    return;
  }
  if (method.isJsNative() || (!method.isFinal() && !method.isStatic())) {
    logError(method,"JsOverlay method '%s' cannot be non-final nor native.",methodDescription);
  }
}
