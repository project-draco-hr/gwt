{
  if (x.getEnclosingType().isJsNative()) {
    checkMemberOfNativeJsType(x);
  }
  checkUnusableByJs(x);
  if (!x.isJsProperty()) {
    return;
  }
  if (x.needsDynamicDispatch()) {
    checkLocalName(x);
  }
 else   if (!x.isJsNative() && currentType == x.getEnclosingType()) {
    checkGlobalName(x);
  }
}
