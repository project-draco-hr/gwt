{
  int lastNum=r.nextInt();
  int nextNum;
  boolean someDifferent=false;
  for (int counter=0; counter < 100; counter++) {
    nextNum=r.nextInt();
    if (nextNum != lastNum) {
      someDifferent=true;
    }
    lastNum=nextNum;
  }
  assertTrue("Calling nextInt 100 times resulted in same number",someDifferent);
}
