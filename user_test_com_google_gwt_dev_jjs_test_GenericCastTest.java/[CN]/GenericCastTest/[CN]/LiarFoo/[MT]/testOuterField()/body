{
  new Runnable(){
    @Override public void run(){
      Object a=value;
      try {
        Foo b=value;
        fail("Expected ClassCastException 1");
      }
 catch (      ClassCastException expected) {
      }
      try {
        String s=value.bar;
        fail("Expected ClassCastException 2");
      }
 catch (      ClassCastException expected) {
      }
      try {
        String s=value.baz();
        fail("Expected ClassCastException 3");
      }
 catch (      ClassCastException expected) {
      }
    }
  }
.run();
}
