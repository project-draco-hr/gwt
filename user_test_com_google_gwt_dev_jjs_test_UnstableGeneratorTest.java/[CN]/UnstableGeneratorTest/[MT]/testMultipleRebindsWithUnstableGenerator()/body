{
  String firstResult=GWT.<UnstableResult>create(UnstableResult.class).get();
  assertEquals(firstResult,GWT.<UnstableResult>create(UnstableResult.class).get());
  assertEquals(firstResult,GWT.<UnstableResult>create(UnstableResult.class).get());
  assertEquals(firstResult,GWT.<UnstableResult>create(UnstableResult.class).get());
  assertEquals(firstResult,GWT.<UnstableResult>create(UnstableResult.class).get());
}
