{
  final List<Boolean> called=new ArrayList<>();
  DeckLayoutPanel deck=createPanel();
  SimpleLayoutPanel child=new SimpleLayoutPanel(){
    @Override public void onResize(){
      super.onResize();
      called.add(true);
    }
  }
;
  deck.add(child);
  deck.forceLayout();
  assertEquals(1,called.size());
}
