{
  regExp=RegExp.compile("(([ab])X\\2)([-,])\\1\\3\\2");
  checkExec("_aXa-aXa-a_",1,"aXa-aXa-a","aXa","a","-");
  checkExec("_aXa,aXa,a_",1,"aXa,aXa,a","aXa","a",",");
  checkExec("_bXb-bXb-b_",1,"bXb-bXb-b","bXb","b","-");
  checkExecNoMatch("_aXa-bXb-a_");
  checkExecNoMatch("_aXa,aXa,b_");
  checkExecNoMatch("_aXa,aXa-a_");
}
