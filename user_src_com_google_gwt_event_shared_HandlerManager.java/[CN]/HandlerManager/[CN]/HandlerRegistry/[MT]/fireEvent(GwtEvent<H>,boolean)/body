{
  Type<H> type=event.getAssociatedType();
  int count=getHandlerCount(type);
  if (isReverseOrder) {
    for (int i=count - 1; i >= 0; i--) {
      H handler=this.<H>getHandler(type,i);
      event.dispatch(handler);
    }
  }
 else {
    for (int i=0; i < count; i++) {
      H handler=this.<H>getHandler(type,i);
      event.dispatch(handler);
    }
  }
}
