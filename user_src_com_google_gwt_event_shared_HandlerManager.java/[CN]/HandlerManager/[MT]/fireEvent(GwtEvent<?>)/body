{
  if (!event.isLive()) {
    event.revive();
  }
  Object oldSource=event.getSource();
  event.setSource(source);
  try {
    firingDepth++;
    registry.fireEvent(event,isReverseOrder);
  }
  finally {
    firingDepth--;
    if (firingDepth == 0) {
      handleQueuedAddsAndRemoves();
    }
    if (oldSource == null) {
      event.kill();
    }
 else {
      event.setSource(oldSource);
    }
  }
}
