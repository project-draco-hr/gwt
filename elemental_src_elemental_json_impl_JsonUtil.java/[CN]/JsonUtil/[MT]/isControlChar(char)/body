{
  return (c >= 0x00 && c <= 0x1f) || (c >= 0x7f && c <= 0x9f) || c == '\u00ad' || c == '\u070f' || c == '\u17b4' || c == '\u17b5' || c == '\ufeff' || (c >= '\u0600' && c <= '\u0604') || (c >= '\u200c' && c <= '\u200f') || (c >= '\u2028' && c <= '\u202f') || (c >= '\u2060' && c <= '\u206f') || (c >= '\ufff0' && c <= '\uffff');
}
