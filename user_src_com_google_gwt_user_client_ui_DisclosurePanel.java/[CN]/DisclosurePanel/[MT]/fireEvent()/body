{
  if (handlers == null) {
    return;
  }
  DisclosureEvent event=new DisclosureEvent(this);
  for (Iterator it=handlers.iterator(); it.hasNext(); ) {
    DisclosureHandler handler=(DisclosureHandler)it.next();
    if (isOpen) {
      handler.onOpen(event);
    }
 else {
      handler.onClose(event);
    }
  }
}
