{
  try {
    if (moduleName == null) {
      onPageUnload();
      return true;
    }
    ModuleSpaceHost msh=getHost().createModuleSpaceHost(BrowserWidgetMoz.this,moduleName);
    ModuleSpace moduleSpace=new ModuleSpaceMoz(msh,scriptObject);
    attachModuleSpace(moduleName,moduleSpace);
    return true;
  }
 catch (  Throwable e) {
    getHost().getLogger().log(TreeLogger.ERROR,"Failure to load module '" + moduleName + "'",e);
    return false;
  }
}
