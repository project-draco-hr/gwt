{
  if (moduleName == null) {
    return handleUnload(scriptObject);
  }
  TreeLogger logger=getHost().getLogger().branch(TreeLogger.DEBUG,"Loading an instance of module '" + moduleName + "'");
  try {
    if (!validHostedHtmlVersion(version)) {
      return false;
    }
    Object key=new Integer(scriptObject);
    ModuleSpaceHost msh=getHost().createModuleSpaceHost(logger,BrowserWidgetMoz.this,moduleName);
    ModuleSpace moduleSpace=new ModuleSpaceMoz(logger,msh,scriptObject,moduleName,key);
    attachModuleSpace(logger,moduleSpace);
    return true;
  }
 catch (  Throwable e) {
    logger.log(TreeLogger.ERROR,"Failure to load module '" + moduleName + "'",e);
    return false;
  }
}
