{
  List<Column<EmployeeRecord,?,?>> columns=new ArrayList<Column<EmployeeRecord,?,?>>();
  columns.add(new TextColumn<EmployeeRecord>(){
    @Override public String getValue(    EmployeeRecord object){
      return object.getUserName();
    }
  }
);
  columns.add(new TextColumn<EmployeeRecord>(){
    @Override public String getValue(    EmployeeRecord object){
      return object.getDisplayName();
    }
  }
);
  columns.add(new IdentityColumn<EmployeeRecord>(new ActionCell<EmployeeRecord>("Show",places.<EmployeeRecord>getDetailsGofer())));
  return columns;
}
