{
  if (frag1 == frag2) {
    return frag1;
  }
  if (frag1 == 0) {
    return 0;
  }
  if (frag2 == 0) {
    return 0;
  }
  int initPos1=initialSeq.indexOf(frag1);
  int initPos2=initialSeq.indexOf(frag2);
  if (initPos1 >= 0 && initPos2 >= 0) {
    if (initPos1 < initPos2) {
      return frag1;
    }
    return frag2;
  }
  if (initPos1 >= 0) {
    return frag1;
  }
  if (initPos2 >= 0) {
    return frag2;
  }
  assert(initPos1 < 0 && initPos2 < 0);
  assert(frag1 != frag2);
  return CodeSplitter.getLeftoversFragmentNumber(numSps);
}
