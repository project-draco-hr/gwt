{
  int eventKind=OS.GetEventKind(theEvent);
switch (eventKind) {
case OS.kEventRawKeyDown:
    return kEventRawKeyDown(nextHandler,theEvent,userData);
case OS.kEventRawKeyModifiersChanged:
  return kEventRawKeyModifiersChanged(nextHandler,theEvent,userData);
case OS.kEventRawKeyRepeat:
return kEventRawKeyRepeat(nextHandler,theEvent,userData);
case OS.kEventRawKeyUp:
return kEventRawKeyUp(nextHandler,theEvent,userData);
}
return OS.eventNotHandledErr;
}
