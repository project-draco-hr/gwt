{
  DataOutputStream stream=channel.getStreamToOtherSide();
  stream.writeByte(MessageType.USER_AGENT_ICON.getId());
  if (iconBytes == null) {
    stream.writeInt(0);
  }
 else {
    stream.writeInt(iconBytes.length);
    for (    byte b : iconBytes) {
      stream.writeByte(b);
    }
  }
  stream.flush();
}
