def __init__(self, auxiliary_dir, template_dir, base_package):
    'Constructor for the DartGenerator.\n\n    Args:\n      auxiliary_dir -- location of auxiliary handwritten classes\n      template_dir -- location of template files\n      base_package -- the base package name for the generated code.\n    '
    self._auxiliary_dir = auxiliary_dir
    self._template_dir = template_dir
    self._base_package = base_package
    self._auxiliary_files = {}
    self._dart_templates_re = re.compile('[\\w.:]+<([\\w\\.<>:]+)>')
    self._emitters = None
