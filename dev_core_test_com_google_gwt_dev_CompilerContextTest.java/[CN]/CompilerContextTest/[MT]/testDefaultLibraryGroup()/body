{
  CompilerContext compilerContext=new CompilerContext();
  assertFalse(compilerContext.getLibraryGroup().containsBuildResource("com/google/gwt/Foo.xml"));
  compilerContext=new CompilerContext.Builder().build();
  assertFalse(compilerContext.getLibraryGroup().containsBuildResource("com/google/gwt/Foo.xml"));
}
