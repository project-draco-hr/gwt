{
  JMethod currentMethod=getCurrentMethod();
  if (program.codeGenTypes.contains(currentMethod.getEnclosingType()) || program.typeOracle.isExportedMethod(currentMethod) || program.typeOracle.isJsFunctionMethod(currentMethod)|| program.typeOracle.isJsTypeMethod(currentMethod)) {
    return;
  }
  tighten(x);
}
