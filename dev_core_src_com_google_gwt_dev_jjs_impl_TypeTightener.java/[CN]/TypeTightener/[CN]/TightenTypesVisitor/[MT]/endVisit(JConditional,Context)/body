{
  if (x.getType() instanceof JReferenceType) {
    JReferenceType refType=(JReferenceType)x.getType();
    JReferenceType resultType=program.strengthenType(refType,Arrays.asList((JReferenceType)x.getThenExpr().getType(),(JReferenceType)x.getElseExpr().getType()));
    if (refType != resultType) {
      x.setType(resultType);
      madeChanges();
    }
  }
}
