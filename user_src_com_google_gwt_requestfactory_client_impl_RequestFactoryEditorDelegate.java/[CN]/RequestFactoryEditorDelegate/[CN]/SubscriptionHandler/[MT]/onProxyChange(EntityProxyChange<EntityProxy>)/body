{
  if (event.getWriteOperation().equals(WriteOperation.UPDATE) && event.getProxy().stableId().equals(((EntityProxy)getObject()).stableId())) {
    List<String> paths=new ArrayList<String>();
    traverse(paths);
    EntityProxyId id=event.getProxy().stableId();
    factory.find(id).with(paths.toArray(new String[paths.size()])).fire(new SubscriptionReceiver());
  }
}
