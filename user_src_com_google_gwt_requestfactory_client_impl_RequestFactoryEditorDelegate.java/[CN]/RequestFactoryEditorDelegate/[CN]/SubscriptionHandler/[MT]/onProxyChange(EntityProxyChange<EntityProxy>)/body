{
  if (event.getWriteOperation().equals(WriteOperation.UPDATE) && event.getProxyId().equals(((EntityProxy)getObject()).stableId())) {
    List<String> paths=new ArrayList<String>();
    traverse(paths);
    @SuppressWarnings("rawtypes") EntityProxyId id=event.getProxyId();
    doFind(paths,id);
  }
}
