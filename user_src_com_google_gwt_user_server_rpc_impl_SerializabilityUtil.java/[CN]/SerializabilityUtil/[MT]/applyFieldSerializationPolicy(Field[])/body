{
  ArrayList<Field> fieldList=new ArrayList<Field>();
  for (  Field field : fields) {
    assert(field != null);
    int fieldModifiers=field.getModifiers();
    if (Modifier.isStatic(fieldModifiers) || Modifier.isTransient(fieldModifiers) || Modifier.isFinal(fieldModifiers)) {
      continue;
    }
    fieldList.add(field);
  }
  Field[] fieldSubset=fieldList.toArray(new Field[fieldList.size()]);
  Comparator<Field> comparator=new Comparator<Field>(){
    public int compare(    Field f1,    Field f2){
      return f1.getName().compareTo(f2.getName());
    }
  }
;
  Arrays.sort(fieldSubset,0,fieldSubset.length,comparator);
  return fieldSubset;
}
