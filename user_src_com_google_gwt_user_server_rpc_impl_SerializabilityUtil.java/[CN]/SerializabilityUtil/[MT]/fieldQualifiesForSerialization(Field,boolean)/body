{
  if (Throwable.class == field.getDeclaringClass()) {
    if ("detailMessage".equals(field.getName())) {
      assert(isNotStaticOrTransient(field));
      return true;
    }
 else {
      return false;
    }
  }
 else {
    return isNotStaticOrTransient(field) && (includeFinal || isNotFinal(field));
  }
}
