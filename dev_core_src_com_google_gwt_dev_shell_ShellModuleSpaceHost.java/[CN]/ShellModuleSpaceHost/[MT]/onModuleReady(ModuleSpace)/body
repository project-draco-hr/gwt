{
  this.space=readySpace;
  ModuleSpacePropertyOracle propOracle=new ModuleSpacePropertyOracle(module.getProperties(),readySpace);
  Rules rules=module.getRules();
  rebindOracle=new StandardRebindOracle(module.getCompilationState(logger),propOracle,module,rules,genDir,shellDir,new ArtifactSet());
  classLoader=new CompilingClassLoader(logger,module.getCompilationState(logger),readySpace);
}
