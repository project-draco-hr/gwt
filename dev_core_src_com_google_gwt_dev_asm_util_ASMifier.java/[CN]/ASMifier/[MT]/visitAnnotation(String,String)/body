{
  buf.setLength(0);
  buf.append("{\n");
  buf.append("AnnotationVisitor av").append(id + 1).append(" = av");
  buf.append(id).append(".visitAnnotation(");
  appendConstant(buf,name);
  buf.append(", ");
  appendConstant(buf,desc);
  buf.append(");\n");
  text.add(buf.toString());
  ASMifier a=createASMifier("av",id + 1);
  text.add(a.getText());
  text.add("}\n");
  return a;
}
