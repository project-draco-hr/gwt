{
  JdtCompiler.setRemoveUnusedImports(false);
  assertUnitHasErrors("com.example.UnusedImports","package com.example;","import java.util.BlahBlahBlah;","public class UnusedImports {","}");
  JdtCompiler.setRemoveUnusedImports(true);
  assertUnitCompilesWithNoErrors("com.example.UnusedImports","package com.example;","import java.util.BlahBlahBlah;","public class UnusedImports {","}");
}
