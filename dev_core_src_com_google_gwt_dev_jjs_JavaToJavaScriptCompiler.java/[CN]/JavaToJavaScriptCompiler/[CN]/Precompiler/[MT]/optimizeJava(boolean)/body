{
  if (options.getOptimizationLevel() > OptionOptimize.OPTIMIZE_LEVEL_DRAFT && !singlePermutation) {
    if (options.isOptimizePrecompile()) {
      optimizeJavaToFixedPoint();
    }
 else {
      optimizeJavaOneTime("Early Optimization",jprogram.getNodeCount(),new FullOptimizerContext(jprogram));
    }
  }
}
