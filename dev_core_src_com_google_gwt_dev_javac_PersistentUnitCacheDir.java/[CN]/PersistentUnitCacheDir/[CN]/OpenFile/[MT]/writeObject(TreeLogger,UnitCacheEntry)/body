{
  CompilationUnit unit=Preconditions.checkNotNull(entry.getUnit());
  try {
    stream.writeObject(unit);
    unitsWritten++;
    return true;
  }
 catch (  IOException e) {
    logger.log(TreeLogger.ERROR,"Error saving compilation unit to cache file: " + file,e);
    throw new UnableToCompleteException();
  }
}
