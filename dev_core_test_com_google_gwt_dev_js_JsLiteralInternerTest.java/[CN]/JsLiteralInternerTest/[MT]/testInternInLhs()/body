{
  checkTranslation(String.format("var a = %1$s;a[%1$s]++;",AS_STRING),String.format("var $intern_0=%1$s;var a=$intern_0;a[$intern_0]++;",AS_STRING));
  checkTranslation(String.format("var a = %1$s;++a[%1$s];",AS_STRING),String.format("var $intern_0=%1$s;var a=$intern_0;++a[$intern_0];",AS_STRING));
}
