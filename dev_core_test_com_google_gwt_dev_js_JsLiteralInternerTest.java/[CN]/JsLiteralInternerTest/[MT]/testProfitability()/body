{
  checkTranslation(String.format("var x = %1$s, y = %1$s, z = %1$s;",SMALL_BUT_INTERNABLE),String.format("var x=%1$s,y=%1$s,z=%1$s;",SMALL_BUT_INTERNABLE));
  checkTranslation(String.format("var x = %1$s, y = %1$s, z = %1$s, z = %1$s, z = %1$s, z = %1$s, z = %1$s, " + "z = %1$s;",SMALL_BUT_INTERNABLE),String.format("var $intern_0=%1$s;var x=$intern_0,y=$intern_0,z=$intern_0,z=$intern_0," + "z=$intern_0,z=$intern_0,z=$intern_0,z=$intern_0;",SMALL_BUT_INTERNABLE));
  checkTranslation(String.format("var x = %1$s, y = %1$s, z = %1$s;",BIG),String.format("var $intern_0=%1$s;var x=$intern_0,y=$intern_0,z=$intern_0;",BIG));
}
