{
  if (param.getType() == objectType) {
    logger.branch(TreeLogger.ERROR,"In order to produce smaller client-side code, methods cannot specify 'Object' for parameter types; please choose a more specific parameter type",null);
  }
 else {
    logger.branch(TreeLogger.ERROR,"Parameter '" + param.getName() + "' of type '"+ param.getType().getParameterizedQualifiedSourceName()+ "' is not serializable and/or has no serializable subtypes",null);
  }
}
