{
  if (returnType == objectType) {
    logger.branch(TreeLogger.ERROR,"In order to produce smaller client-side code, methods cannot specify 'Object' for return types; please choose a more specific return type",null);
  }
 else {
    logger.branch(TreeLogger.ERROR,"Return type " + returnType.getParameterizedQualifiedSourceName() + " is not serializable and/or has no serializable subtypes",null);
  }
}
