{
  contents.add(widget);
  if (widget instanceof Editor<?>) {
    @SuppressWarnings("unchecked") E isEditor=(E)widget;
    setEditor(isEditor);
  }
 else   if (widget instanceof IsEditor<?>) {
    @SuppressWarnings("unchecked") E isEditor=((IsEditor<E>)widget).asEditor();
    setEditor(isEditor);
  }
}
