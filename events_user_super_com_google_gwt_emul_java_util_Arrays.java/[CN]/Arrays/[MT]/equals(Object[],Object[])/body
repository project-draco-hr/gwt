{
  if (array1 == array2) {
    return true;
  }
  if (array1 == null || array2 == null) {
    return false;
  }
  if (array1.length != array2.length) {
    return false;
  }
  for (int i=0; i < array1.length; ++i) {
    Object val1=array1[i];
    Object val2=array2[i];
    if (!val1.equals(val2)) {
      return false;
    }
  }
  return true;
}
