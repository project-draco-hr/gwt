{
  if (isFullyInitialized()) {
    throw new IllegalStateException("wrapElement() cannot be called twice, or after a call to getElement()" + "has forced the widget to render itself (e.g. after adding it to a" + "panel)");
  }
  setElement(element);
  html=null;
  if (wrapInitializationCallback != null) {
    wrapInitializationCallback.execute();
    wrapInitializationCallback=null;
  }
}
