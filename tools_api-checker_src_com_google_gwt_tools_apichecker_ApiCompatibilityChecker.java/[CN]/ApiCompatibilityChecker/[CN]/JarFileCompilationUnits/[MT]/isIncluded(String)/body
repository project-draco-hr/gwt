{
  int index=fileName.length();
  do {
    fileName=fileName.substring(0,index);
    if (includedPaths.contains(fileName)) {
      return true;
    }
    if (excludedPaths.contains(fileName)) {
      return false;
    }
    index=fileName.lastIndexOf("/");
  }
 while (index != -1);
  return false;
}
