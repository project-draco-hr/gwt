{
  Object o1=new Object();
  Object o2=new Object();
  LinkedList<Object> l=new LinkedList<Object>();
  try {
    l.remove();
    fail();
  }
 catch (  NoSuchElementException e) {
  }
  l.add(o1);
  assertEquals(o1,l.remove());
  assertTrue(l.isEmpty());
  l.add(o1);
  l.add(o2);
  assertEquals(o1,l.remove());
  checkListSizeAndContent(l,o2);
}
