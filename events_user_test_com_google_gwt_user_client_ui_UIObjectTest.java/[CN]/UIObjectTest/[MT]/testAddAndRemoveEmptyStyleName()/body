{
  MyObject o=new MyObject();
  o.setStylePrimaryName("primary");
  try {
    o.addStyleName("");
    fail();
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    o.addStyleName(" ");
    fail();
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    o.removeStyleName("");
    fail();
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    o.removeStyleName(" ");
    fail();
  }
 catch (  IllegalArgumentException e) {
  }
  assertEquals("primary",o.getStylePrimaryName());
}
